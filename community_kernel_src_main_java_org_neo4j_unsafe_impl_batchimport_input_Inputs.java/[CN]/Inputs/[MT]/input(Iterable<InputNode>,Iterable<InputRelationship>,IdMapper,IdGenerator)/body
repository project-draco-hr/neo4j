{
  final ResourceIterable<InputNode> resourceNodes=asResourceIterable(nodes);
  final ResourceIterable<InputRelationship> resourceRelationships=asResourceIterable(relationships);
  return new Input(){
    @Override public ResourceIterable<InputRelationship> relationships(){
      return resourceRelationships;
    }
    @Override public ResourceIterable<InputNode> nodes(){
      return resourceNodes;
    }
    @Override public IdMapper idMapper(){
      return idMapper;
    }
    @Override public IdGenerator idGenerator(){
      return idGenerator;
    }
  }
;
}
