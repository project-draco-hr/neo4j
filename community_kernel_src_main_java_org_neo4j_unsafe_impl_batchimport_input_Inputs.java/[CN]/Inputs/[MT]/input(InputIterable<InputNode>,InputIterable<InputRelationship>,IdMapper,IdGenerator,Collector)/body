{
  return new Input(){
    @Override public InputIterable<InputRelationship> relationships(){
      return relationships;
    }
    @Override public InputIterable<InputNode> nodes(){
      return nodes;
    }
    @Override public IdMapper idMapper(){
      return idMapper;
    }
    @Override public IdGenerator idGenerator(){
      return idGenerator;
    }
    @Override public Collector badCollector(){
      return badCollector;
    }
  }
;
}
