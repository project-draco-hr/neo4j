{
  return new Input(){
    @Override public InputIterable<InputRelationship> relationships(){
      return relationships;
    }
    @Override public InputIterable<InputNode> nodes(){
      return nodes;
    }
    @Override public IdMapper idMapper(){
      return idMapper;
    }
    @Override public IdGenerator idGenerator(){
      return idGenerator;
    }
    @Override public boolean specificRelationshipIds(){
      return specificRelationshipIds;
    }
    @Override public Collector<InputRelationship> badRelationshipsCollector(    OutputStream out){
      return Collectors.badRelationshipsCollector(out,badTolerance);
    }
  }
;
}
