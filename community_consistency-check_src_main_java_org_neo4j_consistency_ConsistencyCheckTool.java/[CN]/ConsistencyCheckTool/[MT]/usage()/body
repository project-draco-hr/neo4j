{
  return joinAsLines(jarUsage(getClass(),"[-propowner] [-config <neo4j.conf>] [-v] <storedir>"),"WHERE:   -propowner          also check property owner consistency (more time consuming)","         -config <filename>  is the location of an optional properties file","                             containing tuning parameters for the consistency check","         -v                  produce execution output","         <storedir>          is the path to the store to check");
}
