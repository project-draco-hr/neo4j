{
  NodeRecord before=new NodeRecord(nodeId,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue());
  before.setInUse(false);
  NodeRecord after=new NodeRecord(nodeId,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue());
  after.setInUse(true);
  InlineNodeLabels nodeLabels=new InlineNodeLabels(-1,after);
  nodeLabels.put(new long[]{labelId},neoStore.getNodeStore());
  return new Command.NodeCommand(neoStore.getNodeStore(),before,after);
}
