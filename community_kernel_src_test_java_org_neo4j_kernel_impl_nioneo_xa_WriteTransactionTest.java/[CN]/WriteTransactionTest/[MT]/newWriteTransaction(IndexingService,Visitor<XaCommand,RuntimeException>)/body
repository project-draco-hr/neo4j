{
  VerifyingXaLogicalLog log=new VerifyingXaLogicalLog(fs.get(),verifier);
  WriteTransaction result=new WriteTransaction(0,0l,log,transactionState,neoStore,cacheAccessBackDoor,indexing,NO_LABEL_SCAN_STORE,new IntegrityValidator(neoStore,indexing),kernelTransaction,locks);
  result.setCommitTxId(neoStore.getLastCommittedTx() + 1);
  return result;
}
