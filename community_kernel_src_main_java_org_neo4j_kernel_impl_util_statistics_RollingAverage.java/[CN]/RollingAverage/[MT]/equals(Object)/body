{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  RollingAverage other=(RollingAverage)o;
  return populatedWindows == other.populatedWindows && parameters.equals(other.parameters) && ArrayUtil.approximatelyEqual(averages,other.averages,parameters.equalityTolerance);
}
