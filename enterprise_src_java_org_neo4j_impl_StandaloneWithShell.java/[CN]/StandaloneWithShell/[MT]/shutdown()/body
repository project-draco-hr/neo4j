{
  if (shutdownInitiated.compareAndSet(false,true)) {
    System.out.println("Shutting down...");
    try {
      if (getNeo() != null) {
        getNeo().shutdown();
        this.embeddedNeo=null;
      }
    }
 catch (    Throwable t) {
      System.err.println("Error shutting down Neo: " + t);
    }
  }
}
