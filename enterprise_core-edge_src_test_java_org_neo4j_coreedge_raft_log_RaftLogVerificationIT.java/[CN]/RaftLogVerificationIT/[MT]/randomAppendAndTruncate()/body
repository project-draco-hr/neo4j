{
  ThreadLocalRandom tlr=ThreadLocalRandom.current();
  for (int i=0; i < operations(); i++) {
    int r=tlr.nextInt(10);
    while (r-- > 0) {
      raftLog.append(new RaftLogEntry(i,ReplicatedInteger.valueOf(i)));
    }
    r=tlr.nextInt(10);
    while (r-- > 0) {
      raftLog.truncate(r);
    }
  }
}
