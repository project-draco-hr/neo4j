{
  ConsoleEvaluationJob job;
  try {
    while (true) {
      if (scriptEngine == null) {
        scriptEngine=GremlinFactory.createGremlinScriptEngine();
      }
      job=jobQueue.take();
      job.setResult(performEvaluation(job.getScript()));
      if (sepukko) {
        break;
      }
    }
  }
 catch (  InterruptedException e) {
  }
}
