{
  if (ctx.currentTransaction != null) {
    try {
      ctx.currentTransaction.close();
    }
 catch (    Throwable e) {
      ctx.error(Neo4jError.from(e));
    }
  }
  if (ctx.authSubject != null) {
    try {
      ctx.authSubject.logout();
    }
 catch (    Throwable e) {
      ctx.error(Neo4jError.from(e));
    }
  }
  return STOPPED;
}
