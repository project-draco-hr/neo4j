{
  GraphDatabaseService db=new GraphDatabaseFactory().newEmbeddedDatabase(TargetDirectory.forTest(getClass()).makeGraphDbDir().getAbsolutePath());
  try (Transaction tx=db.beginTx()){
    Node node1=db.createNode(TestLabels.LABEL_ONE);
    Node node2=db.createNode();
    node2.setProperty("key","some very nice value");
    node1.createRelationshipTo(node2,MyRelTypes.TEST);
    tx.success();
  }
   System.exit(1);
}
