{
  RelIdArray[] local=relationships;
  int low=0;
  int high=local.length - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    RelIdArray midVal=local[mid];
    int midId=midVal.getType();
    if (midId < type)     low=mid + 1;
 else     if (midId > type)     high=mid - 1;
 else     return midVal;
  }
  return null;
}
