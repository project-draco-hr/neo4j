{
  RelIdArray[] array=relationships;
  String expectedType=addRels.getType();
  for (int i=0; i < array.length; i++) {
    if (array[i].getType().equals(expectedType)) {
      array[i]=addRels;
      return;
    }
  }
  array=Arrays.copyOf(array,array.length + 1);
  array[array.length - 1]=addRels;
  relationships=array;
}
