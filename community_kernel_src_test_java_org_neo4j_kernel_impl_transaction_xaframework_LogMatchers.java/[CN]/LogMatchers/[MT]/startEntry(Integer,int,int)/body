{
  return new TypeSafeMatcher<LogEntry.Start>(){
    @Override public boolean matchesSafely(    LogEntry.Start entry){
      return entry != null && entry.getIdentifier() == identifier && entry.getMasterId() == masterId && entry.getLocalId() == localId;
    }
    @Override public void describeTo(    Description description){
      description.appendText("Start[" + identifier + ",xid=<Any Xid>,master="+ masterId+ ",me="+ localId+ ",time=<Any Date>]");
    }
  }
;
}
