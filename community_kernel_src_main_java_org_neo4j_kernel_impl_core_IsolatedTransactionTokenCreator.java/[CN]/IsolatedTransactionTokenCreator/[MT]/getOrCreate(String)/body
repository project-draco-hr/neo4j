{
  KernelAPI kernel=kernelProvider.instance();
  try (KernelTransaction transaction=kernel.newTransaction()){
    try (Statement statement=transaction.acquireStatement()){
      int id=createKey(statement,name);
      transaction.success();
      return id;
    }
   }
 catch (  Exception e) {
    throw new org.neo4j.graphdb.TransactionFailureException("Failure to rollback after creating token failed",e);
  }
}
