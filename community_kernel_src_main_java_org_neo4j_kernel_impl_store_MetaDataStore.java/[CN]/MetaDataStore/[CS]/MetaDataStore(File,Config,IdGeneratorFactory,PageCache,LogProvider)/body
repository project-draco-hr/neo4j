{
  super(fileName,conf,IdType.NEOSTORE_BLOCK,idGeneratorFactory,pageCache,logProvider);
  this.transactionCloseWaitLogger=new CappedOperation<Void>(time(30,SECONDS)){
    @Override protected void triggered(    Void event){
      log.info(format("Waiting for all transactions to close...%n committed:  %s%n  committing: %s%n  closed:     %s",lastCommittedTx,lastCommittingTxField,lastClosedTx));
    }
  }
;
}
