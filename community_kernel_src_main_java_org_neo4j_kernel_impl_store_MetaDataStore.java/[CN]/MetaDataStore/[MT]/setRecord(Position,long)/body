{
  long id=position.id;
  long pageId=pageIdForRecord(id);
  setHighestPossibleIdInUse(id);
  try (PageCursor cursor=storeFile.io(pageId,PF_SHARED_WRITE_LOCK)){
    if (cursor.next()) {
      setRecord(cursor,position,value);
    }
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
