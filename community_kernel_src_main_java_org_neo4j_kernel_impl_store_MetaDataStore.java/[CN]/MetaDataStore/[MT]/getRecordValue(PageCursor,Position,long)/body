{
  MetaDataRecord record=newRecord();
  try {
    record.setId(position.id);
    recordFormat.read(record,cursor,FORCE,RECORD_SIZE);
    if (record.inUse()) {
      return record.getValue();
    }
    return defaultValue;
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
