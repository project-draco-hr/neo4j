{
  if (type.equalsIgnoreCase("String")) {
    packer.pack("Hello, world!");
  }
 else   if (type.startsWith("Map")) {
    packer.packMapHeader(1);
    packer.pack("k");
    packer.pack(12345);
  }
 else   if (type.startsWith("List")) {
    packer.packListHeader(2);
    packer.pack(1);
    packer.pack(2);
  }
 else {
    throw new RuntimeException("Unknown type: " + type);
  }
}
