{
  try {
    NodeManager nm=NodeManager.getManager();
    Node node0=nm.createNode();
    Node node1=nm.createNode();
    Node node2=nm.createNode();
    Relationship rel0=NodeManager.getManager().createRelationship(node0,node1,MyRelTypes.TEST);
    Relationship rel1=NodeManager.getManager().createRelationship(node0,node2,MyRelTypes.TEST);
    node1.delete();
    rel0.delete();
    TransactionFactory.getUserTransaction().commit();
    TransactionFactory.getUserTransaction().begin();
    node2.delete();
    rel1.delete();
    node0.delete();
  }
 catch (  Exception e) {
    fail("" + e);
  }
}
