{
  buffer.clear();
  buffer.limit(4);
  if (fileChannel.read(buffer) != buffer.limit()) {
    return false;
  }
  buffer.flip();
  int identifier=buffer.getInt();
  StartEntry entry=xidIdentMap.get(identifier);
  if (entry == null) {
    return false;
  }
  Xid xid=entry.getXid();
  if (xid == null) {
    return false;
  }
  try {
    xaRm.injectTwoPhaseCommit(xid);
  }
 catch (  XAException e) {
    throw new IOException(e);
  }
  return true;
}
