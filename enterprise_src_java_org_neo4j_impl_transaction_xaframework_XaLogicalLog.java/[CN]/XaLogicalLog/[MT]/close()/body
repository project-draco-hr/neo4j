{
  if (xidIdentMap.size() > 0) {
    log.info("Active transactions: " + xidIdentMap.size());
    log.info("Closing dirty log: " + fileName);
    if (writeBuffer != null) {
      writeBuffer.force();
    }
    writeBuffer=null;
    fileChannel.close();
    return;
  }
  if (writeBuffer != null) {
    writeBuffer.force();
  }
  writeBuffer=null;
  fileChannel.close();
  File file=new File(fileName);
  if (!file.exists()) {
    throw new IOException("Logical log[" + fileName + "] not found");
  }
  boolean deleted=false;
  try {
    deleted=file.delete();
  }
 catch (  Exception e) {
  }
  if (!deleted) {
    try {
      System.gc();
      deleted=file.delete();
    }
 catch (    Exception ee) {
    }
  }
  if (!deleted) {
    log.warning("Unable to delete clean logical log[" + fileName + "]");
    log.info("Since you're not using a real operating system " + "you will be punished for it at next startup");
  }
}
