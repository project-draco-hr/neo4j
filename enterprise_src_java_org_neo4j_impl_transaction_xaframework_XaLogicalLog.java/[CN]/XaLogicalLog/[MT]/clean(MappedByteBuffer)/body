{
  AccessController.doPrivileged(new PrivilegedAction<Object>(){
    public Object run(){
      if (mappedBuffer == null) {
        return null;
      }
      try {
        Method getCleanerMethod=mappedBuffer.getClass().getMethod("cleaner",new Class[0]);
        getCleanerMethod.setAccessible(true);
        sun.misc.Cleaner cleaner=(sun.misc.Cleaner)getCleanerMethod.invoke(mappedBuffer,new Object[0]);
        cleaner.clean();
      }
 catch (      Exception e) {
        log.log(Level.INFO,"Unable to invoke cleaner method on " + mappedBuffer,e);
      }
      return null;
    }
  }
);
}
