{
  buffer.clear();
  buffer.limit(4);
  if (fileChannel.read(buffer) != buffer.limit()) {
    return false;
  }
  buffer.flip();
  int identifier=buffer.getInt();
  Integer key=new Integer(identifier);
  Xid xid=xidIdentMap.get(key);
  xaRm.pruneXid(xid);
  xidIdentMap.remove(key);
  recoveredTxMap.remove(key);
  return true;
}
