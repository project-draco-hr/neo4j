{
  String message=time();
  if (cause != null) {
    message=message + " " + level+ " [org.neo4j]: "+ msg+ " "+ cause.getMessage();
  }
 else {
    message=message + " " + level+ "  [org.neo4j]: "+ msg;
  }
  out.println(message);
  if (cause != null) {
    cause.printStackTrace(out);
  }
  if (flush) {
    out.flush();
  }
  checkRotation();
}
