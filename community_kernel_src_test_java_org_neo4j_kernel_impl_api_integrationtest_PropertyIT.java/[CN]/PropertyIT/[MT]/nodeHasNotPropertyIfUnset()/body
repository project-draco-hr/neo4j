{
  newTransaction();
  Node node=db.createNode();
  long propertyId=statement.propertyKeyGetOrCreateForName(getState(),"clown");
  long nodeId=node.getId();
  assertFalse(statement.nodeHasProperty(getState(),nodeId,propertyId));
  commit();
  newTransaction();
  assertFalse(statement.nodeHasProperty(getState(),nodeId,propertyId));
}
