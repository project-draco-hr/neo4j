{
  long propertyId;
  long nodeId;
{
    DataStatement statement=dataStatementInNewTransaction();
    Node node=db.createNode();
    propertyId=statement.propertyKeyGetOrCreateForName("clown");
    nodeId=node.getId();
    assertFalse(statement.nodeHasProperty(nodeId,propertyId));
    commit();
  }
{
    DataStatement statement=dataStatementInNewTransaction();
    assertFalse(statement.nodeHasProperty(nodeId,propertyId));
  }
}
