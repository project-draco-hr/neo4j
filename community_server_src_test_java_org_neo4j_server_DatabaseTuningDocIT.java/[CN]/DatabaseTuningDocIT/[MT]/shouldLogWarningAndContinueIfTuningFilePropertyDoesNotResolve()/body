{
  AssertableLogProvider logProvider=new AssertableLogProvider();
  server=CommunityServerBuilder.server(logProvider).usingDatabaseDir(folder.directory(name.getMethodName()).getAbsolutePath()).withNonResolvableTuningFile().build();
  server.start();
  logProvider.assertContainsMessageContaining("Config file [%s] does not exist.");
}
