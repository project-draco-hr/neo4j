{
  ByteArrayOutputStream output=new ByteArrayOutputStream();
  ExecutionResultSerializer serializer=new ExecutionResultSerializer(output,null,NullLogProvider.getInstance());
  Result executionResult=mockExecutionResult(map("path",mockPath(map("key1","value1"),map("key2","value2"),map("key3","value3"))));
  serializer.statementResult(executionResult,false);
  serializer.finish();
  String result=output.toString(UTF_8.name());
  assertEquals("{\"results\":[{\"columns\":[\"path\"]," + "\"data\":[{\"row\":[[{\"key1\":\"value1\"},{\"key2\":\"value2\"},{\"key3\":\"value3\"}]]}]}]," + "\"errors\":[]}",result);
}
