{
  ByteArrayOutputStream output=new ByteArrayOutputStream();
  ExecutionResultSerializer serializer=new ExecutionResultSerializer(output,null,NullLogProvider.getInstance());
  Result executionResult=mockExecutionResult(map("column1","value1","column2","value2"));
  serializer.transactionCommitUri(URI.create("commit/uri/1"));
  serializer.statementResult(executionResult,false);
  serializer.finish();
  String result=output.toString(UTF_8.name());
  assertEquals("{\"commit\":\"commit/uri/1\",\"results\":[{\"columns\":[\"column1\",\"column2\"]," + "\"data\":[{\"row\":[\"value1\",\"value2\"]}]}],\"errors\":[]}",result);
}
