{
switch (type) {
case without:
    for (int i=0; i < RUNS; i++) {
      System.err.println(withoutGuard());
    }
  break;
case enabled:
for (int i=0; i < RUNS; i++) {
  System.err.println(guardEnabled());
}
break;
case activeOpscount:
for (int i=0; i < RUNS; i++) {
System.err.println(guardEnabledAndActiveOpsCount());
}
break;
case activeTimeout:
for (int i=0; i < RUNS; i++) {
System.err.println(guardEnabledAndActiveTimeout());
}
break;
}
}
