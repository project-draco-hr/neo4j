{
  final CypherStatementRunner statementRunner=new CypherStatementRunner(queryExecutionEngine,txBridge);
  BoltStateMachine.SPI spi=new BoltStateMachineSPI(connectionDescriptor,usageData,gds,queryExecutionEngine,logging,authentication,txBridge,statementRunner,connectionTracker);
  return new BoltStateMachine(spi,onClose);
}
