{
  if (!baseDir.isDirectory()) {
    throw new IllegalArgumentException(baseDir.getAbsolutePath() + " must be a directory.");
  }
  if (!targetDir.exists()) {
    targetDir.mkdirs();
  }
  for (  File file : baseDir.listFiles()) {
    if (!file.equals(targetDir)) {
      moveFileToDirectory(file,targetDir);
    }
  }
}
