{
  RECORD record=store.forceGetRecord(id);
  Object transform=transform(record);
  if (transform != null) {
    if (!"".equals(transform)) {
      out.println(transform);
    }
  }
 else {
    out.print(record);
    byte[] rawRecord=store.getRawRecordData(id);
    dumpHex(record,ByteBuffer.wrap(rawRecord),id,size);
  }
  return record.inUse();
}
