{
  assertNotNull("task execution failed to communicate message",nulls);
  List<Throwable> failures=new ArrayList<Throwable>();
  for (  String id : nulls) {
    try {
      assertNotNull(id,null);
    }
 catch (    Throwable e) {
      failures.add(e);
    }
  }
  MultipleFailureException.assertEmpty(failures);
}
