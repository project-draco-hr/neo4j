{
  Node a=getNodeWithName("a");
  expectPaths(description().evaluator(lastRelationshipTypeIs(INCLUDE_AND_PRUNE,EXCLUDE_AND_CONTINUE,Types.C)).traverse(a),"a,b,c,d,e","a,f,g","a,b,h");
  expectPaths(description().evaluator(lastRelationshipTypeIs(INCLUDE_AND_CONTINUE,EXCLUDE_AND_CONTINUE,Types.C)).traverse(a),"a,b,c,d,e","a,f,g","a,b,h","a,b,h,i,k");
}
