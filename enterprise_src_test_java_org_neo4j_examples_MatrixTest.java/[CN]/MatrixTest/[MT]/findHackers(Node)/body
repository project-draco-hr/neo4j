{
  TraversalDescription td=Traversal.description().breadthFirst().relationships(RelTypes.CODED_BY,Direction.OUTGOING).relationships(RelTypes.KNOWS,Direction.OUTGOING).filter(new Predicate<Path>(){
    public boolean accept(    final Path path){
      Relationship rel=path.lastRelationship();
      return rel != null && rel.isType(RelTypes.CODED_BY);
    }
  }
);
  return td.traverse(startNode);
}
