{
  return new Cursor<T>(){
    Iterator<T> iterator=items.iterator();
    T current;
    @Override public boolean next(){
      if (iterator.hasNext()) {
        current=iterator.next();
        return true;
      }
 else {
        return false;
      }
    }
    @Override public void close(){
      iterator=items.iterator();
      current=null;
    }
    @Override public T get(){
      if (current == null) {
        throw new IllegalStateException();
      }
      return current;
    }
  }
;
}
