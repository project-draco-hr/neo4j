{
  return new Cursor<T>(){
    int idx=0;
    T current;
    @Override public boolean next(){
      if (idx < items.length) {
        current=items[idx++];
        return true;
      }
 else {
        return false;
      }
    }
    @Override public void close(){
      idx=0;
      current=null;
    }
    @Override public T get(){
      if (current == null) {
        throw new IllegalStateException();
      }
      return current;
    }
  }
;
}
