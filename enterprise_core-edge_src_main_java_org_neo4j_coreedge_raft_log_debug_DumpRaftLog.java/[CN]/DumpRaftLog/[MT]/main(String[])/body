{
  for (  String arg : args) {
    File logDirectory=new File(arg);
    System.out.println("logDirectory = " + logDirectory);
    NaiveDurableRaftLog log=new NaiveDurableRaftLog(new DefaultFileSystemAbstraction(),logDirectory,new RaftContentSerializer(),new Monitors());
    new LogPrinter(log).print(System.out);
    System.out.println();
  }
}
