{
  return new ResourceIterable<InputNode>(){
    @Override public ResourceIterator<InputNode> iterator(){
      return new InputGroupsDeserializer<InputNode>(nodeDataFactory.iterator(),nodeHeaderFactory,config,idType){
        @Override protected ResourceIterator<InputNode> entityDeserializer(        CharSeeker dataStream,        Header dataHeader,        Function<InputNode,InputNode> decorator){
          return new InputNodeDeserializer(dataHeader,dataStream,delimiter,decorator,idType.idsAreExternal());
        }
      }
;
    }
  }
;
}
