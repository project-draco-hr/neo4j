{
  return new ResourceIterable<InputRelationship>(){
    @Override public ResourceIterator<InputRelationship> iterator(){
      relationshipIds.reset();
      return new InputGroupsDeserializer<InputRelationship>(relationshipDataFactory.iterator(),relationshipHeaderFactory,config,idType){
        @Override protected ResourceIterator<InputRelationship> entityDeserializer(        CharSeeker dataStream,        Header dataHeader){
          return new InputRelationshipDeserializer(dataHeader,dataStream,delimiter,relationshipIds);
        }
      }
;
    }
  }
;
}
