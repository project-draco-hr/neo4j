{
  WebServer server=new Jetty6WebServer();
  try {
    server.setAddress("127.0.0.1");
    server.setPort(7878);
    server.addJAXRSPackages(Arrays.asList(new String[]{"org.neo4j.server.web"}),"/");
    server.start();
    server.stop();
    server.start();
  }
  finally {
    server.stop();
  }
}
