{
  GraphDatabaseService db=null;
  try {
    db=new EmbeddedGraphDatabase(path);
    fail(failMessage);
  }
 catch (  TransactionFailureException e) {
    if (!(e.getCause() instanceof NotCurrentStoreVersionException)) {
      throw e;
    }
  }
 finally {
    if (db != null) {
      db.shutdown();
    }
  }
}
