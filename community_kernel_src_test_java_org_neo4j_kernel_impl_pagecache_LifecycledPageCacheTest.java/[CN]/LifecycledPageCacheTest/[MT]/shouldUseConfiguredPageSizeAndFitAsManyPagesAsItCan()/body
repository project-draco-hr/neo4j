{
  Config config=new Config();
  config.applyChanges(stringMap(mapped_memory_page_size.name(),"4096",all_stores_total_mapped_memory_size.name(),Integer.toString(4096 * 16)));
  PageCache cache=new LifecycledPageCache(fsRule.get(),new Neo4jJobScheduler(),config);
  assertThat(cache.pageSize(),equalTo(4096));
  assertThat(cache.maxCachedPages(),equalTo(16));
}
