{
  if (serviceName.startsWith("/")) {
    serviceName=serviceName.substring(1);
  }
  StringBuilder sb=new StringBuilder();
  try {
    sb.append(baseUri().toString());
    sb.append(serviceName);
    sb.append("/");
    return new URI(sb.toString());
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
