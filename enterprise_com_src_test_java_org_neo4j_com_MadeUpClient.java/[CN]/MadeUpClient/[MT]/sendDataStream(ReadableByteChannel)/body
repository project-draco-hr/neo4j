{
  return sendRequest(MadeUpRequestType.SEND_DATA_STREAM,getRequestContext(),new Serializer(){
    @Override public void write(    ChannelBuffer buffer) throws IOException {
      try (BlockLogBuffer writer=new BlockLogBuffer(buffer,new Monitors().newMonitor(ByteCounterMonitor.class))){
        writer.write(data);
      }
     }
  }
,Protocol.VOID_DESERIALIZER);
}
