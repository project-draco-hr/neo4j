{
  return sendRequest(MadeUpServer.MadeUpRequestType.FETCH_DATA_STREAM,getRequestContext(),new Serializer(){
    @Override public void write(    ChannelBuffer buffer) throws IOException {
      buffer.writeInt(dataSize);
    }
  }
,new Deserializer<Void>(){
    @Override public Void read(    ChannelBuffer buffer,    ByteBuffer temporaryBuffer) throws IOException {
      writer.write(new BlockLogReader(buffer));
      return null;
    }
  }
);
}
