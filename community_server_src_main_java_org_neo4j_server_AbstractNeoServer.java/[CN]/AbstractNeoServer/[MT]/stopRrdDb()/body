{
  try {
    if (rrdDbScheduler != null) {
      rrdDbScheduler.stopJobs();
    }
    if (rrdDbWrapper != null) {
      rrdDbWrapper.close();
    }
    log.info("Successfully shutdown Neo4j Server");
  }
 catch (  IOException e) {
    log.error("Unable to cleanly shut down statistics database",e);
  }
}
