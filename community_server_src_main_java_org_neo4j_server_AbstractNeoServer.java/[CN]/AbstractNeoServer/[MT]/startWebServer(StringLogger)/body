{
  try {
    setUpHttpLogging();
    setUpTimeoutFilter();
    webServer.start();
    if (logger != null) {
      logger.logMessage("Server started on: " + baseUri());
    }
    log.info(format("Remote interface ready and available at [%s]",baseUri()));
  }
 catch (  RuntimeException e) {
    log.error(format("Failed to start Neo Server on port [%d], reason [%s]",getWebServerPort(),e.getMessage()));
    throw e;
  }
}
