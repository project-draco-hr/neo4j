{
  super(channel);
  if (recordSize <= 0) {
    throw new IOException("Illegal recordSize[" + recordSize + "]");
  }
  if (channel == null) {
    throw new IOException("Null file channel");
  }
  this.recordSize=recordSize;
  this.buffer=new Buffer(this);
  this.buffer.setByteBuffer(ByteBuffer.allocate(recordSize));
}
