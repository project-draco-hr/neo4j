{
  if (indexUpdatesExtractor.containsAnyNodeOrPropertyUpdate()) {
    LazyIndexUpdates updates=new LazyIndexUpdates(nodeStore,propertyStore,propertyLoader,indexUpdatesExtractor.propertyCommandsByNodeIds(),indexUpdatesExtractor.nodeCommandsById());
    indexingService.applyUpdates(updates,affectedIndexesConsumer,IndexUpdateMode.BATCHED);
  }
}
