{
  try (BufferedReader reader=new BufferedReader(new InputStreamReader(templateFile,"UTF-8"));PrintWriter writer=new PrintWriter(file)){
    String input=reader.readLine();
    while (input != null) {
      String output=substitutor.substitute(input,substitutionFunction);
      if (output == null) {
        continue;
      }
      writer.println(output);
      input=reader.readLine();
    }
  }
 }
