{
  final NeoTransactionStoreApplier applier=newApplier(false);
  final RelationshipTypeTokenRecord record=new RelationshipTypeTokenRecord(42);
  final boolean result=applier.visitRelationshipTypeTokenCommand(new Command.RelationshipTypeTokenCommand().init(record));
  assertTrue(result);
  verify(relationshipTypeTokenStore,times(1)).updateRecord(record);
}
