{
  NeoCommandHandler applier=new NeoStoreTransactionApplier(neoStore,cacheAccess,lockService,new LockGroup(),transactionId);
  if (recovery) {
    applier=new HighIdTransactionApplier(applier,neoStore);
    applier=new CacheInvalidationTransactionApplier(applier,neoStore,cacheAccess);
  }
  return applier;
}
