{
  final CommandHandler applier=newApplier(false);
  final LabelTokenRecord record=new LabelTokenRecord(42);
  applier.begin(transactionToApply);
  final boolean result=applier.visitLabelTokenCommand((LabelTokenCommand)new Command.LabelTokenCommand().init(record));
  applier.end();
  assertFalse(result);
  verify(labelTokenStore,times(1)).updateRecord(record);
}
