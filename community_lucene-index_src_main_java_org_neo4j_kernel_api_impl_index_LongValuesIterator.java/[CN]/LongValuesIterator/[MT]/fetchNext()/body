{
  try {
    if (ensureValidDisi()) {
      int nextDoc=currentDisi.nextDoc();
      if (nextDoc != DocIdSetIterator.NO_MORE_DOCS) {
        index++;
        return next(currentDocValues.get(nextDoc));
      }
 else {
        currentDisi=null;
        return fetchNext();
      }
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return false;
}
