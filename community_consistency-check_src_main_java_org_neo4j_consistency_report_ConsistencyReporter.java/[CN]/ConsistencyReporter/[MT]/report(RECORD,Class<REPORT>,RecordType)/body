{
  ProxyFactory<REPORT> proxyFactory=ProxyFactory.get(cls);
  ReportInvocationHandler<RECORD,REPORT> handler=new ReportHandler<RECORD,REPORT>(report,proxyFactory,recordType,records,record,monitor){
    @Override protected void inconsistencyReported(){
      updateSummary();
    }
  }
;
  return handler.report();
}
