{
  try {
    Store store=Store.createStore("testStore.db");
    store.close();
  }
  finally {
    File file=new File("testStore.db");
    if (file.exists()) {
      assertTrue(file.delete());
    }
    file=new File("testStore.db.id");
    if (file.exists()) {
      assertTrue(file.delete());
    }
  }
}
