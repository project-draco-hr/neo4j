{
  BatchImporter importer=new ParallelBatchImporter(directory.absolutePath(),fs,smallBatchSizeConfig(),new DevNullLoggingService(),new SilentExecutionMonitor());
  List<InputNode> nodeData=randomNodeData();
  List<InputRelationship> relationshipData=randomRelationshipData(nodeData);
  importer.doImport(csv(nodeDataAsFile(nodeData),relationshipDataAsFile(relationshipData),IdType.STRING,COMMAS));
  verifyImportedData(nodeData,relationshipData);
}
