{
  try {
    if (ignored) {
      out.handleIgnoredMessage();
    }
 else     if (error != null) {
      out.handleFailureMessage(error);
    }
 else {
      out.handleSuccessMessage(successMetadata());
    }
  }
 catch (  IOException e) {
    log.error("Failed to write response to driver",e);
  }
 finally {
    onCompleted.run();
    clearState();
  }
}
