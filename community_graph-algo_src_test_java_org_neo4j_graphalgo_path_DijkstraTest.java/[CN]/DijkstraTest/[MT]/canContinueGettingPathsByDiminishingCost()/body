{
  Node nodeA=graph.makeNode("A");
  graph.makeNode("B");
  graph.makeNode("C");
  Node nodeD=graph.makeNode("D");
  graph.makeEdge("A","B","length",2d);
  graph.makeEdge("B","C","length",3d);
  graph.makeEdge("C","D","length",1d);
  graph.makeEdge("B","D","length",5d);
  graph.makeEdge("A","D","length",6d);
  Dijkstra algo=new Dijkstra(Traversal.expanderForAllTypes(Direction.OUTGOING),CommonEvaluators.doubleCostEvaluator("length"),false);
  assertPaths(algo.findAllPaths(nodeA,nodeD),"A,B,C,D","A,B,D","A,D");
}
