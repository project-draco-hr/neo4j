{
  KernelAPI kernel=mockKernel();
  TransitionalTxManagementKernelTransaction transactionContext=(TransitionalTxManagementKernelTransaction)kernel.newTransaction();
  TransactionRegistry registry=mock(TransactionRegistry.class);
  when(registry.begin()).thenReturn(1337l);
  TransactionHandle handle=new TransactionHandle(kernel,mock(ExecutionEngine.class),registry,uriScheme,StringLogger.DEV_NULL);
  ExecutionResultSerializer output=mock(ExecutionResultSerializer.class);
  handle.execute(deserilizationErrors(new Neo4jError(StatusCode.INVALID_REQUEST_FORMAT,new Exception())),output);
  verify(transactionContext).rollback();
  verify(registry).forget(1337l);
  InOrder outputOrder=inOrder(output);
  outputOrder.verify(output).transactionCommitUri(uriScheme.txCommitUri(1337));
  outputOrder.verify(output).errors(argThat(hasErrors(StatusCode.INVALID_REQUEST_FORMAT)));
  outputOrder.verify(output).finish();
  verifyNoMoreInteractions(output);
}
