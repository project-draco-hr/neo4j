{
  SendToMyself leaderOnlyReplicator=new SendToMyself(member,outbound);
  RaftMembershipManager membershipManager=new RaftMembershipManager(leaderOnlyReplicator,memberSetBuilder,raftLog,logProvider,expectedClusterSize,electionTimeout,clock,catchupTimeout,raftMembership,0);
  RaftLogShippingManager logShipping=new RaftLogShippingManager(outbound,logProvider,raftLog,clock,member,membershipManager,retryTimeMillis,catchupBatchSize,maxAllowedShippingLag,inFlightMap);
  RaftInstance raft=new RaftInstance(member,termState,voteState,raftLog,raftStateMachine,electionTimeout,heartbeatInterval,renewableTimeoutService,outbound,logProvider,membershipManager,logShipping,databaseHealthSupplier,inFlightMap,monitors);
  inbound.registerHandler(raft);
  return raft;
}
