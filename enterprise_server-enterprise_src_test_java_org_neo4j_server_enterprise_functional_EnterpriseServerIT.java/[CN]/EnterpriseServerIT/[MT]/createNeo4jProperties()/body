{
  File tuningFile=File.createTempFile("neo4j-test","properties");
  FileOutputStream fos=new FileOutputStream(tuningFile);
  try {
    Properties neo4jProps=new Properties();
    neo4jProps.put("ha.server_id","1");
    neo4jProps.store(fos,"");
    return tuningFile;
  }
  finally {
    fos.close();
  }
}
