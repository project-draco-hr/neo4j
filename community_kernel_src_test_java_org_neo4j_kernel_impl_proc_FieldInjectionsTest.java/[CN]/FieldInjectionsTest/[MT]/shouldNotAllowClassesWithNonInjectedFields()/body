{
  FieldInjections injections=new FieldInjections(new ComponentRegistry());
  exception.expect(ProcedureException.class);
  exception.expectMessage("Field `someState` on `ProcedureWithNonInjectedMemberFields` is not annotated as a @Resource and is not static. " + "If you want to store state along with your procedure, please use a static field.");
  injections.setters(ProcedureWithNonInjectedMemberFields.class);
}
