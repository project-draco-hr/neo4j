{
  for (boolean hasNext=source.hasNext(); hasNext; hasNext=source.hasNext()) {
    long value=source.next();
    next(value);
    if ((removedElements == null || !removedElements.contains(value)) && (addedElements == null || !addedElements.contains(value))) {
      return;
    }
  }
  transitionToAddedElements();
}
