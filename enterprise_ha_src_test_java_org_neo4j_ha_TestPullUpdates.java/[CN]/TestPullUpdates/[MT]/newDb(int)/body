{
  return new HighlyAvailableGraphDatabase(dir.directory("" + i,true).getAbsolutePath(),stringMap(HaConfig.CONFIG_KEY_SERVER_ID,"" + i,HaConfig.CONFIG_KEY_SERVER,"localhost:" + (6666 + i),HaConfig.CONFIG_KEY_COORDINATORS,zoo.getConnectionString(),HaConfig.CONFIG_KEY_PULL_INTERVAL,PULL_INTERVAL + "ms"));
}
