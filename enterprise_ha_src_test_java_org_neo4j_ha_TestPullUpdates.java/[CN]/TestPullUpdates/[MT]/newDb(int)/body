{
  return (HighlyAvailableGraphDatabase)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(dir.directory("" + i,true).getAbsolutePath()).setConfig(HaSettings.server_id,"" + i).setConfig(HaSettings.server,"localhost:" + (6666 + i)).setConfig(HaSettings.coordinators,zoo.getConnectionString()).setConfig(HaSettings.pull_interval,PULL_INTERVAL + "ms").newGraphDatabase();
}
