{
  GraphDatabaseService db=new TestGraphDatabaseFactory().setFileSystem(fs.get()).newImpermanentDatabase(storeDir.getPath());
  try (Transaction tx=db.beginTx()){
    db.createNode();
    tx.success();
  }
   EphemeralFileSystemAbstraction snapshot=fs.get().snapshot();
  db.shutdown();
  return snapshot;
}
