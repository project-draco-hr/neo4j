{
  populator=newPopulator();
  List<NodePropertyUpdate> updates=Arrays.asList(NodePropertyUpdate.add(1,1,"foo",new long[]{1}),NodePropertyUpdate.add(2,1,"bar",new long[]{1}),NodePropertyUpdate.add(42,1,"bar",new long[]{1}));
  populator.add(updates);
  index.maybeRefreshBlocking();
  try (IndexReader reader=index.getIndexReader()){
    assertArrayEquals(new long[]{1,2,42},PrimitiveLongCollections.asArray(reader.scan()));
  }
 }
