{
  dataSource=new LuceneDataSource(new Config(StringLogger.DEV_NULL,fileSystem,config(),Collections.<Class<?>>singletonList(GraphDatabaseSettings.class)),indexStore,new DefaultFileSystemAbstraction(),new XaFactory(new Config(StringLogger.DEV_NULL,fileSystem,config(),Collections.<Class<?>>singletonList(GraphDatabaseSettings.class)),TxIdGenerator.DEFAULT,new PlaceboTm(),new DefaultLogBufferFactory(),new DefaultFileSystemAbstraction(),StringLogger.DEV_NULL,RecoveryVerifier.ALWAYS_VALID));
  IndexIdentifier identifier=identifier("foo");
  IndexWriter writer=dataSource.getIndexWriter(identifier);
  assertSame(writer,dataSource.getIndexWriter(identifier));
}
