{
  dataSource=new LuceneDataSource(new Config(config(),GraphDatabaseSettings.class),indexStore,new DefaultFileSystemAbstraction(),new XaFactory(TxIdGenerator.DEFAULT,new PlaceboTm(null,null),new DefaultLogBufferFactory(),new DefaultFileSystemAbstraction(),new DevNullLoggingService(),RecoveryVerifier.ALWAYS_VALID,LogPruneStrategies.NO_PRUNING),null);
  dataSource.start();
  IndexIdentifier identifier=identifier("foo");
  IndexWriter writer=dataSource.getIndexSearcher(identifier).getWriter();
  assertSame(writer,dataSource.getIndexSearcher(identifier).getWriter());
}
