{
  dataSource=new LuceneDataSource(new Config(StringLogger.DEV_NULL,fileSystem,config()),indexStore,new DefaultFileSystemAbstraction(),new XaFactory(new Config(StringLogger.DEV_NULL,fileSystem,config()),TxIdGenerator.DEFAULT,new PlaceboTm(),new DefaultLogBufferFactory(),new DefaultFileSystemAbstraction(),StringLogger.DEV_NULL,CommonFactories.defaultRecoveryVerifier()));
  IndexIdentifier identifier=identifier("foo");
  IndexWriter writer=dataSource.getIndexWriter(identifier);
  assertSame(writer,dataSource.getIndexWriter(identifier));
}
