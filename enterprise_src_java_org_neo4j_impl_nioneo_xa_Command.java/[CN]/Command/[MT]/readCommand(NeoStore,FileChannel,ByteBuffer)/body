{
  buffer.clear();
  buffer.limit(1);
  if (fileChannel.read(buffer) != buffer.limit()) {
    return null;
  }
  buffer.flip();
  byte commandType=buffer.get();
switch (commandType) {
case NODE_COMMAND:
    System.out.println(" Node");
  return NodeCommand.readCommand(neoStore,fileChannel,buffer);
case PROP_COMMAND:
System.out.println(" PROP");
return PropertyCommand.readCommand(neoStore,fileChannel,buffer);
case PROP_INDEX_COMMAND:
System.out.println(" INDEX");
return PropertyIndexCommand.readCommand(neoStore,fileChannel,buffer);
case REL_COMMAND:
System.out.println(" REL");
return RelationshipCommand.readCommand(neoStore,fileChannel,buffer);
case REL_TYPE_COMMAND:
System.out.println(" RELTYPE");
return RelationshipTypeCommand.readCommand(neoStore,fileChannel,buffer);
default :
throw new IOException("Unkown command type[" + commandType + "]");
}
}
