{
  assertCanEncodeInBothCasings("hello world");
  assertCanEncode("hello_world");
  assertCanEncode("_hello_world");
  assertCanEncode("hello::world");
  assertCanEncode("hello//world");
  assertCanEncode("hello world");
  assertCanEncode("http://ok");
  assertCanEncode("::::::::");
  assertCanEncode(" _.-:/ _.-:/");
}
