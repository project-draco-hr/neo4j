{
  lastTraverser=traversalDescription.order(new SelectorFactory(end)).evaluator(includeWhereEndNodeIs(end)).traverse(start);
  return new Iterable<WeightedPath>(){
    @Override public Iterator<WeightedPath> iterator(){
      return new StopAfterWeightIterator(lastTraverser.iterator(),costEvaluator);
    }
  }
;
}
