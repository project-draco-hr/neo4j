{
  lastTraverser=traversalDescription.order(new SelectorFactory(end,pathInterest(true,stopAfterLowestWeight))).evaluator(includeWhereEndNodeIs(end)).traverse(start);
  return new Iterable<WeightedPath>(){
    @Override public Iterator<WeightedPath> iterator(){
      return new WeightedPathIterator(lastTraverser.iterator(),costEvaluator,stopAfterLowestWeight);
    }
  }
;
}
