{
  LogEntryReader<ReadableClosablePositionAwareChannel> reader=new VersionAwareLogEntryReader<>(new RecordStorageCommandReaderFactory());
  InMemoryClosableChannel channel=new InMemoryClosableChannel();
  int count=100;
  channel.put(new byte[count],count);
  for (int i=0; i < count; i++) {
    LogEntry entry=reader.readLogEntry(channel);
    assertNull(entry);
    assertEquals(i + 1,channel.readerPosition());
  }
}
