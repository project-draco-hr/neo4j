{
  if (properties == null || properties.isEmpty()) {
    return IteratorUtil.emptyIterator();
  }
  return new IteratorWrapper<PropertyBlock,Map.Entry<String,Object>>(properties.entrySet().iterator()){
    @Override protected PropertyBlock underlyingObjectToObject(    Entry<String,Object> property){
      return propertyCreator.encodePropertyValue(getOrCreatePropertyKeyId(property.getKey()),property.getValue());
    }
  }
;
}
