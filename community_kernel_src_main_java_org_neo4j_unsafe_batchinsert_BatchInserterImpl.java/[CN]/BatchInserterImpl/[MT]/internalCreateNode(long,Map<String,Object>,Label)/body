{
  NodeRecord nodeRecord=new NodeRecord(nodeId,false,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue());
  nodeRecord.setInUse(true);
  nodeRecord.setCreated();
  nodeRecord.setNextProp(createPropertyChain(properties));
  if (labels.length > 0) {
    setNodeLabels(nodeRecord,labels);
  }
  getNodeStore().updateRecord(nodeRecord);
  return nodeId;
}
