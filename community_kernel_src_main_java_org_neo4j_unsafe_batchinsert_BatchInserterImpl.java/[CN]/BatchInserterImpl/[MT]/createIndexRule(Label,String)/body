{
  SchemaStore schemaStore=getSchemaStore();
  IndexRule schemaRule=new IndexRule(schemaStore.nextId(),getOrCreateLabelId(label),this.schemaIndexProviders.getDefaultProvider().getProviderDescriptor(),getOrCreatePropertyIndex(propertyKey));
  for (  DynamicRecord record : schemaStore.allocateFrom(schemaRule))   schemaStore.updateRecord(record);
  schemaCache.addSchemaRule(schemaRule);
}
