{
  SchemaStore schemaStore=getSchemaStore();
  IndexRule schemaRule=new IndexRule(schemaStore.nextId(),getOrCreateLabelId(label.name()),this.schemaIndexProviders.getDefaultProvider().getProviderDescriptor(),getOrCreatePropertyKeyId(propertyKey));
  for (  DynamicRecord record : schemaStore.allocateFrom(schemaRule))   schemaStore.updateRecord(record);
  schemaCache.addSchemaRule(schemaRule);
}
