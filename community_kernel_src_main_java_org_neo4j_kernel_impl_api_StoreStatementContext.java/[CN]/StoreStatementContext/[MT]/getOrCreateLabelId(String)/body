{
  try {
    return labelTokenHolder.getOrCreateId(label);
  }
 catch (  TransactionFailureException e) {
    if (e.getCause() != null && e.getCause() instanceof UnderlyingStorageException && e.getCause().getMessage().equals("Id capacity exceeded")) {
      throw new DataIntegrityKernelException.TooManyLabelsException(e);
    }
 else {
      throw e;
    }
  }
}
