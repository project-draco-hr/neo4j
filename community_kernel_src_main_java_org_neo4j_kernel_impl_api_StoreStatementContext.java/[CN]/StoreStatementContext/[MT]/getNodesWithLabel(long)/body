{
  final NodeStore nodeStore=neoStore.getNodeStore();
  final long highestId=nodeStore.getHighestPossibleIdInUse();
  return new PrefetchingIterator<Long>(){
    private long id=0;
    @Override protected Long fetchNextOrNull(){
      while (id <= highestId) {
        NodeRecord node=nodeStore.forceGetRecord(id++);
        for (        long label : nodeStore.getLabelsForNode(node))         if (label == labelId)         return node.getId();
      }
      return null;
    }
  }
;
}
