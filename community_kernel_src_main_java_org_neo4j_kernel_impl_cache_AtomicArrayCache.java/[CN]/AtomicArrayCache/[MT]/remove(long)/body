{
  if (id < 0) {
    throw new IllegalArgumentException("Can not remove enity with negative id (" + id + ")");
  }
  int pos=getPosition(id);
  E obj=cache.get(pos);
  if (obj != null) {
    if (!cache.compareAndSet(pos,obj,null)) {
      remove(id);
    }
 else {
      currentSize.addAndGet(obj.size() * -1);
    }
  }
  return obj;
}
