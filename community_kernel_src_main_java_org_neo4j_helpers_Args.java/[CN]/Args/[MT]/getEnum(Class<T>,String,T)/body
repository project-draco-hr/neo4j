{
  String raw=this.map.get(key);
  if (raw == null) {
    return defaultValue;
  }
  for (  T candidate : enumClass.getEnumConstants()) {
    if (candidate.name().equals(raw)) {
      return candidate;
    }
  }
  throw new IllegalArgumentException("No enum instance '" + raw + "' in "+ enumClass.getName());
}
