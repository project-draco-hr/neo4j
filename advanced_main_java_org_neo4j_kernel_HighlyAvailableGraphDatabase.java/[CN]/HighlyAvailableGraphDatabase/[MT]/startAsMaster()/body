{
  assertIWasntMasterWhenShutDown();
  this.localGraph=new EmbeddedGraphDbImpl(storeDir,config,this,CommonFactories.defaultLockManagerFactory(),new MasterIdGeneratorFactory(),CommonFactories.defaultRelationshipTypeCreator(),CommonFactories.defaultTxIdGeneratorFactory(),CommonFactories.defaultTxFinishHook(),new ZooKeeperLastCommittedTxIdSetter(broker));
  graphDbStarted();
  markThatIAmMaster();
  this.masterServer=(MasterServer)broker.instantiateMasterServer(this);
  instantiateIndexIfNeeded();
}
