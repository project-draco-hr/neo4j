{
  return new BrokerFactory(){
    public Broker create(    String storeDir,    Map<String,String> config){
      return new ZooKeeperBroker(storeDir,getClusterNameFromConfig(config),getMachineIdFromConfig(config),getZooKeeperServersFromConfig(config),getHaServerFromConfig(config),HighlyAvailableGraphDatabase.this);
    }
  }
;
}
