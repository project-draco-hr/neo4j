{
  try {
    reevaluateMyself(master);
  }
 catch (  ZooKeeperException ee) {
    ee.printStackTrace();
  }
catch (  HaCommunicationException ee) {
    ee.printStackTrace();
  }
catch (  Throwable t) {
    t.printStackTrace();
    System.out.println("Reevaluation ended in unknown exception " + t + " so shutting down");
    shutdown();
  }
}
