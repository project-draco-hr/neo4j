{
  String headerValue=request.getHeader(MAX_EXECUTION_TIME_HEADER);
  if (headerValue != null) {
    try {
      return Long.parseLong(headerValue);
    }
 catch (    NumberFormatException e) {
      log.error(String.format("Fail to parse `%s` header with value: '%s'. Should be a positive number.",MAX_EXECUTION_TIME_HEADER,headerValue),e);
    }
  }
  return -1;
}
