{
  if (key == null) {
    throw new IllegalArgumentException("cannot remove null key");
  }
  Page<V> toRemove=cache.remove(key);
  if (toRemove == null || toRemove.value == null) {
    return null;
  }
  currentSize.decrementAndGet();
  V toReturn=toRemove.value;
  toRemove.value=null;
  toRemove.flag=false;
  return toReturn;
}
