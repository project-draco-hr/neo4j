{
  final ZooKeeperServerWrapper zooKeeperServer=new ZooKeeperServerWrapper(2,new File("var/zookeeper"),CLIENT_PORT,Arrays.asList(ZOO_KEEPER_SERVERS),new HashMap<String,String>());
  System.out.println("Zoo keeper server up, stop it with Ctrl-C");
  Runtime.getRuntime().addShutdownHook(new Thread(){
    @Override public void run(){
      System.out.print("Shutting down zoo keeper server... ");
      zooKeeperServer.shutdown();
      System.out.println("OK");
    }
  }
);
  while (true) {
    Thread.sleep(1000);
  }
}
