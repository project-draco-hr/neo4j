{
  PageSwapperFactory factory=swapperFactory();
  PageSwapper pageSwapper=factory.createPageSwapper(new File("does not exist"),cachePageSize(),NO_CALLBACK,true);
  ByteBufferPage page=createPage();
  page.putLong(X,0);
  pageSwapper.write(0,page);
  clear(page);
  pageSwapper.read(0,page);
  assertThat(page.getLong(0),is(X));
}
