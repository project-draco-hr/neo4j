{
  File fileA=new File("a");
  File fileB=new File("b");
  ensureFileExists(fileA);
  ensureFileExists(fileB);
  PageSwapperFactory factory=swapperFactory();
  PageSwapper swapperA=factory.createPageSwapper(fileA,cachePageSize(),NO_CALLBACK,false);
  PageSwapper swapperB=factory.createPageSwapper(fileB,cachePageSize(),NO_CALLBACK,false);
  ByteBufferPage page=createPage();
  page.putLong(X,0);
  swapperA.write(0,page);
  page.putLong(Y,8);
  swapperB.write(0,page);
  clear(page);
  swapperA.read(0,page);
  assertThat(page.getLong(0),is(X));
  assertThat(page.getLong(8),is(0L));
}
