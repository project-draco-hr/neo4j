{
  File fileA=file("a");
  File fileB=file("b");
  PageSwapperFactory factory=swapperFactory();
  PageSwapper swapperA=factory.createPageSwapper(fileA,cachePageSize(),NO_CALLBACK,true);
  PageSwapper swapperB=factory.createPageSwapper(fileB,cachePageSize(),NO_CALLBACK,true);
  ByteBufferPage page=createPage();
  page.putLong(X,0);
  swapperA.write(0,page);
  page.putLong(Y,8);
  swapperB.write(0,page);
  clear(page);
  swapperA.read(0,page);
  assertThat(page.getLong(0),is(X));
  assertThat(page.getLong(8),is(0L));
}
