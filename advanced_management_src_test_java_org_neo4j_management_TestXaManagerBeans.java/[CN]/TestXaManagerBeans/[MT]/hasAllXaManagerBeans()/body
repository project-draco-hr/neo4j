{
  for (  XaDataSource xaDataSource : graphDb.getConfig().getTxModule().getXaDataSourceManager().getAllRegisteredDataSources()) {
    XaResourceInfo info=getByName(xaDataSource.getName());
    assertEquals("wrong branchid for XA data source " + xaDataSource.getName(),XaManagerBean.toHexString(xaDataSource.getBranchId()),info.getBranchId());
    assertEquals("wrong log version for XA data source " + xaDataSource.getName(),xaDataSource.getCurrentLogVersion(),info.getLogVersion());
    assertEquals("wrong last tx ID for XA data source " + xaDataSource.getName(),xaDataSource.getLastCommittedTxId(),info.getLastTxId());
  }
}
