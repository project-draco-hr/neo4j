{
  return format("state{myself=%s, term=%s, leader=%s, leaderCommit=%d, appended=%d, committed=%d, " + "votedFor=%s, votesForMe=%s, lastLogIndexBeforeWeBecameLeader=%d, followerStates=%s}",myself,term,leader,leaderCommit,entryLog.appendIndex(),commitIndex,votedFor,votesForMe,lastLogIndexBeforeWeBecameLeader,followerStates);
}
