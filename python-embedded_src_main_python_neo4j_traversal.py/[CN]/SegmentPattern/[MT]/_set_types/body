def _set_types(self, typedef):
    self.types = []
    if (typedef is None):
        return
    if (not isinstance(typedef, (list, tuple))):
        typedef = (typedef,)
    for t in typedef:
        if (not isinstance(t, (str, unicode))):
            self.types.append(t.name())
        else:
            self.types.append(t)
