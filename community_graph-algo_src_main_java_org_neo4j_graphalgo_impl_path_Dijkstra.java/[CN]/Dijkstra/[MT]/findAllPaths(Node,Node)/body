{
  lastTraverser=TRAVERSAL.expand(expander).order(new SelectorFactory(costEvaluator)).evaluator(Evaluators.includeWhereEndNodeIs(end)).traverse(start);
  return new Iterable<WeightedPath>(){
    public Iterator<WeightedPath> iterator(){
      return new StopAfterWeightIterator(lastTraverser.iterator(),costEvaluator);
    }
  }
;
}
