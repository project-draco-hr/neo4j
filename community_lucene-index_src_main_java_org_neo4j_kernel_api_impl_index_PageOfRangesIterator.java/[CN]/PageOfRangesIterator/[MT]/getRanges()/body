{
  if (rangesIterator != null) {
    return rangesIterator;
  }
  try {
    DocValuesCollector docValuesCollector=new DocValuesCollector();
    searcher.search(query,docValuesCollector);
    rangesIterator=new LongValuesIterator(docValuesCollector.getMatchingDocs(),docValuesCollector.getTotalHits(),BitmapDocumentFormat.RANGE);
    return rangesIterator;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
