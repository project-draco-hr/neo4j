{
  life=new LifeSupport();
  monitor=new TrackingMonitor();
  IndexStorageFactory indexStorageFactory=new IndexStorageFactory(directoryFactory,new DefaultFileSystemAbstraction(),dir);
  IndexStorage indexStorage=indexStorageFactory.labelScanStorage();
  store=life.add(new LuceneLabelScanStore(strategy,indexStorage,standard(),asStream(existingData),monitor));
  life.start();
  assertTrue(monitor.initCalled);
}
