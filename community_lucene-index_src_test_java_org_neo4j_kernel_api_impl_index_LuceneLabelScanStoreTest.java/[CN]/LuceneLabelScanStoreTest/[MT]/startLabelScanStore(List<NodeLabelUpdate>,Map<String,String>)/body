{
  life=new LifeSupport();
  monitor=new TrackingMonitor();
  labelScanStore=new LuceneLabelScanStore(strategy,directoryFactory,dir,new DefaultFileSystemAbstraction(),tracking(),asStream(existingData),new Config(configParams),OperationalMode.single,monitor);
  store=life.add(labelScanStore);
  life.start();
}
