{
  usePersistentDirectory();
  List<NodeLabelUpdate> data=asList(labelChanges(1,NO_LABELS,new long[]{1}),labelChanges(2,NO_LABELS,new long[]{1,2}));
  start(data);
  scrambleIndexFilesAndRestart(data);
  assertTrue("Didn't rebuild the store on startup",monitor.corruptIndexCalled & monitor.rebuildingCalled & monitor.rebuiltCalled);
  assertNodesForLabel(1,1,2);
  assertNodesForLabel(2,2);
}
