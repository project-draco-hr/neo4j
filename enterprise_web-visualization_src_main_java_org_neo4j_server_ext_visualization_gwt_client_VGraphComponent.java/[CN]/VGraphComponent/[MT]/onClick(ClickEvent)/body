{
  System.out.println("Click " + event.getNativeButton());
  if (event.isShiftKeyDown()) {
    Widget[] list=nodes.toArray(new Widget[nodes.size()]);
    createLine(list[(int)Random.nextInt(list.length)],createNode(event.getRelativeX(canvas.getElement()),event.getRelativeY(canvas.getElement())));
    event.preventDefault();
  }
}
