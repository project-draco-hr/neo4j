{
  Integer nodeIds[]=relationship.getNodeIds();
  ((NodeImpl)getNodeForProxy(nodeIds[0].intValue())).addRelationship(relationship.getType(),(int)relationship.getId());
  ((NodeImpl)getNodeForProxy(nodeIds[1].intValue())).addRelationship(relationship.getType(),(int)relationship.getId());
  relCache.add((int)relationship.getId(),relationship);
}
