{
  try {
    RawRelationshipData rawRels[]=persistenceManager.loadRelationships((int)node.getId());
    List<RelationshipImpl> relList=new ArrayList<RelationshipImpl>();
    for (    RawRelationshipData rawRel : rawRels) {
      int relId=rawRel.getId();
      RelationshipImpl rel=relCache.get(relId);
      if (rel == null) {
        RelationshipType type=getRelationshipTypeById(rawRel.getType());
        if (type == null) {
          continue;
        }
        rel=new RelationshipImpl(relId,rawRel.getFirstNode(),rawRel.getSecondNode(),type,false,this);
        relCache.add(relId,rel);
      }
      relList.add(rel);
    }
    return relList;
  }
 catch (  Exception e) {
    log.severe("Failed loading relationships for node[" + node.getId() + "]");
    throw new RuntimeException(e);
  }
}
