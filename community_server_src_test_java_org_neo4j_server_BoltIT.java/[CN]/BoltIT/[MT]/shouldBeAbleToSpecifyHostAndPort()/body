{
  server=server().withProperty(boltConnector("0").enabled.name(),"true").withProperty(boltConnector("0").encryption_level.name(),"REQUIRED").withProperty(boltConnector("0").address.name(),"localhost:8776").usingDataDir(tmpDir.getRoot().getAbsolutePath()).build();
  server.start();
  assertEventuallyServerResponds("localhost",8776);
}
