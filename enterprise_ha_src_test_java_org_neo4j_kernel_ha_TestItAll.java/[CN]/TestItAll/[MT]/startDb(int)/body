{
  HighlyAvailableGraphDatabase database=(HighlyAvailableGraphDatabase)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(path(serverId)).setConfig(ClusterSettings.cluster_server,"127.0.0.1:" + (5001 + serverId)).setConfig(ClusterSettings.initial_hosts,"127.0.0.1:5001").setConfig(HaSettings.server_id,"" + serverId).setConfig(HaSettings.ha_server,"127.0.0.1:" + (8001 + serverId)).newGraphDatabase();
  databases[serverId]=database;
  return database;
}
