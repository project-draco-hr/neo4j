{
  if (getUnit().equals(other.getUnit()) && getName().equals(other.getName())) {
    return new WorkerMetric(getAggregation(),getName(),aggregation.aggregate(getValue(),other.getValue()),getUnit(),shouldTrackRegression() || other.shouldTrackRegression());
  }
 else {
    throw new RuntimeException("Can only aggregate worker metrics with the same name and unit. " + "(" + this + ", "+ other+ ").");
  }
}
