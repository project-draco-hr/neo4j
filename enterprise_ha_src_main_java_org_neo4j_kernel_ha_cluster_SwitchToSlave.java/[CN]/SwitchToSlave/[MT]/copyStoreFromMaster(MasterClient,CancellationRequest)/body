{
  FileSystemAbstraction fs=resolver.resolveDependency(FileSystemAbstraction.class);
  PageCache pageCache=resolver.resolveDependency(PageCache.class);
  userLog.info("Copying store from master");
  new StoreCopyClient(storeDir,config,kernelExtensions,logService.getUserLogProvider(),fs,pageCache,storeCopyMonitor).copyStore(new StoreCopyClient.StoreCopyRequester(){
    @Override public Response<?> copyStore(    StoreWriter writer){
      return masterClient.copyStore(new RequestContext(0,config.get(ClusterSettings.server_id).toIntegerIndex(),0,BASE_TX_ID,0),writer);
    }
    @Override public void done(){
    }
  }
,cancellationRequest);
  startServicesAgain();
  userLog.info("Finished copying store from master");
}
