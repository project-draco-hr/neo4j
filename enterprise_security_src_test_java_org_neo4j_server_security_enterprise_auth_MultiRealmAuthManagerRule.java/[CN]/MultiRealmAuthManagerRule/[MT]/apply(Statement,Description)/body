{
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      try {
        setupAuthManager(authStrategy);
        base.evaluate();
      }
 catch (      Throwable t) {
        fail("Got unexpected exception " + t);
      }
 finally {
        try {
          tearDown();
        }
 catch (        Throwable t) {
          throw new RuntimeException("Failed to shut down MultiRealmAuthManager",t);
        }
      }
    }
  }
;
}
