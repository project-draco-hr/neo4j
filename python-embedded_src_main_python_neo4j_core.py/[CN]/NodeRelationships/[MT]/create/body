def create(self, t, *nodes, **properties):
    if (not nodes):
        raise TypeError('No target node specified')
    rels = []
    node = self.__node
    if (type(t) in strings):
        t = rel_type(t)
    for other in nodes:
        rels.append(node.createRelationshipTo(other, t))
    for rel in rels:
        for (key, val) in properties.items():
            rel[key] = val
    if (len(rels) == 1):
        return rels[0]
    return rels
