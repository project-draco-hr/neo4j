{
  int size=0, chunk=0;
  List<byte[]> data=new LinkedList<byte[]>();
  ByteBuffer buffer=ByteBuffer.allocateDirect(1 * MEGA);
  while ((chunk=channel.read(buffer)) >= 0) {
    size+=chunk;
    byte[] bytes=new byte[chunk];
    buffer.flip();
    buffer.get(bytes);
    buffer.clear();
    data.add(bytes);
  }
  this.data=data;
  this.size=size;
}
