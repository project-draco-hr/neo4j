{
  int aLength=Array.getLength(a);
  int bLength=Array.getLength(b);
  if (aLength != bLength) {
    return false;
  }
  for (int i=0; i < aLength; i++) {
    Object aObj=Array.get(a,i);
    Object bObj=Array.get(b,i);
    if (!valueCompare(aObj,bObj)) {
      return false;
    }
  }
  return true;
}
