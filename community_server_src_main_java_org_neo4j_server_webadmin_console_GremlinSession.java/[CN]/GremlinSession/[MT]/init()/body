{
  StringBuffer out=new StringBuffer();
  out.append("\n");
  out.append("         \\,,,/\n");
  out.append("         (o o)\n");
  out.append("-----oOOo-(_)-oOOo-----\n");
  out.append("\n");
  out.append("Available variables:\n");
  for (  String variable : initialBindings) {
    out.append("  " + variable + "\t= ");
    out.append(evaluate(variable));
  }
  out.append("\n");
  return out.toString();
}
