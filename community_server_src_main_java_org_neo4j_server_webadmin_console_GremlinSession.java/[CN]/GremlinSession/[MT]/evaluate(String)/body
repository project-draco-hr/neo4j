{
  String result=null;
  try {
    if (script.equals(INIT_FUNCTION)) {
      result=init();
    }
 else {
      scriptEngine.execute(script);
      result=baos.toString();
      resetIO();
    }
  }
 catch (  GroovyRuntimeException ex) {
    result=ex.getMessage();
  }
  return Pair.of(result,null);
}
