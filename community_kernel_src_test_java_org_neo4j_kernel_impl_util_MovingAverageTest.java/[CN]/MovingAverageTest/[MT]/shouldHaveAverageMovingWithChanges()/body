{
  MovingAverage average=new MovingAverage(5);
  long avg=average.average();
  for (int i=0; i < 5; i++) {
    average.add(10);
    assertEquals(10L,average.average());
    avg=average.average();
  }
  assertEquals(10L,average.average());
  for (int i=0; i < 5; i++) {
    average.add(100);
    assertThat(average.average(),greaterThan(avg));
    avg=average.average();
  }
  assertEquals(100L,average.average());
}
