{
  try {
    if (rrdDb != null) {
      rrdDb.close();
    }
    if (graph != null) {
      graph.shutdown();
    }
    log.info("Successfully shutdown database");
  }
 catch (  Exception e) {
    log.error("Database did not shut down cleanly. Reason [%s]",e.getMessage());
    throw new RuntimeException(e);
  }
}
