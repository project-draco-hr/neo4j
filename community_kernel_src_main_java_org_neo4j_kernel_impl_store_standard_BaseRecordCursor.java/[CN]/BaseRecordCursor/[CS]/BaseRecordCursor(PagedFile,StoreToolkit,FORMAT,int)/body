{
  this.file=file;
  this.toolkit=toolkit;
  this.format=format;
  this.pageCacheFlags=pageCursorFlags(flags);
  if ((flags & SF_REVERSE_CURSOR) == 0) {
    this.currentRecordId=toolkit.firstRecordId() - 1;
    this.stepSize=1;
  }
 else {
    this.currentRecordId=toolkit.highestKnownId() + 1;
    this.stepSize=-1;
  }
}
