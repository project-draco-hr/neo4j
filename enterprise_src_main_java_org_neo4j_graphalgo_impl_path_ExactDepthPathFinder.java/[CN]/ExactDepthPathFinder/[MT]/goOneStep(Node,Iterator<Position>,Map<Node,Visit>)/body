{
  if (!visitor.hasNext()) {
    return null;
  }
  Position position=visitor.next();
  Visit visit=visits.get(position.node());
  if (visit != null) {
    if (visitor != visit.visitor) {
      return new Position[]{visit.position,position};
    }
  }
 else {
    visits.put(position.node(),new Visit(position,visitor));
  }
  return null;
}
