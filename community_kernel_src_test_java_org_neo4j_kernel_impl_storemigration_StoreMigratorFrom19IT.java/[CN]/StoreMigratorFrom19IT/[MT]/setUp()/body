{
  Config config=MigrationTestUtils.defaultConfig();
  config=config.with(MapUtil.stringMap(GraphDatabaseSettings.neo_store.name(),storeDir.getAbsolutePath() + File.separator + NeoStore.DEFAULT_NAME));
  PageCache pageCache=pageCacheRule.getPageCache(fs,config);
  storeFactory=new StoreFactory(config,idGeneratorFactory,pageCache,fs,StringLogger.DEV_NULL,null);
}
