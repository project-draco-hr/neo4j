{
  int f=0, o=0;
  while (f < firstLength && o < otherLength) {
    if (first[f] == other[o]) {
      return true;
    }
    if (first[f] < other[o]) {
      while (++f < firstLength && first[f] < other[o])       ;
    }
 else {
      while (++o < otherLength && first[f] > other[o])       ;
    }
  }
  return false;
}
