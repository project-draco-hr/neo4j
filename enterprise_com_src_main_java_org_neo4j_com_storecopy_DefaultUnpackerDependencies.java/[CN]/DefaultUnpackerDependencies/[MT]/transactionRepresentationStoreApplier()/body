{
  return new BatchingTransactionRepresentationStoreApplier(resolver.resolveDependency(LockService.class),resolver.resolveDependency(IndexConfigStore.class),resolver.resolveDependency(IdOrderingQueue.class),resolver.resolveDependency(LabelScanStore.class),resolver.resolveDependency(LegacyIndexApplierLookup.class),resolver.resolveDependency(NeoStores.class),resolver.resolveDependency(CacheAccessBackDoor.class),resolver.resolveDependency(IndexingService.class),resolver.resolveDependency(KernelHealth.class));
