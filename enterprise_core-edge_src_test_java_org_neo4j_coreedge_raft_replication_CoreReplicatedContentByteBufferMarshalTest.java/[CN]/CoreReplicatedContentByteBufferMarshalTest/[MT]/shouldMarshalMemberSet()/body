{
  ByteBuf buffer=Unpooled.buffer();
  ReplicatedContent message=new CoreMemberSet(asSet(new CoreMember(new AdvertisedSocketAddress("host_a:1"),new AdvertisedSocketAddress("host_a:2")),new CoreMember(new AdvertisedSocketAddress("host_b:101"),new AdvertisedSocketAddress("host_b:102"))));
  marshal.marshal(message,buffer);
  assertThat(marshal.unmarshal(buffer),equalTo(message));
}
