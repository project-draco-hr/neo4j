{
  File dbPath=folder.directory("data");
  outputFile=folder.file("metrics.csv");
  db=new GraphDatabaseFactory().newEmbeddedDatabaseBuilder(dbPath).setConfig(MetricsSettings.neoEnabled,Settings.TRUE).setConfig(csvEnabled,Settings.TRUE).setConfig(csvFile,single.name()).setConfig(csvPath,outputFile.getAbsolutePath()).newGraphDatabase();
}
