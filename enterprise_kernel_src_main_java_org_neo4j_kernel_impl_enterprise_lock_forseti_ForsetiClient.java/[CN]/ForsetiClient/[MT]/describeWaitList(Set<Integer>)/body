{
  if (waitList.size() <= 1) {
    return format("%n<Tx[%d], running>",clientId);
  }
  StringBuilder sb=new StringBuilder(format("%n<Tx[%d], waiting for ",clientId));
  PrimitiveIntIterator iter=waitList.iterator();
  boolean first=true;
  while (iter.hasNext()) {
    int id=iter.next();
    if (id != clientId) {
      involvedParties.add(id);
      sb.append(first ? "" : ",").append("Tx[" + id + "]");
      first=false;
    }
  }
  sb.append(">");
  return sb.toString();
}
