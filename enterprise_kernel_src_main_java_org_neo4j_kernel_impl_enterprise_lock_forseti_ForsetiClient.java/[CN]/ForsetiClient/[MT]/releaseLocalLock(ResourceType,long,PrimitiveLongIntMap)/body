{
  int lockCount=localLocks.remove(resourceId);
  if (lockCount == -1) {
    throw new IllegalStateException(this + " cannot release lock that it does not hold: " + type+ "["+ resourceId+ "].");
  }
  if (lockCount > 1) {
    localLocks.put(resourceId,decrementExactToZero(lockCount));
    return true;
  }
  return false;
}
