{
  StringBuilder buf=new StringBuilder();
  buf.append("DynamicRecord[").append(getId()).append(",used=").append(inUse()).append(",").append("light=").append(isLight).append("(").append(length).append("),data=");
  if (data != null) {
    buf.append("byte[" + data.length + "],");
  }
 else {
    buf.append("null,");
  }
  buf.append("next=").append(nextBlock).append("]");
  return buf.toString();
}
