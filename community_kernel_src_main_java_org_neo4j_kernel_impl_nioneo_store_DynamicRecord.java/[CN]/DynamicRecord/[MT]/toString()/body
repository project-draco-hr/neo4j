{
  StringBuilder buf=new StringBuilder();
  buf.append("DynamicRecord[").append(getId()).append(",used=").append(inUse());
  if (inUse()) {
    buf.append(",");
    if (isLight) {
      buf.append("light,");
    }
    if (data != null) {
      buf.append("byte[" + data.length + "]");
    }
  }
  buf.append(",").append(nextBlock).append("]");
  return buf.toString();
}
