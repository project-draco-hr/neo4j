{
  try {
    for (    Map.Entry<Long,Collection<Pair<Long,String>>> entry : state.getAddedIndexRules().entrySet())     for (    Pair<Long,String> indexedProperty : entry.getValue())     delegate.addIndexRule(entry.getKey(),indexedProperty.other());
  }
 catch (  SchemaException e) {
    throw new TransactionFailureException("Late unexpected schema exception",e);
  }
}
