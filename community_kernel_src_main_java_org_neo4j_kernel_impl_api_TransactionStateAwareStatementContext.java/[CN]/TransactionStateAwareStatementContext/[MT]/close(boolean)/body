{
  if (successful) {
    for (    TxState.NodeState node : state.getNodes()) {
      for (      Long labelId : node.getAddedLabels())       delegate.addLabelToNode(labelId,node.getId());
      for (      Long labelId : node.getRemovedLabels())       delegate.removeLabelFromNode(labelId,node.getId());
    }
  }
  delegate.close(successful);
}
