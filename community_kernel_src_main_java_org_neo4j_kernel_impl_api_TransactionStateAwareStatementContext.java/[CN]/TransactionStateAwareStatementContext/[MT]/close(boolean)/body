{
  if (successful) {
    for (    TxState.NodeState node : state.getNodes()) {
      for (      Long labelId : node.getAddedLabels())       super.addLabelToNode(labelId,node.getId());
      for (      Long labelId : node.getRemovedLabels())       super.removeLabelFromNode(labelId,node.getId());
    }
  }
  super.close(successful);
}
