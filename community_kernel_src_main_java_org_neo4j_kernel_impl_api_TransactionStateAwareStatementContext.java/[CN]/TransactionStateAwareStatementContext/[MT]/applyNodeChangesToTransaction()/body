{
  for (  TxState.NodeState node : state.getNodeStates()) {
    DiffSets<Long> labelDiffSets=node.getLabelDiffSets();
    for (    Long labelId : labelDiffSets.getAdded())     delegate.addLabelToNode(labelId,node.getId());
    for (    Long labelId : labelDiffSets.getRemoved())     delegate.removeLabelFromNode(labelId,node.getId());
  }
}
