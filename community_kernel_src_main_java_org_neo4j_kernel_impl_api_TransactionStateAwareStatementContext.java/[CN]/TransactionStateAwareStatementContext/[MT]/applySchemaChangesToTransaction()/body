{
  try {
    for (    Map.Entry<Long,Collection<Pair<Long,Long>>> entry : state.getAddedIndexRules().entrySet())     for (    Pair<Long,Long> indexedProperty : entry.getValue())     delegate.addIndexRule(entry.getKey(),indexedProperty.other());
  }
 catch (  ConstraintViolationKernelException e) {
    throw new TransactionFailureException("Late unexpected schema exception",e);
  }
}
