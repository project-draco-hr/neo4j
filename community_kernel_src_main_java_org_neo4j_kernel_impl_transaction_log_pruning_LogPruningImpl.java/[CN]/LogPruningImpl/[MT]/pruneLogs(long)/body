{
  if (pruneLock.tryLock()) {
    String prefix="Log Rotation [" + upToVersion + "]: ";
    msgLog.info(prefix + " Starting log pruning.");
    try {
      pruneStrategy.prune(upToVersion);
    }
  finally {
      pruneLock.unlock();
      msgLog.info(prefix + " Log pruning complete.");
    }
  }
}
