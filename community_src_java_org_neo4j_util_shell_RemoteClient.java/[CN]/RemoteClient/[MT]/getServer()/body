{
  try {
    this.server.welcome();
  }
 catch (  RemoteException e) {
    RmiLocation lastLookup=ShellLobby.getInstance().getLastServerLookup();
    if (lastLookup != null) {
      try {
        this.server=ShellLobby.getInstance().findRemoteServer(lastLookup);
        getOutput().println("[Reconnected to server]");
      }
 catch (      ShellException ee) {
      }
catch (      RemoteException ee) {
      }
    }
  }
  return this.server;
}
