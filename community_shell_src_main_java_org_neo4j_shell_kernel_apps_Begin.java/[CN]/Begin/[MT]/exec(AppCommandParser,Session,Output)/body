{
  Transaction tx=currentTransaction(getServer());
  getServer().getDb().beginTx();
  Integer txCount=(Integer)session.get("tx_count");
  int count;
  if (txCount == null) {
    if (tx == null) {
      count=0;
    }
 else {
      count=1;
    }
  }
 else {
    count=txCount;
  }
  session.set("tx_count",++count);
  out.println("Transaction started");
  return null;
}
