{
  String[] shellStartupCommands={format("set PATH=\"%s\";\"%s\";%%PATH%%",jreBinDirectory,binDirectory),"set REPO=" + binDirectory,"cd /D " + workingDirectory,"echo Neo4j Command Prompt","echo.","echo This window is configured with Neo4j on the path.","echo.","echo Available commands:","echo * Neo4jShell","echo * Neo4jImport"};
  String[] cmdArray={"cmd","/C",format("start \"Neo4j Command Prompt\" cmd /K \"%s\"",join(shellStartupCommands," && "))};
  getRuntime().exec(cmdArray);
}
