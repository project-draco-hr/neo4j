{
  Node node1=getGraphDb().createNode();
  Node node2=getGraphDb().createNode();
  node1.createRelationshipTo(node2,MyRelTypes.TEST);
  node1.delete();
  try {
    Transaction tx=getTransaction();
    tx.success();
    tx.finish();
    fail("Should not validate");
  }
 catch (  Exception e) {
  }
  setTransaction(getGraphDb().beginTx());
}
