{
  File storeDir=new File("dir");
  EphemeralFileSystemAbstraction fs=fsr.get();
  fs.mkdirs(storeDir);
  Config configuration=new Config(stringMap(config));
  StoreFactory storeFactory=new StoreFactory(storeDir,configuration,new DefaultIdGeneratorFactory(fs),pageCacheRule.getPageCache(fs),fs,NullLogProvider.getInstance());
  return cleanup.add(storeFactory.openNeoStores(SF_CREATE));
}
