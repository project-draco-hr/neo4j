{
  File storeDir=new File("dir");
  EphemeralFileSystemAbstraction fs=fsr.get();
  fs.mkdirs(storeDir);
  Config configuration=new Config(stringMap(config));
  StoreFactory storeFactory=new StoreFactory(storeDir,configuration,new DefaultIdGeneratorFactory(fs),pageCacheRule.getPageCache(fs),fs,NullLogProvider.getInstance(),new Monitors());
  return cleanup.add(storeFactory.newNeoStore(true));
}
