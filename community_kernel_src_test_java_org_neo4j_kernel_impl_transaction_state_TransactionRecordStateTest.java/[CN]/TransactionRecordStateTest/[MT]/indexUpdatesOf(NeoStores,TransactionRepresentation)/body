{
  NodePropertyCommandsExtractor extractor=new NodePropertyCommandsExtractor();
  transaction.accept(extractor);
  LazyIndexUpdates lazyIndexUpdates=new LazyIndexUpdates(neoStores.getNodeStore(),neoStores.getPropertyStore(),new PropertyLoader(neoStores),extractor.propertyCommandsByNodeIds(),extractor.nodeCommandsById());
  return lazyIndexUpdates;
}
