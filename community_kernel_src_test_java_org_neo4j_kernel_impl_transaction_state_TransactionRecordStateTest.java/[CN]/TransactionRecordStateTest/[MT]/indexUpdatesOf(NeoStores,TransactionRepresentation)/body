{
  NodePropertyCommandsExtractor extractor=new NodePropertyCommandsExtractor();
  transaction.accept(extractor);
  OnlineIndexUpdates lazyIndexUpdates=new OnlineIndexUpdates(neoStores.getNodeStore(),neoStores.getPropertyStore(),new PropertyLoader(neoStores),new PropertyPhysicalToLogicalConverter(neoStores.getPropertyStore()));
  lazyIndexUpdates.feed(extractor.propertyCommandsByNodeIds(),extractor.nodeCommandsById());
  return lazyIndexUpdates;
}
