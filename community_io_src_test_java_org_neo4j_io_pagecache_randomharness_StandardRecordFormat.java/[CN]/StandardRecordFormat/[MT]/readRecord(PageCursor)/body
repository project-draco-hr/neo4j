{
  int offset=cursor.getOffset();
  byte t;
  byte f;
  short f1;
  int r;
  long f2;
  do {
    cursor.setOffset(offset);
    t=cursor.getByte();
    f=cursor.getByte();
    f1=cursor.getShort();
    r=cursor.getInt();
    f2=cursor.getLong();
  }
 while (cursor.shouldRetry());
  return new StandardRecord(t,f,f1,r,f2);
}
