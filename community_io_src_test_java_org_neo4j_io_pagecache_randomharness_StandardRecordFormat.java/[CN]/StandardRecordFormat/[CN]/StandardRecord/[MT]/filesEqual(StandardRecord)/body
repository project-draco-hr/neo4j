{
  if (file == record.file) {
    return true;
  }
  if (file == null || record.file == null) {
    return false;
  }
  byte[] thisPath=file.getPath().getBytes(CHARSET);
  byte[] thatPath=record.file.getPath().getBytes(CHARSET);
  return thisPath[thisPath.length - 1] == thatPath[thatPath.length - 1];
}
