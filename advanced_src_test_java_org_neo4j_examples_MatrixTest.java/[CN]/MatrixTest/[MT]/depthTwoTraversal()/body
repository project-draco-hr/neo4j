{
  TraversalDescription td=Traversal.description().depthFirst().relationships(RelTypes.KNOWS).uniqueness(Uniqueness.NONE).prune(Traversal.pruneAfterDepth(2)).filter(new Predicate<Path>(){
    public boolean accept(    Path item){
      return item.length() > 1;
    }
  }
);
  for (  Path path : td.traverse(getNeoNode())) {
    System.out.println(path);
  }
}
