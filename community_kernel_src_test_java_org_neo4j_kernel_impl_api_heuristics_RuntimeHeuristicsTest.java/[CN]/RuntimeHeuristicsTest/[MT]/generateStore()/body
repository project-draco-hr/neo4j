{
  StoreReadLayer store=mock(StoreReadLayer.class);
  when(store.highestNodeIdInUse()).thenReturn(1000l);
  when(store.nodeExists(anyLong())).thenReturn(true);
  when(store.nodeGetLabels(anyLong())).then(answerWithDistribution(20,ids(0),80,ids(1)));
  when(store.nodeGetRelationshipTypes(anyLong())).then(answerWithDistribution(40,ids(0),60,ids(1)));
  when(store.nodeGetDegree(anyLong(),eq(Direction.INCOMING),anyInt())).then(answerWithDistribution(10,degree(10),30,degree(40),90,degree(50)));
  when(store.nodeGetDegree(anyLong(),eq(Direction.OUTGOING),anyInt())).then(answerWithDistribution(10,degree(1),20,degree(4),85,degree(5)));
  return store;
}
