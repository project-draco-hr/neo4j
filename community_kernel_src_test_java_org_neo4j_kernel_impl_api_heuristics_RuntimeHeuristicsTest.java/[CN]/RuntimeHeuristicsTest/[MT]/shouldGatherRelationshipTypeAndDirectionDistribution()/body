{
  RuntimeHeuristicsService heuristics=new RuntimeHeuristicsService(generateStore(),null);
  heuristics.run();
  heuristics.run();
  assertThat(heuristics.relationshipTypeDistribution(),closeToDistribution(new LabelledDistribution<Integer>().record(asList(0),40).record(asList(1),60).recalculate()));
}
