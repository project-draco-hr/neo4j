def test_traverse_string_types(self):
    self.create_data()
    t = neo4j.Traversal.description().depthFirst()
    res = list(t.relationships('related_to').traverse(self.source).nodes())
    self.assertEqual(len(res), 2)
    res = list(t.relationships('related_to', OUTGOING).traverse(self.source).nodes())
    self.assertEqual(len(res), 2)
    res = list(t.relationships('related_to', INCOMING).traverse(self.source).nodes())
    self.assertEqual(len(res), 1)
