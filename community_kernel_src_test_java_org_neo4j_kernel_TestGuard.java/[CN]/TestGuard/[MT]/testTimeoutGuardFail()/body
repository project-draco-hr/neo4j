{
  ImpermanentGraphDatabase db=new ImpermanentGraphDatabase(stringMap("enable_execution_guard","true"));
  db.beginTx();
  db.getGuard().startTimeout(50);
  int i=0;
  try {
    for (i=0; i < 1000; i++) {
      db.createNode();
      sleep(1);
    }
    fail();
  }
 catch (  GuardTimeoutException e) {
  }
  assertTrue(i > 1);
  assertTrue(i < 100);
}
