{
  ImpermanentGraphDatabase db=new ImpermanentGraphDatabase(stringMap("enable_execution_guard","true"));
  db.beginTx();
  db.getGuard().startTimeout(2);
  int i=0;
  try {
    for (i=0; i < 1000; i++) {
      db.createNode();
    }
    fail();
  }
 catch (  GuardException e) {
    assertTrue(e instanceof GuardTimeoutException);
  }
  assertTrue(i > 0);
}
