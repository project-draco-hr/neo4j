{
  GraphDatabaseAPI db=(GraphDatabaseAPI)new TestGraphDatabaseFactory().newImpermanentDatabaseBuilder().setConfig(GraphDatabaseSettings.execution_guard_enabled,GraphDatabaseSetting.TRUE).newGraphDatabase();
  db.beginTx();
  int timeout=1000;
  db.getGuard().startTimeout(timeout);
  long startTime=currentTimeMillis();
  try {
    for (int i=0; i < 1000; i++) {
      db.createNode();
    }
  }
 catch (  GuardTimeoutException e) {
    if (currentTimeMillis() - startTime < timeout) {
      throw e;
    }
  }
 finally {
    db.shutdown();
  }
}
