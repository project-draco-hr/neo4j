{
  startMaster(2,"round_robin",5001);
  addSlaves(4,5001);
  createTransactionOnMaster();
  assertLastTxId(2,2);
  assertLastTxId(2,3);
  assertLastTxId(1,4);
  assertLastTxId(1,5);
  createTransactionOnMaster();
  assertLastTxId(2,2);
  assertLastTxId(3,3);
  assertLastTxId(3,4);
  assertLastTxId(1,5);
  createTransactionOnMaster();
  assertLastTxId(2,2);
  assertLastTxId(3,3);
  assertLastTxId(4,4);
  assertLastTxId(4,5);
  createTransactionOnMaster();
  assertLastTxId(5,2);
  assertLastTxId(3,3);
  assertLastTxId(4,4);
  assertLastTxId(5,5);
}
