{
  int i=slaves.size() + 1;
  Process process=StartInstanceInAnotherJvm.start(dir.directory("" + (1 + i),true).getAbsolutePath(),MapUtil.stringMap(HaSettings.server_id.name(),"" + (i + 1),HaSettings.ha_server.name(),":" + (6361 + i),HaSettings.cluster_discovery_enabled.name(),"false",HaSettings.cluster_server.name(),"localhost:" + (basePort + i),HaSettings.initial_hosts.name(),"localhost:" + basePort));
  new ProcessStreamHandler(process,false,"[REMOTE SLAVE]").launch();
  return process;
}
