{
  for (int i=1; i <= count; i++)   slaves.add((GraphDatabaseAPI)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(dir.directory("" + (1 + i),true).getAbsolutePath()).setConfig(HaSettings.coordinators,zoo.getConnectionString()).setConfig(HaSettings.server_id,"" + (i + 1)).setConfig(HaSettings.server,"localhost:" + (6361 + i)).newGraphDatabase());
}
