{
  master=(GraphDatabaseAPI)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(dir.directory("1",true).getAbsolutePath()).setConfig(HaSettings.server_id,"1").setConfig(HaSettings.tx_push_factor,"" + pushFactor).setConfig(HaSettings.cluster_discovery_enabled,"false").setConfig(HaSettings.tx_push_strategy,pushStrategy).setConfig(HaSettings.cluster_server,"localhost:" + basePort).newGraphDatabase();
  return master;
}
