{
  startMaster(2,"fixed");
  addSlaves(3);
  createTransactionOnSlave(2);
  assertLastTxId(2,4);
  assertLastTxId(1,3);
  assertLastTxId(2,2);
  assertLastTxId(2,1);
  createTransactionOnSlave(3);
  assertLastTxId(3,4);
  assertLastTxId(3,3);
  assertLastTxId(2,2);
  assertLastTxId(3,1);
  createTransactionOnSlave(4);
  assertLastTxId(4,4);
  assertLastTxId(4,3);
  assertLastTxId(2,2);
  assertLastTxId(4,1);
}
