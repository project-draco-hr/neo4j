{
  Throwable firstError=null;
  for (  Reservation aggregate : aggregates) {
    if (aggregate != null) {
      try {
        aggregate.release();
      }
 catch (      Throwable t) {
        if (firstError == null) {
          firstError=t;
        }
      }
    }
  }
  if (firstError != null) {
    throw Exceptions.launderedException(firstError);
  }
}
