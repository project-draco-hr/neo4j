{
  StoreFileChannel channel=(StoreFileChannel)actualFileSystem.open(fileName,mode);
  if (fileName.getName().equals("neostore.nodestore.db") || fileName.getName().equals("neostore.nodestore.db.labels") || fileName.getName().equals("neostore.relationshipstore.db")|| fileName.getName().equals("neostore.propertystore.db")|| fileName.getName().equals("neostore.propertystore.db.strings")|| fileName.getName().equals("neostore.propertystore.db.arrays")) {
    return new IdJumpingFileChannel(channel,recordSizeFor(fileName));
  }
  return channel;
}
