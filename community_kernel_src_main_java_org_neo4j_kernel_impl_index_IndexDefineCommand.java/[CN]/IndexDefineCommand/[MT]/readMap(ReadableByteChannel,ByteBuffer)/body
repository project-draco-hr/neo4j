{
  Byte size=readByte(channel,buffer);
  if (size == null) {
    return null;
  }
  Map<String,Byte> result=new HashMap<String,Byte>();
  for (int i=0; i < size; i++) {
    String key=read2bLengthAndString(channel,buffer);
    Byte id=readByte(channel,buffer);
    if (key == null || id == null) {
      return null;
    }
    result.put(key,id);
  }
  return result;
}
