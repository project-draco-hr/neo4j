{
  page(page());
  return new PrefetchingIterator<T>(){
    private final int end=position() + pageSize;
    @Override protected T fetchNextOrNull(){
      if (position() >= end) {
        return null;
      }
      return PagingIterator.this.hasNext() ? PagingIterator.this.next() : null;
    }
  }
;
}
