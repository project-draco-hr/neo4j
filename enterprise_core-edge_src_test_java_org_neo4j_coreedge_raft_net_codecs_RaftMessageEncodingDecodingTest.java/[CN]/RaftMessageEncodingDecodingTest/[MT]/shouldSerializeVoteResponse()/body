{
  CoreMember sender=new CoreMember(new AdvertisedSocketAddress("127.0.0.1:5001"),new AdvertisedSocketAddress("127.0.0.2:5001"));
  RaftMessages.Vote.Response<CoreMember> request=new VoteResponseBuilder<CoreMember>().from(sender).grant().term(3).build();
  serializeReadBackAndVerifyMessage(request);
}
