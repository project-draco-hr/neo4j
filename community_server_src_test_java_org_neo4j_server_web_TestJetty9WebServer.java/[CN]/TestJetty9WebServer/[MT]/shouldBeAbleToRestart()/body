{
  WrappingNeoServer neoServer=new WrappingNeoServer(dbRule.getGraphDatabaseAPI());
  WebServer server=neoServer.getWebServer();
  try {
    server.setAddress("127.0.0.1");
    server.setPort(7878);
    server.start();
    server.stop();
    server.start();
  }
  finally {
    try {
      server.stop();
    }
 catch (    Throwable t) {
    }
  }
}
