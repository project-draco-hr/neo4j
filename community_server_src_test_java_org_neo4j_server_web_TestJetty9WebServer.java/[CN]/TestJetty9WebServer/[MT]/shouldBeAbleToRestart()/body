{
  Jetty9WebServer server=new Jetty9WebServer(NullLogProvider.getInstance(),new Config());
  try {
    server.setAddress("127.0.0.1");
    server.setPort(7878);
    server.start();
    server.stop();
    server.start();
  }
  finally {
    try {
      server.stop();
    }
 catch (    Throwable t) {
    }
  }
}
