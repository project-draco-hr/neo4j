{
  seeker=seeker("v\uD800\uDC00lue one\t\"v\uD801\uDC01lue two\"\tv\uD804\uDC03lue three");
  assertTrue(seeker.seek(mark,TAB));
  assertEquals("v????lue one",seeker.extract(mark,extractors.string()).value());
  assertTrue(seeker.seek(mark,TAB));
  assertEquals("v????lue two",seeker.extract(mark,extractors.string()).value());
  assertTrue(seeker.seek(mark,TAB));
  assertEquals("v????lue three",seeker.extract(mark,extractors.string()).value());
}
