{
  int dir=random.nextInt(3);
  RelationshipType type=withName("TYPE_" + random.nextInt(4));
  Relationship rel;
  if (dir == 0) {
    rel=node.createRelationshipTo(other,type);
  }
 else   if (dir == 1) {
    rel=other.createRelationshipTo(node,type);
  }
 else {
    rel=node.createRelationshipTo(node,type);
  }
  return rel;
}
