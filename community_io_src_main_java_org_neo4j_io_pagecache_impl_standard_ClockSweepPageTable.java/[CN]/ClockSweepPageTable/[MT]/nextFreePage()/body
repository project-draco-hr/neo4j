{
  StandardPinnablePage page;
  do {
    page=freeList.get();
    if (page == null) {
      IOException exception=sweeperException;
      if (exception != null) {
        throw new IOException(exception);
      }
      LockSupport.unpark(sweeperThread);
    }
  }
 while (page == null || !freeList.compareAndSet(page,page.next));
  return page;
}
