{
  int loadedPages;
  outerLoop:   do {
    LockSupport.parkNanos(TimeUnit.MILLISECONDS.toNanos(10));
    if (Thread.currentThread().isInterrupted() || freeList.get() == null) {
      return;
    }
    loadedPages=0;
    for (    StandardPinnablePage page : pages) {
      if (page.loaded) {
        loadedPages++;
        if (freeList.get() == null) {
          break outerLoop;
        }
      }
    }
  }
 while (loadedPages <= minLoadedPages);
}
