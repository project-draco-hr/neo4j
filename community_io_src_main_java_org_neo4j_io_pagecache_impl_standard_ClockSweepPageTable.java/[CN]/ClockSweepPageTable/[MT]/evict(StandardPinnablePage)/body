{
  long pageId=page.pageId();
  PageSwapper swapper=page.io();
  page.flush();
  page.setAllBytesToZero();
  page.evicted();
  page.reset(null,UNBOUND_PAGE_ID);
  page.loaded=false;
  do {
    page.next=freeList.get();
  }
 while (!freeList.compareAndSet(page.next,page));
  monitor.evict(pageId,swapper);
}
