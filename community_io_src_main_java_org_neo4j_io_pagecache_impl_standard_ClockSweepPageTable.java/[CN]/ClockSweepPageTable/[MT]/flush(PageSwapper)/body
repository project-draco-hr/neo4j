{
  assertNoSweeperException();
  for (  StandardPinnablePage page : pages) {
    page.lock(PagedFile.PF_SHARED_LOCK);
    try {
      if (page.isBackedBy(io)) {
        assertNoSweeperException();
        page.flush();
      }
    }
  finally {
      page.unlock(PagedFile.PF_SHARED_LOCK);
    }
  }
}
