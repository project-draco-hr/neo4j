{
  File storeDir=new File("dir");
  fs.mkdirs(storeDir);
  idGeneratorFactory=spy(new DefaultIdGeneratorFactory(fs){
    @Override protected IdGenerator instantiate(    FileSystemAbstraction fs,    File fileName,    int grabSize,    long maxValue,    boolean aggressiveReuse,    long highId){
      return spy(super.instantiate(fs,fileName,grabSize,maxValue,aggressiveReuse,highId));
    }
  }
);
  StoreFactory factory=new StoreFactory(storeDir,new Config(),idGeneratorFactory,pageCache,fs,NullLogProvider.getInstance());
  neoStores=factory.openAllNeoStores(true);
  nodeStore=neoStores.getNodeStore();
  return nodeStore;
}
