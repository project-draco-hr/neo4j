{
  File storeDir=new File("dir");
  fs.mkdirs(storeDir);
  IdGeneratorFactory idGeneratorFactory=new DefaultIdGeneratorFactory();
  Monitors monitors=new Monitors();
  StoreFactory factory=new StoreFactory(storeDir,new Config(),idGeneratorFactory,pageCacheRule.getPageCache(fs),fs,NullLogProvider.getInstance(),monitors);
  factory.createNodeStore();
  nodeStore=factory.newNodeStore();
  return nodeStore;
}
