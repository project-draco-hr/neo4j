{
  idGeneratorFactory=new HaIdGeneratorFactory(masterDelegateInvocationHandler,logging,requestContextFactory);
  SwitchToSlave switchToSlave=new SwitchToSlave(logging.getConsoleLog(HighAvailabilityModeSwitcher.class),config,getDependencyResolver(),(HaIdGeneratorFactory)idGeneratorFactory,logging,masterDelegateInvocationHandler,clusterMemberAvailability,clusterClient,requestContextFactory,updateableSchemaState,monitors,kernelExtensions.listFactories());
  SwitchToMaster switchToMaster=new SwitchToMaster(logging,msgLog,this,(HaIdGeneratorFactory)idGeneratorFactory,config,getDependencyResolver(),masterDelegateInvocationHandler,clusterMemberAvailability,monitors);
  highAvailabilityModeSwitcher=new HighAvailabilityModeSwitcher(switchToSlave,switchToMaster,clusterClient,clusterMemberAvailability,logging.getMessagesLog(HighAvailabilityModeSwitcher.class));
  clusterClient.addBindingListener(highAvailabilityModeSwitcher);
  memberStateMachine.addHighAvailabilityMemberListener(highAvailabilityModeSwitcher);
  paxosLife.add(highAvailabilityModeSwitcher);
  ((HaIdGeneratorFactory)idGeneratorFactory).switchToMaster();
  return idGeneratorFactory;
}
