{
  ConcurrentMap<Long,DynamicOwner> dynamicOwners=dynamicOwners(PROPERTY_KEY_NAME);
  if (dynamicOwners == null) {
    return checker;
  }
  return new NameCheckerDecorator<PropertyIndexRecord,ConsistencyReport.PropertyKeyConsistencyReport>(checker,dynamicOwners){
    @Override DynamicOwner.NameOwner owner(    PropertyIndexRecord record){
      return new DynamicOwner.PropertyKey(record);
    }
  }
;
}
