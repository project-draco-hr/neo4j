{
  File storeDir=directory.absolutePath();
  pageCache=new ConfiguringPageCacheFactory(REAL_FS,Config.defaults().augment(stringMap(pagecache_memory.name(),"8m")),NULL,NullLog.getInstance()).getOrCreatePageCache();
  StoreFactory storeFactory=new StoreFactory(storeDir,pageCache,REAL_FS,NullLogProvider.getInstance());
  neoStores=storeFactory.openAllNeoStores(true);
}
