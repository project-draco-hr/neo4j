{
  clusterManager=new ClusterManager(provider,storeDir,stringMap(default_timeout.name(),"1s",tx_push_factor.name(),"0"),new HashMap<Integer,Map<String,String>>(),dbFactory);
  clusterManager.start();
  ClusterManager.ManagedCluster cluster=clusterManager.getDefaultCluster();
  cluster.await(allSeesAllAsAvailable());
  return cluster;
}
