{
  TestLogging testLogging=new TestLogging();
  Files.createDirectories(authFile.getParent());
  Files.write(authFile,"neo4j:fc4c600b43ffe4d5857b4439c35df88f:SHA-256,A42E541F276CF17036DB7818F8B09B1C229AAD52A17F69F4029617F3A554640F,FB7E8AE08A6A7C741F678AD22217808F:\n".getBytes(Charsets.UTF_8));
  FileUserRepository users=new FileUserRepository(authFile,testLogging);
  users.start();
  assertThat(users.numberOfUsers(),equalTo(0));
  testLogging.getMessagesLog(FileUserRepository.class).assertExactly(error(format("Ignoring authorization file \"%s\" (wrong number of line fields [line 1])",authFile.toAbsolutePath())));
}
