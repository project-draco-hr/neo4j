{
  this.storeDir=storeDir;
  graphDbInstance=new GraphDbInstance(storeDir,true);
  Map<Object,Object> params=graphDbInstance.start(graphDbService,config,kernelPanicEventGenerator,new SyncHookFactory());
  nodeManager=graphDbInstance.getConfig().getGraphDbModule().getNodeManager();
  this.graphDbService=graphDbService;
  jmxShutdownHook=initJMX(params);
  enableRemoteShellIfConfigSaysSo(params);
}
