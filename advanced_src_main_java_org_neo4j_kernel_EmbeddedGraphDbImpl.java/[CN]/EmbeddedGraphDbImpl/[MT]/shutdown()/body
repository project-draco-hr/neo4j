{
  if (graphDbInstance.started()) {
    sendShutdownEvent();
    jmxShutdownHook.run();
  }
  if (this.shellService != null) {
    try {
      this.shellService.shutdown();
      this.shellService=null;
    }
 catch (    Throwable t) {
      log.warning("Error shutting down shell server: " + t);
    }
  }
  graphDbInstance.shutdown();
}
