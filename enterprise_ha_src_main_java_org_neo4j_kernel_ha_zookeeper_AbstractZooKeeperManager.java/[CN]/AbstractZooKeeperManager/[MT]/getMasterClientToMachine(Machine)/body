{
  if (master == Machine.NO_MACHINE || master.getServer() == null) {
    return NO_MASTER;
  }
  return new MasterClient(master.getServer().first(),master.getServer().other(),this.msgLog,storeIdGetter,ConnectionLostHandler.NO_ACTION,clientReadTimeout,clientLockReadTimeout,maxConcurrentChannelsPerSlave);
}
