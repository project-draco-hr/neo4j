{
  try {
    if (ignored) {
      handler.onIgnored();
    }
 else     if (error != null) {
      publishError(handler,error);
    }
 else {
      handler.onSuccess(getMetadata());
    }
  }
 catch (  Throwable e) {
    log.error("Failed to write response to driver",e);
  }
 finally {
    onFinish.run();
    clearState();
  }
}
