{
  EphemeralFileSystemAbstraction fsa=new EphemeralFileSystemAbstraction();
  fsa.mkdir(testDir.directory());
  OnDiskVoteState<RaftTestMember> oldOnDiskVoteState=new OnDiskVoteState<>(fsa,testDir.directory(),100,mock(Supplier.class),new RaftTestMemberMarshal());
  oldOnDiskVoteState.votedFor(RaftTestMember.member(1),1);
  OnDiskVoteState<RaftTestMember> newOnDiskVoteState=new OnDiskVoteState<>(fsa,testDir.directory(),100,mock(Supplier.class),new RaftTestMemberMarshal());
  assertEquals(oldOnDiskVoteState.term(),newOnDiskVoteState.term());
  assertEquals(oldOnDiskVoteState.votedFor(),newOnDiskVoteState.votedFor());
}
