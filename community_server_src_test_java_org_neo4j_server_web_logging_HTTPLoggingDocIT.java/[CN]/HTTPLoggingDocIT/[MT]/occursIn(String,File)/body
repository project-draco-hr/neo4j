{
  if (!file.exists()) {
    return false;
  }
  try {
    String s=readTextFile(file,Charsets.UTF_8);
    System.out.println(s);
    System.out.println();
    System.out.println("Does not contain: " + lookFor);
    return s.contains(lookFor);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
