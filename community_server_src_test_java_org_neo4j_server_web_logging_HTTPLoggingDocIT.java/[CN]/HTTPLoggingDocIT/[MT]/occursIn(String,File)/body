{
  if (!file.exists()) {
    return false;
  }
  Scanner scanner=null;
  try {
    scanner=new Scanner(file);
    while (scanner.hasNext()) {
      if (scanner.next().contains(lookFor)) {
        return true;
      }
    }
    return false;
  }
 catch (  FileNotFoundException e) {
    throw new RuntimeException(e);
  }
 finally {
    if (scanner != null) {
      scanner.close();
    }
  }
}
