{
  GraphDatabaseService db=null;
  try {
    db=new EmbeddedGraphDatabase(storeDir);
    new EmbeddedReadOnlyGraphDatabase(storeDir);
    fail();
  }
 catch (  RuntimeException e) {
    assertThat(e.getCause().getCause(),instanceOf(StoreLockException.class));
  }
 finally {
    db.shutdown();
  }
}
