{
  dbRule.getGraphDatabaseService();
  try {
    new TestGraphDatabaseFactory().newEmbeddedDatabaseBuilder(dbRule.getStoreDir().getAbsolutePath()).setConfig(GraphDatabaseSettings.read_only,Settings.TRUE).newGraphDatabase();
