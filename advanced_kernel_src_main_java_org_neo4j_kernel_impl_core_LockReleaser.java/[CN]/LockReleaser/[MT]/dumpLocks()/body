{
  System.out.print("Locks held: ");
  java.util.Iterator<?> itr=lockMap.keySet().iterator();
  if (!itr.hasNext()) {
    System.out.println("NONE");
  }
 else {
    System.out.println();
  }
  while (itr.hasNext()) {
    Transaction transaction=(Transaction)itr.next();
    System.out.println("" + transaction + "->"+ lockMap.get(transaction).size());
  }
}
