{
  Position position=null;
  while (position == null) {
    ExpansionSource next=current.next(this);
    if (next != null) {
      queue.add(next);
      position=next.position(this);
    }
 else {
      current=queue.poll();
      if (current == null) {
        return null;
      }
    }
  }
  return position;
}
