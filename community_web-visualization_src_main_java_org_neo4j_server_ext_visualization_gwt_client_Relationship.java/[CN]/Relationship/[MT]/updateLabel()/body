{
  Style style=label.getElement().getStyle();
  double x=getLabelCenter(from.getCenterX(),to.getCenterX()) - label.getOffsetWidth() / 2;
  style.setLeft(x,Unit.PX);
  double y=getLabelCenter(from.getCenterY(),to.getCenterY()) - label.getOffsetHeight() / 2;
  style.setTop(y,Unit.PX);
  return style;
}
