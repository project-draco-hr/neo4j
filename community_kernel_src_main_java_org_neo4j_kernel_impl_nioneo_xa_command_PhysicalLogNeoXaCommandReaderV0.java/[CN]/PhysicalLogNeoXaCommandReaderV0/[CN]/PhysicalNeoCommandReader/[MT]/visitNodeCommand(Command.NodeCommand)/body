{
  if (!readAndFlip(byteChannel,scratch,8)) {
    return false;
  }
  long id=scratch.getLong();
  NodeRecord before=readNodeRecord(id);
  if (before == null) {
    return false;
  }
  NodeRecord after=readNodeRecord(id);
  if (after == null) {
    return false;
  }
  if (!before.inUse() && after.inUse()) {
    after.setCreated();
  }
  command.init(before,after);
  return true;
}
