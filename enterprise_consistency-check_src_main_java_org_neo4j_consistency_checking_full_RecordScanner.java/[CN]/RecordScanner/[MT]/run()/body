{
  try {
    int entryCount=0;
    for (    RECORD record : store) {
      if (!continueScanning) {
        return;
      }
      processor.process(record);
      progress.set(entryCount++);
    }
  }
  finally {
    try {
      store.close();
    }
 catch (    IOException e) {
      progress.failed(e);
    }
    processor.close();
    progress.done();
  }
}
