{
  final String mainClassName="org.neo4j.release.it.std.exec.HelloWorldApp";
  ObservablePlatform platform=buildPlatform(forMainClass(named(mainClassName)),includingTargetClasses(),includingTargetTestClasses(),provisioning(mavenBundle().groupId("org.neo4j").artifactId("neo4j-kernel").version("1.2-1.2.M01")));
  platform.start();
}
