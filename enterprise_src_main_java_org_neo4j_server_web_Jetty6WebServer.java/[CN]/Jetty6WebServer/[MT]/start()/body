{
  jetty=new Server(jettyPort);
  jetty.setStopAtShutdown(true);
  MovedContextHandler redirector=new MovedContextHandler();
  jetty.addHandler(redirector);
  loadRootRedirect();
  loadStaticContent();
  loadJAXRSPackages();
  try {
    jetty.start();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
