{
  params.put(Configuration.editionName.name(),"Community");
  params.put(Configuration.operationalMode.name(),UsageDataKeys.OperationalMode.single.name());
  return super.newFacade(storeDir,params,newDependencies(dependencies).settingsClasses(toList(append(GraphDatabaseSettings.class,dependencies.settingsClasses()))),graphDatabaseFacade);
}
