{
  for (  DynamicRecord record : records) {
    DiffRecordStore<DynamicRecord> store=(record.getType() == PropertyType.STRING.intValue()) ? getStringStore() : getArrayStore();
    store.forceUpdateRecord(record);
    if (!Record.NO_NEXT_BLOCK.value(record.getNextBlock()))     getPropertyStore().markDirty(record.getNextBlock());
  }
}
