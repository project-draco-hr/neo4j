{
  File authStore=ServerTestUtils.getRelativeFile(GraphDatabaseSettings.auth_store);
  FileUtils.deleteFile(authStore);
  server=EnterpriseServerBuilder.server().withProperty(GraphDatabaseSettings.auth_enabled.name(),Boolean.toString(authEnabled)).withProperty(GraphDatabaseSettings.auth_store.name(),authStore.getAbsolutePath()).build();
  server.start();
}
