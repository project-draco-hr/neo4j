{
  try {
    Map<String,String> extraParams=stringMap(GraphDatabaseSettings.pagecache_swapper.name(),TEST_PAGESWAPPER_NAME);
    CoreClusterMember clusterMember=new CoreClusterMember(0,3,buildAddresses(set(0,1,2)),discovery,defaultFormat().toString(),storeDir.directory(),extraParams,emptyMap());
    clusterMember.start();
    fail("Should not have created database with custom IO configuration in Core Mode.");
  }
 catch (  RuntimeException ex) {
    assertEquals(CoreGraphDatabase.CUSTOM_IO_EXCEPTION_MESSAGE,ex.getMessage());
  }
}
