{
  try {
    txManager.begin();
    id=(int)idGenerator.nextId(RelationshipType.class);
    persistence.createRelationshipType(id,name);
    txManager.commit();
    success=true;
  }
 catch (  Throwable t) {
    t.printStackTrace();
    try {
      txManager.rollback();
    }
 catch (    Throwable tt) {
      tt.printStackTrace();
    }
  }
 finally {
    this.notify();
  }
}
