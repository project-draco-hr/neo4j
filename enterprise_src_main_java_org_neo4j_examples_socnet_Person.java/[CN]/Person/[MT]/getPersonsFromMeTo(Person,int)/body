{
  PathFinder<Path> finder=GraphAlgoFactory.shortestPath(Traversal.expanderForTypes(FRIEND,Direction.BOTH),maxDepth);
  Path path=finder.findSinglePath(underlyingNode,otherPerson.getUnderlyingNode());
  return new IterableWrapper<Person,Node>(path.nodes()){
    @Override protected Person underlyingObjectToObject(    Node node){
      return new Person(node);
    }
  }
;
}
