{
  PropertyReader propertyReader=mock(PropertyReader.class);
  PropertyBlock propertyBlock=mock(PropertyBlock.class);
  when(propertyBlock.getKeyIndexId()).thenReturn(propertyKeyId);
  when(propertyReader.propertyBlocks(nodeRecord)).thenReturn(asList(propertyBlock));
  when(propertyReader.propertyValue(any(PropertyBlock.class))).thenReturn(stringProperty(propertyKeyId,propertyValue));
  return propertyReader;
}
