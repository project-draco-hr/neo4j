{
  data.get();
  String parameterString=createParameterString(params);
  String queryString="{\"query\": \"" + createScript(script) + "\","+ parameterString+ ","+ "\"format\": \""+ CypherService.GENERICS_FRIENDLY+ "\"}";
  gen.get().expectedStatus(status.getStatusCode()).payload(queryString).description(AsciidocHelper.createCypherSnippet(script));
  return gen.get().post(endpoint).entity();
}
