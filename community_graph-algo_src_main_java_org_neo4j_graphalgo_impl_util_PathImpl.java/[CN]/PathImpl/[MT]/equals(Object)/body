{
  if (this == obj) {
    return true;
  }
 else   if (obj instanceof Path) {
    Path other=(Path)obj;
    if (!start.equals(other.startNode())) {
      return false;
    }
    return iteratorsEqual(this.relationships().iterator(),other.relationships().iterator());
  }
 else {
    return false;
  }
}
