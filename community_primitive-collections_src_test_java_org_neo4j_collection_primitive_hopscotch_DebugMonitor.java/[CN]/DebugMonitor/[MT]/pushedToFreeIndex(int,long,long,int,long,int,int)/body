{
  if (indexes.contains(intendedIndex) || indexes.contains(neighborIndex) || indexes.contains(fromIndex)|| indexes.contains(toIndex)|| values.contains(key)) {
    System.out.println(format("[%d] --- [%d] -->> [%d] --> %d --> [%d]  %s",intendedIndex,neighborIndex,fromIndex,key,toIndex,hopBitsAsString(oldNeighborHopBits,newNeighborHopBits)));
  }
  return true;
}
