{
  RandomAccessFile rFile=new RandomAccessFile(failureFile,"r");
  try {
    FileChannel channel=rFile.getChannel();
    byte[] data=new byte[(int)channel.size()];
    data[0]=1;
    channel.read(ByteBuffer.wrap(data));
    channel.close();
    return !allZero(data);
  }
  finally {
    rFile.close();
  }
}
