{
  File failureFile=failureFile(indexId);
  if (!failureFile.exists()) {
    return null;
  }
  StringBuilder result=new StringBuilder();
  int count=0;
  for (  String line : asIterable(failureFile,"utf-8")) {
    if (count++ > 0) {
      result.append(lineSeparator());
    }
    result.append(line);
  }
  return result.toString();
}
