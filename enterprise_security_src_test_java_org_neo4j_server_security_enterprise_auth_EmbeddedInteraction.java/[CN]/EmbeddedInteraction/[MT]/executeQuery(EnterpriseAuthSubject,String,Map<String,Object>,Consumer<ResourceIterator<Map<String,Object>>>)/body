{
  try (InternalTransaction tx=db.beginTransaction(KernelTransaction.Type.implicit,subject)){
    Map<String,Object> p=(params == null) ? Collections.emptyMap() : params;
    resultConsumer.accept(db.execute(call,p));
    tx.success();
    return "";
  }
 catch (  Exception e) {
    return e.getMessage();
  }
}
