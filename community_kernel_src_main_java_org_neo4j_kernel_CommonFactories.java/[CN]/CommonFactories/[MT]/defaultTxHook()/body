{
  return new TxHook(){
    @Override public void initializeTransaction(    int eventIdentifier){
    }
    public boolean hasAnyLocks(    Transaction tx){
      return false;
    }
    public void finishTransaction(    int eventIdentifier,    boolean success){
    }
    @Override public boolean freeIdsDuringRollback(){
      return true;
    }
  }
;
}
