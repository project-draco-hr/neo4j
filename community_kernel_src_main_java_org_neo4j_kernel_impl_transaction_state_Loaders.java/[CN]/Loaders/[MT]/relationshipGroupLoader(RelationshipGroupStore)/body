{
  return new Loader<Long,RelationshipGroupRecord,Integer>(){
    @Override public RelationshipGroupRecord newUnused(    Long key,    Integer type){
      return new RelationshipGroupRecord(key,type);
    }
    @Override public RelationshipGroupRecord load(    Long key,    Integer type){
      return store.getRecord(key);
    }
    @Override public void ensureHeavy(    RelationshipGroupRecord record){
    }
    @Override public RelationshipGroupRecord clone(    RelationshipGroupRecord record){
      throw new UnsupportedOperationException();
    }
  }
;
}
