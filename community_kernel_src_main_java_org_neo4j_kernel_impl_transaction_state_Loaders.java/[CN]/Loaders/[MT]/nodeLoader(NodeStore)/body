{
  return new Loader<Long,NodeRecord,Void>(){
    @Override public NodeRecord newUnused(    Long key,    Void additionalData){
      return andMarkAsCreated(new NodeRecord(key,false,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue()));
    }
    @Override public NodeRecord load(    Long key,    Void additionalData){
      return store.getRecord(key);
    }
    @Override public void ensureHeavy(    NodeRecord record){
      store.ensureHeavy(record);
    }
    @Override public NodeRecord clone(    NodeRecord nodeRecord){
      return nodeRecord.clone();
    }
  }
;
}
