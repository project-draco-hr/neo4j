{
  return new Loader<Integer,LabelTokenRecord,Void>(){
    @Override public LabelTokenRecord newUnused(    Integer key,    Void additionalData){
      return andMarkAsCreated(new LabelTokenRecord(key));
    }
    @Override public LabelTokenRecord load(    Integer key,    Void additionalData){
      return store.getRecord(key);
    }
    @Override public void ensureHeavy(    LabelTokenRecord record){
      store.ensureHeavy(record);
    }
    @Override public LabelTokenRecord clone(    LabelTokenRecord record){
      throw new UnsupportedOperationException();
    }
  }
;
}
