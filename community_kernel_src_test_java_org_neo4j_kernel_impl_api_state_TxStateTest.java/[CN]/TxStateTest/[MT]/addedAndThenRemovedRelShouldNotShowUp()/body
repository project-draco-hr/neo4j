{
  int startNode=1, endNode=2, relType=0;
  long relId=state.relationshipDoCreate(relType,startNode,endNode);
  state.relationshipDoDelete(relId,startNode,endNode,relType);
  long otherRel=relId + 1;
  assertThat(state.augmentRelationships(startNode,OUTGOING,iterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(startNode,BOTH,iterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(endNode,INCOMING,iterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(endNode,BOTH,iterator(otherRel)),containsLongs(otherRel));
}
