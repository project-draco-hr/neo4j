{
  int startNode=1, endNode=2, relType=0;
  long relId=state.relationshipDoCreate(relType,startNode,endNode);
  state.relationshipDoDelete(relId);
  long otherRel=relId + 1;
  assertThat(state.augmentRelationships(startNode,OUTGOING,asPrimitiveIterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(startNode,BOTH,asPrimitiveIterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(endNode,INCOMING,asPrimitiveIterator(otherRel)),containsLongs(otherRel));
  assertThat(state.augmentRelationships(endNode,BOTH,asPrimitiveIterator(otherRel)),containsLongs(otherRel));
}
