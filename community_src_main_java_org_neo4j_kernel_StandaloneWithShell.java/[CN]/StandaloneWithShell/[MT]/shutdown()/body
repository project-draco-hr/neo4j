{
  if (shutdownInitiated.compareAndSet(false,true)) {
    log.info("Shutting down...");
    try {
      if (getGraphDb() != null) {
        getGraphDb().shutdown();
        this.embeddedDb=null;
      }
    }
 catch (    Throwable t) {
      log.warning("Error shutting down Neo: " + t);
    }
  }
}
