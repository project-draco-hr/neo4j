{
  Supplier<TransactionIdStore> transactionIdStore=neoStoreDataSource.getDependencyResolver().provideDependency(TransactionIdStore.class);
  SessionStateMachine.SPI spi=new StandardStateMachineSPI(connectionDescriptor,usageData,gds,statementRunner,logging,authentication,txBridge,transactionIdStore,sessionTracker);
  return new SessionStateMachine(spi);
}
