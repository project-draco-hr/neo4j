{
  exception.expect(ProcedureException.class);
  exception.expectMessage("Don't know how to map `char[]` to the Neo4j Type System.\n" + "Please refer to to the documentation for full details.\n" + "For your reference, known types are: [boolean, double, java.lang.Boolean, java.lang"+ ".Double, java.lang.Long, java.lang.Number, java.lang.Object, java.lang.String, java"+ ".util.List, java.util.Map, long]");
  compile(FunctionWithInvalidOutput.class).get(0);
}
