{
  return new ComparativeRecordChecker<PropertyRecord,PropertyKeyTokenRecord,ConsistencyReport.PropertyConsistencyReport>(){
    @Override public void checkReference(    PropertyRecord record,    PropertyKeyTokenRecord referred,    CheckerEngine<PropertyRecord,ConsistencyReport.PropertyConsistencyReport> engine,    RecordAccess records){
      if (!referred.inUse()) {
        engine.report().keyNotInUse(block,referred);
      }
    }
    @Override public String toString(){
      return "PROPERTY_KEY";
    }
  }
;
}
