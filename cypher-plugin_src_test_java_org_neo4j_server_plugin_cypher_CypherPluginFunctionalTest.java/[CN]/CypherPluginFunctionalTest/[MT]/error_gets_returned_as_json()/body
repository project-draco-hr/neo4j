{
  String script="start x  = (" + data.get().get("I").getId() + ") return x.dummy";
  gen.get().expectedStatus(Status.BAD_REQUEST.getStatusCode()).payload("{\"query\": \"" + script + "\"}").description(formatCypher(script));
  String response=gen.get().post(ENDPOINT).entity();
  assertEquals(3,((Map)JsonHelper.jsonToMap(response)).size());
}
