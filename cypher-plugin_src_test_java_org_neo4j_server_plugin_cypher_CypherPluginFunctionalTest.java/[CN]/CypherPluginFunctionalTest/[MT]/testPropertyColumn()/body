{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"query\": \"start x  = (" + data.get().get("I").getId() + ") match (x) --> (n) return n.name?, n.age?\"}").post(ENDPOINT).entity();
  System.out.println(response);
  assertTrue(response.contains("you"));
  assertTrue(response.contains("him"));
  assertTrue(!response.contains("\"x\""));
}
