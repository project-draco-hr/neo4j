{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("query=" + URLEncoder.encode("start x  = (" + data.get().get("I").getId() + ") match (x) -- (n) return n, n.name","UTF-8")).payloadType(MediaType.APPLICATION_FORM_URLENCODED_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("you"));
  assertTrue(response.contains("him"));
}
