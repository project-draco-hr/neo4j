{
  String script="start x  = (" + data.get().get("I").getId() + ") match path = (x--friend) return path, friend.name";
  gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"query\": \"" + script + "\"}").description(AsciidocHelper.createCypherSnippet(script));
  String response=gen.get().post(ENDPOINT).entity();
  assertEquals(2,(JsonHelper.jsonToMap(response)).size());
  assertTrue(response.contains("data"));
  assertTrue(response.contains("you"));
}
