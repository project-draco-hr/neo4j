{
  try {
    server.setNodeProperty(nodeId,key,input.readValue(body));
  }
 catch (  BadInputException e) {
    return output.badRequest(e);
  }
catch (  NodeNotFoundException e) {
    return output.notFound(e);
  }
catch (  PropertyValueException e) {
    return output.badRequest(e);
  }
  return nothing();
}
