{
  File file=getFile();
  if (null == file) {
    showMessageDialog(parentComponent,"Did not find location of .vmoptions file","Error",ERROR_MESSAGE);
    return;
  }
  try {
    ensureFileAndParentDirectoriesExists(file);
    model.editFile(file);
  }
 catch (  IOException e) {
    e.printStackTrace(System.out);
    showWrappedMessageDialog(parentComponent,format("Couldn't openDirectory %s, please openDirectory the file manually",file.getAbsolutePath()),"Error",ERROR_MESSAGE);
  }
}
