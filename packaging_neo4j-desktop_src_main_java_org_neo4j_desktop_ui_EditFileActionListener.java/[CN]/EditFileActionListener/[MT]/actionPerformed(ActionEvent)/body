{
  File file=getFile();
  if (null == file) {
    showMessageDialog(parentComponent,"Did not find location of .vmoptions file","Error",ERROR_MESSAGE);
    return;
  }
  try {
    ensureFileAndParentDirectoriesExists(file);
    environment.editFile(file);
  }
 catch (  IOException e) {
    e.printStackTrace();
    showMessageDialog(parentComponent,format("Couldn't open %s, please open the file manually",file.getAbsolutePath()),"Error",ERROR_MESSAGE);
  }
}
