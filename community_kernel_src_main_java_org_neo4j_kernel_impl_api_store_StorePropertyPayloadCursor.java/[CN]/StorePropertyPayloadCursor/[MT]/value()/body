{
switch (type()) {
case BOOL:
    return booleanValue();
case BYTE:
  return byteValue();
case SHORT:
return shortValue();
case CHAR:
return charValue();
case INT:
return intValue();
case LONG:
return longValue();
case FLOAT:
return floatValue();
case DOUBLE:
return doubleValue();
case SHORT_STRING:
return shortStringValue();
case STRING:
return stringValue();
case SHORT_ARRAY:
return shortArrayValue();
case ARRAY:
return arrayValue();
default :
throw new IllegalStateException("No such type:" + type());
}
}
