{
  try {
    if (masterServer == null) {
      if (broker.getMaster() == null && broker instanceof ZooKeeperBroker) {
        messageLog.logMessage("ZooKeeper broker returned null master");
        newMaster(storeId,new NullPointerException("master returned from broker"));
      }
 else       if (broker.getMaster().first() == null) {
        newMaster(storeId,new NullPointerException("master returned from broker"));
      }
      slaveOperations.receive(broker.getMaster().first().pullUpdates(slaveOperations.getSlaveContext(-1)));
    }
  }
 catch (  ZooKeeperException e) {
    newMaster(storeId,e);
    throw e;
  }
catch (  NoMasterException e) {
    newMaster(storeId,e);
    throw e;
  }
catch (  ComException e) {
    throw e;
  }
}
