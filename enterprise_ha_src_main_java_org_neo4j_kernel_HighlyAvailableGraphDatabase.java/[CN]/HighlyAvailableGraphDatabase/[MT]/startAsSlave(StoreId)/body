{
  messageLog.logMessage("Starting[" + machineId + "] as slave",true);
  this.storeId=storeId;
  SlaveGraphDatabase slaveGraphDatabase=new SlaveGraphDatabase(storeDir,config,this,broker,messageLog,responseReceiver,slaveUpdateMode.createUpdater(broker),nodeLookup,relationshipLookups);
  logHaInfo("Started as slave");
  this.startupTime=System.currentTimeMillis();
  return slaveGraphDatabase;
}
