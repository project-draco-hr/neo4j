{
  try {
    MasterUtil.applyReceivedTransactions(response,HighlyAvailableGraphDatabase.this,MasterUtil.NO_ACTION,machineId);
    updateTime();
    return response.response();
  }
 catch (  IOException e) {
    newMaster(e);
    throw new RuntimeException(e);
  }
 finally {
    response.close();
  }
}
