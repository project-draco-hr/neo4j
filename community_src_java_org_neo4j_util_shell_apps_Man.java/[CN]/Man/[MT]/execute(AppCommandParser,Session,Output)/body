{
  if (parser.arguments().size() == 0) {
    throw new ShellException("Need to supply an app: man <app>");
  }
  App app=this.getApp(parser);
  try {
    out.println("");
    out.println(this.fixDesciption(app.getDescription()));
    println(out,"");
    boolean hasOptions=false;
    for (    String option : app.getAvailableOptions()) {
      hasOptions=true;
      String description=this.fixDesciption(app.getDescription(option));
      OptionValueType type=app.getOptionValueType(option);
      println(out,"-" + option + "\t"+ description+ " "+ (type == OptionValueType.NONE ? "(no value)" : "")+ (type == OptionValueType.MAY ? "(may have value)" : "")+ (type == OptionValueType.MUST ? "(must have value)" : ""));
    }
    if (hasOptions) {
      println(out,"");
    }
  }
 catch (  RemoteException e) {
    throw new ShellException(e);
  }
  return null;
}
