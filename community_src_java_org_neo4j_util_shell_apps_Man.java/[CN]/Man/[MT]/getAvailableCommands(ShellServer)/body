{
  if (availableCommands == null) {
    Set<String> packages=((AbstractAppServer)server).getPackages();
    availableCommands=new TreeSet<String>();
    for (    Class<? extends App> appClass : ClassLister.listClassesExtendingOrImplementing(App.class,packages)) {
      if (packages.contains(appClass.getPackage().getName())) {
        availableCommands.add(appClass.getSimpleName().toLowerCase());
      }
    }
    for (    String exitCommand : AbstractClient.getExitCommands()) {
      availableCommands.add(exitCommand);
    }
  }
  return availableCommands;
}
