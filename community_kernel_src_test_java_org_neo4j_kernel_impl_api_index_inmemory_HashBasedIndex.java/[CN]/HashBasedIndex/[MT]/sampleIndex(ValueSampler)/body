{
  try {
    iterateAll(new IndexEntryIterator(){
      @Override public void visitEntry(      Object value,      Set<Long> nodeIds) throws Exception {
        for (int i=nodeIds.size(); i > 0; i--) {
          sampler.include(value.toString());
        }
      }
    }
);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
