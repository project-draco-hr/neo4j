{
  return !ids.hasNext() ? Predicates.<NodeRecord>TRUE() : new Predicate<NodeRecord>(){
    private long current=ids.next();
    private boolean end;
    @Override public boolean accept(    NodeRecord node){
      if (!end && current == node.getId()) {
        end=!ids.hasNext();
        if (!end) {
          current=ids.next();
        }
        return false;
      }
      return true;
    }
  }
;
}
