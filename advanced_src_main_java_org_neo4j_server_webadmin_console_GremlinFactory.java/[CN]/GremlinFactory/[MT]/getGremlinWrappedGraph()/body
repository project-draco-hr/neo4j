{
  GraphDatabaseService dbInstance=NeoServer.INSTANCE.database();
  System.out.println("GremlinFactory: " + dbInstance);
  TransactionalGraph graph;
  try {
    graph=new Neo4jGraph(dbInstance,new LuceneIndexService(dbInstance));
  }
 catch (  UnsupportedOperationException e) {
    graph=new Neo4jGraph(dbInstance,new MockIndexService());
  }
  return graph;
}
