{
  final StoreFactory storeFactory=new StoreFactory(fileSystem,workingDir,pageCache,NullLogProvider.getInstance());
  try (NeoStores neoStores=storeFactory.openNeoStores(NeoStores.StoreType.PROPERTY,NeoStores.StoreType.NODE,NeoStores.StoreType.SCHEMA)){
    PropertyStore propertyStore=neoStores.getPropertyStore();
    NodeStore nodeStore=neoStores.getNodeStore();
    SchemaStore schemaStore=neoStores.getSchemaStore();
    PrimitiveLongObjectMap<List<DuplicateCluster>> duplicateClusters=collectConflictingProperties(propertyStore);
    resolveConflicts(duplicateClusters,propertyStore,nodeStore,schemaStore);
  }
 }
