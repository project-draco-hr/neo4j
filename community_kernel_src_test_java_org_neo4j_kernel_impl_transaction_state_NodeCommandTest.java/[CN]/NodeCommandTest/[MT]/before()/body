{
  File dir=new File("dir");
  fs.get().mkdirs(dir);
  Config config=StoreFactory.configForStoreDir(new Config(),dir);
  Monitors monitors=new Monitors();
  @SuppressWarnings("deprecation") StoreFactory storeFactory=new StoreFactory(config,new DefaultIdGeneratorFactory(),pageCacheRule.getPageCache(fs.get()),fs.get(),StringLogger.DEV_NULL,monitors);
  storeFactory.createNodeStore();
  nodeStore=storeFactory.newNodeStore();
}
