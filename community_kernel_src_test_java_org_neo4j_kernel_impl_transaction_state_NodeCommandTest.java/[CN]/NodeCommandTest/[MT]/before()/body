{
  File dir=new File("dir");
  fs.get().mkdirs(dir);
  Monitors monitors=new Monitors();
  @SuppressWarnings("deprecation") StoreFactory storeFactory=new StoreFactory(dir,new Config(),new DefaultIdGeneratorFactory(),pageCacheRule.getPageCache(fs.get()),fs.get(),NullLogProvider.getInstance(),monitors);
  storeFactory.createNodeStore();
  nodeStore=storeFactory.newNodeStore();
}
