{
  LifecycleException ex=null;
  for (int i=instances.size() - 1; i >= 0; i--) {
    LifecycleInstance lifecycleInstance=instances.get(i);
    try {
      lifecycleInstance.stop();
    }
 catch (    LifecycleException e) {
      if (ex != null) {
        ex.addSuppressed(e);
      }
 else {
        ex=e;
      }
    }
  }
  return ex;
}
