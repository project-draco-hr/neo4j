{
  long nodeId=0l;
  try (Transaction transaction=slave.beginTx()){
    Node node=slave.getNodeById(nodeId);
    assertDidntStartMasterTx();
    count(node.getLabels());
    assertDidntStartMasterTx();
    readAllRels(node);
    assertDidntStartMasterTx();
    readEachProperty(node);
    assertDidntStartMasterTx();
    transaction.success();
  }
   assertDidntStartMasterTx();
}
