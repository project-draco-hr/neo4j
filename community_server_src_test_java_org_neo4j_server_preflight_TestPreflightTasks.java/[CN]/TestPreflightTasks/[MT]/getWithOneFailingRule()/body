{
  PreflightTask[] rules=new PreflightTask[5];
  for (int i=0; i < rules.length; i++) {
    rules[i]=new PreflightTask(){
      @Override public boolean run(){
        return true;
      }
      @Override public String getFailureMessage(){
        return "blah blah";
      }
    }
;
  }
  rules[rules.length / 2]=new PreflightTask(){
    @Override public boolean run(){
      return false;
    }
    @Override public String getFailureMessage(){
      return "blah blah";
    }
  }
;
  return rules;
}
