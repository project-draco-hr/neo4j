{
  return new CountsTracker(resourceManager.logProvider(),resourceManager.fileSystem(),resourceManager.pageCache(),Config.empty(),resourceManager.testPath()).setInitializer(new DataInitializer<CountsAccessor.Updater>(){
    @Override public void initialize(    CountsAccessor.Updater updater){
    }
    @Override public long initialVersion(){
      return FileVersion.INITIAL_TX_ID;
    }
  }
);
}
