{
  final int size=10;
  SneakyEntity oldEntity=new SneakyEntity(0l,size){
    @Override void doThisBadStuffInSizeCall(){
      updateSize(size + 1);
      cache.updateSize(this,size,size + 1);
    }
  }
;
  cache.put(oldEntity);
  oldEntity.updateSize(size);
  Entity newEntity=new Entity(0l,11);
  cache.put(newEntity);
  assertEquals(11,cache.size());
}
