{
  Transaction transaction=neo.beginTx();
  Traverser traverser=neo.getReferenceNode().traverse(Order.BREADTH_FIRST,StopEvaluator.DEPTH_ONE,ReturnableEvaluator.ALL_BUT_START_NODE,MyRelTypes.STOREDGRAPH,Direction.BOTH);
  Node result=null;
  for (  Node node : traverser) {
    if (!node.hasProperty("GraphIdentifier")) {
      continue;
    }
    if (node.getProperty("GraphIdentifier").equals(identifier)) {
      result=node;
      break;
    }
  }
  transaction.success();
  transaction.finish();
  if (result == null) {
    return null;
  }
  return new Graph(result);
}
