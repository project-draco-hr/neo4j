{
  long startNode=helper.createNode();
  String jsonString="{\"to\" : \"" + functionalTestHelper.dataUri() + "node/"+ "999999\", \"type\" : \"LOVES\", \"data\" : {\"foo\" : \"bar\"}}";
  ClientResponse response=Client.create().resource(functionalTestHelper.dataUri() + "node/" + startNode+ "/relationships").type(MediaType.APPLICATION_JSON).accept(MediaType.APPLICATION_JSON).entity(jsonString).post(ClientResponse.class);
  assertEquals(400,response.getStatus());
  response.close();
}
