{
  ValueNode valueNode=new ValueNode(12L,asList(label("User"),label("Banana")),map("name","Bob","age",14));
  assertThat(serialized(valueNode),equalTo("B1 71 91 B3 4E 0C 92 84 55 73 65 72 86 42 61 6E" + lineSeparator() + "61 6E 61 A2 84 6E 61 6D 65 83 42 6F 62 83 61 67"+ lineSeparator()+ "65 0E"));
}
