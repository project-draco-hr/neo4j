{
  ValueRelationship valueRelationship=new ValueRelationship(12L,1L,2L,RelationshipType.withName("KNOWS"),map("name","Bob","age",14));
  assertThat(serialized(valueRelationship),equalTo("B1 71 91 B5 52 0C 01 02 85 4B 4E 4F 57 53 A2 84" + lineSeparator() + "6E 61 6D 65 83 42 6F 62 83 61 67 65 0E"));
}
