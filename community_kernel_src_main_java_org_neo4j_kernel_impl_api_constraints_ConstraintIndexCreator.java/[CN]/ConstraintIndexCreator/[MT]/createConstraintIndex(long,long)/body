{
  return new Transactor.Statement<IndexDescriptor,SchemaKernelException>(){
    @Override public IndexDescriptor perform(    StatementOperationParts statement,    StatementState kernelStatement) throws SchemaKernelException {
      IndexDescriptor rule=new IndexDescriptor(labelId,propertyKeyId);
      kernelStatement.txState().constraintIndexRuleDoAdd(rule);
      return rule;
    }
  }
;
}
