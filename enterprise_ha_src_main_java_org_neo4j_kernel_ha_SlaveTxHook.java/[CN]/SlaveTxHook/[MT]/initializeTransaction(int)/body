{
  try {
    responseReceiver.receive(broker.getMaster().first().initializeTx(responseReceiver.getSlaveContext(eventIdentifier)));
  }
 catch (  ZooKeeperException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
catch (  ComException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
}
