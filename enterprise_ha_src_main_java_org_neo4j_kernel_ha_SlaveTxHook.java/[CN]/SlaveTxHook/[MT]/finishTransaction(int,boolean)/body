{
  try {
    responseReceiver.receive(broker.getMaster().first().finishTransaction(responseReceiver.getSlaveContext(eventIdentifier),success));
  }
 catch (  ZooKeeperException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
catch (  ComException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
}
