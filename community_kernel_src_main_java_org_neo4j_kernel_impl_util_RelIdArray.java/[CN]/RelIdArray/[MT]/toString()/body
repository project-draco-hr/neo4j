{
  IdBlock loopBlock=DirectionWrapper.BOTH.getBlock(this);
  return "RelIdArray for type " + type + ":\n"+ "  out: "+ (outBlock != null ? outBlock.toString() : "")+ "\n"+ "  in:  "+ (inBlock != null ? inBlock.toString() : "")+ "\n"+ "  loop:"+ (loopBlock != null ? loopBlock.toString() : "")+ "\n";
}
