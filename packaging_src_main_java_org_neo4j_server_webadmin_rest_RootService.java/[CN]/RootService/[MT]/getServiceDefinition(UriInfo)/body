{
  ServerRootRepresentation rootRepresentation=new ServerRootRepresentation(uriInfo.getBaseUri(),new ConsoleService((SessionFactory)null,null,null),new JmxService(null,null),new MonitorService(null));
  String entity=null;
  try {
    entity=JsonRenderers.DEFAULT.render(rootRepresentation);
  }
 catch (  BadInputException e) {
    return Response.serverError().build();
  }
  return Response.ok(entity).header("Content-Type",MediaType.APPLICATION_JSON).build();
}
