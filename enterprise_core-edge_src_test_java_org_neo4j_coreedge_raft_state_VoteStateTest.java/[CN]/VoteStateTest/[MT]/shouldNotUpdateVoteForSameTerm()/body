{
  VoteState<CoreMember> voteState=new VoteState<>();
  CoreMember member1=new CoreMember(new AdvertisedSocketAddress("host1:1001"),new AdvertisedSocketAddress("host1:2001"));
  CoreMember member2=new CoreMember(new AdvertisedSocketAddress("host2:1001"),new AdvertisedSocketAddress("host2:2001"));
  voteState.update(member1,0);
  try {
    voteState.update(member2,0);
    fail("Should have thrown IllegalArgumentException");
  }
 catch (  IllegalArgumentException expected) {
  }
}
