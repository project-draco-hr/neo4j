{
  if (storeId == null) {
    channel.put((byte)0);
    return;
  }
  channel.put((byte)1);
  channel.putLong(storeId.getCreationTime());
  channel.putLong(storeId.getRandomId());
  channel.putLong(storeId.getStoreVersion());
  channel.putLong(storeId.getUpgradeTime());
  channel.putLong(storeId.getUpgradeId());
}
