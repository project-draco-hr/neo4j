{
  if (storeId == null) {
    byteBuf.writeByte(0);
    return;
  }
  byteBuf.writeByte(1);
  byteBuf.writeLong(storeId.getCreationTime());
  byteBuf.writeLong(storeId.getRandomId());
  byteBuf.writeLong(storeId.getStoreVersion());
  byteBuf.writeLong(storeId.getUpgradeTime());
  byteBuf.writeLong(storeId.getUpgradeId());
}
