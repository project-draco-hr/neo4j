{
  try {
    Transaction tx=transactions.get(txId);
    if (tx == null) {
      txManager.begin();
      tx=txManager.getTransaction();
      transactions.put(txId,tx);
    }
    return tx;
  }
 catch (  NotSupportedException e) {
    throw new RuntimeException(e);
  }
catch (  SystemException e) {
    throw new RuntimeException(e);
  }
}
