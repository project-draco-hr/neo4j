{
  if (state.compareAndSet(State.INIT,State.CLOSED))   return super.close();
  if (state.compareAndSet(State.STARTING,State.CLOSED))   throw new IllegalStateException("Concurrent close while creating index");
  if (state.compareAndSet(State.STARTED,State.CLOSED)) {
    ensureNoOpenCalls("close");
    return super.close();
  }
  throw new IllegalStateException("IndexProxy already closed");
}
