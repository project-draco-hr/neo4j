{
  super.backup();
  File file=getLogFile();
  if (!file.exists()) {
    org.junit.Assert.fail("Backup log file not created.");
  }
  if (!file.isFile()) {
    org.junit.Assert.fail("Backup log file is not a file.");
  }
  if (file.length() <= 0) {
    org.junit.Assert.fail("Backup log file is empty.");
  }
}
