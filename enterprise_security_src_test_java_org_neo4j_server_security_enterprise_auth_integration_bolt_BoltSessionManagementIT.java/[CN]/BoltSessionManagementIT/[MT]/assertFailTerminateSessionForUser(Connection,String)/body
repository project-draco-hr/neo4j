{
  client.send(TransportTestUtil.chunk(run("CALL dbms.terminateSessionsForUser( '" + username + "' ) YIELD username, sessionCount"),pullAll()));
  assertThat(client,eventuallyRecieves(msgFailure(Status.Security.Forbidden,AuthProcedures.PERMISSION_DENIED),msgIgnored()));
  client.send(TransportTestUtil.chunk(reset()));
  assertThat(client,eventuallyRecieves(msgSuccess()));
}
