{
  if (entityId instanceof RelationshipId) {
    RelationshipId relationshipId=(RelationshipId)entityId;
    Document document=newBaseDocument(relationshipId.id);
    document.add(new Field(LuceneIndex.KEY_START_NODE_ID,"" + relationshipId.startNode,Store.YES,org.apache.lucene.document.Field.Index.NOT_ANALYZED));
    document.add(new Field(LuceneIndex.KEY_END_NODE_ID,"" + relationshipId.endNode,Store.YES,org.apache.lucene.document.Field.Index.NOT_ANALYZED));
    return document;
  }
  return newBaseDocument((Long)entityId);
}
