{
  BufferingLogger bufferingLogger=new BufferingLogger();
  new JvmChecker(bufferingLogger,new CannedJvmMetadataRepository("Java HotSpot(TM) 64-Bit Server VM","1.8.0_05")).checkJvmCompatibilityAndIssueWarning();
  assertThat(bufferingLogger.toString().trim(),is(INCOMPATIBLE_JVM_VERSION_WARNING));
}
