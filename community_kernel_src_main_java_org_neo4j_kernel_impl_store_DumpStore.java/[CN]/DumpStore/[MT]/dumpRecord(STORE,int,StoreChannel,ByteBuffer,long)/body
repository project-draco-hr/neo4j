{
  RECORD record=store.forceGetRecord(id);
  Object transform=transform(record);
  if (transform != null) {
    if (!"".equals(transform)) {
      out.println(transform);
    }
  }
 else {
    out.print(record);
    buffer.clear();
    fileChannel.read(buffer,id * size);
    buffer.flip();
    dumpHex(record,buffer,id,size);
  }
  return record.inUse();
}
