{
  try {
    new DumpStore<R,TokenStore<R,T>>(System.out){
      @Override protected Object transform(      R record) throws Exception {
        if (record.inUse()) {
          store.ensureHeavy(record);
          return record.getId() + ": \"" + store.getStringFor(record)+ "\": "+ record;
        }
        return null;
      }
    }
.dump(stores,store,ids);
  }
  finally {
    store.close();
  }
}
