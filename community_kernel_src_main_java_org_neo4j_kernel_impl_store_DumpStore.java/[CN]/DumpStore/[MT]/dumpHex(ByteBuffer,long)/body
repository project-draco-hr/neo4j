{
  for (int count=0; buffer.remaining() > 0; count++, offset++) {
    int b=buffer.get();
    if (count % 16 == 0) {
      out.printf("%n    @ 0x%08x: ",offset);
    }
 else     if (count % 4 == 0) {
      out.print(" ");
    }
    out.printf(" %x%x",0xF & (b >> 4),0xF & b);
  }
  out.println();
}
