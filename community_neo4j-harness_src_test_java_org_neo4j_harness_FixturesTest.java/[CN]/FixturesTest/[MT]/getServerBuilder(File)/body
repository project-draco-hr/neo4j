{
  TestServerBuilder serverBuilder=newInProcessBuilder(targetFolder);
  serverBuilder.withConfig(ServerSettings.tls_key_file.name(),ServerTestUtils.getRelativePath(testDir.directory(),ServerSettings.tls_key_file));
  serverBuilder.withConfig(ServerSettings.tls_certificate_file.name(),ServerTestUtils.getRelativePath(testDir.directory(),ServerSettings.tls_certificate_file));
  return serverBuilder;
}
