{
  TestServerBuilder serverBuilder=newInProcessBuilder(targetFolder).withConfig(ServerSettings.tls_key_file.name(),ServerTestUtils.getRelativePath(testDir.directory(),ServerSettings.tls_key_file)).withConfig(ServerSettings.tls_certificate_file.name(),ServerTestUtils.getRelativePath(testDir.directory(),ServerSettings.tls_certificate_file)).withConfig(GraphDatabaseSettings.auth_store.name(),ServerTestUtils.getRelativePath(testDir.directory(),GraphDatabaseSettings.auth_store));
  return serverBuilder;
}
