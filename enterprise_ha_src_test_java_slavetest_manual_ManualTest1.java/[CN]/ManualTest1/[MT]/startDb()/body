{
  return new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(PATH.getPath()).setConfig(HaSettings.server_id,"1").setConfig(HaSettings.ha_server,ME).setConfig(ShellSettings.remote_shell_enabled,GraphDatabaseSetting.TRUE).setConfig(GraphDatabaseSettings.keep_logical_logs,GraphDatabaseSetting.TRUE).setConfig(OnlineBackupSettings.online_backup_enabled,GraphDatabaseSetting.TRUE).newGraphDatabase();
}
