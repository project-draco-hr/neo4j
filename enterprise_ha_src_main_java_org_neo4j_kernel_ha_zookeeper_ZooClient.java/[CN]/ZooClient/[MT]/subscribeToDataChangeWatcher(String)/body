{
  String root=getRoot();
  String path=root + "/" + child;
  try {
    zooKeeper.getData(path,true,null);
  }
 catch (  KeeperException e) {
    msgLog.logMessage("Couldn't get master notify node",e);
  }
catch (  InterruptedException e) {
    Thread.interrupted();
    throw new ZooKeeperException("Interrupted",e);
  }
}
