{
  int timeOut=conf.isSet(lock_read_timeout) ? conf.getInteger(lock_read_timeout) : conf.getInteger(read_timeout);
  return new MasterServer(new MasterImpl(graphDb,timeOut),Machine.splitIpAndPort(haServer).other(),graphDb.getMessageLog(),conf.getInteger(max_concurrent_channels_per_slave),clientLockReadTimeout,new BranchDetectingTxVerifier(graphDb));
}
