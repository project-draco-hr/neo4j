{
  int timeOut=conf.isSet(lock_read_timeout) ? conf.get(lock_read_timeout) : conf.get(read_timeout);
  return new MasterServer(new MasterImpl(graphDb,timeOut),Machine.splitIpAndPort(haServer).other(),graphDb.getMessageLog(),conf.get(max_concurrent_channels_per_slave),timeOut,new BranchDetectingTxVerifier(graphDb),conf.get(com_chunk_size));
}
