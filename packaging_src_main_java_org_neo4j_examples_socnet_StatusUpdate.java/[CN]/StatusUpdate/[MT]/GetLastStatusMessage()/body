{
  TraversalDescription traversalDescription=Traversal.description().depthFirst().relationships(NEXT,Direction.BOTH).filter(Traversal.returnAll());
  Traverser traverser=traversalDescription.traverse(getUnderlyingNode());
  Node lastStatus=null;
  for (  Path i : traverser) {
    lastStatus=i.endNode();
  }
  return lastStatus;
}
