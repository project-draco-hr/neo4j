{
  IterableWrapper<StatusUpdate,Relationship> statusIterator=new IterableWrapper<StatusUpdate,Relationship>(underlyingNode.getRelationships(PersonRepository.NEXT)){
    @Override protected StatusUpdate underlyingObjectToObject(    Relationship nextRel){
      return new StatusUpdate(nextRel.getOtherNode(underlyingNode));
    }
  }
;
  if (!statusIterator.iterator().hasNext())   return null;
  return statusIterator.iterator().next();
}
