{
  assertSerializes(new NodeRecord(12,false,false,0,0,0));
  assertSerializes(new NodeRecord(12,true,false,13,14,1337));
  assertSerializes(new NodeRecord(12,true,true,13,14,1337));
  assertSerializes(new NodeRecord(12,true,true,13,14,0));
  assertSerializes(new NodeRecord(12,true,false,IdType.RELATIONSHIP.getMaxValue(),IdType.PROPERTY.getMaxValue(),IdType.LABEL_TOKEN.getMaxValue()));
  assertSerializes(new NodeRecord(12,true,true,IdType.RELATIONSHIP.getMaxValue(),IdType.PROPERTY.getMaxValue(),1l << (8 * 5 - 1)));
}
