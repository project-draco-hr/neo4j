{
  NodeStoreFormat_v2_2.NodeRecordCursor cursor=format.createCursor(pagedFile,storeToolkit,Store.SF_NO_FLAGS);
  NodeRecord record=new NodeRecord(12,true,true,1,2,3);
  writeToPagedFile(record);
  cursor.position(12);
  assertThat(cursor.recordId(),equalTo(12l));
  assertThat(cursor.firstRelationship(),equalTo(1l));
  assertThat(cursor.inUse(),equalTo(true));
  assertThat(cursor.record().toString(),equalTo(record.toString()));
}
