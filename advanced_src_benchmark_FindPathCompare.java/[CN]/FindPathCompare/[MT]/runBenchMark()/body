{
  Transaction tx=neo.beginTx();
  startNode=graph.getRandomNode(null);
  endNode=graph.getRandomNode(startNode);
  FindPath findPath=new FindPath(startNode,endNode,Direction.BOTH,MyRelTypes.R1);
  FindSinglePath findSinglePath=new FindSinglePath(startNode,endNode,MyRelTypes.R1,Integer.MAX_VALUE);
  findPath.getPath();
  tx.finish();
}
