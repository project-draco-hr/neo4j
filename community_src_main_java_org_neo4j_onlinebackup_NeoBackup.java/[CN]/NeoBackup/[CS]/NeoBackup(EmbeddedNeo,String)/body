{
  if (sourceNeo == null) {
    throw new IllegalArgumentException("The EmbeddedNeo instance is null.");
  }
  if (destDir == null) {
    throw new IllegalArgumentException("Destination dir is null.");
  }
  if (!new File(destDir).exists()) {
    throw new RuntimeException("Unable to locate local onlineNeo store in[" + destDir + "]");
  }
  this.onlineNeo=sourceNeo;
  this.destDir=destDir;
}
