{
  try {
    if (!readOnly) {
      forceEverything();
    }
    xaContainer.close();
    life.shutdown();
    unbindLogicalLog();
    neoStore.close();
  }
 catch (  IOException e) {
    msgLog.error("Something went wrong while shutting down the store. Recovery will happen on next startup. " + "Please inspect the logs for the root cause and address that before attempting to restart the database " + "(for example, low disk space).");
    throw e;
  }
  msgLog.info("NeoStore closed");
}
