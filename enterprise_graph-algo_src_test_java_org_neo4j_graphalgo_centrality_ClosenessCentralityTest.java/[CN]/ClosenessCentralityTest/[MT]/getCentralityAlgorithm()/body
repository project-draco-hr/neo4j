{
  return new ClosenessCentrality<Double>(getSingleSourceShortestPath(),new DoubleAdder(),0.0,graph.getAllNodes(),new CostDivider<Double>(){
    public Double divideByCost(    Double d,    Double c){
      return d / c;
    }
    public Double divideCost(    Double c,    Double d){
      return c / d;
    }
  }
);
}
