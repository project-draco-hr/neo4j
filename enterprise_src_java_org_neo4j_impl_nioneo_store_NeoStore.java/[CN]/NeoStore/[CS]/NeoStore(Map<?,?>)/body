{
  super((String)config.get("neo_store"),config);
  if (getConfig() != null) {
    String grabSize=(String)getConfig().get("relationship_grab_size");
    if (grabSize != null) {
      REL_GRAB_SIZE=Integer.parseInt(grabSize);
    }
 else {
      REL_GRAB_SIZE=100;
    }
  }
 else {
    REL_GRAB_SIZE=100;
  }
}
