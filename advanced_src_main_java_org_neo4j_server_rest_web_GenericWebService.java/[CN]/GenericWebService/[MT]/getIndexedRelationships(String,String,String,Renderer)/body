{
  if (!database.getIndexManager().existsForRelationships(indexName)) {
    return Response.status(Status.NOT_FOUND).build();
  }
  List<IndexedRelationshipRepresentation> representations=actions.getIndexedRelationships(indexName,key,value);
  String entity=renderer.render(representations.toArray(new IndexedRelationshipRepresentation[representations.size()]));
  return addHeaders(Response.ok(entity,renderer.getMediaType())).build();
}
