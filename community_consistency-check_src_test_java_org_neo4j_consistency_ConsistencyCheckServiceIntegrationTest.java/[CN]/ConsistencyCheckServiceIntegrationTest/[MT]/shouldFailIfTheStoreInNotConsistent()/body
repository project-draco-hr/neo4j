{
  breakNodeStore();
  Date timestamp=new Date();
  ConsistencyCheckService service=new ConsistencyCheckService(timestamp);
  Config configuration=new Config(settings(GraphDatabaseSettings.logs_directory.name(),testDirectory.directory().getPath()),GraphDatabaseSettings.class,ConsistencyCheckSettings.class);
  ConsistencyCheckService.Result result=runFullConsistencyCheck(service,configuration);
  assertEquals(ConsistencyCheckService.Result.FAILURE,result);
  File reportFile=new File(configuration.get(GraphDatabaseSettings.logs_directory),defaultLogFileName(timestamp));
  assertTrue("Inconsistency report file " + reportFile + " not generated",reportFile.exists());
}
