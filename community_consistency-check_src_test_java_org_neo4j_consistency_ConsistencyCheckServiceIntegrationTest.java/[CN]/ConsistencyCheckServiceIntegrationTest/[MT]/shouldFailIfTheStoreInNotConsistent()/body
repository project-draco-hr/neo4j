{
  breakNodeStore();
  ConsistencyCheckService service=new ConsistencyCheckService();
  ConsistencyCheckService.Result result=service.runFullConsistencyCheck(fixture.directory().getPath(),new Config(stringMap(),GraphDatabaseSettings.class,ConsistencyCheckSettings.class),ProgressMonitorFactory.NONE,StringLogger.DEV_NULL);
  assertEquals(ConsistencyCheckService.Result.FAILURE,result);
  File reportFile=new File(fixture.directory(),service.defaultLogFileName());
  assertTrue("Inconsistency report file " + reportFile + " not generated",reportFile.exists());
}
