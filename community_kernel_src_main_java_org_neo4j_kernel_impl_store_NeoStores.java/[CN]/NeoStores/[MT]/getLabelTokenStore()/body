{
  if (labelTokenStore == null) {
    File fileName=getStoreFileName(StoreFactory.LABEL_TOKEN_STORE_NAME);
    labelTokenStore=new LabelTokenStore(fileName,config,idGeneratorFactory,pageCache,logProvider,getLabelTokenNamesStore(),versionMismatchHandler,createIfNotExist);
    labelTokenStore.initialise(createIfNotExist);
  }
  return labelTokenStore;
}
