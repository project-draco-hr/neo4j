{
  return new IndexReader(){
    @Override public PrimitiveLongIterator lookup(    Object value){
      if (entries.containsKey(value)) {
        return PrimitiveLongCollections.iterator(entries.get(value));
      }
      return emptyIterator();
    }
    @Override public int getIndexedCount(    long nodeId,    Object propertyValue){
      long[] candidates=entries.get(propertyValue);
      if (candidates == null) {
        return 0;
      }
      int count=0;
      for (int i=0; i < candidates.length; i++) {
        if (candidates[i] == nodeId) {
          count++;
        }
      }
      return count;
    }
    @Override public void close(){
    }
  }
;
}
