{
  try {
    Outcome<MEMBER> outcome=currentRole.handler.validate(incomingMessage,state,log,localDatabase);
    boolean processable=outcome.isProcessable();
    if (!processable) {
      checkForSnapshotNeed(outcome);
    }
    return processable;
  }
 catch (  Throwable e) {
    panicAndStop(incomingMessage,e);
    throw e;
  }
}
