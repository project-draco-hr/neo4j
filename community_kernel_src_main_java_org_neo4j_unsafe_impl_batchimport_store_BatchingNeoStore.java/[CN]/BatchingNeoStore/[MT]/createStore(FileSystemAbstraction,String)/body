{
  PageCache pageCache=new BatchingPageCache(fileSystem,Configuration.DEFAULT.fileChannelBufferSize(),BatchingPageCache.SYNCHRONOUS,Monitor.NO_MONITOR,APPEND_ONLY);
  StoreFactory storeFactory=new StoreFactory(fileSystem,new File(storeDir),pageCache,StringLogger.DEV_NULL,new Monitors());
  storeFactory.createNeoStore().close();
  pageCache.close();
}
