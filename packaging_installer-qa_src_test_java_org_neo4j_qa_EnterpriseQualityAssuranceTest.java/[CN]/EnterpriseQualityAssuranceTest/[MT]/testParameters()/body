{
  Map<String,Object[]> platforms=new HashMap<String,Object[]>();
  List<Object[]> testParameters=new ArrayList<Object[]>();
  VirtualMachine win1=vm(Neo4jVM.WIN_1);
  VirtualMachine win2=vm(Neo4jVM.WIN_2);
  VirtualMachine win3=vm(Neo4jVM.WIN_3);
  VirtualMachine ubuntu1=vm(Neo4jVM.UBUNTU_1);
  VirtualMachine ubuntu2=vm(Neo4jVM.UBUNTU_2);
  VirtualMachine ubuntu3=vm(Neo4jVM.UBUNTU_3);
  platforms.put(Platforms.WINDOWS,new Object[]{Platforms.WINDOWS,new EnterpriseDriver[]{new WindowsEnterpriseDriver(win1,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER),new WindowsEnterpriseDriver(win2,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER),new WindowsEnterpriseDriver(win3,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER)}});
  platforms.put(Platforms.UBUNTU_DEB,new Object[]{Platforms.UBUNTU_DEB,new EnterpriseDriver[]{new UbuntuDebEnterpriseDriver(ubuntu1,SharedConstants.UBUNTU_ENTERPRISE_INSTALLER,SharedConstants.UBUNTU_COORDINATOR_INSTALLER),new UbuntuDebEnterpriseDriver(ubuntu2,SharedConstants.UBUNTU_ENTERPRISE_INSTALLER,SharedConstants.UBUNTU_COORDINATOR_INSTALLER),new UbuntuDebEnterpriseDriver(ubuntu3,SharedConstants.UBUNTU_ENTERPRISE_INSTALLER,SharedConstants.UBUNTU_COORDINATOR_INSTALLER)}});
  for (  String platform : Platforms.getPlaformsToUse()) {
    testParameters.add(platforms.get(platform));
  }
  return testParameters;
}
