{
  clusterRule.withNumberOfCoreMembers(3);
  clusterRule.withNumberOfEdgeMembers(3);
  Cluster cluster=clusterRule.startCluster();
  cluster.addEdgeMemberWithId(3).start();
  cluster.addEdgeMemberWithId(4).start();
  Matcher<List<MemberInfo>> expected=allOf(containsAddress("127.0.0.1:8000"),containsAddress("127.0.0.1:8001"),containsAddress("127.0.0.1:8002"),containsAddress("127.0.0.1:9000"),containsAddress("127.0.0.1:9001"),containsAddress("127.0.0.1:9002"),containsRole(Role.LEADER,1),containsRole(Role.FOLLOWER,2),containsRole(Role.READ_REPLICA,5),containsAddress("127.0.0.1:9003"),containsAddress("127.0.0.1:9004"));
  for (int coreServerId=0; coreServerId < 3; coreServerId++) {
    assertEventualOverview(cluster,expected,coreServerId);
  }
}
