{
  clusterRule.withNumberOfCoreMembers(3);
  clusterRule.withNumberOfEdgeMembers(0);
  Cluster cluster=clusterRule.startCluster();
  Matcher<List<MemberInfo>> expected=allOf(containsAddress("127.0.0.1:8000"),containsAddress("127.0.0.1:8001"),containsAddress("127.0.0.1:8002"),containsRole(Role.LEADER,1),containsRole(Role.FOLLOWER,2),doesNotContainRole(Role.READ_REPLICA));
  for (int coreServerId=0; coreServerId < 3; coreServerId++) {
    assertEventualOverview(cluster,expected,coreServerId);
  }
}
