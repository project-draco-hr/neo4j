{
  File directory=new File("myDirectory");
  Setting<File> home=setting("home",PATH,directory.getAbsolutePath());
  Setting<File> config=setting("config",PATH,new File(directory,"config.properties").getAbsolutePath(),basePath(home),isFile);
  assertThat(config.apply(map(stringMap())).getAbsolutePath(),equalTo(new File(directory,"config.properties").getAbsolutePath()));
}
