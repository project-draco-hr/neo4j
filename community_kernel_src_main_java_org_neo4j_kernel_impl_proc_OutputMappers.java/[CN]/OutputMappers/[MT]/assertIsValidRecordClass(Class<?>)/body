{
  if (userClass.isPrimitive() || userClass.isArray() || userClass.getPackage() != null && userClass.getPackage().getName().startsWith("java.")) {
    throw new ProcedureException(Status.Procedure.TypeError,Messages.get(proc_invalid_return_type_description,userClass.getSimpleName(),userClass.getSimpleName()));
  }
}
