{
  if (!updates.isEmpty()) {
    Collections.sort(updates,UPDATES_SORTER);
    try (LabelScanWriter writer=labelScanStore.newWriter()){
      for (      NodeLabelUpdate update : updates) {
        writer.write(update);
      }
    }
 catch (    IndexCapacityExceededException e) {
      throw new IOException(e);
    }
    updates.clear();
  }
}
