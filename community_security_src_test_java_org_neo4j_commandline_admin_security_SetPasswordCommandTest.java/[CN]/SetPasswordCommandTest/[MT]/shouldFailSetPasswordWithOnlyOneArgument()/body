{
  UsersCommand usersCommand=new UsersCommand(testDir.directory("home").toPath(),testDir.directory("conf").toPath(),mock(OutsideWorld.class));
  String[] arguments={"set-password","neo4j"};
  try {
    usersCommand.execute(arguments);
    fail("Should have thrown an exception.");
  }
 catch (  IncorrectUsage e) {
    assertThat(e.getMessage(),containsString("password"));
  }
}
