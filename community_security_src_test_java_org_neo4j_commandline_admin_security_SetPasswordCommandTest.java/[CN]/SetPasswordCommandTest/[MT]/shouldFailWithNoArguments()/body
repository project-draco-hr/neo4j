{
  SetPasswordCommand setPasswordCommand=new SetPasswordCommand(testDir.directory("home").toPath(),testDir.directory("conf").toPath());
  String[] arguments={};
  try {
    setPasswordCommand.execute(arguments);
    fail("Should have thrown an exception.");
  }
 catch (  IncorrectUsage e) {
    assertThat(e.getMessage(),containsString("password"));
  }
}
