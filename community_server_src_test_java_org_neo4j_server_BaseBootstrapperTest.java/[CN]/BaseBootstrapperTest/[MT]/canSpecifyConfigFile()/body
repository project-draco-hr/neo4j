{
  bootstrapper=newBootstrapper();
  File configFile=tempDir.newFile("neo4j.config");
  MapUtil.store(stringMap(forced_kernel_id.name(),"ourcustomvalue"),configFile);
  start(bootstrapper,new String[]{"-C",configFile.getAbsolutePath(),"-c:" + legacy_db_location.name(),tempDir.getRoot().getAbsolutePath()});
  assertThat(bootstrapper.getServer().getConfig().get(forced_kernel_id),equalTo("ourcustomvalue"));
}
