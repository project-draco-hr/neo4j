{
  int resultCode=start(bootstrapper,commandLineConfig("-c",configOption(ServerSettings.legacy_db_location.name(),tempDir.getRoot().getAbsolutePath()),"-c",configOption(GraphDatabaseSettings.auth_store.name(),tempDir.newFile().getAbsolutePath()),"-c",configOption(ServerSettings.tls_certificate_file.name(),new File(tempDir.getRoot(),"cert.cert").getAbsolutePath()),"-c",configOption(ServerSettings.tls_key_file.name(),new File(tempDir.getRoot(),"key.key").getAbsolutePath())));
  assertEquals(Bootstrapper.OK,resultCode);
  assertNotNull(bootstrapper.getServer());
}
