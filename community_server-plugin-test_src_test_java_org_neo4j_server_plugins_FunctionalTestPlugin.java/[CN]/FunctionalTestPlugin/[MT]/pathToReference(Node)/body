{
  PathFinder<Path> finder=GraphAlgoFactory.shortestPath(Traversal.expanderForAllTypes(),6);
  Transaction tx=me.getGraphDatabase().beginTx();
  try {
    @SuppressWarnings("deprecation") Path path=finder.findSinglePath(me.getGraphDatabase().getNodeById(0l),me);
    tx.success();
    return path;
  }
  finally {
    tx.finish();
  }
}
