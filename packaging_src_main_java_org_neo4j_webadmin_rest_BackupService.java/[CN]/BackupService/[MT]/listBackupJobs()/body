{
  try {
    String entity=JsonRenderers.DEFAULT.render(BackupManager.INSTANCE.getConfig());
    return addHeaders(Response.ok(entity,JsonRenderers.DEFAULT.getMediaType())).build();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return buildExceptionResponse(Status.INTERNAL_SERVER_ERROR,"An unexpected internal server error occurred.",e,JsonRenderers.DEFAULT);
  }
}
