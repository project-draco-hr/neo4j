{
  FileChannel source=new RandomAccessFile("nioneo_logical.log","r").getChannel();
  FileChannel dest=new RandomAccessFile("nioneo_logical.log.bak","rw").getChannel();
  ByteBuffer buffer=ByteBuffer.allocate(1024);
  int read=-1;
  do {
    read=source.read(buffer);
    buffer.flip();
    dest.write(buffer);
    buffer.clear();
  }
 while (read == 1024);
}
