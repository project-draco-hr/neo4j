{
  if (bytesWritten == 0 || bytesWritten == IS_ERROR)   g.writeRaw("null");
  g.writeNumberField("status",status);
  if (message != null)   g.writeStringField("message",message);
 else {
    if (errorStream != null) {
      g.writeStringField("message",errorStream.toString());
    }
  }
  g.close();
}
