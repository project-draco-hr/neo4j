{
  if (iniFile != null) {
    try {
      for (      String line : listIo.read(new ArrayList<String>(),iniFile)) {
        if (jvmArgumentParser.isMaxHeap(line)) {
          return jvmArgumentParser.parseMaxHeap(line);
        }
      }
    }
 catch (    IOException e) {
    }
  }
  return jvmArgumentParser.getCurrentMaxHeap();
}
