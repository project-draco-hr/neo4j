{
  FileSystemAbstraction fs=mock(FileSystemAbstraction.class);
  when(fs.fileExists("logical_log.active")).thenReturn(false);
  when(fs.fileExists("logical_log")).thenReturn(false);
  when(fs.fileExists("logical_log.1")).thenReturn(true);
  when(fs.fileExists("logical_log.2")).thenReturn(false);
  XaLogicalLogFiles files=new XaLogicalLogFiles("logical_log",fs);
  assertThat(files.determineState(),is(XaLogicalLogFiles.State.NO_ACTIVE_FILE));
}
