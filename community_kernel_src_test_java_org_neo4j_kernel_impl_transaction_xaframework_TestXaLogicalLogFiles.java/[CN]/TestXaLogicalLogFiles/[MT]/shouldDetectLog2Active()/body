{
  FileSystemAbstraction fs=mock(FileSystemAbstraction.class);
  when(fs.fileExists("logical_log.active")).thenReturn(true);
  when(fs.fileExists("logical_log")).thenReturn(false);
  when(fs.fileExists("logical_log.1")).thenReturn(false);
  when(fs.fileExists("logical_log.2")).thenReturn(true);
  FileChannel fc=mockedFileChannel(XaLogicalLogTokens.LOG2);
  when(fs.open(eq("logical_log.active"),anyString())).thenReturn(fc);
  XaLogicalLogFiles files=new XaLogicalLogFiles("logical_log",fs);
  assertThat(files.determineState(),is(XaLogicalLogFiles.State.LOG_2_ACTIVE));
}
