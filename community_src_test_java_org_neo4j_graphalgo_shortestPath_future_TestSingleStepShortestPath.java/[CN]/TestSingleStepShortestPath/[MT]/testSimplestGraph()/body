{
  graph.makeEdge("s","t");
  graph.makeEdge("s","t");
  SingleStepShortestPathsFinder finder=new SingleStepShortestPathsFinder(1,RelationshipExpander.forTypes(MyRelTypes.R1,Direction.BOTH));
  Collection<Path> paths=finder.paths(graph.getNode("s"),graph.getNode("t"));
  assertEquals(2,paths.size());
  assertPaths(paths,"s,t","s,t");
}
