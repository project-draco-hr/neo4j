{
  if (desktopSupports(Desktop.Action.BROWSE)) {
    try {
      getDesktop().browse(new URI(link));
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
catch (    URISyntaxException e) {
      e.printStackTrace();
    }
  }
 else {
    throw new UnsupportedOperationException();
  }
}
