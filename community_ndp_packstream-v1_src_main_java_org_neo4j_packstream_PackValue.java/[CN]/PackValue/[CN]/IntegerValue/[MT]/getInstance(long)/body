{
  if (value >= positiveValues.length) {
    return new IntegerValue(value);
  }
 else   if (value >= 0) {
    return positiveValues[(int)value];
  }
 else {
    long absoluteValue=-value;
    if (absoluteValue >= negativeValues.length) {
      return new IntegerValue(value);
    }
 else {
      return negativeValues[(int)absoluteValue];
    }
  }
}
