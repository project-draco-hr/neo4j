{
  shuttingDown=true;
  silentChannelExecutor.shutdown();
  unfinishedTransactionExecutor.shutdown();
  targetCallExecutor.shutdown();
  channelGroup.close().awaitUninterruptibly();
  executor.shutdown();
  msgLog.logMessage(getClass().getSimpleName() + " shutdown",true);
  requestTarget=null;
  txVerifier=null;
}
