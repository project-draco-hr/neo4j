{
  flushAll();
synchronized (activeRowWindows) {
    for (    BrickElement element : brickArray) {
      if (element.getWindow() != null) {
        element.getWindow().close();
        element.setWindow(null);
      }
    }
    fileChannel=null;
    activeRowWindows.clear();
  }
  activeRowWindows=null;
  dumpStatistics();
}
