{
  int nodeId=-1;
  Node node=getNeo().createNode();
  nodeId=(int)node.getId();
  getNeo().getNodeById(nodeId);
  node.delete();
  Transaction tx=getTransaction();
  tx.success();
  tx.finish();
  setTransaction(getNeo().beginTx());
  try {
    getNeo().getNodeById(nodeId);
    fail("Node[" + nodeId + "] should be deleted.");
  }
 catch (  NotFoundException e) {
  }
}
