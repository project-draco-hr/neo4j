{
  try {
    Payload payload=serializer.broadcast(new MemberIsAvailable(role,serverClusterId,Iterables.iterable(getHaUri(serverClusterId),getBackupUri(serverClusterId))));
    serializer.receive(payload);
    cluster.broadcast(payload);
  }
 catch (  Throwable e) {
    logger.warn("Could not distribute member availability",e);
  }
}
