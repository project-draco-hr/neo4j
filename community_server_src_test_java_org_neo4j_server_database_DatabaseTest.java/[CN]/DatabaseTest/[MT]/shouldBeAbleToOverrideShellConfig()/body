{
  int customPort=findFreeShellPortToUse(8881);
  File tempDir=createTempDir();
  Database otherDb=new Database(EMBEDDED_GRAPH_DATABASE_FACTORY,tempDir.getAbsolutePath(),stringMap(ShellSettings.remote_shell_enabled.name(),GraphDatabaseSetting.TRUE,ShellSettings.remote_shell_port.name(),"" + customPort));
  otherDb.startup();
  ShellLobby.newClient(customPort).shutdown();
  otherDb.shutdown();
  FileUtils.forceDelete(tempDir);
}
