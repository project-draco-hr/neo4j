{
  int customPort=findFreeShellPortToUse(8881);
  File tempDir=createTempDir();
  Database otherDb=new Database(EMBEDDED_GRAPH_DATABASE_FACTORY,tempDir.getAbsolutePath(),stringMap(ENABLE_REMOTE_SHELL,"port=" + customPort));
  otherDb.startup();
  ShellLobby.newClient(customPort).shutdown();
  otherDb.shutdown();
  FileUtils.forceDelete(tempDir);
}
