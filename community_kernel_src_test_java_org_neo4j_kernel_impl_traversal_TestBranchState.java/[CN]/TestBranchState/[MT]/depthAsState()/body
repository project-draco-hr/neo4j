{
  createGraph("a to b","b to c","c to d","b to e","e to f","f to d","f to g","g to h");
  try (Transaction tx=beginTx()){
    DepthStateExpander expander=new DepthStateExpander();
    count(getGraphDb().traversalDescription().expand(expander,new InitialBranchState.State<>(0,0)).traverse(getNodeWithName("a")));
    tx.success();
  }
 }
