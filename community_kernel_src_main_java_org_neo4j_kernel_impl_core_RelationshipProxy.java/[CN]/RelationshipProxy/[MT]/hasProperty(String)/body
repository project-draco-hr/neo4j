{
  if (null == key)   return false;
  try (ReadStatement statement=statementCtxProvider.readStatement()){
    long propertyId=statement.propertyKeyGetForName(key);
    return propertyId != KeyReadOperations.NO_SUCH_PROPERTY_KEY && statement.relationshipGetProperty(relId,propertyId).isDefined();
  }
 catch (  EntityNotFoundException e) {
    throw new IllegalStateException(e);
  }
}
