{
  int numberOfBytes=14;
  Bits bits=bits(numberOfBytes);
  for (byte i=0; i < numberOfBytes; i++) {
    bits.pushRight(i);
  }
  byte[] bytes=bits.asLeftBytes();
  for (byte i=0; i < numberOfBytes; i++) {
    assertEquals(numberOfBytes - 1 - i,bytes[i]);
  }
  bytes=Bits.bitsFromBytesLeft(bytes).asLeftBytes();
  for (byte i=0; i < numberOfBytes; i++) {
    assertEquals(numberOfBytes - 1 - i,bytes[i]);
  }
}
