{
switch (source) {
case NODE:
    if (acceptedNodeProperties != null) {
      return acceptedNodeProperties.contains(key);
    }
 else {
      return true;
    }
case RELATIONSHIP:
  if (acceptedRelationshipProperties != null) {
    return acceptedRelationshipProperties.contains(key);
  }
 else {
    return true;
  }
default :
return false;
}
}
