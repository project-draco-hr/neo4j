{
  Collection<SchemaRule> rules=Arrays.<SchemaRule>asList(new IndexRule(1,0,new long[]{5}),new IndexRule(2,1,new long[]{6}),new IndexRule(3,1,new long[]{7}));
  for (  SchemaRule rule : rules)   storeRule(rule);
  Collection<SchemaRule> readRules=asCollection(store.loadAll());
  assertEquals(rules,readRules);
}
