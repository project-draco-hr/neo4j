{
  Collection<SchemaRule> rules=Arrays.<SchemaRule>asList(new IndexRule(store.nextId(),0,PROVIDER_DESCRIPTOR,5),new IndexRule(store.nextId(),1,PROVIDER_DESCRIPTOR,6),new IndexRule(store.nextId(),1,PROVIDER_DESCRIPTOR,7));
  for (  SchemaRule rule : rules)   storeRule(rule);
  Collection<SchemaRule> readRules=asCollection(store.loadAll());
  assertEquals(rules,readRules);
}
