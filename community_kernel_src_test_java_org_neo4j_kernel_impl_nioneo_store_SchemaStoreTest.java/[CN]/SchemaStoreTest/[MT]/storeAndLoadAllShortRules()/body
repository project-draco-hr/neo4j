{
  Collection<SchemaRule> rules=Arrays.<SchemaRule>asList(new IndexRule(store.nextId(),0,new long[]{5}),new IndexRule(store.nextId(),1,new long[]{6}),new IndexRule(store.nextId(),1,new long[]{7}));
  for (  SchemaRule rule : rules)   storeRule(rule);
  Collection<SchemaRule> readRules=asCollection(store.loadAll());
  assertEquals(rules,readRules);
}
