{
  Collection<SchemaRule> rules=Arrays.<SchemaRule>asList(createLongIndexRule(0,"name"),createLongIndexRule(1,"size"),createLongIndexRule(2,"hair"));
  for (  SchemaRule rule : rules)   storeRule(rule);
  Collection<SchemaRule> readRules=asCollection(store.loadAll());
  assertEquals(rules,readRules);
}
