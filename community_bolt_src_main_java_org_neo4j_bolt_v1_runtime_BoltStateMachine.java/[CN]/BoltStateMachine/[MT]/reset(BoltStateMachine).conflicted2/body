{
  if (machine.ctx.interruptCounter.decrementAndGet() > 0) {
    machine.ctx.markIgnored();
    return INTERRUPTED;
  }
  try {
    machine.ctx.statementProcessor.reset();
    return READY;
  }
 catch (  Throwable e) {
    fail(machine,Neo4jError.from(e));
    throw new BoltConnectionFatality(e.getMessage());
  }
}
