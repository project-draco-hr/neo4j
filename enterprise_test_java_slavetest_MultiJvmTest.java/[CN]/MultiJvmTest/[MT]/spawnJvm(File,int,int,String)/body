{
  Collection<String> list=new ArrayList<String>(Arrays.asList("java","-cp",System.getProperty("java.class.path"),StandaloneDb.class.getName(),"-path",path.getAbsolutePath(),"-port","" + port,"-id","" + machineId,"-master-id","0"));
  list.addAll(Arrays.asList(extraArgs));
  Runtime.getRuntime().exec(list.toArray(new String[list.size()]));
  return awaitJvmStarted(port);
}
