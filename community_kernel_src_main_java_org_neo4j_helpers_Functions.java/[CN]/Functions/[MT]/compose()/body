{
  return new Function2<Function<From,From2>,Function<From2,To>,Function<From,To>>(){
    @Override public Function<From,To> apply(    final Function<From,From2> f1,    final Function<From2,To> f2){
      return new Function<From,To>(){
        @Override public To apply(        From from){
          return f2.apply(f1.apply(from));
        }
      }
;
    }
  }
;
}
