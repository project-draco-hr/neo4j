{
  if (resourceList.size() == 0) {
    logger.warn("Detected zero resources in resourceList");
    return true;
  }
  Iterator<ResourceElement> itr=resourceList.iterator();
  Xid xid=itr.next().getXid();
  while (itr.hasNext()) {
    if (!xid.equals(itr.next().getXid())) {
      return false;
    }
  }
  return true;
}
