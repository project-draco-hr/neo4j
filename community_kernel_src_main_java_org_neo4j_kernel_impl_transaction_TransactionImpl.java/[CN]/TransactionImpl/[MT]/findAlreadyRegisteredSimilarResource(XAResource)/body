{
  Xid sameXid=null;
  ResourceElement sameResource=null;
  for (  ResourceElement re : resourceList) {
    if (sameXid == null && re.getResource().isSameRM(xaRes)) {
      sameXid=re.getXid();
    }
    if (xaRes == re.getResource()) {
      sameResource=re;
    }
  }
  return sameXid == null && sameResource == null ? Pair.<Xid,ResourceElement>empty() : Pair.of(sameXid,sameResource);
}
