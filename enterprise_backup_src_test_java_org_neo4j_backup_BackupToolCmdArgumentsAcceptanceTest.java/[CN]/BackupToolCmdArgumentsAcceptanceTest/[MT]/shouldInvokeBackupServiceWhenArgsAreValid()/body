{
  String[] args=argsAsString.split(" ");
  BackupService backupService=mock(BackupService.class);
  PrintStream printStream=mock(PrintStream.class);
  BackupTool backupTool=new BackupTool(backupService,printStream);
  backupTool.run(args);
  verify(backupService).doIncrementalBackupOrFallbackToFull(eq(HOST),eq(PORT),eq(PATH),eq(expectedVerifyStoreValue),any(Config.class),eq(BackupClient.BIG_READ_TIMEOUT));
}
