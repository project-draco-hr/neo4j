{
  String[] args=argsAsString.split(" ");
  BackupService backupService=mock(BackupService.class);
  PrintStream printStream=mock(PrintStream.class);
  BackupTool backupTool=new BackupTool(backupService,printStream);
  backupTool.run(args);
  verify(backupService).doIncrementalBackupOrFallbackToFull(eq(HOST),eq(PORT),eq(PATH),expectedVerifyStoreValue ? eq(ConsistencyCheck.DEFAULT) : eq(ConsistencyCheck.NONE),any(Config.class),eq(BackupClient.BIG_READ_TIMEOUT),eq(false));
}
