{
  assertMember(db);
  InstanceId serverId=db.getDependencyResolver().resolveDependency(Config.class).get(ClusterSettings.server_id);
  members.remove(serverId);
  life.remove(db);
  db.shutdown();
  await(entireClusterSeesMemberAsNotAvailable(db));
  return wrap(new StartDatabaseAgainKit(this,serverId));
}
