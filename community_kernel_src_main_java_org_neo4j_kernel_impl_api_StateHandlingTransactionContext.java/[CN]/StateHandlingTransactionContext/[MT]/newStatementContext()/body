{
  StoreStatementContext storeContext=(StoreStatementContext)super.newStatementContext();
  StatementContext result=storeContext;
  result=new CachingStatementContext(result,persistenceCache,schemaCache);
  AuxiliaryStoreOperations auxStoreOperations=storeContext;
  auxStoreOperations=new LegacyAutoIndexAuxStoreOps(auxStoreOperations,propertyKeyTokenHolder,nodeManager.getNodePropertyTrackers(),nodeManager.getRelationshipPropertyTrackers(),nodeManager);
  result=new StateHandlingStatementContext(result,new SchemaStateConcern(schemaState),state,constraintIndexCreator,auxStoreOperations);
  return result;
}
