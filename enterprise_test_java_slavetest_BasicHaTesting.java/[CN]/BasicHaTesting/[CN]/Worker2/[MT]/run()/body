{
  Transaction tx=db.beginTx();
  try {
    db.getNodeById(node2).setProperty("2","T2 2");
    barrier1.countDown();
    try {
      barrier2.await();
    }
 catch (    InterruptedException e1) {
      Thread.interrupted();
      e1.printStackTrace();
    }
    db.getNodeById(node1).setProperty("1","T2 2");
    tx.success();
    successfull=true;
  }
 catch (  DeadlockDetectedException e) {
    deadlocked=true;
  }
catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    tx.finish();
  }
}
