{
  return new PreFlightTasks(logging,new EnsurePreparedForHttpLogging(configurator.configuration()),new PerformUpgradeIfNecessary(getConfiguration(),configurator.getDatabaseTuningProperties(),logging,StoreUpgrader.NO_MONITOR),new PerformRecoveryIfNecessary(getConfiguration(),configurator.getDatabaseTuningProperties(),System.out,logging));
}
