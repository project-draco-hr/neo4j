{
  when(request.getHeader(HttpHeaderUtils.MAX_EXECUTION_TIME_HEADER)).thenReturn(String.valueOf(CUSTOM_TRANSACTION_TIMEOUT));
  CypherExecutor cypherExecutor=new CypherExecutor(database,logProvider);
  cypherExecutor.start();
  cypherExecutor.createTransactionContext(QUERY,Collections.emptyMap(),request);
  verify(databaseQueryService).beginTransaction(KernelTransaction.Type.implicit,AccessMode.Static.FULL,CUSTOM_TRANSACTION_TIMEOUT,TimeUnit.MILLISECONDS);
  logProvider.assertNoLoggingOccurred();
}
