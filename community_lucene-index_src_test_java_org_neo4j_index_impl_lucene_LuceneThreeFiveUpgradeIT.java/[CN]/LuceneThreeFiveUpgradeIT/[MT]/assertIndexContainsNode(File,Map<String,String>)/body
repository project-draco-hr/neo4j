{
  GraphDatabaseService db=new GraphDatabaseFactory().newEmbeddedDatabaseBuilder(storeDir.getAbsolutePath()).setConfig(config).newGraphDatabase();
  try {
    Node node=db.getNodeById(1);
    assertEquals(node,db.index().forNodes("persons").get("name",node.getProperty("name")).getSingle());
  }
  finally {
    db.shutdown();
  }
}
