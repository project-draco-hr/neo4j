{
  File storeDir=Unzip.unzip(getClass(),"1.5-store.zip");
  Process process=Runtime.getRuntime().exec(new String[]{"java","-cp",System.getProperty("java.class.path"),StartAndKill.class.getName(),storeDir.getAbsolutePath(),"-" + Config.ALLOW_STORE_UPGRADE + "=true"});
  new ProcessStreamHandler(process,true).launch();
  assertEquals(1,process.waitFor());
  assertIndexContainsNode(storeDir,stringMap());
}
