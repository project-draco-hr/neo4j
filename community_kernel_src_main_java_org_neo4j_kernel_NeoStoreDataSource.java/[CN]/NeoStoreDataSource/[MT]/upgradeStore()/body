{
  LabelScanStoreProvider labelScanStoreProvider=dependencyResolver.resolveDependency(LabelScanStoreProvider.class,HighestSelectionStrategy.getInstance());
  VisibleMigrationProgressMonitor progressMonitor=new VisibleMigrationProgressMonitor(logService.getInternalLog(StoreMigrator.class));
  new DatabaseMigrator(progressMonitor,fs,config,logService,schemaIndexProvider,labelScanStoreProvider,indexProviders,pageCache).migrate(storeDir);
}
