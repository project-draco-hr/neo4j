{
  LogFile logFile=transactionLogModule.logFile();
  if (logFile.rotationNeeded()) {
    boolean rotate;
synchronized (logFile) {
      if (rotate=logFile.rotationNeeded()) {
        awaitAllTransactionsClosed();
        forceEverything();
        logFile.rotate();
      }
    }
    if (rotate) {
      monitor.rotatedLog();
    }
    return rotate;
  }
 else {
    return false;
  }
}
