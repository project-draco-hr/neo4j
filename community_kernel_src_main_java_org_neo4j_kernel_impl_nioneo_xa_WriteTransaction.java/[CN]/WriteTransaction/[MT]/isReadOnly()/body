{
  if (isRecovered()) {
    return nodeCommands.size() == 0 && propCommands.size() == 0 && relCommands.size() == 0 && schemaRuleCommands.size() == 0 && relTypeCommands == null && propIndexCommands == null;
  }
  return nodeRecords.changeSize() == 0 && relRecords.changeSize() == 0 && schemaRuleRecords.size() == 0 && propertyRecords.changeSize() == 0 && relTypeRecords == null && propIndexRecords == null;
}
