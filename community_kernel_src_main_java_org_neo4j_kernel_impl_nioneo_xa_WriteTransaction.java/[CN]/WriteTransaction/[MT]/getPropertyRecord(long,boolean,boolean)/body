{
  PropertyRecord result=propertyRecords.get(propertyId);
  if (result == null) {
    if (light) {
      result=getPropertyStore().getLightRecord(propertyId);
    }
 else {
      result=getPropertyStore().getRecord(propertyId);
    }
    if (store) {
      addPropertyRecord(result);
    }
  }
  return result;
}
