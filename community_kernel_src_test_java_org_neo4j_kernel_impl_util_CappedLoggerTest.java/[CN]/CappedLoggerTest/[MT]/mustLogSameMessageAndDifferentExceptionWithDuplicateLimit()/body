{
  logger.setDuplicateFilterEnabled(true);
  logMethod.log(logger,"### AAA ###",new ExceptionWithoutStackTrace("exc_aaa"));
  logMethod.log(logger,"### AAA ###",new ExceptionWithoutStackTrace("exc_bbb"));
  logMethod.log(logger,"### AAA ###",new ExceptionWithoutStackTrace2("exc_bbb"));
  String[] messages=new String[]{"### AAA ###","exc_aaa","### AAA ###","exc_bbb","### AAA ###","exc_bbb"};
  assertLoggedLines(messages,0,messages.length);
}
