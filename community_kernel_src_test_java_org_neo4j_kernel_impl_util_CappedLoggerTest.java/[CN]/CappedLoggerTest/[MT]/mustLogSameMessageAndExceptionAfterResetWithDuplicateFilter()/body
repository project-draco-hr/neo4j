{
  logger.setDuplicateFilterEnabled(true);
  logMethod.log(logger,"### AAA ###",new ExceptionWithoutStackTrace("xyz"));
  logger.reset();
  logMethod.log(logger,"### AAA ###",new ExceptionWithoutStackTrace("xyz"));
  String[] messages=new String[]{"### AAA ###","xyz","### AAA ###","xyz"};
  assertLoggedLines(messages,0,messages.length);
}
