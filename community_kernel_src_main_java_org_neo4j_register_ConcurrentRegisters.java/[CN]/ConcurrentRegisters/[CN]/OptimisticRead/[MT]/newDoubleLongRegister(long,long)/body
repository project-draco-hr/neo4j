{
  return new Register.DoubleLongRegister(){
    private long first=initialFirst;
    private long second=initialSecond;
    private final StampedLock lock=new StampedLock();
    @Override public long readFirst(){
      long stamp=lock.tryOptimisticRead();
      long result=first;
      if (!lock.validate(stamp)) {
        stamp=lock.readLock();
        try {
          result=first;
        }
  finally {
          lock.unlock(stamp);
        }
      }
      return result;
    }
    @Override public long readSecond(){
      long stamp=lock.tryOptimisticRead();
      long result=second;
      if (!lock.validate(stamp)) {
        stamp=lock.readLock();
        try {
          result=second;
        }
  finally {
          lock.unlock(stamp);
        }
      }
      return result;
    }
    @Override public void copyTo(    Register.DoubleLong.Out target){
      long stamp=lock.tryOptimisticRead();
      long firstCopy=this.first;
      long secondCopy=this.second;
      if (!lock.validate(stamp)) {
        stamp=lock.readLock();
        try {
          firstCopy=this.first;
          secondCopy=this.second;
        }
  finally {
          lock.unlock(stamp);
        }
      }
      target.write(firstCopy,secondCopy);
    }
    @Override public void write(    long first,    long second){
      long stamp=lock.writeLock();
      try {
        this.first=first;
        this.second=second;
      }
  finally {
        lock.unlock(stamp);
      }
    }
    @Override public void increment(    long firstDelta,    long secondDelta){
      long stamp=lock.writeLock();
      try {
        this.first+=firstDelta;
        this.second+=secondDelta;
      }
  finally {
        lock.unlock(stamp);
      }
    }
  }
;
}
