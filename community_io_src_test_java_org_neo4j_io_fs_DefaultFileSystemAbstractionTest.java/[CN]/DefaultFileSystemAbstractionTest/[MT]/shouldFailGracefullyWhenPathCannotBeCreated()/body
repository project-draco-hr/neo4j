{
  path=new File("target/" + UUID.randomUUID()){
    @Override public boolean mkdirs(){
      return false;
    }
  }
;
  try {
    defaultFileSystemAbstraction.mkdirs(path);
    fail();
  }
 catch (  IOException e) {
    assertThat(path.exists(),is(false));
    assertThat(e.getMessage(),is(String.format(DefaultFileSystemAbstraction.UNABLE_TO_CREATE_DIRECTORY_FORMAT,path)));
  }
}
