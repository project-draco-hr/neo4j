{
  List<String> nodeIds=nodeIds();
  Configuration config=Configuration.COMMAS;
  BatchImporterTool.main(arguments("--into",directory.absolutePath(),"--nodes",nodeHeader(config).getAbsolutePath() + pathSeparator + nodeData(false,config,nodeIds,alwaysTrue()).getAbsolutePath(),"--relationships",relationshipHeader(config).getAbsolutePath() + pathSeparator + relationshipData(false,config,nodeIds,alwaysTrue()).getAbsolutePath()));
  verifyData();
}
