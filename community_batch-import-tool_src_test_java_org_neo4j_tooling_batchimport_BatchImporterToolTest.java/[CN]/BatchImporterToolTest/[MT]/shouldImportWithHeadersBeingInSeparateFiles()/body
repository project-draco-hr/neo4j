{
  List<String> nodeIds=nodeIds();
  Configuration config=Configuration.COMMAS;
  BatchImporterTool.main(arguments("--into",directory.absolutePath(),"--nodes",nodeHeader(config).getAbsolutePath() + pathSeparator + nodeData(false,config,nodeIds).getAbsolutePath(),"--relationships",relationshipHeader(config).getAbsolutePath() + pathSeparator + relationshipData(false,config,nodeIds).getAbsolutePath()));
  verifyData();
}
