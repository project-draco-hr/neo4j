{
  char delimiter=config.delimiter();
  char arrayDelimiter=config.arrayDelimiter();
  for (int i=0; i < nodeIds.size(); i++) {
    if (linePredicate.accept(i)) {
      System.out.println("writing " + i + " ("+ nodeIds.get(i)+ ") into "+ writer);
      writer.println(nodeIds.get(i) + delimiter + randomName()+ delimiter+ randomLabels(arrayDelimiter));
    }
  }
}
