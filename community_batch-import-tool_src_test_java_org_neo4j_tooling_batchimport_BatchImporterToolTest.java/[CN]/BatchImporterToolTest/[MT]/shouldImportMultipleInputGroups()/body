{
  List<String> nodeIds=nodeIds();
  Configuration config=Configuration.COMMAS;
  BatchImporterTool.main(arguments("--into",directory.absolutePath(),"--nodes",nodeHeader(config).getAbsolutePath() + pathSeparator + nodeData(false,config,nodeIds,lines(0,NODE_COUNT / 2)).getAbsolutePath(),"--nodes",nodeData(true,config,nodeIds,lines(NODE_COUNT / 2,NODE_COUNT * 3 / 4)).getAbsolutePath() + pathSeparator + nodeData(false,config,nodeIds,lines(NODE_COUNT * 3 / 4,NODE_COUNT)).getAbsolutePath(),"--relationships",relationshipHeader(config).getAbsolutePath() + pathSeparator + relationshipData(false,config,nodeIds,alwaysTrue()).getAbsolutePath()));
  verifyData();
}
