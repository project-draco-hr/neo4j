{
  if (relTypes.get(name) == null) {
    if (!create) {
      return null;
    }
    int id=createRelationshipType(name);
    relTranslation.put(id,name);
    printIt(id,name);
  }
 else {
    relTranslation.put(relTypes.get(name),name);
    printIt(relTypes.get(name),name);
  }
  return new RelationshipTypeImpl(name);
}
