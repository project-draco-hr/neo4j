{
  acquireLock(this,LockType.READ);
  try {
    if (propertyMap.containsKey(key)) {
      return propertyMap.get(key).getValue();
    }
    ensureFullProperties();
    if (propertyMap.containsKey(key)) {
      return propertyMap.get(key).getValue();
    }
  }
  finally {
    releaseLock(this,LockType.READ);
  }
  throw new NotFoundException("" + key + " property not found.");
}
