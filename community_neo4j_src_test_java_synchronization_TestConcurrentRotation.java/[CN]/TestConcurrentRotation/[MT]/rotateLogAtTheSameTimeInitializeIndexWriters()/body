{
  run(new CreateInitialStateTask());
  restart();
  commitIndexWriter.enable();
  run(new LoadIndexesTask(2,false));
  RotateIndexLogTask rotateTask=new RotateIndexLogTask();
  runInThread(rotateTask);
  barrier1.await();
  run(new LoadIndexesTask(3,true));
  resumeFlushThread();
  barrier2.await();
  run(new Verifier());
}
