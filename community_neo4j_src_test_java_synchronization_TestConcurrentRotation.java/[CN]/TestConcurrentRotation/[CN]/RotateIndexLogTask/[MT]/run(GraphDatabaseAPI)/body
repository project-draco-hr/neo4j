{
  try {
    graphdb.getDependencyResolver().resolveDependency(XaDataSourceManager.class).getXaDataSource(LuceneDataSource.DEFAULT_NAME).rotateLogicalLog();
    setSuccess(graphdb,true);
  }
 catch (  Exception e) {
    setSuccess(graphdb,false);
    throw Exceptions.launderedException(e);
  }
 finally {
    rotateDone();
  }
}
