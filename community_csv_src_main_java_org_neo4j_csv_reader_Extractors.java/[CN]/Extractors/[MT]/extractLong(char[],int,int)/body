{
  if (length == 0) {
    throw new NumberFormatException("For input string \"" + String.valueOf(data,offset,length) + "\"");
  }
  long result=0;
  int i=0;
  boolean negate=false;
  if (data[offset] == '-') {
    negate=true;
    i++;
  }
  for (; i < length; i++) {
    result=result * 10 + digit(data[offset + i]);
  }
  return negate ? -result : result;
}
