{
  try {
    if (state.nodeIsDeletedInThisTx(nodeId)) {
      return false;
    }
    Property property=nodeGetProperty(nodeId,propertyKeyId);
    if (property.isNoProperty()) {
      return false;
    }
    return property.valueEquals(value);
  }
 catch (  EntityNotFoundException e) {
    return false;
  }
}
