{
  final DynamicRelationshipType type=DynamicRelationshipType.withName("KNOWS");
  db.beginTx();
  createRelationshipChain(db.getReferenceNode(),type,1);
  executeCommand("dump start n=node(0) match n-[r]->m return n,r,m;","begin","start _0 = node\\(0\\) with _0 ","create \\(_1\\)","_0-\\[:`KNOWS`\\]->_1","commit");
}
