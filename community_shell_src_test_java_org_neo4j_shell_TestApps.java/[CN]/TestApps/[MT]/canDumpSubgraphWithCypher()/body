{
  final DynamicRelationshipType type=DynamicRelationshipType.withName("KNOWS");
  beginTx();
  createRelationshipChain(db.createNode(),type,1);
  finishTx();
  executeCommand("dump start n=node(0) match n-[r]->m return n,r,m;","begin","create _0","create \\(_1\\)","_0-\\[:`KNOWS`\\]->_1","commit");
}
