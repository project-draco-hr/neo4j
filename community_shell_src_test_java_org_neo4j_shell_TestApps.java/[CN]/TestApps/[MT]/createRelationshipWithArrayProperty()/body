{
  String type="ARRAY";
  executeCommand("mkrel -ct " + type + " --rp \"{'values':[1,2,3,4]}\"");
  Transaction transaction=db.beginTx();
  try {
    assertThat(getCurrentNode().getSingleRelationship(withName(type),OUTGOING),inTx(db,hasProperty("values").withValue(new int[]{1,2,3,4})));
  }
  finally {
    transaction.finish();
  }
}
