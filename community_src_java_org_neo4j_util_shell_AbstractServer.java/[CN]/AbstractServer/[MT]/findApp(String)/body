{
  for (  String pkg : this.packages) {
    String name=pkg + "." + command.substring(0,1).toUpperCase()+ command.substring(1,command.length()).toLowerCase();
    try {
      App theApp=(App)Class.forName(name).newInstance();
      theApp.setServer(this);
      return theApp;
    }
 catch (    Exception e) {
    }
  }
  return null;
}
