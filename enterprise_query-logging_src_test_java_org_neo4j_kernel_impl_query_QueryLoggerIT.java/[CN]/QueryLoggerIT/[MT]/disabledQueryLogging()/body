{
  final File logFilename=new File(testDirectory.graphDbDir(),"query.log");
  GraphDatabaseService database=databaseBuilder.setConfig(GraphDatabaseSettings.log_queries,Settings.FALSE).setConfig(GraphDatabaseSettings.log_queries_filename,logFilename.getPath()).newGraphDatabase();
  executeQueryAndShutdown(database);
  List<String> lines=readAllLines(logFilename);
  assertTrue("Should not have any stream logged since query log is disabled",lines.isEmpty());
}
