{
  final File logFilename=new File(testDirectory.graphDbDir(),"messages.log");
  GraphDatabaseService database=databaseBuilder.setConfig(GraphDatabaseSettings.log_queries,Settings.TRUE).newGraphDatabase();
  executeQueryAndShutdown(database);
  List<String> lines=readAllLines(logFilename);
  boolean found=false;
  for (  String line : lines) {
    if (line.endsWith(GraphDatabaseSettings.log_queries.name() + " is enabled but no " + GraphDatabaseSettings.log_queries_filename.name()+ " has not been provided in configuration, hence query logging is suppressed")) {
      found=true;
    }
  }
  assertTrue(found);
}
