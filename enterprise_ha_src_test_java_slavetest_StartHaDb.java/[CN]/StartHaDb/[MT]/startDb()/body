{
  return new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(PATH.getPath()).setConfig(HaSettings.server_id,"" + MY_MACHINE_ID).setConfig(HaSettings.ha_server,ME).setConfig(ShellSettings.remote_shell_enabled,GraphDatabaseSetting.TRUE).setConfig(GraphDatabaseSettings.keep_logical_logs,GraphDatabaseSetting.TRUE).newGraphDatabase();
