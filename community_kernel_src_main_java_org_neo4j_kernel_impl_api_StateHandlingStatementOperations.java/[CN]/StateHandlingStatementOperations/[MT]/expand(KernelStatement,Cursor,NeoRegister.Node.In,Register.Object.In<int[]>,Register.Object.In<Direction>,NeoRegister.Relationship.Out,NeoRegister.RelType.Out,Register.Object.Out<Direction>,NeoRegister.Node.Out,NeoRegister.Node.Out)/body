{
  if (statement.hasTxStateWithChanges()) {
    return new AugmentWithLocalStateExpandCursor(storeLayer,statement.txState(),inputCursor,nodeId,types,expandDirection,relId,relType,direction,startNodeId,neighborNodeId);
  }
  return storeLayer.expand(inputCursor,nodeId,types,expandDirection,relId,relType,direction,startNodeId,neighborNodeId);
}
