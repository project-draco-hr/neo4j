{
  if (state.hasTxStateWithChanges()) {
    if (state.txState().nodeIsDeletedInThisTx(nodeId)) {
      return IteratorUtil.emptyPrimitiveIntIterator();
    }
    if (state.txState().nodeIsAddedInThisTx(nodeId)) {
      return toPrimitiveIntIterator(state.txState().nodeStateLabelDiffSets(nodeId).getAdded().iterator());
    }
    return state.txState().nodeStateLabelDiffSets(nodeId).applyPrimitiveIntIterator(storeLayer.nodeGetLabels(state,nodeId));
  }
  return storeLayer.nodeGetLabels(state,nodeId);
}
