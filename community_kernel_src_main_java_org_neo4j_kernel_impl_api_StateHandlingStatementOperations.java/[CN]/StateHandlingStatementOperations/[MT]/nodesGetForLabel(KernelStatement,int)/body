{
  if (state.hasTxStateWithChanges()) {
    PrimitiveLongIterator wLabelChanges=state.txState().nodesWithLabelChanged(labelId).applyPrimitiveLongIterator(storeLayer.nodesGetForLabel(state,labelId));
    return state.txState().nodesDeletedInTx().applyPrimitiveLongIterator(wLabelChanges);
  }
  return storeLayer.nodesGetForLabel(state,labelId);
}
