{
  if (state.hasTxStateWithChanges()) {
    PrimitiveLongIterator wLabelChanges=state.txState().nodesWithLabelChanged(labelId).augment(storeLayer.nodesGetForLabel(state.getStoreStatement(),labelId));
    return state.txState().addedAndRemovedNodes().augmentWithRemovals(wLabelChanges);
  }
  return storeLayer.nodesGetForLabel(state.getStoreStatement(),labelId);
}
