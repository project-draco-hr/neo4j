{
  ExpansionSource result=null;
  while (result == null) {
    ExpansionSource next=current.next(rules);
    if (next != null) {
      queue.add(next);
      if (rules.okToReturn(next)) {
        result=next;
      }
    }
 else {
      current=queue.poll();
      if (current == null) {
        return null;
      }
    }
  }
  return result;
}
