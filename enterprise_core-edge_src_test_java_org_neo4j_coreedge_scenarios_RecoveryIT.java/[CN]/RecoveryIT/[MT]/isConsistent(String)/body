{
  ConsistencyCheckService.Result result;
  try {
    result=new ConsistencyCheckService().runFullConsistencyCheck(new File(storeDir),Config.defaults(),ProgressMonitorFactory.NONE,FormattedLogProvider.toOutputStream(System.err),true);
  }
 catch (  ConsistencyCheckIncompleteException e) {
    throw new RuntimeException(e);
  }
  return result.isSuccessful();
}
