{
  for (  CoreGraphDatabase theDb : cluster.coreServers()) {
    boolean tryAgain;
    do {
      tryAgain=false;
      try (Transaction tx=theDb.beginTx()){
        Node node=theDb.createNode(label("second-round"));
        node.setProperty("server","val");
        tx.success();
      }
 catch (      Throwable t) {
        tryAgain=true;
        parkNanos(MILLISECONDS.toNanos(100));
      }
    }
 while (tryAgain);
  }
}
