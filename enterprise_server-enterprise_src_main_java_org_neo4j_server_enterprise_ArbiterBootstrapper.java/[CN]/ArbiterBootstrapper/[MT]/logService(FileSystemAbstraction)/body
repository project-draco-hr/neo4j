{
  String logDir=System.getProperty("org.neo4j.cluster.logdirectory","data/log");
  try {
    return StoreLogService.withUserLogProvider(FormattedLogProvider.toOutputStream(System.out)).inStoreDirectory(fileSystem,new File(logDir));
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
