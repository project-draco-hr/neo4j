{
  return new Neo4jJobScheduler(){
    @Override public JobHandle schedule(    Group group,    Runnable job){
      return super.schedule(group,slowRunnable(job));
    }
    @Override public JobHandle schedule(    Group group,    Runnable job,    Map<String,String> metadata){
      return super.schedule(group,slowRunnable(job),metadata);
    }
  }
;
}
