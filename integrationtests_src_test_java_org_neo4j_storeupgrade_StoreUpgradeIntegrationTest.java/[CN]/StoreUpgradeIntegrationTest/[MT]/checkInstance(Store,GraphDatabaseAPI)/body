{
  try (Transaction ignored=db.beginTx()){
    long count=count(GlobalGraphOperations.at(db).getAllNodes());
    assertThat(count,is(store.expectedNodeCount));
    long lastCommittedTxId=db.getDependencyResolver().resolveDependency(NeoStoreXaDataSource.class).getDependencyResolver().resolveDependency(LogFileInformation.class).getLastCommittedTxId();
    assertEquals(store.lastTxId,lastCommittedTxId);
  }
 }
