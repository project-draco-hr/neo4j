{
  GraphDatabaseService db=newGraphDbService();
  db.index().forNodes("index");
  db.shutdown();
  Runtime.getRuntime().exec(new String[]{"java","-cp",System.getProperty("java.class.path"),AddDeleteQuit.class.getName(),getDbPath()}).waitFor();
  new EmbeddedGraphDatabase(getDbPath()).shutdown();
  db.shutdown();
}
