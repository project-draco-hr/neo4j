{
  if (message.hasHeader(Message.TO)) {
    String to=message.getHeader(Message.TO);
    if (to.equals(Message.BROADCAST)) {
      broadcast(message);
    }
 else     if (to.equals(me.toString())) {
      receive(message);
    }
 else {
      send(message);
    }
  }
 else {
    receive(message);
  }
}
