{
  Assume.assumeFalse(Legacy23Store.LEGACY_VERSION.equals(version));
  final int shortFileLength=5 * 3;
  assertTrue(shortFileLength < UTF8.encode("StringPropertyStore " + version).length);
  truncateToFixedLength(fileSystem,new File(workingDirectory,"neostore.relationshiptypestore.db"),shortFileLength);
  final UpgradableDatabase upgradableDatabase=new UpgradableDatabase(fileSystem,new StoreVersionCheck(pageCacheRule.getPageCache(fileSystem)),new LegacyStoreVersionCheck(fileSystem));
  final boolean result=upgradableDatabase.storeFilesUpgradeable(workingDirectory);
  assertFalse(result);
}
