{
  try (ServerControls server=TestServerBuilders.newInProcessBuilder().withExtension("/myExtension",MyUnmanagedExtension.class).withFixture(new Function<GraphDatabaseService,Void>(){
    @Override public Void apply(    GraphDatabaseService graphDatabaseService) throws RuntimeException {
      try (Transaction tx=graphDatabaseService.beginTx()){
        graphDatabaseService.createNode(Label.label("User"));
        tx.success();
      }
       return null;
    }
  }
).newServer()){
    Result result=server.graph().execute("MATCH (n:User) return n");
    assertEquals(1,IteratorUtil.count(result));
  }
 }
