{
  super("Node counts",config,false);
  add(new ReadNodeRecordsStep(control(),config.batchSize(),config.movingAverageSize(),nodeStore));
  add(new RecordProcessorStep<>(control(),"COUNT",config.workAheadSize(),config.movingAverageSize(),new NodeCountsProcessor(nodeStore,cache,highLabelId,countsUpdater),true,additionalStatsProviders));
}
