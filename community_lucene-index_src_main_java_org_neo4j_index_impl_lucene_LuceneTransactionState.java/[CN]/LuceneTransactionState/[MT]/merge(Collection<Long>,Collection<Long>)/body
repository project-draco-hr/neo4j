{
  if (c1 == null && c2 == null) {
    return Collections.<Long>emptySet();
  }
 else   if (c1 != null && c2 != null) {
    if (c1.isEmpty()) {
      return c2;
    }
    if (c2.isEmpty()) {
      return c1;
    }
    Collection<Long> result=new HashSet<>(c1.size() + c2.size(),1);
    result.addAll(c1);
    result.addAll(c2);
    return result;
  }
 else {
    return c1 != null ? c1 : c2;
  }
}
