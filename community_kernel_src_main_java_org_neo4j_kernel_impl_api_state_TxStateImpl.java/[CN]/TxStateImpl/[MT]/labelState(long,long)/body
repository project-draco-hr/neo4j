{
  NodeState nodeState=getState(nodeStatesMap(),nodeId,null);
  if (nodeState != null) {
    DiffSets<Long> labelDiff=nodeState.labelDiffSets();
    if (labelDiff.isAdded(labelId)) {
      return Boolean.TRUE;
    }
    if (labelDiff.isRemoved(labelId)) {
      return Boolean.FALSE;
    }
  }
  return null;
}
