{
  Node node;
  try (Transaction tx=db.beginTx()){
    node=db.createNode();
    node.setProperty("name","not a value");
    tx.success();
  }
   db.getDependencyResolver().resolveDependency(NodeManager.class).clearCache();
  try (Transaction tx=db.beginTx()){
    db.getNodeById(node.getId());
    tx.success();
  }
   return node;
}
