{
  return new TypeSafeMatcher<RecordingCallback.Call>(){
    @Override protected boolean matchesSafely(    RecordingCallback.Call item){
      return item instanceof RecordingCallback.InitSuccess && ((RecordingCallback.InitSuccess)item).credentialsExpired();
    }
    @Override public void describeTo(    Description description){
      description.appendText("SUCCESS (but password change required)");
    }
  }
;
}
