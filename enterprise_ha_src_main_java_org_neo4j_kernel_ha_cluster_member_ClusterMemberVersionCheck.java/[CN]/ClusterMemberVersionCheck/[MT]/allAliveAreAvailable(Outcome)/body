{
  outcome.clearUnavailable();
  for (  ClusterMember member : clusterMembers.getMembers()) {
    if (!myId.equals(member.getInstanceId()) && member.isInitiallyKnown() && member.isAlive()&& HighAvailabilityModeSwitcher.UNKNOWN.equals(member.getHARole())) {
      outcome.addUnavailable(member.getInstanceId());
    }
  }
}
