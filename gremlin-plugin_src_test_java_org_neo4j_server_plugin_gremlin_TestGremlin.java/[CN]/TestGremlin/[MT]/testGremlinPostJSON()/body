{
  server=new WrappingNeoServerBootstrapper(graphdb);
  server.start();
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"script\":\"g.v(2).outE.inV\"}").payloadType(MediaType.APPLICATION_JSON_TYPE).post("http://localhost:7474/db/data/ext/GremlinPlugin/graphdb/execute_script").entity();
  System.out.println(response);
  assertTrue(response.contains("you"));
  server.stop();
}
