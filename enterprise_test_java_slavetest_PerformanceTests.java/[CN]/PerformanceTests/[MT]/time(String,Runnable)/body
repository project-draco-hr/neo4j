{
  long t=System.currentTimeMillis();
  try {
    runnable.run();
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
  long time=System.currentTimeMillis() - t;
  System.out.println(message + ": " + time+ "ms");
}
