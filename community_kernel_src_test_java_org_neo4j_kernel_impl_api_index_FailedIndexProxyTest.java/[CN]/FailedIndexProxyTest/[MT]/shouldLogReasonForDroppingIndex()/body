{
  StringLogger log=mock(StringLogger.class);
  new FailedIndexProxy(new IndexDescriptor(0,0),new SchemaIndexProvider.Descriptor("foo","bar"),"foo",mock(IndexPopulator.class),IndexPopulationFailure.failure("it broke"),log).drop();
  verify(log).info("FailedIndexProxy#drop index on foo dropped due to:\nit broke");
}
