{
  StringLogger log=mock(StringLogger.class);
  FailedIndexProxy index=new FailedIndexProxy(descriptor,config,providerDescriptor,userDescription,indexPopulator,indexPopulationFailure,indexCountsRemover,log);
  index.drop();
  verify(indexPopulator).drop();
  verify(indexCountsRemover).remove();
  verifyNoMoreInteractions(indexPopulator,indexCountsRemover);
}
