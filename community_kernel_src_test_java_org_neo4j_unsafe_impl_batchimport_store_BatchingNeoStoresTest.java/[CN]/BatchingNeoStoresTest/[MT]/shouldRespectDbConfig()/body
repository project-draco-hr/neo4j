{
  int size=10;
  Config config=new Config(stringMap(GraphDatabaseSettings.array_block_size.name(),String.valueOf(size),GraphDatabaseSettings.string_block_size.name(),String.valueOf(size)));
  try (BatchingNeoStores store=new BatchingNeoStores(fsr.get(),storeDir,DEFAULT,NullLogService.getInstance(),EMPTY,config)){
    assertEquals(size + BLOCK_HEADER_SIZE,store.getPropertyStore().getArrayBlockSize());
    assertEquals(size + BLOCK_HEADER_SIZE,store.getPropertyStore().getStringBlockSize());
  }
 }
