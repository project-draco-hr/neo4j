{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("query=" + URLEncoder.encode("start n  = (" + data.get().get("I").getId() + ") return n.name","UTF-8") + "&returns=n").payloadType(MediaType.APPLICATION_FORM_URLENCODED_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("I"));
}
