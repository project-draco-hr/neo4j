{
  RelIdArray array=new RelIdArray();
  array.add(1);
  array.add(2);
  array.add(3);
  RelIdIterator itr=array.iterator();
  assertTrue(itr.hasNext());
  assertTrue(itr.hasNext());
  assertEquals(1L,itr.next());
  assertTrue(itr.hasNext());
  assertEquals(2L,itr.next());
  assertTrue(itr.hasNext());
  assertEquals(3L,itr.next());
  assertFalse(itr.hasNext());
  assertFalse(itr.hasNext());
}
