{
  DocsGenerator.create("Property values can not be nested","Nesting properties is not supported. " + "You could store the json as a string instead.").payload("{\"foo\" : {\"bar\" : \"baz\"}}").expectedStatus(Response.Status.BAD_REQUEST).post(functionalTestHelper.nodeUri());
  ClientResponse response=setNodePropertyOnServer("jim",new HashMap<String,Object>());
  assertEquals(400,response.getStatus());
}
