{
  Node jim=data.get().get("jim");
  gen.get().payload(JsonHelper.createJsonFrom("bar")).expectedStatus(204).put(getPropertyUri(jim,"foo").toString());
  assertTrue(jim.hasProperty("foo"));
  gen.get().payload(JsonHelper.createJsonFrom("bar2")).expectedStatus(204).put(getPropertyUri(jim,"foo2").toString());
  assertTrue(jim.hasProperty("foo"));
  assertTrue(jim.hasProperty("foo2"));
}
