{
  long timestamp=System.currentTimeMillis();
  File branchDir=new File(dir,"branched-" + timestamp);
  branchDir.mkdirs();
  for (  File file : dir.listFiles()) {
    if (!file.equals(StringLogger.DEFAULT_NAME) && !file.getName().startsWith("branched-"))     assertTrue(FileUtils.renameFile(file,new File(branchDir,file.getName())));
  }
  return timestamp;
}
