{
  if (type == int.class) {
    return 666;
  }
  if (type == long.class) {
    return 17L;
  }
  if (type == double.class) {
    return 6.28318530717958647692d;
  }
  if (type == float.class) {
    return 3.14f;
  }
  if (type == short.class) {
    return (short)8733;
  }
  if (type == byte.class) {
    return (byte)123;
  }
  if (type == boolean.class) {
    return false;
  }
  if (type == char.class) {
    return 'Z';
  }
  if (type == String.class) {
    return "hello world";
  }
  throw new IllegalArgumentException(type.getName());
}
