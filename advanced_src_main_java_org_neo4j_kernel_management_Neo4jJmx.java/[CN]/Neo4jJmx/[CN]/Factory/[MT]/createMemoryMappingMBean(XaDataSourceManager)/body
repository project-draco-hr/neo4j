{
  NeoStoreXaDataSource datasource=(NeoStoreXaDataSource)datasourceMananger.getXaDataSource("nioneodb");
  if (!register(new MemoryMappingMonitor.MXBeanImplementation(instance.id,datasource))) {
    if (!register(new MemoryMappingMonitor.MemoryMapping(instance.id,datasource)))     failedToRegister("MemoryMappingMBean");
  }
}
