  bandNode.createRelationshipTo(city2,TestRelationshipTypes.LIKE);
  bandNode.createRelationshipTo(city3,TestRelationshipTypes.HATE);
  bandNode.createRelationshipTo(city4,TestRelationshipTypes.LIKE);
