{
  for (  Class<?> iface : clazz.getInterfaces()) {
    if (iface == DynamicMBean.class) {
      return clazz.getSimpleName();
    }
    try {
      return iface.getField("NAME").get(null);
    }
 catch (    Exception e) {
    }
  }
  throw new IllegalStateException("Invalid Neo4jMonitor implementation.");
}
