{
  pageCache=pageCacheRule.getPageCache(fs);
  schemaIndexProvider=new LuceneSchemaIndexProvider(fs,DirectoryFactory.PERSISTENT,storeDir.directory(),NullLogProvider.getInstance(),Config.empty(),OperationalMode.single);
  labelScanStoreProvider=new LabelScanStoreProvider(new InMemoryLabelScanStore(),1);
  upgradableDatabase=new UpgradableDatabase(fs,new StoreVersionCheck(pageCache),new LegacyStoreVersionCheck(fs),recordFormat);
}
