{
  if (fileSystem == null) {
    fileSystem=new EphemeralFileSystemAbstraction();
  }
  File directory=new File(SEGMENTED_LOG_DIRECTORY_NAME);
  fileSystem.mkdir(directory);
  SegmentedRaftLog newRaftLog=new SegmentedRaftLog(fileSystem,directory,1024,new DummyRaftableContentSerializer(),NullLogProvider.getInstance(),"1 entries",8,new FakeClock(),new OnDemandJobScheduler());
  life.add(newRaftLog);
  life.init();
  life.start();
  return newRaftLog;
}
