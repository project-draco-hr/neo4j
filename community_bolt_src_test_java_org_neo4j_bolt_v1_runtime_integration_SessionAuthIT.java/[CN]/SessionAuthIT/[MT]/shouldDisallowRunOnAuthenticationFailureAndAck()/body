{
  Session session=env.newSession("test");
  RecordingCallback recorder=new RecordingCallback();
  session.init("TestClient/1.0.0",map("scheme","basic","principal","neo4j","credentials","j4oen"),-1,recorder);
  session.ackFailure(recorder);
  session.run("RETURN 1337",map(),recorder);
  assertThat(recorder,recorded(failedWith(Status.Security.Unauthorized),success(),failedWith(Status.Request.Invalid)));
}
