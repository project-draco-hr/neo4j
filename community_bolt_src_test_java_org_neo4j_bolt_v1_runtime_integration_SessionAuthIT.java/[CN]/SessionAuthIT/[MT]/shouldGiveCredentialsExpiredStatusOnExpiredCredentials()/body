{
  Session session=env.newSession("test");
  RecordingCallback recorder=new RecordingCallback();
  session.init("TestClient/1.0.0",map("scheme","basic","principal","neo4j","credentials","neo4j"),-1,recorder);
  session.run("CREATE ()",map(),recorder);
  assertThat(recorder,recorded(successButRequiresPasswordChange(),failedWith(Status.Security.CredentialsExpired)));
}
