{
  Session session=env.newSession("test");
  RecordingCallback recorder=new RecordingCallback();
  session.init("TestClient/1.0.0",map("scheme","basic","principal","neo4j","credentials","j4oen"),-1,null,recorder);
  session.reset(null,recorder);
  session.init("TestClient/1.0.0",map("scheme","basic","principal","neo4j","credentials","neo4j"),-1,null,recorder);
  assertThat(recorder,recorded(failed(),success(),success()));
}
