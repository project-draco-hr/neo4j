{
  ExecutionResult result=mock(ExecutionResult.class);
  when(result.iterator()).thenReturn(IteratorUtils.emptyIterator());
  when(result.columns()).thenReturn(new ArrayList<String>());
  Map<String,Object> serialized=serialize(new CypherResultRepresentation(result,false,false));
  assertFalse("Didn't expect to see a plan here",serialized.containsKey("plan"));
}
