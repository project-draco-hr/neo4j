{
  preflightTasks=new PreFlightTasks(DevNullLoggingService.DEV_NULL){
    @Override public boolean run(){
      return false;
    }
    @Override public PreflightTask failedTask(){
      return new PreflightTask(){
        @Override public String getFailureMessage(){
          return "mockFailure";
        }
        @Override public boolean run(){
          return false;
        }
      }
;
    }
  }
;
  return this;
}
