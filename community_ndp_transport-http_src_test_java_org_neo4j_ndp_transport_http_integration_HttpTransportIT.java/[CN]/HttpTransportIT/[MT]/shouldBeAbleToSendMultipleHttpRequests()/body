{
  HTTP.Response rs=http.POST("/session/");
  String sessionLocation=rs.location();
  http.POST(sessionLocation,createHttpPayload(serialize(run("UNWIND [1,2,3] AS a RETURN a, a * a AS a_squared"),pullAll())));
  rs=http.POST(sessionLocation,createHttpPayload(serialize(run("UNWIND [1,2,3] AS a RETURN a, a * a AS a_squared"),pullAll())));
  assertThat(messages(rs.rawContent()),equalTo(asList(success(map("fields",asList("a","a_squared"))),Messages.record(record(1l,1l)),Messages.record(record(2l,4l)),Messages.record(record(3l,9l)),success(map()))));
}
