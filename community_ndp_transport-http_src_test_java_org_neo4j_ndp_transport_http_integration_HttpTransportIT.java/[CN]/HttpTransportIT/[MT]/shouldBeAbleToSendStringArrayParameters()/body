{
  HTTP.Response rs=http.POST("/session/");
  String sessionLocation=rs.location();
  String[] arrayValue=new String[]{"Mj??lnir","Mj??lnir","Mj??lnir"};
  rs=http.POST(sessionLocation,createHttpPayload(serialize(run("CREATE (a {value:{value}}) RETURN a.value",map("value",arrayValue)),pullAll())));
  assertThat(messages(rs.rawContent()),equalsMessages(msgSuccess(),msgRecord(StreamMatchers.eqRecord(equalTo(asList(arrayValue)))),msgSuccess()));
}
