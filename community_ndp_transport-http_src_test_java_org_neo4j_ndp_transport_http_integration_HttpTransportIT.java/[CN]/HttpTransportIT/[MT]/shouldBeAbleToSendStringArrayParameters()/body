{
  HTTP.Response rs=http.POST("/session/");
  String sessionLocation=rs.location();
  String[] arrayValue=new String[]{"Mj??lnir","Mj??lnir","Mj??lnir"};
  rs=http.POST(sessionLocation,messages(run("CREATE (a {value:{value}}) RETURN a.value",map("value",arrayValue)),pullAll()));
  assertThat(messages(rs),equalsMessages(msgSuccess(),msgItem(StreamMatchers.record(equalTo(asList(arrayValue)))),msgSuccess()));
}
