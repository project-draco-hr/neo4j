{
  super(machineId,graphDb);
  this.clusterName=clusterName;
  this.machineId=machineId;
  this.haServer=haServer;
  this.zooClient=new ZooClient(zooKeeperServers,machineId,getRootPathGetter(graphDb.getStoreDir()),receiver,haServer,backupPort,clientReadTimeout,clientLockReadTimeout,maxConcurrentChannelsPerClient,writeLastCommittedTx,graphDb);
}
