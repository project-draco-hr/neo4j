{
  Iterator<Relationship> relItr=newRelIterator(dir,new RelationshipType[]{type});
  if (relItr.hasNext()) {
    Relationship rel=relItr.next();
    if (relItr.hasNext()) {
      throw new NotFoundException("More than one relationship[" + type + ", "+ dir+ "] found for "+ this);
    }
    return rel;
  }
  return null;
}
