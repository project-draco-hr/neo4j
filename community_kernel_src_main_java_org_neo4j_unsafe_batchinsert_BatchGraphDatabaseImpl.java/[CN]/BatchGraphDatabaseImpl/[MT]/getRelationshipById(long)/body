{
  RelationshipBatchImpl rel=rels.get(id);
  if (rel == null) {
    try {
      BatchRelationship simpleRel=batchInserter.getRelationshipById(id);
      Map<String,Object> props=batchInserter.getRelationshipProperties(id);
      rel=new RelationshipBatchImpl(simpleRel,this,mutableCopyOf(props));
      rels.put(id,rel);
    }
 catch (    InvalidRecordException e) {
      throw new NotFoundException(e);
    }
  }
  return rel;
}
