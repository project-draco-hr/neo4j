{
  try {
    neoDataSource=new NeoStoreXaDataSource(config,storeFactory,lockManager,lockReleaser,logging.getLogger(Loggers.DATASOURCE),xaFactory,transactionInterceptorProviders,dependencyResolver);
    xaDataSourceManager.registerDataSource(neoDataSource);
  }
 catch (  IOException e) {
    throw new IllegalStateException("Could not create Neo XA datasource",e);
  }
}
