{
  neoDataSource=new NeoStoreXaDataSource(config,storeFactory,logging.getMessagesLog(NeoStoreXaDataSource.class),jobScheduler,logging,updateableSchemaState,new NonTransactionalTokenNameLookup(labelTokenHolder,propertyKeyTokenHolder),dependencyResolver,propertyKeyTokenHolder,labelTokenHolder,relationshipTypeTokenHolder,lockManager,this,transactionEventHandlers,monitors.newMonitor(IndexingService.Monitor.class),fileSystem,storeMigrationProcess,transactionMonitor,kernelHealth,txIdGenerator,createHeaderInformationFactory(),startupStatistics,caches,nodeManager,guard,indexStore,getCommitProcessFactory());
  dataSourceManager.register(neoDataSource);
}
