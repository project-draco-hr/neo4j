{
  neoDataSource=new NeoStoreXaDataSource(config,storeFactory,lockManager,logging.getMessagesLog(NeoStoreXaDataSource.class),xaFactory,stateFactory,transactionInterceptorProviders,jobScheduler,logging,updateableSchemaState,nodeManager,dependencyResolver);
  xaDataSourceManager.registerDataSource(neoDataSource);
}
