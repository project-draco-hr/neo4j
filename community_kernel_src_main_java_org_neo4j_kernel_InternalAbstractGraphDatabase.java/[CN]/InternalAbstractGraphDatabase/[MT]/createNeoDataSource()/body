{
  try {
    neoDataSource=new NeoStoreXaDataSource(config,storeFactory,lockManager,logging.getMessagesLog(NeoStoreXaDataSource.class),xaFactory,stateFactory,transactionInterceptorProviders,dependencyResolver);
    xaDataSourceManager.registerDataSource(neoDataSource);
  }
 catch (  IOException e) {
    throw new IllegalStateException("Could not create Neo XA datasource",e);
  }
}
