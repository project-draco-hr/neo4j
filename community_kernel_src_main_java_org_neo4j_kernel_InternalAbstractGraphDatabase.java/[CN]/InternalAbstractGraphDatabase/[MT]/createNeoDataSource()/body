{
  neoDataSource=new NeoStoreXaDataSource(config,storeFactory,logging.getMessagesLog(NeoStoreXaDataSource.class),xaFactory,stateFactory,transactionInterceptorProviders,jobScheduler,logging,updateableSchemaState,new NonTransactionalTokenNameLookup(labelTokenHolder,propertyKeyTokenHolder),dependencyResolver,txManager,propertyKeyTokenHolder,labelTokenHolder,relationshipTypeTokenHolder,persistenceManager,lockManager,nodeManager,this);
