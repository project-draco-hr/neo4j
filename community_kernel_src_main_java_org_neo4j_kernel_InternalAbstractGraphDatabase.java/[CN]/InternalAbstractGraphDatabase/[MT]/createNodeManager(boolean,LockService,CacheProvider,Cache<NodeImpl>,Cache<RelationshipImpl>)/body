{
  if (readOnly) {
    return new ReadOnlyNodeManager(logging,this,locks,txManager,persistenceManager,persistenceSource,relationshipTypeTokenHolder,cacheType,propertyKeyTokenHolder,labelTokenHolder,createNodeLookup(),createRelationshipLookups(),nodeCache,relCache,xaDataSourceManager,statementContextProvider);
  }
  return new NodeManager(logging,this,locks,txManager,persistenceManager,persistenceSource,relationshipTypeTokenHolder,cacheType,propertyKeyTokenHolder,labelTokenHolder,createNodeLookup(),createRelationshipLookups(),nodeCache,relCache,xaDataSourceManager,statementContextProvider);
}
