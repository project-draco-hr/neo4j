{
  return new NodeProxy.NodeLookup(){
    @Override public GraphDatabaseService getGraphDatabase(){
      return InternalAbstractGraphDatabase.this;
    }
    @Override public NodeManager getNodeManager(){
      return nodeManager;
    }
    @Override public NodeImpl lookup(    long nodeId){
      assertDatabaseRunning();
      return nodeManager.getNodeForProxy(nodeId);
    }
  }
;
}
