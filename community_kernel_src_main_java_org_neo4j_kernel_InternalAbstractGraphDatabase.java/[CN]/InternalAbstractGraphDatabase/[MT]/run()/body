{
  create();
  try {
    registerRecovery();
    life.start();
    if (txManager.getRecoveryError() != null) {
      throw txManager.getRecoveryError();
    }
  }
 catch (  Throwable throwable) {
    msgLog.logMessage("Startup failed",throwable);
    shutdown();
    throw new RuntimeException(throwable);
  }
}
