{
  assertTrue(tm.getStatus() == Status.STATUS_NO_TRANSACTION);
  tm.begin();
  assertTrue(tm.getStatus() == Status.STATUS_ACTIVE);
  tm.getTransaction().setRollbackOnly();
  assertTrue(tm.getStatus() == Status.STATUS_MARKED_ROLLBACK);
  tm.rollback();
  assertTrue(tm.getStatus() == Status.STATUS_NO_TRANSACTION);
}
