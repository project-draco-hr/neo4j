{
  try {
    grabAvailableCommands();
    successfulGrabAvailableCommands=true;
  }
 catch (  Exception e) {
    if (successfulGrabAvailableCommands) {
      System.err.println("Couldn't grab available commands: " + e.toString());
      successfulGrabAvailableCommands=false;
    }
  }
  try {
    return (String)consoleReader.getClass().getMethod("readLine").invoke(consoleReader);
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
