{
  startSecondDatabase();
  HighAvailability secondHa=new Neo4jManager(secondDatabase.getDependencyResolver().resolveDependency(JmxKernelExtension.class).getSingleManagementBean(Kernel.class)).getHighAvailabilityBean();
  assertEquals(2,secondHa.getInstancesInCluster().length);
  stopSecondDb();
  Neo4jManager neo4j=new Neo4jManager(database.getDependencyResolver().resolveDependency(JmxKernelExtension.class).getSingleManagementBean(Kernel.class));
  HighAvailability ha=neo4j.getHighAvailabilityBean();
  assertEquals(1,ha.getInstancesInCluster().length);
  assertMasterInformation(ha);
}
