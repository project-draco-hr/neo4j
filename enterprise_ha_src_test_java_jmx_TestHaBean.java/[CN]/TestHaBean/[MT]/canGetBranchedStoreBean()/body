{
  Neo4jManager neo4j=new Neo4jManager(database.getDependencyResolver().resolveDependency(JmxKernelExtension.class).getSingleManagementBean(Kernel.class));
  BranchedStore bs=neo4j.getBranchedStoreBean();
  assertNotNull("could not get ha bean",bs);
  assertEquals("no branched stores for new db",0,bs.getBranchedStores().length);
}
