{
  GraphDatabaseService db=createDb(storeDir,defaultBackupPortHostParams());
  createAndIndexNode(db,1);
  createAndIndexNode(db,2);
  createAndIndexNode(db,3);
  createAndIndexNode(db,4);
  BackupService backupService=new BackupService(fileSystem);
  backupService.doFullBackup(BACKUP_HOST,backupPort,backupDir.getAbsolutePath(),false,new Config(defaultBackupPortHostParams()),false);
  db.shutdown();
  checkPreviousCommittedTxIdFromFirstLog(DEFAULT_DATA_SOURCE_NAME);
  checkPreviousCommittedTxIdFromFirstLog(DEFAULT_NAME);
}
