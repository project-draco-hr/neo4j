{
  long p=getBoundedPointer(offset,SIZE_OF_LONG);
  long value;
  if (UnsafeUtil.allowUnalignedMemoryAccess) {
    value=UnsafeUtil.getLong(p);
    if (!UnsafeUtil.storeByteOrderIsNative) {
      value=Long.reverseBytes(value);
    }
  }
 else {
    value=getLongBigEndian(p);
  }
  return value;
}
