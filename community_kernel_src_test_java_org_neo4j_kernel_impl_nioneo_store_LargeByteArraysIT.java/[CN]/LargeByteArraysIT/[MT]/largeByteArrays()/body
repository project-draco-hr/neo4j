{
  EmbeddedGraphDatabase db=new EmbeddedGraphDatabase(forTest(getClass()).directory("bytearrays",true).getAbsolutePath());
  try {
    for (int i=0; i < 100000; i++) {
      createNodeWithBigArray(db);
      if (i > 0 && i % 100 == 0)       System.out.println(i);
    }
  }
  finally {
    db.shutdown();
  }
}
