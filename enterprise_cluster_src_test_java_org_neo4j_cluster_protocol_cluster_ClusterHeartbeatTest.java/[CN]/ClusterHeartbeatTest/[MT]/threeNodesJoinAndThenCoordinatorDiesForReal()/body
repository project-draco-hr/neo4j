{
  final Map<String,URI> roles=new HashMap<String,URI>();
  testCluster(3,DEFAULT_NETWORK(),new ClusterTestScriptDSL().rounds(1000).join(100,1).join(100,2).join(100,3).message(100,"*** All nodes up and ok").getRoles(0,roles).down(500,1).message(1000,"*** Should have seen failure by now").verifyCoordinatorRoleSwitched(roles).leave(0,1).leave(300,2).leave(300,3));
}
