{
  final Map<String,InstanceId> roles=new HashMap<String,InstanceId>();
  testCluster(3,DEFAULT_NETWORK(),new ClusterTestScriptDSL().rounds(1000).join(100,1,1).join(100,2,1).join(100,3,1).message(3000,"*** All nodes up and ok").getRoles(roles).down(800,1).message(2000,"*** Should have seen failure by now").verifyCoordinatorRoleSwitched(roles).leave(0,1).leave(300,2).leave(300,3));
