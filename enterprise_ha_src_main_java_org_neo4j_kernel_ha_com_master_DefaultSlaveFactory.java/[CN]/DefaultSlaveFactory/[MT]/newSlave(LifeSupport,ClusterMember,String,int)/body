{
  return life.add(new SlaveClient(clusterMember.getInstanceId(),clusterMember.getHAUri().getHost(),clusterMember.getHAUri().getPort(),originHostNameOrIp,logging,storeId,2,chunkSize,monitors.newMonitor(ByteCounterMonitor.class,SlaveClient.class),monitors.newMonitor(RequestMonitor.class,SlaveClient.class)));
}
