{
  if (params == null) {
    params=Collections.emptyMap();
  }
  try {
    subject.client.send(TransportTestUtil.chunk(RunMessage.run(call,params),PullAllMessage.pullAll()));
    resultConsumer.accept(collectResults(subject.client));
    return "";
  }
 catch (  Exception e) {
    return e.getMessage();
  }
}
