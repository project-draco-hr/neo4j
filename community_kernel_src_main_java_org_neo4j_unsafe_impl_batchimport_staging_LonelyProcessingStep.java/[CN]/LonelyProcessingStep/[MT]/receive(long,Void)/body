{
  new Thread(){
    @Override public void run(){
      assertHealthy();
      try {
        lastProcessingTimestamp=currentTimeMillis();
        process();
      }
 catch (      Throwable e) {
        issuePanic(e);
      }
 finally {
        endOfUpstream();
      }
    }
  }
.start();
  return 0;
}
