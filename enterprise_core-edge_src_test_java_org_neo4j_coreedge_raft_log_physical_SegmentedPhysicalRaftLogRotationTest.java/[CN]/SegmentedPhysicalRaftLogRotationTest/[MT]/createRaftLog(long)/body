{
  if (fileSystem == null) {
    fileSystem=new EphemeralFileSystemAbstraction();
  }
  File directory=new File("raft-log");
  fileSystem.mkdir(directory);
  SegmentedPhysicalRaftLog newRaftLog=new SegmentedPhysicalRaftLog(fileSystem,directory,rotateAtSize,new DummyRaftableContentSerializer(),NullLogProvider.getInstance(),1000);
  life.add(newRaftLog);
  life.init();
  life.start();
  return newRaftLog;
}
