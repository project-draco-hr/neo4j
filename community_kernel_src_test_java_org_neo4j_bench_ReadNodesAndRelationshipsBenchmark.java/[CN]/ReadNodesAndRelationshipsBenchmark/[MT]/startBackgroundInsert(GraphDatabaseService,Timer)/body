{
  timer.schedule(new TimerTask(){
    @Override public void run(){
      Transaction tx=db.beginTx();
      try {
        Node node1=db.createNode();
        Node node2=db.createNode();
        node1.createRelationshipTo(node2,withName("LIKES"));
        tx.success();
      }
  finally {
        tx.finish();
      }
    }
  }
,10,200);
}
