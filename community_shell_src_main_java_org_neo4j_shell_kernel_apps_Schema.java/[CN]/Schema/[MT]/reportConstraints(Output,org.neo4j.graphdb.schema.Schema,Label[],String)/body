{
  int j=0;
  for (  ConstraintDefinition constraint : constraintsByLabelAndProperty(schema,labels,property)) {
    if (j == 0) {
      out.println();
      out.println("Constraints");
    }
    out.println(constraint.toString());
    j++;
  }
  if (j == 0) {
    out.println();
    out.println("No constraints");
  }
}
