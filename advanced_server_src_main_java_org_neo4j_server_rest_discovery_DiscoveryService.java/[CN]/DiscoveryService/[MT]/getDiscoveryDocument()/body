{
  String webAdminManagementUri=configuration.getString(Configurator.WEB_ADMIN_PATH_PROPERTY_KEY);
  String dataUri=configuration.getString(Configurator.REST_API_PATH_PROPERTY_KEY,Configurator.DEFAULT_REST_API_PATH);
  DiscoveryRepresentation dr=new DiscoveryRepresentation(webAdminManagementUri,dataUri);
  return outputFormat.ok(dr);
}
