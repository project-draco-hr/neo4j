{
  File neoStore=new File(fileName);
  if (!neoStore.exists()) {
    throw new IOException("No such neostore " + fileName);
  }
  Set relTypeSet=checkRelTypeStore(fileName + ".relationshiptypestore.db");
  Set propertyIndexSet=checkPropertyIndexStore(fileName + ".propertystore.db.index");
  Set propertySet=checkPropertyStore(fileName + ".propertystore.db",propertyIndexSet);
  Set nodeSet=checkNodeStore(fileName + ".nodestore.db",propertySet);
  checkRelationshipStore(fileName + ".relationshipstore.db",propertySet,relTypeSet,nodeSet);
  if (!propertySet.isEmpty()) {
    System.out.println("Stray properties found : " + propertySet.size());
  }
  if (!nodeSet.isEmpty()) {
    throw new IOException("Node(s) with set relationship(s) found " + "but relationships not found in store " + nodeSet.size());
  }
}
