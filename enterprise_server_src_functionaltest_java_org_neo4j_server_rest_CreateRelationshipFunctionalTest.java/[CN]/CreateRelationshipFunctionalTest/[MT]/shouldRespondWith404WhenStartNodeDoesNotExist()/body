{
  long endNode=helper.createNode();
  String jsonString="{\"to\" : \"" + server.restApiUri() + "node/"+ endNode+ "\", \"type\" : \"LOVES\", \"data\" : {\"foo\" : \"bar\"}}";
  ClientResponse response=Client.create().resource(server.restApiUri() + "node/999999/relationships").type(MediaType.APPLICATION_JSON).accept(MediaType.APPLICATION_JSON).entity(jsonString).post(ClientResponse.class);
  assertEquals(404,response.getStatus());
}
