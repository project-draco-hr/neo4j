{
  try {
    context.future.get();
    return true;
  }
 catch (  InterruptedException e) {
    return false;
  }
catch (  ExecutionException e) {
    context.throwable=e.getCause();
    slaveCommitFailureLogger.error("Slave " + context.slave.getServerId() + ": Replication commit threw"+ (context.throwable instanceof ComException ? " communication" : "")+ " exception:",context.throwable);
    return false;
  }
catch (  CancellationException e) {
    return false;
  }
}
