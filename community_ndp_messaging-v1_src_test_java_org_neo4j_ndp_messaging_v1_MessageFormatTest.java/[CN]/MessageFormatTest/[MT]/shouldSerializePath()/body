{
  assertSerializesNeoValue(new ValuePath(new ValueNode(12l,asList(label("User"),label("Banana")),map("name","Bob","age",14)),new ValueRelationship(1l,12l,13l,DynamicRelationshipType.withName("KNOWS"),map("name","Bob","age",14)),new ValueNode(13l,asList(label("User"),label("Banana")),map("name","Bob","age",new int[]{1,2,3}))));
}
