{
  Priority neoLogPriority=null;
switch (level) {
case LOG_INFO:
    neoLogPriority=Priority.INFO;
  break;
case LOG_DEBUG:
neoLogPriority=Priority.DEBUG;
break;
case LOG_ERROR:
neoLogPriority=Priority.ERROR;
break;
case LOG_WARNING:
neoLogPriority=Priority.WARN;
break;
}
log.log(neoLogPriority,message,exception);
}
