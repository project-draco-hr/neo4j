{
  super(String.format("Could not index node with id:%d for propertyValue:[%s], " + "the unique index already contains an entry with that value for node with id:%d",addedNodeId,propertyValue,existingNodeId));
  this.addedNodeId=addedNodeId;
  this.propertyValue=propertyValue;
  this.existingNodeId=existingNodeId;
}
