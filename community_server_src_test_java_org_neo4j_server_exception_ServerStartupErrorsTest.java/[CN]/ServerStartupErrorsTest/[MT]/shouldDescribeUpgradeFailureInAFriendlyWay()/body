{
  AssertableLogProvider logging=new AssertableLogProvider();
  LifecycleException error=new LifecycleException(new Object(),STARTING,STARTED,new RuntimeException("Error starting org.neo4j.kernel.ha.factory.EnterpriseFacadeFactory",new LifecycleException(new Object(),STARTING,STARTED,new LifecycleException(new Object(),STARTING,STARTED,new UpgradeNotAllowedByDatabaseModeException()))));
  translateToServerStartupError(error).describeTo(logging.getLog("console"));
  logging.assertExactly(inLog("console").error("Neo4j cannot be started, because the database files require upgrading and upgrading is not " + "supported in this database mode. Please start the database in stand-alone mode to allow a " + "safe upgrade of the database files."));
}
