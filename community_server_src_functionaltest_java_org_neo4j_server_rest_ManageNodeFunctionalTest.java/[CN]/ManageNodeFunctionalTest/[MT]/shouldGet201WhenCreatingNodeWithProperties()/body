{
  ClientResponse response=DocsGenerator.create("Create a node with properties").payload("{\"foo\" : \"bar\"}").status(Response.Status.CREATED).header("Location").header("Content-Length").post(functionalTestHelper.nodeUri());
  assertNotNull(response.getHeaders().get("Content-Length"));
  assertTrue(response.getLocation().toString().matches(NODE_URI_PATTERN));
}
