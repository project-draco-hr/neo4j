{
  long relId=graphDbService.getBatchInserter().createRelationship(id,otherNode.getId(),type,null);
  RelationshipBatchImpl rel=new RelationshipBatchImpl(new SimpleRelationship(relId,id,otherNode.getId(),type),graphDbService,emptyProps());
  graphDbService.addRelationshipToCache(relId,rel);
  return rel;
}
