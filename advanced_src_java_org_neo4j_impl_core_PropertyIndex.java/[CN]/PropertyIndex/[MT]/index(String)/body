{
  List<PropertyIndex> list=indexMap.get(key);
  TxCommitHook hook=txCommitHooks.get(Thread.currentThread());
  if (hook != null) {
    PropertyIndex index=hook.getIndex(key);
    if (index != null) {
      if (list == null) {
        list=new LinkedList<PropertyIndex>();
      }
      list.add(index);
    }
  }
  if (list == null) {
    list=Collections.emptyList();
  }
  return list;
}
