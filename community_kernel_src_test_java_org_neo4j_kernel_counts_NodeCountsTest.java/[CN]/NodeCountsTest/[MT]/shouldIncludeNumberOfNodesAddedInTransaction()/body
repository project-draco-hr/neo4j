{
  GraphDatabaseService graphDb=db.getGraphDatabaseService();
  try (Transaction tx=graphDb.beginTx()){
    graphDb.createNode();
    graphDb.createNode();
    tx.success();
  }
   long before=numberOfNodes();
  try (Transaction tx=graphDb.beginTx()){
    graphDb.createNode();
    long nodeCount=countsForNode();
    assertEquals(before + 1,nodeCount);
    tx.success();
  }
 }
