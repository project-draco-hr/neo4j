{
  try {
    context.future.get();
    return true;
  }
 catch (  InterruptedException e) {
    return false;
  }
catch (  ExecutionException e) {
    context.throwable=e.getCause();
    slaveCommitFailureLogger.event(context);
    return false;
  }
catch (  CancellationException e) {
    return false;
  }
}
