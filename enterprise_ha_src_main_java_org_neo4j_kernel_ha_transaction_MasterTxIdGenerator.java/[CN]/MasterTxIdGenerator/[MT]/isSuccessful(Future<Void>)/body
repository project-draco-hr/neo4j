{
  try {
    committer.get();
    return true;
  }
 catch (  InterruptedException e) {
    return false;
  }
catch (  ExecutionException e) {
    log.error("Slave commit threw " + (e.getCause() instanceof ComException ? "communication" : "") + " exception",e);
    return false;
  }
catch (  CancellationException e) {
    return false;
  }
}
