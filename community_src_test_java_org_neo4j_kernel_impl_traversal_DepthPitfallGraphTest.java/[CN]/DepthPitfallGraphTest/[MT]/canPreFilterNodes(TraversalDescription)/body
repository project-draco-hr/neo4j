{
  Traverser traverser=description.uniqueness(Uniqueness.NONE).prune(Traversal.pruneAfterDepth(2)).filter(new Predicate<Path>(){
    public boolean accept(    Path position){
      return position.length() == 2;
    }
  }
).traverse(referenceNode());
  expectPaths(traverser,"1,2,6","1,3,5","1,4,5","1,5,3","1,5,4","1,5,6");
}
