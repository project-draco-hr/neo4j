{
  CommunityServerBuilder builder=CommunityServerBuilder.server().withProperty(ServerInternalSettings.webadmin_enabled.name(),"true");
  server=ServerHelper.createNonPersistentServer(builder);
  FunctionalTestHelper functionalTestHelper=new FunctionalTestHelper(server);
  JaxRsResponse response=new RestRequest().get(functionalTestHelper.webAdminUri());
  assertEquals(200,response.getStatus());
  response.close();
}
