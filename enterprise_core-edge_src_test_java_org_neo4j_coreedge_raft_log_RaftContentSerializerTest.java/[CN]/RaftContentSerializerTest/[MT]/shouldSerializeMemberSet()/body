{
  RaftContentSerializer serializer=new RaftContentSerializer();
  CoreMemberSet in=new CoreMemberSet(asSet(new CoreMember(address("host1:1001"),address("host1:1002")),new CoreMember(address("host2:1002"),address("host2:1002"))));
  ByteBuffer buffer=serializer.serialize(in);
  ReplicatedContent out=serializer.deserialize(buffer);
  assertEquals(in,out);
}
