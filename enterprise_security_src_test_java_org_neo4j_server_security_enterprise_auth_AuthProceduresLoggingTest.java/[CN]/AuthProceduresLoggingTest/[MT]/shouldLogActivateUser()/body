{
  authProcedures.createUser("mats","neo4j",false);
  authProcedures.suspendUser("mats");
  log.clear();
  authProcedures.activateUser("mats",false);
  authProcedures.activateUser("mats",false);
  log.assertExactly(info("[admin]: activated user `%s`","mats"),info("[admin]: activated user `%s`","mats"));
}
