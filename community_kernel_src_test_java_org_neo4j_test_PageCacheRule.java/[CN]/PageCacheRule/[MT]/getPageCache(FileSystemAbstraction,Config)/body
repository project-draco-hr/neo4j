{
  if (pageCache != null) {
    pageCache.stop();
  }
  PageCacheFactory pageCacheFactory=new StandardPageCacheFactory();
  PageSwapperFactory swapperFactory=new SingleFilePageSwapperFactory(fs);
  pageCache=new LifecycledPageCache(pageCacheFactory,swapperFactory,jobScheduler,config,new Monitors().newMonitor(PageCacheMonitor.class));
  pageCache.start();
  return pageCache;
}
