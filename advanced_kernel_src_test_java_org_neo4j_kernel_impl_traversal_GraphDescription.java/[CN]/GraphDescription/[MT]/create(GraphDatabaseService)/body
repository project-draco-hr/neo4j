{
  Map<String,Node> nodes=new HashMap<String,Node>();
  Node node=null;
  Transaction tx=graphdb.beginTx();
  try {
    for (    RelationshipDescription rel : description) {
      node=rel.create(graphdb,nodes).getEndNode();
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
  return node;
}
