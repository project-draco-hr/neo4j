{
  try {
    Session session;
    try {
      session=client.startSession();
    }
 catch (    AssertionError e) {
      connect();
      session=client.startSession();
    }
catch (    TransportException e) {
      connect();
      session=client.startSession();
    }
    return session;
  }
 catch (  Exception e) {
    throw new ShellException(e);
  }
}
