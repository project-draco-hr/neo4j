{
  TraversalBranch result=null;
  while (result == null) {
    if (current == null) {
      return null;
    }
    TraversalBranch next=current.next(expander,metadata);
    if (next == null) {
      current=current.parent();
      continue;
    }
    current=next;
    result=current;
  }
  return result;
}
