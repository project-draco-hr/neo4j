{
  for (  File file : new File("var/hadb").listFiles()) {
    try {
      if (file.getName().contains("logical") && !file.getName().endsWith(".active")) {
        System.out.println("\n=== " + file.getPath() + " ===");
        DumpLogicalLog.main(new String[]{file.getPath()});
      }
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
