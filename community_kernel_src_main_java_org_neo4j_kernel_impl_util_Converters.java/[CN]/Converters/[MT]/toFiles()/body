{
  return new Function<String,File[]>(){
    @Override public File[] apply(    String from){
      String[] names=from.split(File.pathSeparator);
      File[] file=new File[names.length];
      for (int i=0; i < names.length; i++) {
        file[i]=new File(names[i]);
      }
      return file;
    }
  }
;
}
