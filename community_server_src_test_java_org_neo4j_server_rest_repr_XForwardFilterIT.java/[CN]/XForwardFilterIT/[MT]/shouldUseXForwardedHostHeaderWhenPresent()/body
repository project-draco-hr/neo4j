{
  ClientResponse response=client.resource("http://localhost:7474/db/manage").accept(APPLICATION_JSON).header(X_FORWARDED_HOST,"jimwebber.org").get(ClientResponse.class);
  String entity=response.getEntity(String.class);
  assertTrue(entity.contains("http://jimwebber.org"));
  assertFalse(entity.contains("http://localhost"));
}
