{
  int masterId=0;
  Map<String,String> config=MapUtil.stringMap(extraConfig,HighlyAvailableGraphDatabase.CONFIG_KEY_SERVER_ID,String.valueOf(masterId),HighlyAvailableGraphDatabase.CONFIG_KEY_READ_TIMEOUT,String.valueOf(TEST_READ_TIMEOUT));
  String path=dbPath(0).getAbsolutePath();
  PlaceHolderGraphDatabaseService placeHolderDb=new PlaceHolderGraphDatabaseService(path);
  Broker broker=makeMasterBroker(masterId,placeHolderDb);
  HighlyAvailableGraphDatabase db=new HighlyAvailableGraphDatabase(path,config,wrapBrokerAndSetPlaceHolderDb(placeHolderDb,broker));
  placeHolderDb.setDb(db);
  return placeHolderDb;
}
