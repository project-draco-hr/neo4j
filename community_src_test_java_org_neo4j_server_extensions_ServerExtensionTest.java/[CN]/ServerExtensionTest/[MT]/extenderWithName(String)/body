{
  return new TypeSafeMatcher<MediaExtender>(){
    private String name;
    @Override public boolean matchesSafely(    MediaExtender extender){
      name=extender.name();
      return expected.equals(name);
    }
    @Override public void describeTo(    Description descr){
      descr.appendText(String.format("expected \"%s\", but got \"%s\"",expected,name));
    }
  }
;
}
