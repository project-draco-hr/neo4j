{
  graph.makeEdge("s","1");
  graph.makeEdge("s","3");
  graph.makeEdge("1","2");
  graph.makeEdge("1","4");
  graph.makeEdge("3","2");
  graph.makeEdge("3","4");
  graph.makeEdge("2","t");
  graph.makeEdge("4","t");
  testShortestPathFinder(new PathFinderTester(){
    @Override public void test(    PathFinder<Path> finder){
      assertPaths(finder.findAllPaths(graph.getNode("s"),graph.getNode("t")),"s,1,2,t","s,1,4,t","s,3,2,t","s,3,4,t");
    }
  }
,PathExpanders.forTypeAndDirection(R1,BOTH),3);
}
