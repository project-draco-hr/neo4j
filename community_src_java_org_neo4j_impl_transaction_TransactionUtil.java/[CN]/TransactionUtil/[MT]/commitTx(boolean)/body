{
  if (!txStarted) {
    return;
  }
  UserTransaction ut=null;
  boolean committed=false;
  Object txId=null;
  try {
    ut=TransactionFactory.getUserTransaction();
    txId=getTransactionId(ut);
    ut.commit();
    committed=true;
  }
 catch (  Exception e) {
    try {
      if (ut != null && ut.getStatus() != Status.STATUS_NO_TRANSACTION) {
        ut.rollback();
      }
    }
 catch (    Exception ee) {
      ee.printStackTrace();
      throw new RuntimeException("Unable to rollback failed commit operation " + ee,e);
    }
    throw new RuntimeException("Unable to commit operation",e);
  }
 finally {
    txEnded(committed,txId);
  }
}
