{
  conn.send(TransportTestUtil.chunk(run("CALL dbms.terminateSessionsForUser( '" + username + "' ) YIELD username, sessionCount"),pullAll()));
  assertThat(conn,eventuallyRecieves(msgSuccess(),msgFailure(SessionTerminated,SESSION_TERMINATED_MSG)));
  conn.send(TransportTestUtil.chunk(run("MATCH (n) RETURN n"),pullAll()));
  assertThat(conn,eventuallyRecieves(msgFailure(SessionTerminated,SESSION_TERMINATED_MSG),msgFailure(SessionTerminated,SESSION_TERMINATED_MSG)));
}
