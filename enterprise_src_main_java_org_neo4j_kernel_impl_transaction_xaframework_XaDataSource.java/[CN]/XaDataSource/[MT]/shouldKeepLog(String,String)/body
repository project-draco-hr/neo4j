{
  if (config != null) {
    if (config.equals(Boolean.TRUE.toString())) {
      return true;
    }
    StringTokenizer tok=new StringTokenizer(config,",");
    while (tok.hasMoreTokens()) {
      String element=tok.nextToken().trim();
      if (resourceName.equals(element)) {
        return true;
      }
    }
  }
  return false;
}
