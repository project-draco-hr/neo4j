{
  try {
    System.out.println(Thread.currentThread().getName() + " pulls updates");
    receive(broker.getMaster().pullUpdates(getSlaveContext()));
  }
 catch (  ZooKeeperException e) {
    somethingIsWrong(e);
    throw e;
  }
catch (  HaCommunicationException e) {
    somethingIsWrong(e);
    throw e;
  }
}
