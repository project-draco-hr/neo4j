{
  try {
    int c;
    File f=File.createTempFile("install","log");
    vm.copyFromVM(path,f.getAbsolutePath());
    FileInputStream in=new FileInputStream(f);
    StringBuilder b=new StringBuilder();
    in.skip(2);
    while ((c=in.read()) != -1) {
      b.append((char)c);
      in.skip(1);
    }
    return b.toString();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
