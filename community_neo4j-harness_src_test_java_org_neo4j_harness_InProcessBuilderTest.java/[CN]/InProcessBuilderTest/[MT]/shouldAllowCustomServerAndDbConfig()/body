{
  trustAllSSLCerts();
  File testDir=this.testDir.directory();
  try (ServerControls server=newInProcessBuilder(testDir).withConfig(Configurator.WEBSERVER_HTTPS_ENABLED_PROPERTY_KEY,"true").withConfig(Configurator.WEBSERVER_HTTPS_CERT_PATH_PROPERTY_KEY,new File(testDir,"cert").getAbsolutePath()).withConfig(Configurator.WEBSERVER_KEYSTORE_PATH_PROPERTY_KEY,new File(testDir,"keystore").getAbsolutePath()).withConfig(Configurator.WEBSERVER_HTTPS_KEY_PATH_PROPERTY_KEY,new File(testDir,"key").getAbsolutePath()).withConfig(GraphDatabaseSettings.cache_type,"none").newServer()){
    assertThat(HTTP.GET(server.httpsURI().toString()).status(),equalTo(200));
    assertDBConfig(server,"none","cache_type");
  }
 }
