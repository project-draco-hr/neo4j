{
  trustAllSSLCerts();
  try (ServerControls server=newInProcessBuilder(testDir.directory()).withConfig(Configurator.WEBSERVER_HTTPS_ENABLED_PROPERTY_KEY,"true").withConfig(GraphDatabaseSettings.cache_type,"none").newServer()){
    assertThat(HTTP.GET(server.httpsURI().toString()).status(),equalTo(200));
    assertDBConfig(server,"none","cache_type");
  }
 }
