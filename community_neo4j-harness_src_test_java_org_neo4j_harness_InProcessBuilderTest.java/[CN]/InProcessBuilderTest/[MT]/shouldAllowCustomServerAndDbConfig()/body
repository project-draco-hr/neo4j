{
  trustAllSSLCerts();
  try (ServerControls server=getTestServerBuilder(testDir.directory()).withConfig(Configurator.WEBSERVER_HTTPS_ENABLED_PROPERTY_KEY,"true").withConfig(ServerSettings.tls_certificate_file.name(),testDir.file("cert").getAbsolutePath()).withConfig(Configurator.WEBSERVER_KEYSTORE_PATH_PROPERTY_KEY,testDir.file("keystore").getAbsolutePath()).withConfig(ServerSettings.tls_key_file.name(),testDir.file("key").getAbsolutePath()).withConfig(GraphDatabaseSettings.dense_node_threshold,"20").newServer()){
    assertThat(HTTP.GET(server.httpsURI().toString()).status(),equalTo(200));
    assertDBConfig(server,"20",GraphDatabaseSettings.dense_node_threshold.name());
  }
 }
