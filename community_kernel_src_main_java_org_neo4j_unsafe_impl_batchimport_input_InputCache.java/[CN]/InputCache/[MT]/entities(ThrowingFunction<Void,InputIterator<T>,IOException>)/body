{
  return new InputIterable<T>(){
    @Override public InputIterator<T> iterator(){
      try {
        return factory.apply(null);
      }
 catch (      IOException e) {
        throw new InputException("Unable to read cached relationship",e);
      }
    }
    @Override public boolean supportsMultiplePasses(){
      return true;
    }
  }
;
}
