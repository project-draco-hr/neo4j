{
  tm.begin();
  assertTrue(tm.getTransaction() != null);
  tm.commit();
  assertEquals(Status.STATUS_NO_TRANSACTION,tm.getStatus());
  try {
    tm.rollback();
    fail("rollback() should throw an exception -> " + "STATUS_NO_TRANSACTION");
  }
 catch (  IllegalStateException e) {
  }
  try {
    tm.commit();
    fail("commit() should throw an exception -> " + "STATUS_NO_TRANSACTION");
  }
 catch (  IllegalStateException e) {
  }
}
