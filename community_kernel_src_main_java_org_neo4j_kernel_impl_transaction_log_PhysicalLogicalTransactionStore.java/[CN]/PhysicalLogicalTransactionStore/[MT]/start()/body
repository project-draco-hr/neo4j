{
  this.appender=batchedWrites ? new BatchingPhysicalTransactionAppender(logFile,logRotation,transactionMetadataCache,transactionIdStore,legacyIndexTransactionOrdering,kernelHealth) : new PhysicalTransactionAppender(logFile,logRotation,transactionMetadataCache,transactionIdStore,legacyIndexTransactionOrdering,kernelHealth);
}
