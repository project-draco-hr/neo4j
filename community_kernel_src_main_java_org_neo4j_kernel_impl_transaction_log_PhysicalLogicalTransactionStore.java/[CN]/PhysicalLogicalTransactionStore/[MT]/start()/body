{
  this.appender=batchedWrites ? new BatchingPhysicalTransactionAppender(logFile,logRotation,transactionMetadataCache,transactionIdStore,legacyIndexTransactionOrdering,ATOMIC_LONG,DEFAULT_WAIT_STRATEGY) : new PhysicalTransactionAppender(logFile,logRotation,transactionMetadataCache,transactionIdStore,legacyIndexTransactionOrdering);
}
