{
  try {
    receiver.receive(broker.getMaster().first().finishTransaction(receiver.getSlaveContext(eventIdentifier)));
  }
 catch (  ZooKeeperException e) {
    receiver.newMaster(broker.getMaster(),e);
    throw e;
  }
catch (  HaCommunicationException e) {
    receiver.newMaster(broker.getMaster(),e);
    throw e;
  }
}
