{
  NodeRecord before=new NodeRecord(nodeId,false,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue());
  before.setInUse(false);
  NodeRecord after=new NodeRecord(nodeId,false,Record.NO_NEXT_RELATIONSHIP.intValue(),Record.NO_NEXT_PROPERTY.intValue());
  after.setInUse(true);
  InlineNodeLabels nodeLabels=new InlineNodeLabels(-1,after);
  nodeLabels.put(new long[]{labelId},neoStore.getNodeStore(),null);
  return new Command.NodeCommand().init(before,after);
}
