{
  NeoTransactionStoreApplier applier=new NeoTransactionStoreApplier(neoStore,mock(IndexingService.class),mock(CacheAccessBackDoor.class),mock(LockService.class),new LockGroup(),transactionId,DEFAULT_HIGH_ID_TRACKING,true);
