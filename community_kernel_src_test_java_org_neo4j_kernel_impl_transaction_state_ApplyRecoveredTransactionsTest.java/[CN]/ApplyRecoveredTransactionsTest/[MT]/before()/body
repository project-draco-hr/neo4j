{
  FileSystemAbstraction fs=fsr.get();
  File storeDir=new File("dir");
  StoreFactory storeFactory=new StoreFactory(storeDir,Config.empty(),new DefaultIdGeneratorFactory(fs),pageCacheRule.getPageCache(fs),fs,StandardV3_0.RECORD_FORMATS,NullLogProvider.getInstance());
  neoStores=storeFactory.openAllNeoStores(true);
}
