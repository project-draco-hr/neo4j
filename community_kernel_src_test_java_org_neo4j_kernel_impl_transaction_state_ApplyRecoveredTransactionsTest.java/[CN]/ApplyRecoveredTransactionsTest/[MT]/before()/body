{
  File storeDir=new File("dir");
  Config config=configForStoreDir(new Config(),storeDir);
  StoreFactory storeFactory=new StoreFactory(config,new DefaultIdGeneratorFactory(),pageCacheRule.getPageCache(fsr.get()),fsr.get(),DEV_NULL,new Monitors());
  neoStore=storeFactory.newNeoStore(true,false);
}
