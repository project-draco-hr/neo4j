{
  NeoStoreTransactionApplier applier=new NeoStoreTransactionApplier(neoStore,mock(CacheAccessBackDoor.class),mock(LockService.class),new LockGroup(),transactionId);
  CommandApplierFacade applierFacade=new CommandApplierFacade(applier,mock(NeoCommandHandler.class),mock(NeoCommandHandler.class),mock(NeoCommandHandler.class));
  new PhysicalTransactionRepresentation(Arrays.asList(commands)).accept(applierFacade);
