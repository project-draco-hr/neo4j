  data.add(new Pair<Long,ReadableByteChannel>(0xDEADCAFEBABEBEEFL,new ByteArrayChannel((byte)0xFF,(byte)0xEE,(byte)0xDD,(byte)0xCC,(byte)0xBB,(byte)0xAA,(byte)0x99,(byte)0x88,(byte)0x77,(byte)0x66,(byte)0x55,(byte)0x44,(byte)0x33,(byte)0x22,(byte)0x11,(byte)0x00)));
