{
  GraphDatabaseAPI db=(GraphDatabaseAPI)new TestGraphDatabaseFactory().newImpermanentDatabaseBuilder().setConfig(GraphDatabaseSettings.cache_type,HighPerformanceCacheProvider.NAME).newGraphDatabase();
  Cache<?> firstCache=firstCache(db);
  db.shutdown();
  db=(GraphDatabaseAPI)new TestGraphDatabaseFactory().newImpermanentDatabaseBuilder().setConfig(GraphDatabaseSettings.cache_type,HighPerformanceCacheProvider.NAME).newGraphDatabase();
  try {
    Cache<?> secondCache=firstCache(db);
    assertEquals(firstCache,secondCache);
  }
  finally {
    db.shutdown();
  }
}
