{
  BatchInserter inserter=newBatchInserter();
  inserter.createDeferredConstraint(label("Hacker")).assertPropertyIsUnique("handle").create();
  try {
    inserter.createDeferredConstraint(label("Hacker")).assertPropertyIsUnique("handle").create();
    fail("Should have thrown exception.");
  }
 catch (  ConstraintViolationException e) {
  }
  inserter.shutdown();
}
