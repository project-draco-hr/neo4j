{
  DoubleLatch latch=latchFetcher.fetch();
  try {
    latch.awaitFirst();
    latch.countDownSecond();
  }
 catch (  RemoteException e) {
    throw new RuntimeException(e);
  }
  boolean result=db.index().forNodes(index).putIfAbsent(db.getNodeById(nodeId),key,value);
  tx.success();
  return result;
}
