{
  IndexUpdater updater=updaterMap.get(descriptor);
  if (null == updater) {
    IndexProxy indexProxy=indexMap.getIndexProxy(descriptor);
    if (null != indexProxy) {
      updater=indexProxy.newUpdater(indexUpdateMode,transactionId);
      updaterMap.put(descriptor,updater);
    }
  }
  return updater;
}
