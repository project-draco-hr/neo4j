{
  GroupSerialization serialization=new GroupSerialization();
  List<GroupRecord> deserialized=serialization.deserializeGroups(UTF8.encode(("admin:Bob,Steve\n" + "publisher:Kelly,Marie\n")));
  assertThat(deserialized,equalTo(asList(new GroupRecord("admin",steveBob),new GroupRecord("publisher",kellyMarie))));
}
