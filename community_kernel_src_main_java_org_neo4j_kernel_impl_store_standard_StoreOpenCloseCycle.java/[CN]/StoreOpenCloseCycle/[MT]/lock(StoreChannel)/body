{
  try {
    this.fileLock=fs.tryLock(dbFileName,channel);
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException("Unable to lock store[" + dbFileName + "]",e);
  }
catch (  OverlappingFileLockException e) {
    throw new IllegalStateException("Unable to lock store [" + dbFileName + "], this is usually caused by another Neo4j kernel already running in "+ "this JVM for this particular store");
  }
}
