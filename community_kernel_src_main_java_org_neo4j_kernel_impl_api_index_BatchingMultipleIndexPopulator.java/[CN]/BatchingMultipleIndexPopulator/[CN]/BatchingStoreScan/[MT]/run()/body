{
  try {
    delegate.run();
    log.info("Completed node store scan. Flushing all pending deletes." + EOL + this);
    flushAll();
  }
 catch (  Throwable scanError) {
    try {
      shutdownExecutor(true);
    }
 catch (    Throwable error) {
      scanError.addSuppressed(error);
    }
    throw scanError;
  }
  shutdownExecutor(false);
}
