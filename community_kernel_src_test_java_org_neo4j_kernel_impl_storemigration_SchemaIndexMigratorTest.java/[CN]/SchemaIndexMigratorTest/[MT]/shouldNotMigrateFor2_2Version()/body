{
  when(upgradableDatabase.hasCurrentVersion(fs,storeDir)).thenReturn(true);
  when(upgradableDatabase.checkUpgradeable(storeDir)).thenThrow(new UnexpectedUpgradingStoreVersionException(NeoStore.DEFAULT_NAME,Legacy21Store.LEGACY_VERSION,AbstractStore.ALL_STORES_VERSION));
  boolean needsMigration=migrator.needsMigration(storeDir);
  assertFalse(needsMigration);
}
