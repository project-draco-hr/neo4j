{
  try {
    if (event == Event.RELATIONSHIPTYPE_CREATE) {
      relTypeConsumer.addRelationshipType(data.getId(),data.getName());
    }
 else {
      throw new PersistenceUpdateFailedException("Unkown event: " + event);
    }
  }
 catch (  IOException e) {
    throw new PersistenceUpdateFailedException("Event[" + event + "]",e);
  }
}
