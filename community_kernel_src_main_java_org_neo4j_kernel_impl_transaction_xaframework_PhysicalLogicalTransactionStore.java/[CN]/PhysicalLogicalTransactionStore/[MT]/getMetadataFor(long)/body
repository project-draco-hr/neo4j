{
  TransactionMetadataCache.TransactionMetadata transactionMetadata=transactionMetadataCache.getTransactionMetadata(transactionId);
  if (transactionMetadata == null) {
    IOCursor cursor=getCursor(transactionId,new TransactionMetadataFiller());
    if (cursor == null) {
      return null;
    }
    while (cursor.next())     ;
  }
  transactionMetadata=transactionMetadataCache.getTransactionMetadata(transactionId);
  return transactionMetadata;
}
