{
  if (startEntryForFoundTransaction == null) {
    throw new NoSuchTransactionException(startTransactionId);
  }
  transactionMetadataCache.cacheTransactionMetadata(startTransactionId,startEntryForFoundTransaction.getStartPosition(),startEntryForFoundTransaction.getMasterId(),startEntryForFoundTransaction.getLocalId(),LogEntry.Start.checksum(startEntryForFoundTransaction));
  return startEntryForFoundTransaction.getStartPosition();
}
