{
  Transaction tx=neo.beginTx();
  EigenvectorCentralityRayleigh eigenvectorCentralityRayleigh=new EigenvectorCentralityRayleigh(Direction.OUTGOING,new CostEvaluator<Double>(){
    public Double getCost(    Relationship relationship,    boolean backwards){
      return 1.0;
    }
  }
,graph.getNodes(),graph.getRelationships(),0.0000001);
  eigenvectorCentralityRayleigh.setMaxIterations(100);
  eigenvectorCentralityRayleigh.calculate();
