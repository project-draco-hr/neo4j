{
  InternalIndexState state=getState(indexDescription,index);
switch (state) {
case POPULATING:
    return false;
case ONLINE:
  return true;
case FAILED:
throw new ProcedureException(Status.Schema.IndexCreationFailed,"Index on %s is in failed state",indexDescription);
default :
throw new IllegalStateException("Unknown index state " + state);
}
}
