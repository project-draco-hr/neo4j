{
  if (status) {
    lazyDone=true;
    lazyDoneRecords=new ArrayList<Integer>();
  }
 else {
    lazyDone=false;
    if (lazyDoneRecords != null) {
      tf.lazyDoneWrite(lazyDoneRecords);
      for (      int identifier : lazyDoneRecords) {
        log.done(identifier);
      }
      lazyDoneRecords=null;
    }
  }
}
