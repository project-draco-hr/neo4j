{
synchronized (activeRowWindows) {
    for (    Set<LockableWindow> windowSet : txIdentifiers.values()) {
      if (windowSet != null) {
        Iterator<LockableWindow> itr=windowSet.iterator();
        while (itr.hasNext()) {
          ((MappedPersistenceWindow)itr.next()).force();
        }
      }
    }
    txIdentifiers.clear();
  }
  fileChannel.force(false);
}
