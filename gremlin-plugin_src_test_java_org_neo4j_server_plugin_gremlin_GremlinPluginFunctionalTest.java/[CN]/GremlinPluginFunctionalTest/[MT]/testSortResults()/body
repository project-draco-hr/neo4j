{
  String script="g.v(" + data.get().get("I").getId() + ").out.sort{it.name}.toList()";
  String payload="{\"script\":\"" + script + "\"}";
  description(formatGroovy(script));
  String response=gen().expectedStatus(Status.OK.getStatusCode()).payload(JSONPrettifier.parse(payload)).payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("you"));
  assertTrue(response.contains("him"));
  assertTrue(response.indexOf("you") > response.indexOf("him"));
}
