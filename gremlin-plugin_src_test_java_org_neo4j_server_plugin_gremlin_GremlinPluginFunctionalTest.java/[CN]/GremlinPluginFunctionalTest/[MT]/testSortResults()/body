{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"script\":\"g.v(" + data.get().get("I").getId() + ").outE.inV.sort{it.name}.toList()\"}").payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("you"));
  assertTrue(response.contains("him"));
  assertTrue(response.indexOf("you") > response.indexOf("him"));
}
