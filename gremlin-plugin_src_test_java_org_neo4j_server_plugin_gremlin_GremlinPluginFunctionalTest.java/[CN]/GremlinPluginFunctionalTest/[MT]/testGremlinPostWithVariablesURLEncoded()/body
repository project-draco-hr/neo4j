{
  final String script="g.v(me).out;";
  final String params="{ \"me\" : " + data.get().get("I").getId() + " }";
  String response=gen.get().description(formatGroovy(script)).expectedStatus(Status.OK.getStatusCode()).payload("script=" + URLEncoder.encode(script,"UTF-8") + "&params="+ URLEncoder.encode(params,"UTF-8")).payloadType(MediaType.APPLICATION_FORM_URLENCODED_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("you"));
}
