{
  data.get();
  String script="writer = new GraphMLWriter(g);" + "out = new java.io.ByteArrayOutputStream();" + "writer.outputGraph(out);"+ "result = out.toString();";
  String payload="{\"script\":\"" + script + "\"}";
  description(formatGroovy(script));
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload(JSONPrettifier.parse(payload)).payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  System.out.println(response);
  assertTrue(response.contains("graphml"));
  assertTrue(response.contains("you"));
}
