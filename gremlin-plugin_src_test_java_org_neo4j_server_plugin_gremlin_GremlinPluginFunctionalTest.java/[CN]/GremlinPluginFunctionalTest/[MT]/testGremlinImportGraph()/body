{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"script\":\"GraphMLReader.inputGraph(g, new URL('https://github.com/tinkerpop/gremlin/raw/master/data/graph-example-1.xml').openStream());g.V\"}").payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("peter"));
}
