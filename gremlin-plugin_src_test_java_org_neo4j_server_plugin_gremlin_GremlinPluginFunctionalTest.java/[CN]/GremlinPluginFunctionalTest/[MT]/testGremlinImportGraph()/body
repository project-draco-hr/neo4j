{
  String payload="{\"script\":\"" + "g.loadGraphML('https://raw.github.com/neo4j/gremlin-plugin/master/src/data/graphml1.xml');" + "g.V\"}";
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload(JSONPrettifier.parse(payload)).payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  assertTrue(response.contains("you"));
  assertTrue(response.contains("him"));
}
