{
  String response=gen.get().expectedStatus(Status.OK.getStatusCode()).payload("{\"script\":\"i = g.v(" + data.get().get("I").getId() + ");"+ "t= new Table();"+ "i.out('know').as('friends').table(t);t;\"}").payloadType(MediaType.APPLICATION_JSON_TYPE).post(ENDPOINT).entity();
  System.out.println(response);
  assertTrue(response.contains("you"));
}
