{
  ConsistencyCheckService service=new ConsistencyCheckService();
  File storeDir=new File(db.getStoreDir());
  ConsistencyCheckService.Result result=service.runFullConsistencyCheck(storeDir,Config.empty(),ProgressMonitorFactory.textual(System.out),FormattedLogProvider.toOutputStream(System.out),true);
  assertTrue("Store is inconsistent",result.isSuccessful());
}
