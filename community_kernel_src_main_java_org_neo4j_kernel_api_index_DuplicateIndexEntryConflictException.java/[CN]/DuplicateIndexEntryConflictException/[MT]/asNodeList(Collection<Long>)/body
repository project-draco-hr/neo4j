{
  TreeSet<Long> ids=new TreeSet<Long>(nodeIds);
  StringBuilder builder=new StringBuilder();
  boolean first=true;
  for (  long nodeId : ids) {
    if (!first) {
      builder.append(", ");
    }
 else {
      first=false;
    }
    builder.append("node(").append(nodeId).append(")");
  }
  return builder.toString();
}
