{
  System.out.print("Locks held: ");
  java.util.Iterator<?> itr=lockMap.keySet().iterator();
  if (!itr.hasNext()) {
    System.out.println("NONE");
  }
 else {
    System.out.println();
  }
  while (itr.hasNext()) {
    Thread thread=(Thread)itr.next();
    System.out.println("" + thread + "->"+ lockMap.get(thread).size());
  }
}
