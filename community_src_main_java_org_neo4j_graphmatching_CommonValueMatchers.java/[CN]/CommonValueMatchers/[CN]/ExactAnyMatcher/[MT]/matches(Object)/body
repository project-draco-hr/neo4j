{
  if (value != null) {
    if (value.getClass().isArray()) {
      for (      Object item : ArrayPropertyUtil.propertyValueToCollection(value)) {
        if (item != null && item.equals(valueToMatch)) {
          return true;
        }
      }
    }
 else     if (value.equals(valueToMatch)) {
      return true;
    }
  }
  return false;
}
