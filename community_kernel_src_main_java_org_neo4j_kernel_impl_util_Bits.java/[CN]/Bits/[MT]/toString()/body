{
  StringBuilder builder=new StringBuilder();
  int byteCounter=0;
  for (  long value : longs) {
    builder.append("[");
    for (int i=63; i >= 0; i--) {
      boolean isSet=(value & (1L << i)) != 0;
      builder.append(isSet ? "1" : "0");
      if (i % 8 == 0 && i > 0) {
        builder.append(",");
      }
    }
    builder.append("]");
    if (byteCounter++ % 8 == 0) {
      builder.append("\n");
    }
  }
  return builder.toString();
}
