{
  byte[] result=new byte[numberOfBytes];
  int i=0;
  for (  long block : longs) {
    long mask=leftOverflowMask(8);
    for (int j=0; j < 8; j++, mask>>>=8) {
      if (leftIgnore-- > 0)       continue;
      if (i >= numberOfBytes)       break;
      result[i++]=(byte)((block & mask) >>> (64 - 8 * j - 8));
    }
  }
  return result;
}
