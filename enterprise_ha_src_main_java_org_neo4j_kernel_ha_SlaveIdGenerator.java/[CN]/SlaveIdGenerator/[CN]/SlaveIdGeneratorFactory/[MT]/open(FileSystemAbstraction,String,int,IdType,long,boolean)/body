{
  if (startup)   new File(fileName).delete();
  IdGenerator localIdGenerator=localFactory.open(fs,fileName,grabSize,idType,highestIdInUse,startup);
  SlaveIdGenerator generator=new SlaveIdGenerator(idType,highestIdInUse,broker,databaseOperations,localIdGenerator);
  generators.put(idType,generator);
  return generator;
}
