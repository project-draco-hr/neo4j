{
  try {
    getTxLog().txStart(globalId);
  }
 catch (  IOException e) {
    log.log(Level.SEVERE,"Error writing transaction log",e);
    setTmNotOk();
    throw logAndReturn("TM error write start record",Exceptions.withCause(new SystemException("TM encountered a problem, " + " error writing transaction log,"),e));
  }
}
