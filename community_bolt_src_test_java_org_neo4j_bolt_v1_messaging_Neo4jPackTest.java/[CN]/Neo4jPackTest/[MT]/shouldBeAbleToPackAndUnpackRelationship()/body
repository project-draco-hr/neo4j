{
  Object unpacked=unpacked(packed(ALICE_KNOWS_BOB));
  assertThat(unpacked,instanceOf(Relationship.class));
  Relationship unpackedRelationship=(Relationship)unpacked;
  assertThat(unpackedRelationship.getId(),equalTo(ALICE_KNOWS_BOB.getId()));
  assertThat(unpackedRelationship.getStartNode().getId(),equalTo(ALICE_KNOWS_BOB.getStartNode().getId()));
  assertThat(unpackedRelationship.getEndNode().getId(),equalTo(ALICE_KNOWS_BOB.getEndNode().getId()));
  assertThat(unpackedRelationship.getType().name(),equalTo(ALICE_KNOWS_BOB.getType().name()));
  assertThat(unpackedRelationship.getAllProperties(),equalTo(ALICE_KNOWS_BOB.getAllProperties()));
}
