{
  if (!storeDir.exists()) {
    return new File[0];
  }
  return storeDir.listFiles(new FileFilter(){
    @Override public boolean accept(    File file){
      return !file.getName().startsWith("metrics") && !file.getName().startsWith("messages.") && !isBranchedDataRootDirectory(file);
    }
  }
);
}
