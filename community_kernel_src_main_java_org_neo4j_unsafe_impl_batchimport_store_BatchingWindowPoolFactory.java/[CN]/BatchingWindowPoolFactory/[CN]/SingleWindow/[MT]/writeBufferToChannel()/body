{
  if (currentWindowIndex == -1) {
    return;
  }
  try {
    writer.write(prepared(currentBuffer.getBuffer()),firstIdInWindow * recordSize,bufferPool);
    currentBuffer=new Buffer(this,bufferPool.acquire());
    currentBuffer.reset();
    currentWindowIndex=-1;
  }
 catch (  IOException e) {
    throw handleIoException(e);
  }
}
