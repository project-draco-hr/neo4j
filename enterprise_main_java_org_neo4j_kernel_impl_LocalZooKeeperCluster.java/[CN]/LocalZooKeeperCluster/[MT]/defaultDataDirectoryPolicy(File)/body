{
  return new DataDirectoryPolicy(){
    public File getDataDirectory(    int id){
      return new File(baseDirectory,zeroLeadingId(id,2));
    }
    private String zeroLeadingId(    int id,    int minLength){
      String result="" + id;
      while (result.length() < minLength) {
        result="0" + result;
      }
      return result;
    }
  }
;
}
