{
  Collection<Hit> depthHits=hits.get(atDepth);
  if (depthHits == null) {
    depthHits=new HashSet<Hit>();
    hits.put(atDepth,depthHits);
  }
  if (depthHits.add(hit)) {
    totalHitCount++;
  }
  if (lowestDepth == 0 || atDepth < lowestDepth) {
    lowestDepth=atDepth;
  }
  return totalHitCount;
}
