{
  StringBuffer txString=new StringBuffer("Transaction[Status=" + txManager.getTxStatusAsString(status) + ",ResourceList=");
  Iterator<ResourceElement> itr=resourceList.iterator();
  while (itr.hasNext()) {
    txString.append(itr.next().toString());
    if (itr.hasNext()) {
      txString.append(",");
    }
  }
  return txString.toString();
}
