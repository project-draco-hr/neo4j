{
  if (!displayProperties) {
    return;
  }
  int longestKey=this.findLongestKey(thing);
  Pattern propertyKeyPattern=filter == null ? null : Pattern.compile(filter);
  for (  String key : thing.getPropertyKeys()) {
    if (propertyKeyPattern != null && !propertyKeyPattern.matcher(key).matches()) {
      continue;
    }
    out.print("*" + key);
    if (displayValues) {
      this.printMany(out," ",longestKey - key.length() + 1);
      Object value=thing.getProperty(key);
      out.print("=[" + value + "]");
      if (verbose) {
        out.print(" (" + this.getNiceType(value) + ")");
      }
    }
    out.println("");
  }
}
