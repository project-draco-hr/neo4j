{
  params.put(Config.READ_ONLY,"true");
  this.graphDbImpl=new EmbeddedGraphDbImpl(storeDir,params,this,CommonFactories.defaultLockManagerFactory(),CommonFactories.defaultIdGeneratorFactory(),CommonFactories.defaultRelationshipTypeCreator(),CommonFactories.defaultTxIdGeneratorFactory(),CommonFactories.defaultTxRollbackHook());
}
