{
  return new SingleSourceShortestPathDijkstra<Integer>(0,startNode,new CostEvaluator<Integer>(){
    public Integer getCost(    Relationship relationship,    boolean backwards){
      return 1;
    }
  }
,new org.neo4j.graphalgo.shortestPath.std.IntegerAdder(),new org.neo4j.graphalgo.shortestPath.std.IntegerComparator(),direction,relTypes);
