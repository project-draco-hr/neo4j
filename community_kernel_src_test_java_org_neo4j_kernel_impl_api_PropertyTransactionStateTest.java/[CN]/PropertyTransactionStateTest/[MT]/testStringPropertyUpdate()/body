{
  String key="foo";
  Node node;
  try (Transaction tx=db.beginTx()){
    node=db.createNode();
    node.setProperty(key,"one");
    tx.success();
  }
   try (Transaction ignore=db.beginTx()){
    node.setProperty(key,"one");
    node.setProperty(key,"two");
    assertEquals("two",node.getProperty(key));
  }
 }
