{
  byte globalId[]=xid.getGlobalTransactionId();
  byte branchId[]=xid.getBranchQualifier();
  int formatId=xid.getFormatId();
  buffer.put(LogEntry.TX_START).put((byte)globalId.length).put((byte)branchId.length).put(globalId).put(branchId).putInt(identifier).putInt(formatId).putInt(masterId).putLong(timeWritten);
}
