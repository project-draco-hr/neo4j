{
  if (config.isUnique()) {
    return new UniqueLuceneIndexAccessor(documentStructure,standard(),writerStatus,directoryFactory,folderLayout.getFolder(indexId));
  }
 else {
    return new NonUniqueLuceneIndexAccessor(documentStructure,standard(),writerStatus,directoryFactory,folderLayout.getFolder(indexId),samplingConfig.bufferSize());
  }
}
