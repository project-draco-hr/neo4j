{
  if (config.isUnique()) {
    return new UniqueLuceneIndexAccessor(documentStructure,reserving(),writerStatus,directoryFactory,folderLayout.getFolder(indexId));
  }
 else {
    return new NonUniqueLuceneIndexAccessor(documentStructure,reserving(),writerStatus,directoryFactory,folderLayout.getFolder(indexId));
  }
}
