{
  if (config.isUnique()) {
    return new UniqueLuceneIndexPopulator(UniqueLuceneIndexPopulator.DEFAULT_BATCH_SIZE,documentStructure,standard(),writerStatus,directoryFactory,folderLayout.getFolder(indexId),failureStorage,indexId);
  }
 else {
    return new NonUniqueLuceneIndexPopulator(NonUniqueLuceneIndexPopulator.DEFAULT_QUEUE_THRESHOLD,documentStructure,standard(),writerStatus,directoryFactory,folderLayout.getFolder(indexId),failureStorage,indexId);
  }
}
