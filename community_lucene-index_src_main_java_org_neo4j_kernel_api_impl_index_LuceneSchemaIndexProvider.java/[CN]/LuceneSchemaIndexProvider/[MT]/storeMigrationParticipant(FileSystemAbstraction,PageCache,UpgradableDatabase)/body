{
  return new SchemaIndexMigrator(fs,pageCache,upgradableDatabase,new SchemaIndexMigrator.SchemaStoreProvider(){
    @Override public SchemaStore provide(    File dir,    PageCache pageCache){
      StoreFactory storeFactory=new StoreFactory(fs,dir,pageCache,NullLogProvider.getInstance(),ALLOW_OLD_VERSION);
      return storeFactory.newSchemaStore();
    }
  }
);
}
