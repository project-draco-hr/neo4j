{
  currentFuture=starter.submit(new Callable<Void>(){
    @Override public Void call() throws Exception {
      try {
        wrapped.shutdown();
      }
 catch (      Throwable throwable) {
        throw new RuntimeException(throwable);
      }
      return null;
    }
  }
);
  currentFuture.get();
  starter.shutdownNow();
}
