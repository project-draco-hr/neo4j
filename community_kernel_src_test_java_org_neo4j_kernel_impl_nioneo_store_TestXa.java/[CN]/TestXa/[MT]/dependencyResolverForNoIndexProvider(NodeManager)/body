{
  return new DependencyResolver.Adapter(){
    @Override public <T>T resolveDependency(    Class<T> type,    SelectionStrategy<T> selector) throws IllegalArgumentException {
      if (SchemaIndexProvider.class.isAssignableFrom(type))       return (T)SchemaIndexProvider.NO_INDEX_PROVIDER;
 else       if (NodeManager.class.isAssignableFrom(type))       return (T)nodeManager;
      throw new IllegalArgumentException(type.toString());
    }
  }
;
}
