{
  String modifiers=Modifier.toString(field.modifiers());
  append("    ").append(modifiers);
  if (!modifiers.isEmpty()) {
    append(" ");
  }
  append(field.type().name()).append(' ').append(field.name());
  if (value != null) {
    append(" = ");
    value.accept(new MethodSourceWriter(target,this));
  }
  append(";\n");
}
