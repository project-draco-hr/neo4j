{
  try {
    committer.get();
    return true;
  }
 catch (  InterruptedException e) {
    return false;
  }
catch (  ExecutionException e) {
    if (!(e.getCause() instanceof ComException)) {
      log.error("Slave commit threw exception",e.getCause());
    }
    return false;
  }
catch (  CancellationException e) {
    return false;
  }
}
