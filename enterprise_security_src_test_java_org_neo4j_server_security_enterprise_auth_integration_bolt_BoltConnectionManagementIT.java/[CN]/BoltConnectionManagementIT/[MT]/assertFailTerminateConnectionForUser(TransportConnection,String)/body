{
  client.send(TransportTestUtil.chunk(run("CALL dbms.terminateConnectionsForUser( '" + username + "' ) YIELD username, connectionCount"),pullAll()));
  assertThat(client,eventuallyReceives(msgFailure(Status.Security.Forbidden,PERMISSION_DENIED),msgIgnored()));
  client.send(TransportTestUtil.chunk(ResetMessage.reset()));
  assertThat(client,eventuallyReceives(msgSuccess()));
}
