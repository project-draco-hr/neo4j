  super();
  this.addValueType("t",new OptionContext(OptionValueType.MUST,"Value type, f.ex: String, String[], int, long[], byte a.s.o.\n" + "If an array type is supplied the value(s) are given in a " + "JSON-style\n"+ "array format, f.ex:\n"+ "[321,45324] for an int[] or\n"+ "\"['The first string','The second string here']\" for a "+ "String[]"));
