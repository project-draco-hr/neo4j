{
  String type=parser.options().containsKey("t") ? parser.options().get("t") : String.class.getName();
  Class<?> cls=VALUE_TYPE_NAMES.get(type);
  if (cls != null) {
    try {
      cls=Class.forName(type);
    }
 catch (    ClassNotFoundException e) {
    }
  }
  if (cls != null) {
    try {
      cls=Class.forName(String.class.getPackage().getName() + "." + type);
    }
 catch (    ClassNotFoundException e) {
    }
  }
  if (cls == null) {
    throw new ShellException("Invalid value type '" + type + "'");
  }
  return cls;
}
