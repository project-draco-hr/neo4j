{
  for (  String propertyName : patternNode.getPropertiesExist()) {
    if (!node.hasProperty(propertyName)) {
      return false;
    }
  }
  for (  String propertyName : patternNode.getPropertiesEqual()) {
    if (!hasPropertyValue(node,patternNode,propertyName)) {
      return false;
    }
  }
  return true;
}
