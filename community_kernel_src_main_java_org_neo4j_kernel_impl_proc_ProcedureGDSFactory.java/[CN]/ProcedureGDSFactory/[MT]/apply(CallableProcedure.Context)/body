{
  KernelTransaction transaction=context.get(CallableProcedure.Context.KERNEL_TRANSACTION);
  GraphDatabaseFacade facade=new GraphDatabaseFacade();
  facade.init(new ProcedureGDBFacadeSPI(transaction,queryExecutor,storeDir,resolver,AutoIndexing.UNSUPPORTED,storeId,availability,urlValidator));
  return facade;
}
