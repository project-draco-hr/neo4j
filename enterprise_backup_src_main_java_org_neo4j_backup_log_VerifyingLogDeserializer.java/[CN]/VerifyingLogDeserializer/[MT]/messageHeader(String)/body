{
  StringBuilder log=new StringBuilder(type).append(" in transaction");
  if (commitEntry != null)   log.append(" (txId=").append(commitEntry.getTxId()).append(")");
 else   if (startEntry != null)   log.append(" (log local id = ").append(startEntry.getIdentifier()).append(")");
  return log.append(":\n\t");
}
