{
  List<?> options=typeDependencies.get(type);
  if (options != null) {
    return selector.select(type,(Iterable<T>)options);
  }
  if (parent != null) {
    DependencyResolver dependencyResolver=parent.get();
    if (dependencyResolver != null)     return dependencyResolver.resolveDependency(type,selector);
  }
  throw new IllegalArgumentException("Weird exception nesting here, but anyways, I couldn't find any dependency for " + type);
}
