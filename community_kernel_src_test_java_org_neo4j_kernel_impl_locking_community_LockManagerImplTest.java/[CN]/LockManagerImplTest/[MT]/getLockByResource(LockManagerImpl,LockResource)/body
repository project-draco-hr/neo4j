{
  final RWLock[] locks=new RWLock[1];
  lockManager.accept(new Visitor<RWLock,RuntimeException>(){
    @Override public boolean visit(    RWLock lock) throws RuntimeException {
      if (resource.equals(lock.resource())) {
        locks[0]=lock;
      }
      return false;
    }
  }
);
  return locks[0];
}
