{
  String arch=System.getProperty("os.arch","?");
  if (!arch.equals("x86_64") && !arch.equals("i386") && !arch.equals("x86")&& !arch.equals("amd64")) {
    throw new IllegalStateException("MuninnPageCache cannot be guaranteed to work on CPU architecture '" + arch + "' "+ "where support for unaligned word access is unknown.");
  }
  if (!UnsafeUtil.hasUnsafe()) {
    throw new IllegalStateException("Muninn requires access to sun.misc.Unsafe");
  }
}
