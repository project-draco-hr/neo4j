{
  try (MajorFlushEvent cacheFlush=monitor.beginCacheFlush()){
    for (int i=0; i < pages.length; i++) {
      MuninnPage page=pages[i];
      long stamp=page.writeLock();
      try {
        page.flush(cacheFlush.flushEventOpportunity());
      }
  finally {
        page.unlockWrite(stamp);
      }
    }
  }
 }
