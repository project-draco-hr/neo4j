{
  if (verification != VerificationLevel.NONE) {
    return new EmbeddedGraphDatabase(targetDirectory,MapUtil.stringMap(Config.INTERCEPT_DESERIALIZED_TRANSACTIONS,"true",TransactionInterceptorProvider.class.getSimpleName() + "." + verification.interceptorName,verification.configValue));
  }
 else   return new EmbeddedGraphDatabase(targetDirectory);
}
