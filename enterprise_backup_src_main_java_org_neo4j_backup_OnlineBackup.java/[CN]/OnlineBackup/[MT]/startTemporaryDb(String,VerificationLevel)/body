{
  if (verification != VerificationLevel.NONE)   return new EmbeddedGraphDatabase(targetDirectory,MapUtil.stringMap(Config.INTERCEPT_DESERIALIZED_TRANSACTIONS,"true",VerifyingTransactionInterceptorProvider.class.getSimpleName() + "." + VerifyingTransactionInterceptorProvider.NAME,"true"));
 else   return new EmbeddedGraphDatabase(targetDirectory);
}
