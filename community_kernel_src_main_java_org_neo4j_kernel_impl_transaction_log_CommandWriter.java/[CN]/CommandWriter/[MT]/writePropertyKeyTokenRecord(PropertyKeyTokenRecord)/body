{
  byte inUse=record.inUse() ? Record.IN_USE.byteValue() : Record.NOT_IN_USE.byteValue();
  channel.put(inUse);
  channel.putInt(record.getPropertyCount()).putInt(record.getNameId());
  if (record.isLight()) {
    channel.putInt(0);
  }
 else {
    writeDynamicRecords(record.getNameRecords());
  }
}
