{
  Transaction transaction=beginTx();
  try {
    Traverser traverser=traversal().traverse(node("1"));
    int count=0;
    for (    Node node : traverser.nodes()) {
      assertNotNull("returned nodes should not be null. node #" + count,node);
      count++;
    }
    assertEquals(13,count);
  }
  finally {
    transaction.finish();
  }
}
