{
  neoStores.makeStoreOk();
  propertyKeyTokenHolder.setInitialTokens(neoStores.getPropertyKeyTokenStore().getTokens(Integer.MAX_VALUE));
  relationshipTypeTokenHolder.setInitialTokens(neoStores.getRelationshipTypeTokenStore().getTokens(Integer.MAX_VALUE));
  labelTokenHolder.setInitialTokens(neoStores.getLabelTokenStore().getTokens(Integer.MAX_VALUE));
  neoStores.rebuildCountStoreIfNeeded();
  loadSchemaCache();
  indexingService.start();
  labelScanStore.start();
}
