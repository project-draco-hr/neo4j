{
  Config configuration=new Config(stringMap(GraphDatabaseSettings.pagecache_memory.name(),"8m"),GraphDatabaseSettings.class,ConsistencyCheckSettings.class);
  AssertableLogProvider logger=new AssertableLogProvider();
  ConsistencyCheckService.Result result=new ConsistencyCheckService().runFullConsistencyCheck(storeDir,configuration,ProgressMonitorFactory.NONE,NullLogProvider.getInstance(),false);
  assertTrue("Consistency check for " + storeDir + " found inconsistencies:\n\n"+ logger.serialize(),result.isSuccessful());
}
