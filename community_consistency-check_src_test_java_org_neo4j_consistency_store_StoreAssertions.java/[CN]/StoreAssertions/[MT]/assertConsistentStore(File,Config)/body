{
  Map<String,String> params=config.getParams();
  params.put(GraphDatabaseSettings.pagecache_memory.name(),"8m");
  final Config configuration=new Config(params,GraphDatabaseSettings.class,ConsistencyCheckSettings.class);
  final ConsistencyCheckService.Result result=new ConsistencyCheckService().runFullConsistencyCheck(dir,configuration,ProgressMonitorFactory.NONE,NullLogProvider.getInstance(),false);
  assertTrue(result.isSuccessful());
}
