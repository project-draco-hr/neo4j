{
  indent().append("try\n");
  indent().append("{\n");
  level.push(LEVEL);
  body.accept(block);
  level.pop();
  indent().append("}\n");
  indent().append("catch ( ").append(exception.type().name()).append(" ").append(exception.name()).append(" )\n");
  indent().append("{\n");
  level.push(LEVEL);
  handler.accept(block);
  level.pop();
  indent().append("}\n");
}
