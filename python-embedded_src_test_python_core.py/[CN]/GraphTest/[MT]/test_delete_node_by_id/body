def test_delete_node_by_id(self):
    db = self.graphdb
    with db.transaction:
        node = db.node()
    some_node_id = node.id
    with db.transaction:
        del db.node[some_node_id]
    try:
        self.graphdb.node[node.id]
        self.assertEqual(True, False)
    except Exception as e:
        self.assertTrue(isinstance(e, KeyError))
