def test_delete_relationship(self):
    db = self.graphdb
    with db.transaction:
        source = db.node()
        target = db.node()
        rel = source.Knows(target)
        rel.delete()
    try:
        self.graphdb.relationship[rel.id]
        self.assertTrue(False)
    except Exception as e:
        self.assertTrue(isinstance(e, KeyError))
