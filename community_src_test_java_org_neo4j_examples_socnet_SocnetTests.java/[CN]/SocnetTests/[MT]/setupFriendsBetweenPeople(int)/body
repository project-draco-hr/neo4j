{
  Transaction tx=graphDb.beginTx();
  try {
    for (    Person person : personRepository.getAllPersons()) {
      int nrOfFriends=r.nextInt(maxNrOfFriendsEach) + 1;
      for (int j=0; j < nrOfFriends; j++) {
        person.addFriend(getRandomPerson());
      }
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
}
