{
  ExpansionSource result=null;
  while (result == null) {
    if (current == null) {
      return null;
    }
    ExpansionSource next=current.next(rules);
    if (next == null) {
      current=current.parent();
      continue;
    }
 else {
      current=next;
    }
    if (current != null) {
      if (rules.okToReturn(current)) {
        result=current;
      }
    }
  }
  return result;
}
