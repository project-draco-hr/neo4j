{
  Transaction transaction=graphDatabaseService.beginTx();
  try {
    Node node1=graphDatabaseService.createNode();
    Node node2=graphDatabaseService.createNode();
    Relationship relationship=node1.createRelationshipTo(node2,DynamicRelationshipType.withName("THE_REL"));
    transaction.success();
    return node1;
  }
  finally {
    transaction.finish();
  }
}
