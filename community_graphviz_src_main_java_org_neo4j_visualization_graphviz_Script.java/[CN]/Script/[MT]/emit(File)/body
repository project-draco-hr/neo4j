{
  GraphDatabaseService graphdb=createGraphDb();
  GraphvizWriter writer=new GraphvizWriter(styles());
  try {
    Transaction tx=graphdb.beginTx();
    try {
      writer.emit(outfile,createGraphWalker(graphdb));
    }
  finally {
      tx.finish();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    graphdb.shutdown();
  }
}
