{
  String bundleName=b.getSymbolicName();
  if (bundleName == null)   bundleName=b.getLocation();
  String state="unknown";
switch (b.getState()) {
case Bundle.ACTIVE:
    state="active";
  break;
case Bundle.INSTALLED:
state="installed";
break;
case Bundle.RESOLVED:
state="resolved";
break;
}
log.info("\t" + state + " "+ bundleName);
}
