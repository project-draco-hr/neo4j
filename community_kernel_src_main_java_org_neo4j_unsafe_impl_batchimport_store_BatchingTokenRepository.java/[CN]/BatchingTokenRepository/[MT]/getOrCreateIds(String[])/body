{
  long[] result=new long[labels.length];
  int from, to;
  for (from=0, to=0; from < labels.length; from++) {
    int id=getOrCreateId(labels[from]);
    if (!contains(result,id,to)) {
      result[to++]=id;
    }
  }
  if (to < from) {
    result=Arrays.copyOf(result,to);
  }
  Arrays.sort(result);
  return result;
}
