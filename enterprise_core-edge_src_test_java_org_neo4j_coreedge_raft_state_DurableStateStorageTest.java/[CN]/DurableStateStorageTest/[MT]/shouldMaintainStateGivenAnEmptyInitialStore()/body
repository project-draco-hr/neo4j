{
  EphemeralFileSystemAbstraction fsa=new EphemeralFileSystemAbstraction();
  fsa.mkdir(testDir.directory());
  DurableStateStorage<AtomicInteger> storage=new DurableStateStorage<>(fsa,testDir.directory(),"state",new AtomicIntegerMarshal(),100,health(),NullLogProvider.getInstance());
  storage.persistStoreData(new AtomicInteger(99));
  assertEquals(4,fsa.getFileSize(stateFileA()));
}
