{
  StoreFactory factory=new StoreFactory(fs.get(),new File("graph.db/neostore"),pageCache,StringLogger.DEV_NULL,new Monitors());
  NeoStore neoStore=factory.newNeoStore(true,false);
  neoStore.close();
  neoStore=factory.newNeoStore(false,false);
  long lastCommittedTransactionId=neoStore.getLastCommittedTransactionId();
  neoStore.close();
  assertEquals(TransactionIdStore.BASE_TX_ID,lastCommittedTransactionId);
}
