{
  if (sortByType) {
    return toSortedExpander(getServer().getDb(),Direction.BOTH,filterMap,caseInsensitiveFilters,looseFilters).expand(node);
  }
 else {
    if (filterMap.isEmpty()) {
      return node.getRelationships();
    }
 else {
      return toExpander(getServer().getDb(),Direction.BOTH,filterMap,caseInsensitiveFilters,looseFilters).expand(node);
    }
  }
}
