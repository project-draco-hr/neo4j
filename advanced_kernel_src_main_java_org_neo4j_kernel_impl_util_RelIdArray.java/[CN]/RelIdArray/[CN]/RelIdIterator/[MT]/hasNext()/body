{
  if (nextElement != null) {
    return true;
  }
  while (true) {
    int blockLength=currentBlock.length();
    if (relativePosition < blockLength) {
      nextElement=currentBlock.get(relativePosition++);
      return true;
    }
 else {
      if (blockIndex < blocks.size()) {
        goToNextBlock();
      }
 else {
        break;
      }
    }
  }
  return false;
}
