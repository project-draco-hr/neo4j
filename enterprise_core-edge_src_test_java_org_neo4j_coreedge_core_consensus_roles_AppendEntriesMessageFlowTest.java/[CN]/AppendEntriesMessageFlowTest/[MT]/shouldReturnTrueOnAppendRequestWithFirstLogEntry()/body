{
  raft.handle(appendEntriesRequest().from(otherMember).leaderTerm(1).prevLogIndex(0).prevLogTerm(0).logEntry(new RaftLogEntry(1,data)).leaderCommit(-1).build());
  verify(outbound).send(same(otherMember),eq(appendEntriesResponse().appendIndex(1).matchIndex(1).from(myself).term(1).success().build()));
}
