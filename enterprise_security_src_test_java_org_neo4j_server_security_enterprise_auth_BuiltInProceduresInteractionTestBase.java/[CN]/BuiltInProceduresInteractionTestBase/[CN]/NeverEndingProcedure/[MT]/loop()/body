{
  testLatch.get().doubleLatch.startAndWaitForAllToStart();
  try {
    while (true) {
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException e) {
        Thread.interrupted();
      }
      guard.check();
    }
  }
  finally {
    testLatch.get().doubleLatch.finish();
  }
}
