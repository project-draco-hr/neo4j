{
  DoubleLatch latch=testLatch;
  if (latch != null) {
    latch.startAndWaitForAllToStart();
  }
  try {
    while (true) {
      try {
        Thread.sleep(250);
      }
 catch (      InterruptedException e) {
        Thread.interrupted();
      }
      guard.check();
    }
  }
  finally {
    if (latch != null) {
      latch.finish();
    }
  }
}
