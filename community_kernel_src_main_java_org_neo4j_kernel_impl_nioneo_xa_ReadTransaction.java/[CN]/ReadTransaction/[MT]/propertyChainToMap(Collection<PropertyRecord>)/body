{
  if (chain == null) {
    return null;
  }
  ArrayMap<Integer,PropertyData> propertyMap=new ArrayMap<Integer,PropertyData>(chain.size(),false,true);
  for (  PropertyRecord propRecord : chain) {
    for (    PropertyBlock propBlock : propRecord.getPropertyBlocks()) {
      propertyMap.put(propBlock.getKeyIndexId(),propBlock.newPropertyData(propRecord));
    }
  }
  return propertyMap;
}
