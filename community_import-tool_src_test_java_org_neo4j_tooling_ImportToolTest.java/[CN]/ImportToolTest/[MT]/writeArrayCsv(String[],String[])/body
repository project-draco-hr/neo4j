{
  File data=file(fileName("whitespace.csv"));
  try (PrintStream writer=new PrintStream(data)){
    writer.print(":LABEL");
    for (    String header : headers) {
      writer.print("," + header);
    }
    writer.println();
    writer.print("PERSON");
    for (    String ignored : headers) {
      boolean comma=true;
      for (      String value : values) {
        if (comma) {
          writer.print(",");
          comma=false;
        }
 else {
          writer.print(";");
        }
        writer.print(value);
      }
    }
    writer.println();
  }
   return data;
}
