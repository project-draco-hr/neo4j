{
  char delimiter=config.delimiter();
  for (int i=0; i < RELATIONSHIP_COUNT; i++) {
    if (linePredicate.accept(i)) {
      writer.println(nodeIds.get(random.nextInt(nodeIds.size())) + delimiter + nodeIds.get(random.nextInt(nodeIds.size()))+ delimiter+ (specifyType ? randomType() : "")+ delimiter+ currentTimeMillis());
    }
  }
}
