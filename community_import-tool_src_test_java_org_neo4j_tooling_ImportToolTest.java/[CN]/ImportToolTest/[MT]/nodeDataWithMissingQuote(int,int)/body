{
  String[] lines=new String[totalLines + 1];
  lines[0]="ID,:LABEL";
  for (int i=1; i <= totalLines; i++) {
    StringBuilder line=new StringBuilder(String.format("%d,",i));
    if (i == unbalancedStartLine) {
      line.append("\"Secret Agent");
    }
 else {
      line.append("Agent");
    }
    lines[i]=line.toString();
  }
  return data(lines);
}
