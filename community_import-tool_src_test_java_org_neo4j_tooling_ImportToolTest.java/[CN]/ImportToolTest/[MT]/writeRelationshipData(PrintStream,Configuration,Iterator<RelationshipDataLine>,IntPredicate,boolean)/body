{
  char delimiter=config.delimiter();
  for (int i=0; i < RELATIONSHIP_COUNT; i++) {
    if (!data.hasNext()) {
      break;
    }
    RelationshipDataLine entry=data.next();
    if (linePredicate.test(i)) {
      writer.println(entry.startNodeId + delimiter + entry.endNodeId+ (specifyType ? (delimiter + entry.type) : "")+ delimiter+ currentTimeMillis()+ delimiter+ (entry.name != null ? entry.name : ""));
    }
  }
}
