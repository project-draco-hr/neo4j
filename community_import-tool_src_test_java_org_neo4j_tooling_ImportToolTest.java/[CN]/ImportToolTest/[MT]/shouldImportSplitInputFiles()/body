{
  List<String> nodeIds=nodeIds();
  Configuration config=Configuration.COMMAS;
  importTool("--into",dbRule.getStoreDir().getAbsolutePath(),"--nodes",nodeHeader(config).getAbsolutePath() + MULTI_FILE_DELIMITER + nodeData(false,config,nodeIds,lines(0,NODE_COUNT / 2)).getAbsolutePath(),"--nodes",nodeData(true,config,nodeIds,lines(NODE_COUNT / 2,NODE_COUNT * 3 / 4)).getAbsolutePath() + MULTI_FILE_DELIMITER + nodeData(false,config,nodeIds,lines(NODE_COUNT * 3 / 4,NODE_COUNT)).getAbsolutePath(),"--relationships",relationshipHeader(config).getAbsolutePath() + MULTI_FILE_DELIMITER + relationshipData(false,config,nodeIds,alwaysTrue(),true).getAbsolutePath());
  verifyData();
}
