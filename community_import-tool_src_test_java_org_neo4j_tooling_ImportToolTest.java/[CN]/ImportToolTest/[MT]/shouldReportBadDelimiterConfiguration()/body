{
  List<String> nodeIds=nodeIds();
  Configuration config=Configuration.TABS;
  try {
    importTool("--into",dbRule.getStoreDirAbsolutePath(),"--delimiter","\\bogus","--array-delimiter",String.valueOf(config.arrayDelimiter()),"--nodes",nodeData(true,config,nodeIds,alwaysTrue()).getAbsolutePath(),"--relationships",relationshipData(true,config,nodeIds,alwaysTrue(),true).getAbsolutePath());
    fail("Should have failed");
  }
 catch (  IllegalArgumentException e) {
    assertThat(e.getMessage(),containsString("bogus"));
  }
}
