{
  List<String> groupOneNodeIds=asList("1","2","3");
  List<String> groupTwoNodeIds=asList("4","5","2");
  Configuration config=Configuration.COMMAS;
  try {
    ImportTool.main(arguments("--into",dbRule.getStoreDir().getAbsolutePath(),"--nodes",nodeHeader(config,"MyGroup").getAbsolutePath() + MULTI_FILE_DELIMITER + nodeData(false,config,groupOneNodeIds,alwaysTrue()).getAbsolutePath(),"--nodes",nodeHeader(config).getAbsolutePath() + MULTI_FILE_DELIMITER + nodeData(false,config,groupTwoNodeIds,alwaysTrue()).getAbsolutePath()));
    fail("Should have failed");
  }
 catch (  Exception e) {
    assertTrue(Exceptions.contains(e,"Mixing specified",IllegalStateException.class));
  }
}
