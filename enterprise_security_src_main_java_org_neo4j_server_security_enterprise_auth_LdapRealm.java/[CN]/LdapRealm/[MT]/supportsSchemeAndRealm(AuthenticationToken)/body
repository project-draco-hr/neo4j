{
  try {
    if (token instanceof ShiroAuthToken) {
      ShiroAuthToken shiroAuthToken=(ShiroAuthToken)token;
      return shiroAuthToken.getScheme().equals(AuthToken.BASIC_SCHEME) && (shiroAuthToken.supportsRealm(LDAP_REALM));
    }
    return false;
  }
 catch (  InvalidAuthTokenException e) {
    return false;
  }
}
