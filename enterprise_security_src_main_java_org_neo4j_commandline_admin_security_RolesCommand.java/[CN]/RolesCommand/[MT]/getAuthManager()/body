{
  if (this.authManager == null) {
    Config config=loadNeo4jConfig(homeDir,configDir);
    this.jobScheduler=new NoOpJobScheduler();
    this.authManager=new EnterpriseAuthManagerFactory().newInstance(config,NullLogProvider.getInstance(),NullLog.getInstance(),outsideWorld.fileSystem(),jobScheduler);
    this.authManager.start();
  }
  return this.authManager;
}
