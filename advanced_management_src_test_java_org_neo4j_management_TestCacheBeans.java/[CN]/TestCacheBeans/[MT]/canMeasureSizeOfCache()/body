{
  long[] before=get(CacheBean.CACHE_SIZE);
  long nodeId;
  try (Transaction tx=graphDb.beginTx()){
    nodeId=graphDb.createNode().getId();
    tx.success();
  }
   try (Transaction tx=graphDb.beginTx()){
    graphDb.getNodeById(nodeId);
  }
   assertChanged("cache size not updated",before,get(CacheBean.CACHE_SIZE));
}
