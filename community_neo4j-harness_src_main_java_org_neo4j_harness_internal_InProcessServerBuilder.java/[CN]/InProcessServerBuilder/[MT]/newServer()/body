{
  Dependencies dependencies=GraphDatabaseDependencies.newDependencies().logging(logging);
  InProcessServerControls controls=new InProcessServerControls(serverFolder,new CommunityNeoServer(new MapConfigurator(config,extensions.toList()),dependencies),logging);
  controls.start();
  try {
    fixtures.applyTo(controls.httpURI());
  }
 catch (  RuntimeException e) {
    controls.close();
    throw e;
  }
  return controls;
}
