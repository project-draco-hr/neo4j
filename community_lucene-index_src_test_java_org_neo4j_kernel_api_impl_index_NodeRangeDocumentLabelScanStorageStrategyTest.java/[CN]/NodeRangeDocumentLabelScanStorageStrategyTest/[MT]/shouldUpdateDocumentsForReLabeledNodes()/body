{
  LabelScanStorageStrategy.StorageService storage=storage(document(format.rangeField(0),format.labelField(7,0x70)));
  LuceneLabelScanWriter writer=new LuceneLabelScanWriter(storage,format,mock(Lock.class));
  writer.write(labelChanges(0,labels(),labels(7,8)));
  writer.close();
  verify(storage).updateDocument(eq(format.rangeTerm(0)),match(document(format.rangeField(0),format.labelField(7,0x71),format.labelField(8,0x01),format.labelSearchField(8))));
}
