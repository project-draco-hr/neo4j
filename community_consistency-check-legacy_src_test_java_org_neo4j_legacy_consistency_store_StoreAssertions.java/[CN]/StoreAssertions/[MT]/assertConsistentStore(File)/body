{
  final Config configuration=new Config(stringMap(GraphDatabaseSettings.pagecache_memory.name(),"8m"),GraphDatabaseSettings.class,ConsistencyCheckSettings.class);
  final ConsistencyCheckService.Result result=new ConsistencyCheckService().runFullConsistencyCheck(dir,configuration,ProgressMonitorFactory.NONE,NullLogProvider.getInstance(),new DefaultFileSystemAbstraction());
  assertTrue(result.isSuccessful());
}
