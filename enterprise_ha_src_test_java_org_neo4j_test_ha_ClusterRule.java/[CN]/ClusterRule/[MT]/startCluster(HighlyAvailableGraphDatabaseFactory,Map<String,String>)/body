{
  config.putAll(stringMap(default_timeout.name(),"1s",tx_push_factor.name(),"0"));
  clusterManager=new ClusterManager(provider,storeDirectory,config,new HashMap<Integer,Map<String,String>>(),databaseFactory);
  try {
    clusterManager.start();
  }
 catch (  Throwable throwable) {
    throw new RuntimeException(throwable);
  }
  ClusterManager.ManagedCluster cluster=clusterManager.getDefaultCluster();
  cluster.await(masterAvailable());
  return cluster;
}
