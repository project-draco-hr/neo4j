{
  String[] parts=line.split(userSeparator,-1);
  if (parts.length != 3) {
    throw new FormatException(format("wrong number of line fields [line %d]",lineNumber));
  }
  return new User.Builder().withName(parts[0]).withCredentials(deserializeCredentials(parts[1],lineNumber)).withRequiredPasswordChange(parts[2].equals("password_change_required")).build();
}
