{
  assertYieldsErrors("{}",new InvalidRequestFormat("Unable to deserialize request, " + "expected [START_OBJECT, FIELD_NAME, START_ARRAY], " + "found [START_OBJECT, END_OBJECT, null]."));
  assertYieldsErrors("[{]}",new InvalidRequestFormat("Unable to deserialize request: Unexpected close marker ']': expected '}' " + "(for OBJECT starting at [Source: TestInputStream; line: 1, column: 1])\n " + "at [Source: TestInputStream; line: 1, column: 4]"));
}
