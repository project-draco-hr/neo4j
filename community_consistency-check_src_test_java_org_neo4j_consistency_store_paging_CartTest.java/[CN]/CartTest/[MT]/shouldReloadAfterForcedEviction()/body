{
  int capacity=10;
  StorageSpy storage=new StorageSpy(capacity * 2);
  Cart cart=new Cart(capacity);
  cart.acquire(storage.page(0),storage);
  cart.forceEvict(storage.page(0));
  cart.acquire(storage.page(0),storage);
  assertArrayEquals(new String[]{"L0","E0","L0"},storage.events.toArray());
}
