{
  final Snapshot snapshot=histogram.getSnapshot();
  line.append(format(",%d,%d,%f,%d,%f,%f,%f,%f,%f,%f,%f",histogram.getCount(),snapshot.getMax(),snapshot.getMean(),snapshot.getMin(),snapshot.getStdDev(),snapshot.getMedian(),snapshot.get75thPercentile(),snapshot.get95thPercentile(),snapshot.get98thPercentile(),snapshot.get99thPercentile(),snapshot.get999thPercentile()));
}
