{
  EntryRecord entryRecord;
  try {
    entryRecord=read(bufferedReader,contentMarshal);
  }
 catch (  EndOfStreamException e) {
    currentRecord.invalidate();
    return false;
  }
catch (  IOException e) {
    hadError=true;
    throw e;
  }
  currentRecord.set(entryRecord);
  position.byteOffset=bufferedReader.position();
  position.logIndex++;
  return true;
}
