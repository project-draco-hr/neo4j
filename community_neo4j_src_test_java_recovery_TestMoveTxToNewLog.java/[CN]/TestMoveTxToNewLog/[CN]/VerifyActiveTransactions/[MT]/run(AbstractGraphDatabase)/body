{
  try {
    XaLogicalLog log=graphdb.getConfig().getTxModule().getXaDataSourceManager().getXaDataSource(Config.DEFAULT_DATA_SOURCE_NAME).getXaContainer().getLogicalLog();
    ArrayMap activeXids=(ArrayMap)inaccessibleField(log,"xidIdentMap").get(log);
