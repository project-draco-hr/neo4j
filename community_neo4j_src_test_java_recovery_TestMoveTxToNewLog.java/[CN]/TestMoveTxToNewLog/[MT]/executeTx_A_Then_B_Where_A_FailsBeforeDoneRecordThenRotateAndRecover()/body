{
  run(new SetPropertyTask());
  runInThread(new RemovePropertyAndFailTask());
  waitForFailure.await();
  beforeCommit.enable();
  run(new CreateNamedNodeTask());
  waitForDone.await();
  run(new VerifyActiveTransactions(1));
  run(new RotateTask());
  run(new VerifyActiveTransactions(1));
  restart();
  run(new VerifyTask());
}
