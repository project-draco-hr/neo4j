{
  if (shutDown) {
    return;
  }
  this.shutDown=true;
  while (awaitAllCompleted && !queue.isEmpty() && panic == null) {
    parkAWhile();
  }
  for (  Processor processor : processors) {
    processor.shutDown=true;
  }
  while (awaitAllCompleted && anyAlive() && panic == null) {
    parkAWhile();
  }
}
