{
  if (shutDown) {
    return;
  }
  this.shutDownCause=cause;
  this.shutDown=true;
  while (awaitAllCompleted && !queue.isEmpty()) {
    parkAWhile();
  }
  for (  Processor processor : processors) {
    processor.shutDown=true;
  }
  while (awaitAllCompleted && anyAlive()) {
    parkAWhile();
  }
}
