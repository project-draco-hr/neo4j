{
  if (shutDown) {
    return;
  }
  boolean awaitAllCompleted=(flags & TaskExecutor.SF_AWAIT_ALL_COMPLETED) != 0;
  while (awaitAllCompleted && !queue.isEmpty() && panic == null) {
    parkAWhile();
  }
  this.shutDown=true;
  this.abortQueued=(flags & TaskExecutor.SF_ABORT_QUEUED) != 0;
  while (awaitAllCompleted && anyAlive() && panic == null) {
    parkAWhile();
  }
}
