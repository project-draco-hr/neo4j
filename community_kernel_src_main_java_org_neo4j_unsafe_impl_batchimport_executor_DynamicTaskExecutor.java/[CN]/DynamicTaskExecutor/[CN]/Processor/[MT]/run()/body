{
  while (!shutDown) {
    Task<LOCAL> task=queue.poll();
    if (task != null) {
      try {
        task.run(threadLocalState);
      }
 catch (      Throwable e) {
        panic=e;
        shutdown(false);
        throw launderedException(e);
      }
    }
 else {
      parkAWhile();
    }
  }
}
