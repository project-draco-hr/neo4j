{
  try {
    if (transactionManager.getTransaction() != null) {
      transactionManager.rollback();
    }
  }
 catch (  IllegalStateException e) {
    throw new TransactionFailureException(e);
  }
catch (  SecurityException e) {
    throw new TransactionFailureException(e);
  }
catch (  SystemException e) {
    throw new TransactionFailureException(e);
  }
}
