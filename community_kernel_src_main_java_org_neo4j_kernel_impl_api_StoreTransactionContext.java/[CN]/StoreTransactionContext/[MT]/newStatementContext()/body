{
  StoreStatementContext context=new StoreStatementContext(propertyKeyTokenHolder,labelTokenHolder,new SchemaStorage(neoStore.getSchemaStore()),neoStore,persistenceManager,indexingService,new IndexReaderFactory.Caching(indexingService));
  return new StatementContextParts(context,context,context,context,context,null,null,context).additionalPart(AuxiliaryStoreOperations.class,context);
}
