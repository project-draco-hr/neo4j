{
  if (success && subdir != null && !keepDirectoryAfterSuccefulTest) {
    try {
      recursiveDelete(subdir);
    }
 catch (    RuntimeException e) {
      if (e.getCause() != null && e.getCause() instanceof FileUtils.MaybeWindowsMemoryMappedFileReleaseProblem) {
        System.err.println("Failed to delete test directory, " + "maybe due to Windows memory-mapped file problem: " + e.getCause().getMessage());
      }
 else {
        throw e;
      }
    }
  }
  subdir=null;
}
