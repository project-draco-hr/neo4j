{
  try {
    ChannelBuffer message=(ChannelBuffer)e.getMessage();
    Pair<ChannelBuffer,SlaveContext> result=handleRequest(realMaster,message);
    rememberChannel(e.getChannel(),result.other());
    e.getChannel().write(result.first());
  }
 catch (  Exception e1) {
    e1.printStackTrace();
    throw e1;
  }
}
