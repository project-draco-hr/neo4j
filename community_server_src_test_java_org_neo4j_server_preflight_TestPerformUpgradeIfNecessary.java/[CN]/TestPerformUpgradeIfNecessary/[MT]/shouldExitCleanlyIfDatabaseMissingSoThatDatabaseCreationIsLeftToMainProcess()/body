{
  ByteArrayOutputStream outputStream=new ByteArrayOutputStream();
  PerformUpgradeIfNecessary upgrader=new PerformUpgradeIfNecessary(buildProperties(true),loadNeo4jProperties(),new PrintStream(outputStream),DEV_NULL);
  boolean exit=upgrader.run();
  assertEquals(true,exit);
  assertEquals("",new String(outputStream.toByteArray()));
}
