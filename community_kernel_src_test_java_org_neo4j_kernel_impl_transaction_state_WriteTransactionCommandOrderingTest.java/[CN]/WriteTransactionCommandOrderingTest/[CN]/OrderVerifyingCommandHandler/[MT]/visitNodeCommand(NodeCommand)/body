{
  if (!nodeVisited) {
    created=false;
    updated=false;
    deleted=false;
  }
  nodeVisited=true;
  assertFalse(relationshipVisited);
  assertFalse(propertyVisited);
switch (command.getMode()) {
case CREATE:
    created=true;
  assertFalse(updated);
assertFalse(deleted);
break;
case UPDATE:
updated=true;
assertFalse(deleted);
break;
case DELETE:
deleted=true;
break;
}
return false;
}
