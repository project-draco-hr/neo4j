{
  TransactionCommitProcess commitProcess=new TransactionRepresentationCommitProcess(dependencyResolver.resolveDependency(TransactionAppender.class),dependencyResolver.resolveDependency(TransactionRepresentationStoreApplier.class),dependencyResolver.resolveDependency(IndexUpdatesValidator.class));
  return new MasterTransactionCommitProcess(commitProcess,txPropagator,dependencyResolver.resolveDependency(IntegrityValidator.class));
}
