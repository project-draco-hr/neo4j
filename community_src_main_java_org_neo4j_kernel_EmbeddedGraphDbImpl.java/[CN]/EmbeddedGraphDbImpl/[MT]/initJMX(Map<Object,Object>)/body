{
  Neo4jJmx.initJMX(new Neo4jJmx.Creator(instanceId,KERNEL_VERSION,(NeoStoreXaDataSource)graphDbInstance.getConfig().getTxModule().getXaDataSourceManager().getXaDataSource("nioneodb")){
    @Override protected void create(    Neo4jJmx.Factory jmx){
      jmx.createPrimitiveMBean(nodeManager);
      jmx.createCacheMBean(nodeManager);
      jmx.createDynamicConfigurationMBean(params);
    }
  }
);
}
