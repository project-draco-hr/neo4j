{
  LinkedList<List<Node>> paths=new LinkedList<List<Node>>();
  List<List<?>> complexPaths=getPaths();
  for (  List<?> complexPath : complexPaths) {
    List<Node> path=new LinkedList<Node>();
    int flipflop=0;
    for (    Object object : complexPath) {
      if (flipflop == 0) {
        path.add((Node)object);
      }
      flipflop=1 - flipflop;
    }
    paths.add(path);
  }
  return paths;
}
