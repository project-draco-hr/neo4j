{
  ZooKeeperException exception=null;
  for (int i=0; i < 5; i++) {
    try {
      return job.doJob();
    }
 catch (    ZooKeeperException e) {
      exception=e;
      try {
        Thread.sleep(300);
      }
 catch (      InterruptedException e1) {
        Thread.interrupted();
      }
    }
  }
  throw exception;
}
