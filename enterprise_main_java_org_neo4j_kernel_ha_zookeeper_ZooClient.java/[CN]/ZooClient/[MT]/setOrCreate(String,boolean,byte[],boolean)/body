{
  String created=null;
  try {
    path=pathIsRelative ? getRoot(addWatch) + "/" + path : path;
    boolean exists=false;
    byte[] existingData=null;
    try {
      existingData=zooKeeper.getData(path,addWatch,null);
      exists=true;
      if (Arrays.equals(data,existingData)) {
        return null;
      }
    }
 catch (    KeeperException e) {
      if (e.code() != KeeperException.Code.NONODE) {
        throw new ZooKeeperException("Couldn't get master notify node",e);
      }
    }
    try {
      if (!exists) {
        created=zooKeeper.create(path,data,ZooDefs.Ids.OPEN_ACL_UNSAFE,CreateMode.PERSISTENT);
      }
      zooKeeper.setData(path,data,-1);
      if (addWatch) {
        zooKeeper.getData(path,true,null);
      }
      return created;
    }
 catch (    KeeperException e) {
      if (e.code() != KeeperException.Code.NODEEXISTS) {
        throw new ZooKeeperException("Couldn't set master notify node",e);
      }
    }
  }
 catch (  InterruptedException e) {
    Thread.interrupted();
    throw new ZooKeeperException("Interrupted",e);
  }
  return created;
}
