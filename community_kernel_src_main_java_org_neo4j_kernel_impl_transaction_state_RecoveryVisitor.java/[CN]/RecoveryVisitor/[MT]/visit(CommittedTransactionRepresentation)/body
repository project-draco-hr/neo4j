{
  long txId=transaction.getCommitEntry().getTxId();
  try (LockGroup locks=new LockGroup()){
    storeApplier.apply(transaction.getTransactionRepresentation(),locks,txId,TransactionApplicationMode.RECOVERY);
  }
   recoveredCount.incrementAndGet();
  lastTransactionIdApplied=txId;
  lastTransactionChecksum=LogEntryStart.checksum(transaction.getStartEntry());
  monitor.transactionRecovered(txId);
  return false;
}
