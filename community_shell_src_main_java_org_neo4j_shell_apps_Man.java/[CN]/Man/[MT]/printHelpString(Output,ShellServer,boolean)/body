{
  String header="Available commands:";
  if (list) {
    out.println(header);
    out.println();
    for (    String command : server.getAllAvailableCommands()) {
      out.println("   " + command);
    }
    out.println();
  }
 else {
    out.println(header + " " + availableCommandsAsString(server));
  }
  out.println("Use " + getShortUsageString() + " for info about each command.");
}
