{
  try {
    MasterTransaction tx=transactions.get(context);
    if (tx.finishAsap()) {
      spi.finishTransaction(false);
      transactions.remove(context);
      return;
    }
    tx.updateTime();
    spi.suspendTransaction();
  }
 catch (  Exception e) {
    throw Exceptions.launderedException(e);
  }
}
