{
  try {
    Pair<Integer,Long> masterId=spi.getMasterIdForCommittedTx(txId);
    return new Response<>(new HandshakeResult(masterId.first(),masterId.other(),epoch),spi.storeId(),Iterables.<CommittedTransactionRepresentation>empty(),ResourceReleaser.NO_OP);
  }
 catch (  IOException e) {
    throw new RuntimeException("Couldn't get master ID for " + txId,e);
  }
}
