{
  try {
    Pair<Integer,Long> masterId=spi.getMasterIdForCommittedTx(txId);
    return ServerUtil.packResponseWithoutTransactionStream(spi.storeId(),new HandshakeResult(masterId.first(),masterId.other(),epoch));
  }
 catch (  IOException e) {
    throw new RuntimeException("Couldn't get master ID for " + txId,e);
  }
}
