{
  return new IOCursor<RaftLogEntry>(){
    private long currentIndex=fromIndex - 1;
    private RaftLogEntry currentEntry;
    @Override public boolean next() throws IOException {
      currentIndex++;
      boolean hasNext=currentIndex <= appendIndex;
      if (hasNext) {
        currentEntry=readLogEntry(currentIndex);
      }
 else {
        currentEntry=null;
      }
      return hasNext;
    }
    @Override public void close() throws IOException {
    }
    @Override public RaftLogEntry get(){
      return currentEntry;
    }
  }
;
}
