{
  if (metaChannel.size() < META_BYTES) {
    prevIndex=-1;
    prevTerm=-1;
    commitIndex=-1;
  }
 else {
    ByteBuffer buffer=ByteBuffer.allocate(META_BYTES);
    metaChannel.read(buffer,0);
    buffer.flip();
    prevIndex=buffer.getLong();
    prevTerm=buffer.getLong();
    commitIndex=buffer.getLong();
  }
}
