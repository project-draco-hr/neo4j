{
  Node node1=createLabeledNode(Labels.First,Labels.Second);
  Node node2=createLabeledNode(Labels.First,Labels.Third);
  removeLabels(node1,Labels.First);
  removeLabels(node2,Labels.Third);
  assertEquals(asSet(node2),getAllNodesWithLabel(Labels.First));
  assertEquals(asSet(node1),getAllNodesWithLabel(Labels.Second));
  assertEquals(emptySetOf(Node.class),getAllNodesWithLabel(Labels.Third));
}
