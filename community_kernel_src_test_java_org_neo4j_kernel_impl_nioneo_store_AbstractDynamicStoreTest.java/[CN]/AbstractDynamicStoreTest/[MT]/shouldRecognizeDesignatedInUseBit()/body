{
  AbstractDynamicStore store=newTestableDynamicStore();
  try {
    byte otherBitsInTheInUseByte=0;
    for (int i=0; i < 8; i++) {
      assertRecognizesByteAsInUse(store,otherBitsInTheInUseByte);
      otherBitsInTheInUseByte<<=1;
      otherBitsInTheInUseByte|=1;
    }
  }
  finally {
    store.close();
  }
}
