{
  assertThat(fromRecordState(true,true),equalTo(org.neo4j.kernel.impl.nioneo.xa.command.Command.Mode.CREATE));
  assertThat(fromRecordState(false,true),equalTo(org.neo4j.kernel.impl.nioneo.xa.command.Command.Mode.UPDATE));
  assertThat(fromRecordState(false,false),equalTo(org.neo4j.kernel.impl.nioneo.xa.command.Command.Mode.DELETE));
  assertThat(fromRecordState(true,false),equalTo(org.neo4j.kernel.impl.nioneo.xa.command.Command.Mode.DELETE));
}
