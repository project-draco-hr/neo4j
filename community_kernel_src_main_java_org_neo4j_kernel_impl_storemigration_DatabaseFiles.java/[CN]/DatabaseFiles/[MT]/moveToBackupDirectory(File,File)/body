{
  if (fs.fileExists(backupDirectory)) {
    throw new StoreUpgrader.UnableToUpgradeException(String.format("Cannot proceed with upgrade " + "because there is an existing upgrade backup in the way at %s from a previous upgrade attempt. " + "If you do not need this backup please delete it or move it out of the way before re-attempting upgrade.",backupDirectory.getAbsolutePath()));
  }
  fs.mkdir(backupDirectory);
  move(workingDirectory,backupDirectory,StoreFile.legacyStoreFiles());
}
