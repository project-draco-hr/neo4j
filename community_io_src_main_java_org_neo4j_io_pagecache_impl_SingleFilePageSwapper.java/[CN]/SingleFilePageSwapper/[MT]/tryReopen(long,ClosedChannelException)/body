{
  int stripe=stripe(filePageId);
  StoreChannel channel=channels[stripe];
  if (channel.isOpen()) {
    return;
  }
  if (closed) {
    throw closedException;
  }
  try {
    channels[stripe]=fs.open(file,"rw");
    if (stripe == tokenChannelStripe) {
      acquireLock();
    }
  }
 catch (  IOException e) {
    closedException.addSuppressed(e);
    throw closedException;
  }
}
