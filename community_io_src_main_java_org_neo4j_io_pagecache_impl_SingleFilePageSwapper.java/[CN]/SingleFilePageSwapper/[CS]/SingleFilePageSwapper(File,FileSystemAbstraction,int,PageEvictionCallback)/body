{
  this.fs=fs;
  this.file=file;
  this.channels=new StoreChannel[channelStripeCount];
  for (int i=0; i < channelStripeCount; i++) {
    channels[i]=fs.open(file,"rw");
  }
  this.filePageSize=filePageSize;
  this.onEviction=onEviction;
  increaseFileSizeTo(channels[tokenChannelStripe].size());
  try {
    acquireLock();
  }
 catch (  IOException e) {
    closeAndCollectExceptions(0,e);
  }
  hasPositionLock=channels[0].getClass() == StoreFileChannel.class && StoreFileChannel.unwrap(channels[0]).getClass() == sun.nio.ch.FileChannelImpl.class;
}
