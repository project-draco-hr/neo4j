{
  TransactionContext result=new TemporaryLabelAsPropertyTransactionContext(propertyIndexManager,persistenceManager,neoStore,indexService);
  result=new StateHandlingTransactionContext(result,persistenceCache,transactionManager.getTransactionState(),schemaCache);
  result=new ConstraintEvaluatingTransactionContext(result);
  result=new LockingTransactionContext(result,lockManager,transactionManager);
  result=new SingleStatementTransactionContext(result);
  return result;
}
