{
  TransactionContext result=new TemporaryLabelAsPropertyTransactionContext(propertyIndexManager,persistenceManager,propertyStore());
  result=new StateHandlingTransactionContext(result,cache,transactionManager.getTransactionState());
  result=new ConstraintEvaluatingTransactionContext(result);
  result=new LockingTransactionContext(result,lockManager,transactionManager);
  result=new SingleStatementTransactionContext(result);
  return result;
}
