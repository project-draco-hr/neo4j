{
  StatementOperationParts parts=buildStatementOperations();
  this.statementOperations=parts;
  ReadOnlyStatementOperations readOnlyParts=new ReadOnlyStatementOperations(parts.keyReadOperations(),parts.schemaStateOperations());
  this.readOnlyStatementOperations=parts.override(parts.keyReadOperations(),readOnlyParts,parts.entityReadOperations(),readOnlyParts,parts.schemaReadOperations(),readOnlyParts,readOnlyParts);
}
