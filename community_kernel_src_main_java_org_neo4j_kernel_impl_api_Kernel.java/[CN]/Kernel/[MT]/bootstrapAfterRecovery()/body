{
  StatementOperationParts parts=newTransaction().newStatementOperations();
  this.statementOperations=parts;
  ReadOnlyStatementOperations readOnlyParts=new ReadOnlyStatementOperations(parts.schemaStateOperations());
  this.readOnlyStatementOperations=parts.override(parts.keyReadOperations(),readOnlyParts,parts.entityReadOperations(),readOnlyParts,parts.schemaReadOperations(),readOnlyParts,readOnlyParts,parts.lifecycleOperations());
}
