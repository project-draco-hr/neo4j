{
  checkIfShutdown();
  return new KernelTransactionImplementation(statementOperations,legacyKernelOperations,readOnly,schemaWriteGuard,labelScanStore,indexService,transactionManager,nodeManager,schemaState,new LockHolderImpl(lockManager,getJTATransaction(),nodeManager),persistenceManager,providerMap,neoStore,getLegacyTxState());
