{
  StringBuffer buf=new StringBuffer();
  buf.append("PropertyRecord[").append(getId()).append(",").append(inUse()).append(",").append(prevProp).append(",").append(nextProp).append(", Value[");
  Iterator<PropertyBlock> itr=blockRecords.iterator();
  while (itr.hasNext()) {
    buf.append(itr.next());
    if (itr.hasNext()) {
      buf.append(", ");
    }
  }
  buf.append("]]");
  return buf.toString();
}
