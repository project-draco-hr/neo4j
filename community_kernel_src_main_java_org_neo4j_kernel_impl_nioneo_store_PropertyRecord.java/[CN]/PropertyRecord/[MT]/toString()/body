{
  StringBuffer buf=new StringBuffer();
  buf.append("PropertyRecord[").append(getId()).append(",").append(inUse()).append(",").append(type).append(",").append(keyIndexId).append(",").append(propBlock).append(",").append(",").append(nextProp);
  buf.append(", Value[");
  for (  DynamicRecord record : valueRecords) {
    buf.append(record);
  }
  buf.append("]]");
  return buf.toString();
}
