{
  Response response=consoleService.exec(new JsonFormat(),"{ \"command\" : \"create (n) return n;\", \"engine\":\"shell\" }");
  assertEquals(200,response.getStatus());
  String result=decode(response).get(0);
  String expected="+-----------+" + LN + "| n         |"+ LN+ "+-----------+"+ LN+ "| Node[0]{} |"+ LN+ "+-----------+"+ LN+ "1 row";
  assertThat(result,containsString(expected));
}
