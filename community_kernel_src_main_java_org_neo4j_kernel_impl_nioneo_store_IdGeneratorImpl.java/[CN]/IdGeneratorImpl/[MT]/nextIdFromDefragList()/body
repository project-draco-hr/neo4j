{
  if (aggressiveReuse) {
    Long id=releasedIdList.poll();
    if (id != null) {
      defraggedIdCount--;
      return id.longValue();
    }
  }
  if (defragedIdList.size() > 0) {
    long id=defragedIdList.removeFirst();
    if (haveMore && defragedIdList.size() == 0) {
      readIdBatch();
    }
    defraggedIdCount--;
    return id;
  }
  return -1;
}
