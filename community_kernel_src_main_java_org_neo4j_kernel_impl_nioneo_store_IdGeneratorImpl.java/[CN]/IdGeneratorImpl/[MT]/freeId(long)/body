{
  if (id < 0 || id >= nextFreeId.get()) {
    throw new IllegalArgumentException("Illegal id[" + id + "]");
  }
  if (fileChannel == null) {
    throw new IllegalStateException("Generator closed " + fileName);
  }
  releasedIdList.add(id);
  defraggedIdCount++;
  if (releasedIdList.size() >= grabSize) {
    writeIdBatch();
  }
}
