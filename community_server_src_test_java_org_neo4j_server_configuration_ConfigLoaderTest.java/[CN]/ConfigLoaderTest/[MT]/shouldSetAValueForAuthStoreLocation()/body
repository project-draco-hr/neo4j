{
  Optional<File> configFile=ConfigFileBuilder.builder(folder.getRoot()).withSetting(DatabaseManagementSystemSettings.data_directory,"the-data-dir").build();
  Config config=configLoader.loadConfig(Optional.of(folder.getRoot()),configFile,log);
  assertThat(config.get(GraphDatabaseSettings.auth_store),is(new File(folder.getRoot(),"the-data-dir/dbms/auth").getAbsoluteFile()));
}
