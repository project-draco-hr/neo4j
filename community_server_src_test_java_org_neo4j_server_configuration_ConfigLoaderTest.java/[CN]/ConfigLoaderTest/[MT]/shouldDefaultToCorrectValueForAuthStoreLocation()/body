{
  Optional<File> configFile=ConfigFileBuilder.builder(folder.getRoot()).withoutSetting(DatabaseManagementSystemSettings.data_directory).build();
  Config config=configLoader.loadConfig(Optional.of(folder.getRoot()),configFile,log);
  assertThat(config.get(GraphDatabaseSettings.auth_store),is(new File(folder.getRoot(),"data/dbms/auth").getAbsoluteFile()));
}
