{
  localDatabase.start();
  MemberId memberId=findCoreMemberToCopyFrom();
  if (localDatabase.isEmpty()) {
    localDatabase.stop();
    localDatabase.copyStoreFrom(memberId,storeFetcher);
    localDatabase.start();
  }
 else {
    localDatabase.ensureSameStoreId(memberId,storeFetcher);
  }
  txPulling.start();
}
