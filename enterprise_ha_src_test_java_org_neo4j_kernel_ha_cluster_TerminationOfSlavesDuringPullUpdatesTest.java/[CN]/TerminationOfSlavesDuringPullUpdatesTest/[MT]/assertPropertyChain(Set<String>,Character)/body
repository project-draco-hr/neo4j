{
  boolean first=true;
  Character actualFirst=null;
  for (  String key : allProperties) {
    if (first) {
      first=false;
      actualFirst=key.charAt(0);
      assertThat("Other prefix than expected",actualFirst,isIn(keyPrefix));
    }
    assertThat("Property key chain is broken " + Arrays.toString(allProperties.toArray()),key.charAt(0),equalTo(actualFirst));
  }
}
