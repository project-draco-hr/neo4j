{
  for (  File file : storeDir.listFiles()) {
    if (file.getName().equals(IndexConfigStore.INDEX_DB_FILE_NAME)) {
      files.add(toNotAStoreTypeFile.apply(file));
    }
 else     if (includeLogs && transactionLogFile(file.getName())) {
      files.add(toNotAStoreTypeFile.apply(file));
    }
  }
}
