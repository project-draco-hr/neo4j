{
  return new StandaloneDatabase(testMethodName,new Bootstrap(path,machineId,HighlyAvailableGraphDatabase.CONFIG_KEY_HA_SERVER,haServer,HighlyAvailableGraphDatabase.CONFIG_KEY_HA_ZOO_KEEPER_SERVERS,zooKeeperConnection){
    @Override HighlyAvailableGraphDatabase start(    String storeDir,    Map<String,String> config){
      HighlyAvailableGraphDatabase db=new HighlyAvailableGraphDatabase(storeDir,config);
      System.out.println("Started HA db (w/ zoo keeper)");
      return db;
    }
  }
);
}
