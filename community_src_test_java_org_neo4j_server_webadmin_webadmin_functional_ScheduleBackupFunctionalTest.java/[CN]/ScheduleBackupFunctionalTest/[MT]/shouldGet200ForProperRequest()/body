{
  Client client=Client.create();
  WebResource createResource=client.resource(TestUtil.SERVER_BASE() + BackupService.ROOT_PATH + BackupService.JOBS_PATH);
  String properJSON="{" + "\"name\":\"Daily\"," + "\"cronExpression\":\"0 0 0 ? 0 0\","+ "\"autoFoundation\":true,"+ "\"backupPath\":\"backup1\"}";
  ClientResponse scheduleResponse=createResource.type(MediaType.APPLICATION_JSON).accept(MediaType.APPLICATION_JSON).entity(properJSON).put(ClientResponse.class);
  assertEquals(200,scheduleResponse.getStatus());
}
