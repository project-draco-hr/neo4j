{
  int max=catalogue.length / (key.size() * 2) - 1;
  int min=0;
  for (int mid; min <= max; ) {
    mid=min + (max - min) / 2;
    int cmp=compare(key.buffer,catalogue,mid * key.size() * 2);
    if (cmp == 0) {
      max=mid;
    }
    if (cmp > 0) {
      cmp=compare(key.buffer,catalogue,mid * key.size() * 2 + key.size());
      if (cmp <= 0) {
        return mid;
      }
 else {
        min=mid + 1;
      }
    }
 else {
      max=mid - 1;
    }
  }
  return min;
}
