{
  int recordSize=searchKey.size() + value.size(), last=maxPage(file.pageSize(),recordSize,totalRecords);
  int record=recordOffset(cursor,searchKey,key,value,(cursor.getCurrentPageId() == 0) ? headerRecords : 0,(cursor.getCurrentPageId() == last) ? (totalRecords % (file.pageSize() / recordSize)) - 1 : file.pageSize() / recordSize);
  return record * recordSize;
}
