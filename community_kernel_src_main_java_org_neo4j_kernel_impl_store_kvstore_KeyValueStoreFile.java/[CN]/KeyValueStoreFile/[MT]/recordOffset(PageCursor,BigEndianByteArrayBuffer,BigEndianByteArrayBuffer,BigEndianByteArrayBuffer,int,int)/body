{
  int recordSize=key.size() + value.size();
  for (int mid; min <= max; ) {
    mid=min + (max - min) / 2;
    readKeyValuePair(cursor,mid * recordSize,key,value);
    if (min == max) {
      break;
    }
    int cmp=compare(searchKey.buffer,key.buffer,0);
    if (cmp > 0) {
      min=mid + 1;
    }
 else {
      max=mid;
    }
  }
  return max;
}
