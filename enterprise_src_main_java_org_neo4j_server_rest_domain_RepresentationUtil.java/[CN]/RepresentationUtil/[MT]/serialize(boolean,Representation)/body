{
  if (!forceArray && representations.length == 0) {
    return "";
  }
 else   if (!forceArray && representations.length == 1) {
    return representations[0].serialize();
  }
 else {
    Collection<Object> objects=new ArrayList<Object>();
    for (    Representation rep : representations) {
      objects.add(rep.serialize());
    }
    return objects;
  }
}
