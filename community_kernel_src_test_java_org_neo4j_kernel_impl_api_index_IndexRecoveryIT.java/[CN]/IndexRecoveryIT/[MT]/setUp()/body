{
  when(mockedIndexProvider.getProviderDescriptor()).thenReturn(TestSchemaIndexProviderDescriptor.PROVIDER_DESCRIPTOR);
  when(mockedIndexProvider.compareTo(any(SchemaIndexProvider.class))).thenReturn(1);
  when(mockedIndexProvider.storeMigrationParticipant(any(FileSystemAbstraction.class),any(UpgradableDatabase.class))).thenReturn(NOT_PARTICIPATING);
