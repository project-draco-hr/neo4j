{
  if (osIsWindows())   return;
  startDb("true");
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-full","-from",BackupTool.DEFAULT_SCHEME + "://localhost","-to",BACKUP_PATH));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
  createSomeData(db);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-incremental","-from",BackupTool.DEFAULT_SCHEME + "://localhost","-to",BACKUP_PATH));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
}
