{
  long end=System.currentTimeMillis() + MAX_WAIT_TIME;
  while (System.currentTimeMillis() < end) {
    try {
      if (!this.getValueOfCssProperty("display").equals("none")) {
        return;
      }
    }
 catch (    NoSuchElementException e) {
    }
    try {
      Thread.sleep(13);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  throw new RuntimeException("Element did not become visible within a reasonable time." + "\nHTML dump:\n" + webDriver.getPageSource());
}
