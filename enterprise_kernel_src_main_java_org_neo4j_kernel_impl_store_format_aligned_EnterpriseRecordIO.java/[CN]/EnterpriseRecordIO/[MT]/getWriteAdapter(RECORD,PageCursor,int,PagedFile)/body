{
  int primaryEndOffset=primaryCursor.getOffset() + recordSize - 1;
  Reference.encode(record.getSecondaryUnitId(),primaryCursor,PAGE_CURSOR_ADAPTER);
  return new SecondaryPageCursorWriteDataAdapter(pageIdForRecord(record.getSecondaryUnitId(),storeFile.pageSize(),recordSize),offsetForId(record.getSecondaryUnitId(),storeFile.pageSize(),recordSize),primaryEndOffset);
}
