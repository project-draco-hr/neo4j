{
  ConfigurationMigrator migrator=new ConfigurationMigrator(StringLogger.SYSTEM);
  assertThat(migrator.migrateConfiguration(stringMap("enable_remote_shell","true")),equalTo(stringMap("remote_shell_enabled","true")));
  assertThat(migrator.migrateConfiguration(stringMap("enable_remote_shell","false")),equalTo(stringMap("remote_shell_enabled","false")));
  assertThat(migrator.migrateConfiguration(stringMap("enable_remote_shell","port=1234")),equalTo(stringMap("remote_shell_enabled","true","remote_shell_port","1234","remote_shell_read_only","false","remote_shell_name","shell")));
}
