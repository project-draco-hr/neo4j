{
  PropertyRecord record=command.getAfter();
  if (record.isNodeSet()) {
    long nodeId=command.getAfter().getNodeId();
    List<PropertyCommand> group=propertyCommandsByNodeIds.get(nodeId);
    if (group == null) {
      propertyCommandsByNodeIds.put(nodeId,group=new ArrayList<>());
    }
    group.add(command);
  }
  return false;
}
