{
  this((AbstractGraphDatabase)createDatabase(factory,databaseStoreDirectory,null));
  console(graph,getClass()).warn("No database tuning properties set in the property file, using defaults. Please specify the " + "performance properties file with org.neo4j.server.db.tuning.properties in the server " + "properties file [%s].",System.getProperty("org.neo4j.server.properties"));
}
