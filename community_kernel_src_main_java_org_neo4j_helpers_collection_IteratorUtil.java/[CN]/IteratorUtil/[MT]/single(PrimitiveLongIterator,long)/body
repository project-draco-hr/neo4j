{
  if (iterator.hasNext()) {
    long result=iterator.next();
    if (iterator.hasNext()) {
      throw new NoSuchElementException("More than one element in " + iterator + ". First element is '"+ result+ "' and the second element is '"+ iterator.next()+ "'");
    }
    return result;
  }
 else {
    return itemIfNone;
  }
}
