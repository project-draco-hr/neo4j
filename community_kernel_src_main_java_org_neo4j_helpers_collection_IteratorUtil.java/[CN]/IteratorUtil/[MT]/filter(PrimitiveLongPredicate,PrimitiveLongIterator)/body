{
  return new AbstractPrimitiveLongIterator(){
{
      computeNext();
    }
    protected void computeNext(){
      for (hasNext=source.hasNext(); hasNext; hasNext=source.hasNext()) {
        nextValue=source.next();
        if (predicate.accept(nextValue)) {
          return;
        }
      }
    }
  }
;
}
