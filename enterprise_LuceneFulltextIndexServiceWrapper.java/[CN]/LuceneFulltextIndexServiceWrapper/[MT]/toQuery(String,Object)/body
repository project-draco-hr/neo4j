{
  TokenStream stream=LOWER_CASE_WHITESPACE_ANALYZER.tokenStream(key,new StringReader(value.toString().toLowerCase()));
  BooleanQuery booleanQuery=new BooleanQuery();
  try {
    while (stream.incrementToken()) {
      String term=stream.getAttribute(TermAttribute.class).term();
      booleanQuery.add(new TermQuery(new Term(key,term)),Occur.MUST);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return booleanQuery;
}
