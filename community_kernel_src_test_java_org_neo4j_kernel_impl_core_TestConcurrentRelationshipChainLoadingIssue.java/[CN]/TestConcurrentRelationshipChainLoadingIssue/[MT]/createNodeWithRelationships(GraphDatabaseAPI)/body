{
  Transaction tx=db.beginTx();
  Node node;
  try {
    node=db.createNode();
    for (int i=0; i < relCount / 2; i++) {
      node.createRelationshipTo(node,MyRelTypes.TEST);
    }
    for (int i=0; i < relCount / 2; i++) {
      node.createRelationshipTo(node,MyRelTypes.TEST2);
    }
    tx.success();
    return node;
  }
  finally {
    tx.finish();
  }
}
