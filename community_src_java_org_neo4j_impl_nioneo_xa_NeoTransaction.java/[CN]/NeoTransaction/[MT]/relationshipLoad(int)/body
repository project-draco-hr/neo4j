{
  RelationshipRecord relRecord=getRelationshipRecord(id);
  if (relRecord != null) {
    if (!relRecord.inUse()) {
      throw new StoreFailureException("Relationship[" + id + "] not in use");
    }
    return new RelationshipData(id,relRecord.getFirstNode(),relRecord.getSecondNode(),relRecord.getType());
  }
  relRecord=getRelationshipStore().getRecord(id,readFromBuffer);
  return new RelationshipData(id,relRecord.getFirstNode(),relRecord.getSecondNode(),relRecord.getType());
}
