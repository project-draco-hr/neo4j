{
  try {
    if (server != null) {
      server.stopServerOnly();
      server.getDatabase().rrdDb().close();
    }
    return 0;
  }
 catch (  Exception e) {
    log.error("Failed to cleanly shutdown Neo Server on port [%d]. Reason [%s] ",server.getWebServerPort(),e.getMessage());
    return 1;
  }
}
