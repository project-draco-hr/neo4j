{
  StringBuilder builder=new StringBuilder();
  builder.append("\"").append(getThreadName()).append("\" ").append(" (").append(getTransaction()).append(") ").append("[tid=").append(getThreadId()).append("(").append(getReadCount()).append("r,").append(getWriteCount()).append("w ),").append(isWaitingOnWriteLock() ? "Write" : "Read").append("Lock]");
  return builder.toString();
}
