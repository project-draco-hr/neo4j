{
  String uri;
  if (address.getAddress().getHostAddress().startsWith("0"))   uri=CLUSTER_SCHEME + "://0.0.0.0:" + address.getPort();
 else {
    uri=CLUSTER_SCHEME + "://" + address.getAddress().getHostName()+ ":"+ address.getPort();
  }
  if (config.name() != null)   uri+="/?name=" + config.name();
  return URI.create(uri);
}
