{
  ServerInterface server=new MadeUpServerProcess().start(new Long[]{storeIdToUse.getCreationTime(),storeIdToUse.getRandomId()});
  server.awaitStarted();
  MadeUpClient client=new MadeUpClient(MadeUpServerProcess.PORT,storeIdToUse);
  assertEquals((Integer)(9 * 5),client.multiply(9,5).response());
  client.streamSomeData(new ToAssertionWriter(),1024 * 1024 * 50);
  client.shutdown();
  server.shutdown();
}
