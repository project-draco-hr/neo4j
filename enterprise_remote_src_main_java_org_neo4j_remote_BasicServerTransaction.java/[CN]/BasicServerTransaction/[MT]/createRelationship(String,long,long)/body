{
  resume();
  @SuppressWarnings("hiding") long id;
  try {
    id=server.createRelationship(connection.graphDb,relationshipTypeName,startNodeId,endNodeId);
  }
 catch (  RuntimeException ex) {
    return response().buildErrorResponse(ex);
  }
  return response().buildRelationshipResponse(id,relationshipTypeName,startNodeId,endNodeId);
}
