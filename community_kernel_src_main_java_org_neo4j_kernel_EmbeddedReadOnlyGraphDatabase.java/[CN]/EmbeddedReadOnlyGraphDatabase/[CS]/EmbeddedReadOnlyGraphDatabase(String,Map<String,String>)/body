{
  super(storeDir);
  params.put(Config.READ_ONLY,"true");
  this.graphDbImpl=new EmbeddedGraphDbImpl(getStoreDir(),null,params,this,CommonFactories.defaultLockManagerFactory(),CommonFactories.defaultIdGeneratorFactory(),CommonFactories.defaultRelationshipTypeCreator(),CommonFactories.defaultTxIdGeneratorFactory(),CommonFactories.defaultTxHook(),CommonFactories.defaultLastCommittedTxIdSetter(),CommonFactories.defaultFileSystemAbstraction());
}
