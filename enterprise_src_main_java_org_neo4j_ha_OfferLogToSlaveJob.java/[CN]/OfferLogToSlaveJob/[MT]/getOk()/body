{
  if (!acquireReadBuffer()) {
    return false;
  }
  try {
    buffer.limit(1);
    int read=connection.read();
    if (read == 1) {
      buffer.flip();
      byte response=buffer.get();
      if (response == HeaderConstants.OK) {
        setStatus(Status.SEND_LOG);
      }
 else       if (response == HeaderConstants.NOT_OK) {
        setNoRequeue();
      }
 else {
        log("Unkown response: " + response);
        close();
      }
      return true;
    }
 else {
      retries++;
      return false;
    }
  }
  finally {
    releaseReadBuffer();
  }
}
