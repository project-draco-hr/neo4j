{
  if (globalId.length > Xid.MAXGTRIDSIZE) {
    throw new IllegalArgumentException("GlobalId length to long, " + globalId.length);
  }
  if (branchId.length > Xid.MAXBQUALSIZE) {
    throw new IllegalArgumentException("BranchId (resource id) to long, " + branchId.length);
  }
  this.globalId=globalId;
  this.branchId=branchId;
  this.formatId=formatId;
}
