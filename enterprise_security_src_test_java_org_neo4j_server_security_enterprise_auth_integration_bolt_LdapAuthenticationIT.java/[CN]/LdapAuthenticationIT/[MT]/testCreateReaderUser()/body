{
  assertAuth("neo4j","abc123");
  client.send(TransportTestUtil.chunk(run("CALL dbms.security.changeUserPassword('neo4j', '123') CALL dbms.security.createUser( 'neo', 'invalid', false ) " + "CALL dbms.security.addRoleToUser( 'reader', 'neo' ) RETURN 0"),pullAll()));
  assertThat(client,eventuallyReceives(msgSuccess()));
}
