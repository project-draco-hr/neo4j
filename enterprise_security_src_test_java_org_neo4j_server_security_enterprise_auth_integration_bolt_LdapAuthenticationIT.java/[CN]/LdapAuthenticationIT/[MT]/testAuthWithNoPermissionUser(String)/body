{
  client.connect(address).send(TransportTestUtil.acceptedVersions(1,0,0,0)).send(TransportTestUtil.chunk(init("TestClient/1.1",map("principal",username,"credentials","abc123","scheme","basic"))));
  assertThat(client,eventuallyRecieves(new byte[]{0,0,0,1}));
  assertThat(client,eventuallyRecieves(msgSuccess()));
  client.send(TransportTestUtil.chunk(run("MATCH (n) RETURN n"),pullAll()));
  assertThat(client,eventuallyRecieves(msgFailure(Status.Security.Forbidden,String.format("Read operations are not allowed for '%s'.",username))));
}
