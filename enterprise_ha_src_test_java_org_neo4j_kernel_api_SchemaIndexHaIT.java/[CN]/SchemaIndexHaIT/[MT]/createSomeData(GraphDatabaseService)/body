{
  Map<Object,Node> result=new HashMap<Object,Node>();
  Transaction tx=db.beginTx();
  try {
    for (int i=0; i < 10; i++) {
      Node node=db.createNode(label);
      Object propertyValue=i;
      node.setProperty(key,propertyValue);
      result.put(propertyValue,node);
    }
    tx.success();
    return result;
  }
  finally {
    tx.finish();
  }
}
