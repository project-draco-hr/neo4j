{
  if (injectLatchPredicate.test(deps.db())) {
    ControlledSchemaIndexProvider provider=new ControlledSchemaIndexProvider(new LuceneSchemaIndexProvider(new DefaultFileSystemAbstraction(),DirectoryFactory.PERSISTENT,context.storeDir(),deps.config()));
    perDbIndexProvider.put(deps.db(),provider);
    return provider;
  }
 else {
    return new LuceneSchemaIndexProvider(new DefaultFileSystemAbstraction(),DirectoryFactory.PERSISTENT,context.storeDir(),deps.config());
  }
}
