{
  Monitors monitors=new Monitors();
  Config config=new Config();
  @SuppressWarnings("deprecation") StoreFactory storeFactory=new StoreFactory(config,new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(monitors,config),fs.get(),StringLogger.DEV_NULL,new DefaultTxHook(),monitors);
  File storeFile=new File("story");
  storeFactory.createNodeStore(storeFile);
  nodeStore=storeFactory.newNodeStore(storeFile);
}
