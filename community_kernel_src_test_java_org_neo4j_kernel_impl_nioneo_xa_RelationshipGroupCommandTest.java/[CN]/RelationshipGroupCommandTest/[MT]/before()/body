{
  File storeDir=new File("dir");
  fs.get().mkdirs(storeDir);
  @SuppressWarnings("deprecation") StoreFactory storeFactory=new StoreFactory(StoreFactory.configForStoreDir(new Config(),storeDir),new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),fs.get(),StringLogger.DEV_NULL,new DefaultTxHook());
  storeFactory.createNodeStore();
  nodeStore=storeFactory.newNodeStore();
}
