{
  Objects.requireNonNull(record1);
  Objects.requireNonNull(record2);
  if (record1.getId() != record2.getId()) {
    return false;
  }
 else   if (record1.inUse() != record2.inUse()) {
    return false;
  }
 else   if (!record1.inUse()) {
    return true;
  }
 else {
    return inUseRecordsEqual(record1,record2);
  }
}
