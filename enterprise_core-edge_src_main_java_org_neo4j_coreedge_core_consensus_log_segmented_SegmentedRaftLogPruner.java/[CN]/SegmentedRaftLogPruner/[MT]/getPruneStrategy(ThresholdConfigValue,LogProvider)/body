{
switch (configValue.type) {
case "size":
    return new SizeBasedLogPruningStrategy(parsedConfigOption.value);
case "txs":
case "entries":
  return new EntryBasedLogPruningStrategy(parsedConfigOption.value,logProvider);
case "hours":
case "days":
case "false":
return new NoPruningPruningStrategy();
default :
throw new IllegalArgumentException("Invalid log pruning configuration value '" + configValue.value + "'. Invalid type '"+ configValue.type+ "', valid are files, size, txs, entries, hours, days.");
}
}
