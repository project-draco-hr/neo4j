{
  startDb(createLuceneIndexFactory());
  IndexDefinition indexDefinition=createIndex(myLabel);
  waitForIndex(indexDefinition);
  long node=createNode(myLabel,12);
  rotateLogs();
  deleteNode(node);
  killDb();
  startDb(createLuceneIndexFactory());
  assertEquals(0,doIndexLookup(myLabel,12).size());
}
