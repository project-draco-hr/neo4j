{
  Map<String,String> config=new HashMap<String,String>();
  config.put("use_memory_mapped_buffers","true");
  config.put("dump_configuration","true");
  config.put(GraphDatabaseSettings.all_stores_total_mapped_memory_size.name(),configuration.get(all_stores_total_mapped_memory_size));
  addLegacyMemoryMappingConfiguration(config,configuration.get(all_stores_total_mapped_memory_size));
  return config;
}
