def find_deprecations():
    for dep_line in e('grep -nr --include=*.java @Deprecated *'):
        if (len(dep_line.strip()) > 0):
            parts = dep_line.split(':')
            if (len(parts) > 1):
                yield (parts[0], parts[1])
            else:
                print ("Unable to comprehend grep output '%s'" % dep_line)
