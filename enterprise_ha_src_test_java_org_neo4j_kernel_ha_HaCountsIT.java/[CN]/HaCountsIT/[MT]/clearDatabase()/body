{
  try (Transaction tx=master.beginTx()){
    for (    IndexDefinition index : master.schema().getIndexes()) {
      index.drop();
    }
    tx.success();
  }
   try (Transaction tx=master.beginTx()){
    for (    Node node : at(master).getAllNodes()) {
      for (      Relationship relationship : node.getRelationships()) {
        relationship.delete();
      }
      node.delete();
    }
    tx.success();
  }
   cluster.sync();
}
