{
  super("Relationship --> Relationship",config,false);
  add(new ReadRelationshipRecordsBackwardsStep(control(),config.batchSize(),config.movingAverageSize(),store));
  add(new RelationshipLinkbackStep(control(),config.workAheadSize(),config.movingAverageSize(),cache));
  add(new UpdateRecordsStep<>(control(),config.workAheadSize(),config.movingAverageSize(),store));
}
