{
  if (propStore == null) {
    return new RecordStore<?>[]{nodeStore,relStore,relationshipTypeTokenStore,relationshipTypeNameStore,labelTokenStore,labelNameStore};
  }
  return new RecordStore<?>[]{schemaStore,nodeStore,relStore,propStore,stringStore,arrayStore,relationshipTypeTokenStore,propertyKeyTokenStore,labelTokenStore,relationshipTypeNameStore,propertyKeyNameStore,labelNameStore};
}
