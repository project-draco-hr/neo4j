{
  this(new StoreFactory(new Config(new ConfigurationDefaults(GraphDatabaseSettings.class).apply(requiredParams(params,path))),new DefaultIdGeneratorFactory(),new DefaultFileSystemAbstraction(),new DefaultLastCommittedTxIdSetter(),initLogger(path),new DefaultTxHook()).attemptNewNeoStore(new File(path,"neostore").getAbsolutePath()));
  this.closeable=true;
}
