{
  try (Transaction tx=graphDb.beginTx()){
    graphDb.createNode(label("Node")).setProperty("prop",store);
    tx.success();
  }
   db.clearCache();
  try (Transaction tx=graphDb.beginTx()){
    assertEquals(1,count(graphDb.findNodesByLabelAndProperty(label("Node"),"prop",lookup)));
    tx.success();
  }
 }
