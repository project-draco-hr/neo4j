{
  try {
    StartupHealthCheck startupHealthCheck=new StartupHealthCheck(rules());
    Jetty6WebServer webServer=new Jetty6WebServer();
    server=new NeoServerWithEmbeddedWebServer(this,new AddressResolver(),startupHealthCheck,getConfigurator(),webServer,getServerModules(),new RealClock());
    server.start();
    addShutdownHook();
    return OK;
  }
 catch (  TransactionFailureException tfe) {
    tfe.printStackTrace();
    log.error(String.format("Failed to start Neo Server on port [%d], because ",server.getWebServerPort()) + tfe + ". Another process may be using database location "+ server.getDatabase().getLocation());
    return GRAPH_DATABASE_STARTUP_ERROR_CODE;
  }
catch (  Exception e) {
    e.printStackTrace();
    log.error("Failed to start Neo Server on port [%s]",server.getWebServerPort());
    return WEB_SERVER_STARTUP_ERROR_CODE;
  }
}
