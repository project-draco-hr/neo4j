{
  try (Transaction tx=db.beginTx()){
    db.index().forNodes("index");
    tx.success();
  }
   String storeDir=db.getStoreDir();
  shutdownDB();
  final Process process=Runtime.getRuntime().exec(new String[]{"java","-cp",System.getProperty("java.class.path"),AddDeleteQuit.class.getName(),storeDir});
  int result=new ProcessStreamHandler(process,true).waitForResult();
  assertEquals(0,result);
  startDB();
  forceRecover();
}
