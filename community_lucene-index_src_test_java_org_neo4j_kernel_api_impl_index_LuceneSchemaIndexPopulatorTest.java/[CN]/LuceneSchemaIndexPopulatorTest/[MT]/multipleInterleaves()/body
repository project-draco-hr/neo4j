{
  index.add(1,"1");
  index.add(2,"2");
  index.update(asList(change(1,"1","1a"),change(2,"2","2a")));
  index.add(3,"3");
  index.add(4,"4");
  index.update(asList(change(1,"1a","1b"),change(4,"4","4a")));
  assertIndexedValues(no("1"),no("1a"),hit("1b",1),no("2"),hit("2a",2),hit("3",3),no("4"),hit("4a",4));
}
