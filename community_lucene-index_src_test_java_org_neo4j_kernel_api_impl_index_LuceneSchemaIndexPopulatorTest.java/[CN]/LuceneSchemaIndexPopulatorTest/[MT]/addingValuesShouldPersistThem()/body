{
  index.add(1,"First");
  index.add(2,"Second");
  index.add(3,(byte)1);
  index.add(4,(short)2);
  index.add(5,3);
  index.add(6,4L);
  index.add(7,5F);
  index.add(8,6D);
  assertIndexedValues(hit("First",1),hit("Second",2),hit((byte)1,3),hit((short)2,4),hit(3,5),hit(4L,6),hit(5F,7),hit(6D,8));
}
