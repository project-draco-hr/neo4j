{
  if (start >= stop || (stop - start) > MAX_TIMESPAN) {
    return output.badRequest(new IllegalArgumentException(format("Start time must be before stop time, and the " + "total time span can be no bigger than " + "%dms. Time span was %dms.",MAX_TIMESPAN,(stop - start))));
  }
  try {
    FetchRequest request=rrdDb.createFetchRequest(AVERAGE,start,stop);
    return output.ok(new RrdDataRepresentation(request.fetchData()));
  }
 catch (  Exception e) {
    return output.serverError(e);
  }
}
