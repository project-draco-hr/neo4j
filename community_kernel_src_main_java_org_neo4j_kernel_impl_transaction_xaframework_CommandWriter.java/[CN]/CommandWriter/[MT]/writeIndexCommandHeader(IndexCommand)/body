{
  channel.put((byte)((command.getCommandType() << 5) | (command.getValueType() << 2) | (command.getEntityType() << 1)| (needsLong(command.getEntityId()))));
  channel.put((byte)((command.startNodeNeedsLong() << 7) | (command.endNodeNeedsLong() << 6) | (command.getIndexNameId())));
  channel.put(command.getKeyId());
}
