{
  Node orphan=graph.makeNode("o");
  EigenvectorCentrality eigenvectorCentrality=getEigenvectorCentrality(Direction.BOTH,new CostEvaluator<Double>(){
    @Override public Double getCost(    Relationship relationship,    Direction direction){
      return 1d;
    }
  }
,graph.getAllNodes(),graph.getAllEdges(),0.01);
  assertApproximateCentrality(eigenvectorCentrality,"o",0d,0.01);
}
