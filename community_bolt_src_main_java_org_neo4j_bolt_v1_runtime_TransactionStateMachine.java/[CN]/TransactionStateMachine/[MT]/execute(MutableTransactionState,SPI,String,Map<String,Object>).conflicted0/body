{
  try {
    return executeQuery(ctx,spi,statement,params);
  }
 catch (  Throwable e) {
    if (ctx.currentTransaction != null) {
      try {
        ctx.currentTransaction.failure();
        ctx.currentTransaction.close();
      }
  finally {
        ctx.currentTransaction=null;
      }
    }
    throw e;
  }
}
