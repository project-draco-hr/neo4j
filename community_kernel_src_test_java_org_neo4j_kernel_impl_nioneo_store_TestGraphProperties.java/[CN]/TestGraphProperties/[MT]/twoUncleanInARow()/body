{
  String storeDir="dir";
  EphemeralFileSystemAbstraction snapshot=produceUncleanStore(fileSystem,storeDir);
  snapshot=produceUncleanStore(snapshot,storeDir);
  snapshot=produceUncleanStore(snapshot,storeDir);
