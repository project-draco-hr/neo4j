{
  StoreUpgrader upgrader=new StoreUpgrader(ALLOW_UPGRADE,fs,StoreUpgrader.NO_MONITOR,NullLogProvider.getInstance());
  upgrader.addParticipant(new SchemaIndexMigrator(fs));
  upgrader.addParticipant(new StoreMigrator(monitor,fs,pageCache,config,NullLogService.getInstance()));
  return upgrader;
}
