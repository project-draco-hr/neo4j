{
  Config allowUpgrade=new Config(MapUtil.stringMap(GraphDatabaseSettings.allow_store_upgrade.name(),"true"));
  StoreUpgrader upgrader=new StoreUpgrader(upgradableDatabase,monitor,allowUpgrade,fs,NullLogProvider.getInstance());
  upgrader.addParticipant(new SchemaIndexMigrator(fs,schemaIndexProvider,labelScanStoreProvider));
  upgrader.addParticipant(new StoreMigrator(fs,pageCache,config,NullLogService.getInstance(),schemaIndexProvider));
  return upgrader;
}
