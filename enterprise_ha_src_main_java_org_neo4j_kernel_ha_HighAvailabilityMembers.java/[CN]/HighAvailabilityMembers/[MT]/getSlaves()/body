{
  List<Member> members=Arrays.asList(internalGetMembers());
  Iterable<Member> aliveMembers=new FilteringIterable<Member>(members,ALIVE_SLAVE);
  return new IterableWrapper<Slave,Member>(aliveMembers){
    @Override protected Slave underlyingObjectToObject(    Member member){
      return member.slaveClient;
    }
  }
;
}
