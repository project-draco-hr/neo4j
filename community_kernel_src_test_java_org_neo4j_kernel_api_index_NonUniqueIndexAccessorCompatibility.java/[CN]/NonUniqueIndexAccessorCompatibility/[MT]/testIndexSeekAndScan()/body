{
  updateAndCommit(asList(NodePropertyUpdate.add(1L,PROPERTY_KEY_ID,"a",new long[]{1000}),NodePropertyUpdate.add(2L,PROPERTY_KEY_ID,"a",new long[]{1000}),NodePropertyUpdate.add(3L,PROPERTY_KEY_ID,"b",new long[]{1000})));
  assertThat(getAllNodesWithProperty("a"),equalTo(asList(1L,2L)));
  assertThat(getAllNodes(),equalTo(asList(1L,2L,3L)));
}
