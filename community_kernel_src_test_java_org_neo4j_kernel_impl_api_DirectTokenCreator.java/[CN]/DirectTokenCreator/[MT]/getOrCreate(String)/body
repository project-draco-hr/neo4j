{
  RecordAccessSet recordAccess=new DirectRecordAccessSet(neoStore);
  int id=(int)neoStore.getRelationshipTypeTokenStore().nextId();
  org.neo4j.kernel.impl.nioneo.xa.TokenCreator<RelationshipTypeTokenRecord> creator=new org.neo4j.kernel.impl.nioneo.xa.TokenCreator<>(neoStore.getRelationshipTypeTokenStore());
  creator.createToken(name,id,recordAccess.getRelationshipTypeTokenChanges());
  recordAccess.close();
  return id;
}
