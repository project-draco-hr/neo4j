{
  int unique=0;
  for (int i=0; i < types.length; i++) {
    int type=types[i];
    for (int j=0; j < unique; j++) {
      if (type == types[j]) {
        type=-1;
        break;
      }
    }
    if (type != -1) {
      types[unique++]=types[i];
    }
  }
  if (unique < types.length) {
    types=Arrays.copyOf(types,unique);
  }
  return types;
}
