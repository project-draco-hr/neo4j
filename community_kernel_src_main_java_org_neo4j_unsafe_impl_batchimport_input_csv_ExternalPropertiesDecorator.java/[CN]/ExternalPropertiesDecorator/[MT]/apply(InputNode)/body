{
  Object id=from.id();
  if (currentExternal != null) {
    if (id.equals(currentExternal.id())) {
      decorate(from);
      currentExternal=null;
    }
 else {
      return from;
    }
  }
  while (deserializer.hasNext()) {
    currentExternal=deserializer.next();
    if (id.equals(currentExternal.id())) {
      decorate(from);
    }
 else {
      break;
    }
  }
  return from;
}
