{
  this.updateBehaviour=updateBehaviour;
  CharSeeker dataStream=data.create(config).stream();
  Header header=headerFactory.create(dataStream,config,idType);
  this.deserializer=new InputEntityDeserializer<>(header,dataStream,config.delimiter(),new InputNodeDeserialization(dataStream,header,new Groups(),idType.idsAreExternal()),Functions.<InputNode>identity(),Validators.<InputNode>emptyValidator(),badCollector);
}
