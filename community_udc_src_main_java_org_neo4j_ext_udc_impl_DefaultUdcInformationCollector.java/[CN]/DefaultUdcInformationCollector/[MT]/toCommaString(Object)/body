{
  StringBuilder result=new StringBuilder();
  if (values instanceof Iterable) {
    for (    Object agent : (Iterable)values) {
      if (agent == null)       continue;
      if (result.length() > 0)       result.append(",");
      result.append(agent);
    }
  }
 else {
    result.append(values);
  }
  return result.toString();
}
