{
  return new LabelCursor(){
    int label=-1;
    int idx=0;
    @Override public boolean seek(    int labelId){
      for (      int label : labels) {
        if (label == labelId) {
          this.label=label;
          return true;
        }
      }
      return false;
    }
    @Override public int getLabel(){
      return label;
    }
    @Override public boolean next(){
      if (idx < labels.size()) {
        this.label=labels.get(idx);
        idx++;
        return true;
      }
 else {
        return false;
      }
    }
    @Override public void close(){
      idx=0;
    }
  }
;
}
