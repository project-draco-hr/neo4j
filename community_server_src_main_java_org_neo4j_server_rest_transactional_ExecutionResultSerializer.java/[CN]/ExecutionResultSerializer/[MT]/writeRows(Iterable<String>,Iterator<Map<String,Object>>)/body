{
  out.writeArrayFieldStart("data");
  try {
    while (data.hasNext()) {
      Map<String,Object> row=data.next();
      out.writeStartObject();
      try {
        out.writeArrayFieldStart("row");
        try {
          for (          String key : columns) {
            Object val=row.get(key);
            out.writeObject(val);
          }
        }
  finally {
          out.writeEndArray();
        }
      }
  finally {
        out.writeEndObject();
      }
    }
  }
  finally {
    out.writeEndArray();
  }
}
