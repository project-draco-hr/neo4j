{
  out.writeArrayFieldStart("data");
  try {
    data.accept(new Result.ResultVisitor<IOException>(){
      @Override public boolean visit(      Result.ResultRow row) throws IOException {
        out.writeStartObject();
        try {
          writer.write(out,columns,row);
        }
  finally {
          out.writeEndObject();
        }
        return true;
      }
    }
);
  }
  finally {
    out.writeEndArray();
    data.close();
  }
}
