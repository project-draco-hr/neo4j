{
  out.writeArrayFieldStart("data");
  try {
    while (data.hasNext()) {
      Map<String,Object> row=data.next();
      out.writeStartObject();
      try {
        writer.write(out,columns,row);
      }
  finally {
        out.writeEndObject();
      }
    }
  }
  finally {
    out.writeEndArray();
    data.close();
  }
}
