{
  String storeDir=TargetDirectory.forTest(TestMatchingOfCircularPattern.class).makeGraphDbDir().getAbsolutePath();
  graphdb=new TestGraphDatabaseFactory().newEmbeddedDatabase(storeDir);
  Transaction tx=graphdb.beginTx();
  try {
    setupGraph();
    tx.success();
  }
  finally {
    tx.finish();
  }
}
