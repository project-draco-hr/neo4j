{
  @SuppressWarnings("unchecked") Function<SchemaRule,T> ruleConversion=(Function)conversion;
  return map(ruleConversion,filter(new Predicate<SchemaRule>(){
    @SuppressWarnings("unchecked") @Override public boolean test(    SchemaRule rule){
      return rule.getLabel() == labelId && ruleType.isAssignableFrom(rule.getKind().getRuleClass()) && predicate.test((R)rule);
    }
  }
,loadAllSchemaRules()));
}
