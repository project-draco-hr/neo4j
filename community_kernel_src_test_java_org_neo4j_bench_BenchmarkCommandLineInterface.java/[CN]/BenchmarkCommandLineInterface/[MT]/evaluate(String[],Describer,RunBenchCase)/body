{
  if (args.length == 1) {
    if ("describe".equals(args[0])) {
      describer.describe(System.out);
      return 0;
    }
    if ("run".equals(args[0])) {
      return benchCase.run(new BasicParameters(new File(getProperty("outputResultsFile")),parseInt(getProperty("totalDuration")) * 1000));
    }
  }
  System.err.println("Usage: specify 'run' or 'describe'");
  return 1;
}
