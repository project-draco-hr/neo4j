{
  super(new NeoServerBootstrapper(),addressResolver,startupHealthCheck,configFile,jetty6WebServer,serverModulesOrDefault(serverModules));
  this.configFile=configFile;
  this.dir=dir;
  if (lastStarted != null) {
    try {
      throw lastStarted;
    }
  finally {
      lastStarted=null;
    }
  }
}
