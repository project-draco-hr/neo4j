{
  StringBuilder builder=new StringBuilder("Member[" + member);
  if (available != null)   builder.append(", available:" + available);
  if (alive != null)   builder.append(", alive:" + alive);
  if (haRole != null)   builder.append(", haRole:" + haRole);
  if (uris != null)   builder.append(", uris:" + uris);
  return builder.append("]").toString();
}
