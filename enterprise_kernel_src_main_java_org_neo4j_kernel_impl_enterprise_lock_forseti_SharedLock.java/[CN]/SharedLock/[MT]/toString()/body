{
  int refCountSnapshot=refCount.get();
  if (isUpdateLock(refCountSnapshot)) {
    return "UpdateLock{" + "objectId=" + System.identityHashCode(this) + ", refCount="+ numberOfHolders(refCountSnapshot)+ ", owner="+ updateHolder+ '}';
  }
 else {
    return "SharedLock{" + "objectId=" + System.identityHashCode(this) + ", refCount="+ refCountSnapshot+ '}';
  }
}
