{
  checkIfDestroyed();
  if (startIsOk) {
    removeListeners();
    reActiveEventListeners=null;
    eventElements=null;
    destroyed=true;
    proActiveEventListeners=new HashMap<Event,List<ProActiveEventListener>>();
    reActiveEventListeners=new HashMap<Event,List<ReActiveEventListener>>();
    eventElements=new LinkedList<EventElement>();
    startIsOk=true;
    destroyed=true;
  }
 else {
    log.severe("EventModule not in stopped state");
  }
}
