{
  System.out.println("Found version: " + version);
  if ("RelationshipTypeStore v0.9.1".endsWith(version)) {
    String base=getStorageFileName().substring(0,getStorageFileName().lastIndexOf("relationshiptypestore.db"));
    if (new File(base + "propertystore.db.index").exists()) {
      return;
    }
    try {
      convertRelTypes();
    }
 catch (    Exception e) {
      e.printStackTrace();
      throw new RuntimeException("Unable to convert encoding in relationship type store ");
    }
  }
}
