{
  for (  Map.Entry<IndexDescriptor,IndexPopulation> entry : populations.entrySet()) {
    IndexDescriptor descriptor=entry.getKey();
    IndexPopulation population=entry.getValue();
    try {
      population.flip();
      populations.remove(descriptor);
    }
 catch (    Throwable t) {
      fail(descriptor,t);
    }
  }
}
