{
  for (int i=0; i < populations.size(); ) {
    IndexPopulation population=populations.get(i);
    log.info("Index population started: [%s]",population.getIndexUserDescription());
    try {
      population.populator.create();
      i++;
    }
 catch (    Throwable t) {
      fail(i,t);
    }
  }
}
