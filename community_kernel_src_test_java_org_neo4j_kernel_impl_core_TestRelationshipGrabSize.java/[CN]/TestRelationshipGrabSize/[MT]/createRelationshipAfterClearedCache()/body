{
  beginTx();
  Node node1=db.createNode();
  Node node2=db.createNode();
  int expectedCount=0;
  for (int i=0; i < 150; i++) {
    node1.createRelationshipTo(node2,TEST);
    expectedCount++;
  }
  finishTx(true);
  beginTx();
  for (int i=0; i < 50; i++) {
    node1.createRelationshipTo(node2,TEST);
    expectedCount++;
  }
  assertEquals(expectedCount,count(node1.getRelationships()));
  finishTx(true);
  beginTx();
  assertEquals(expectedCount,count(node1.getRelationships()));
  finishTx(false);
}
