{
  super(requestTarget,new Configuration(){
    @Override public long getOldChannelThreshold(){
      return DEFAULT_READ_RESPONSE_TIMEOUT_SECONDS * 1000;
    }
    @Override public int getMaxConcurrentTransactions(){
      return DEFAULT_MAX_NUMBER_OF_CONCURRENT_TRANSACTIONS;
    }
    @Override public int getChunkSize(){
      return chunkSize;
    }
    @Override public HostnamePort getServerAddress(){
      return new HostnamePort(null,port);
    }
  }
,getInstance(),FRAME_LENGTH,new ProtocolVersion(applicationProtocolVersion,ProtocolVersion.INTERNAL_PROTOCOL_VERSION),txVerifier,Clocks.systemClock(),new Monitors().newMonitor(ByteCounterMonitor.class),new Monitors().newMonitor(RequestMonitor.class));
  this.internalProtocolVersion=internalProtocolVersion;
}
