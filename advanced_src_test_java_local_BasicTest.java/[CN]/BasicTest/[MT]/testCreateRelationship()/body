{
  System.out.println("\ntestCreateRelationship");
  Transaction tx=graphDb().beginTx();
  try {
    Node start=graphDb().getReferenceNode();
    Node end=graphDb().createNode();
    Relationship rel=start.createRelationshipTo(end,TestRelations.TEST);
    println("Created relationship: " + rel + " from "+ rel.getStartNode()+ " to "+ rel.getEndNode());
    tx.success();
  }
  finally {
    tx.finish();
  }
}
