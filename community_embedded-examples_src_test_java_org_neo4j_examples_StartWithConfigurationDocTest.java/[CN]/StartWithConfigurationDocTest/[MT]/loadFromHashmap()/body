{
  GraphDatabaseService graphDb=new GraphDatabaseFactory().newEmbeddedDatabaseBuilder(storeDir).setConfig(GraphDatabaseSettings.mapped_memory_total_size,"512M").setConfig(GraphDatabaseSettings.string_block_size,"60").setConfig(GraphDatabaseSettings.array_block_size,"300").newGraphDatabase();
  assertNotNull(graphDb);
  graphDb.shutdown();
}
