{
  List<String> strings=new ArrayList<>();
  while (rels.hasNext()) {
    Relationship rel=rels.next();
    try {
      strings.add(format("(%d)-[%d]->(%d)",rel.getStartNode().getId(),rel.getId(),rel.getEndNode().getId()));
    }
 catch (    Exception e) {
      strings.add(rel + " - " + e);
    }
  }
  return strings;
}
