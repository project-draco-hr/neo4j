{
  assertCanEncodeAndDecodeToSame("DA39A3EE5E6B4B0D3255BFEF95601890AFD80709");
  assertCanEncodeAndDecodeToSame("0123456789" + "ABCDEFABCD" + "0A0B0C0D0E"+ "1A1B1C1D1E"+ "F9E8D7C6B5"+ "A4F3");
  assertCannotEncode("DA39A3EE5E6B4B0D3255BFEF95601890AFD80709" + "0123456789" + "ABCDE");
  assertCannotEncode("AAAAAAAAAA" + "BBBBBBBBBB" + "CCCCCCCCCC"+ "DDDDDDDDDD"+ "EEEEEEEEEE"+ "X");
}
