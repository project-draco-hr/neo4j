{
  if (jsonObject == null) {
    throw new org.neo4j.server.rest.web.PropertyValueException("null value not supported");
  }
  if (jsonObject instanceof String || jsonObject instanceof Number || jsonObject instanceof Boolean) {
  }
 else {
    throw new org.neo4j.server.rest.web.PropertyValueException("Unsupported value type " + jsonObject.getClass() + "."+ " Supported value types are all java primitives (byte, char, short, int, "+ "long, float, double) and String, as well as arrays of all those types");
  }
  return jsonObject;
}
