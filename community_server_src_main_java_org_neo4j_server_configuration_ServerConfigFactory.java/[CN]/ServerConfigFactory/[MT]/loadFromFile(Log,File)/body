{
  if (file == null) {
    return new HashMap<>();
  }
  if (file.exists()) {
    try {
      return MapUtil.load(file);
    }
 catch (    IOException e) {
      log.error("Unable to load config file [%s]: %s",file,e.getMessage());
    }
  }
 else {
    log.warn("Config file [%s] does not exist.",file);
  }
  return new HashMap<>();
}
