{
  Integer lockCount=localLocks.remove(resourceId);
  if (lockCount == null) {
    throw new IllegalStateException(this + " cannot release lock that it does not hold: " + type+ "["+ resourceId+ "].");
  }
  if (lockCount > 1) {
    localLocks.put(resourceId,lockCount - 1);
    return true;
  }
  return false;
}
