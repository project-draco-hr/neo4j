{
  File[] tmpDirs=dbDirectory.listFiles(new FilenameFilter(){
    @Override public boolean accept(    File file,    String name){
      return file.isDirectory() && (name.equals(StoreUpgrader.MIGRATION_DIRECTORY) || name.startsWith(StoreUpgrader.MIGRATION_LEFT_OVERS_DIRECTORY));
    }
  }
);
  assertNotNull("Some IO errors occurred",tmpDirs);
  return Arrays.asList(tmpDirs);
}
