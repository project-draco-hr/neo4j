{
  try {
    ChannelBuffer result=reader.read(timeoutSeconds,TimeUnit.SECONDS);
    if (result == null) {
      throw new ComException("Channel has been closed");
    }
    return result;
  }
 catch (  IOException e) {
    throw new ComException(e);
  }
catch (  InterruptedException e) {
    throw new ComException(e);
  }
}
