{
  byte internalProtocolVersion=(byte)((header[0] & 0x7C) >>> 2);
  if (internalProtocolVersion != Server.INTERNAL_PROTOCOL_VERSION) {
    throw new ComException("Unexpected internal protocol version " + internalProtocolVersion + ", expected "+ Server.INTERNAL_PROTOCOL_VERSION);
  }
  if (header[1] != applicationProtocolVersion) {
    throw new ComException("Unexpected application protocol version " + header[1] + ", expected "+ applicationProtocolVersion);
  }
}
