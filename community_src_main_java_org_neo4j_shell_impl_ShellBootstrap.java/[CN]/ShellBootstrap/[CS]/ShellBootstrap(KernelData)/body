{
  String shellConfig=(String)kernel.getParam("enable_remote_shell");
  if (shellConfig != null) {
    if (shellConfig.contains("=")) {
      enable=true;
      config=parseShellConfigParameter(shellConfig);
    }
 else     if (Boolean.parseBoolean(shellConfig)) {
      enable=true;
      config=null;
    }
 else {
      enable=false;
      config=null;
    }
  }
 else {
    enable=false;
    config=null;
  }
}
