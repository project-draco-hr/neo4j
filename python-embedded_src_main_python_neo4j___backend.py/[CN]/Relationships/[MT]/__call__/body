def __call__(self, *nodes, **properties):
    if (not nodes):
        raise TypeError('No target node specified')
    rels = []
    node = self.__node
    type = self.__type
    for other in nodes:
        rels.append(node.createRelationshipTo(other, type))
    for rel in rels:
        for (key, val) in properties.items():
            rel[key] = val
    if (len(rels) == 1):
        return rels[0]
    return rels
