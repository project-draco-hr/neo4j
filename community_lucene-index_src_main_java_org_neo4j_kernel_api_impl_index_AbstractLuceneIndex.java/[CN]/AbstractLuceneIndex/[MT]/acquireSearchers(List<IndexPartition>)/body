{
  List<PartitionSearcher> searchers=new ArrayList<>(partitions.size());
  try {
    for (    IndexPartition partition : partitions) {
      searchers.add(partition.acquireSearcher());
    }
    return searchers;
  }
 catch (  IOException e) {
    IOUtils.closeAllSilently(searchers);
    throw e;
  }
}
