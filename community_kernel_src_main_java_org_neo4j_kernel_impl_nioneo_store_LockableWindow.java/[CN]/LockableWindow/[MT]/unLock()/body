{
  Thread currentThread=Thread.currentThread();
  if (!locked) {
    throw new LockException(currentThread + " doesn't have window lock on " + this);
  }
  locked=false;
  lockingThread=null;
  while (!waitingThreadList.isEmpty()) {
    LockElement le=waitingThreadList.removeLast();
    if (!le.movedOn) {
      le.thread.interrupt();
      break;
    }
  }
}
