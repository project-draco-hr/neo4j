{
  Thread currentThread=Thread.currentThread();
  if (lockCount == 0) {
    throw new LockException("" + currentThread + " don't have window lock on "+ this);
  }
  lockCount--;
  if (lockCount == 0) {
    lockingThread=null;
    if (waitingThreadList.size() > 0) {
      LockElement le=waitingThreadList.removeLast();
      if (!le.movedOn) {
        le.thread.interrupt();
      }
    }
  }
}
