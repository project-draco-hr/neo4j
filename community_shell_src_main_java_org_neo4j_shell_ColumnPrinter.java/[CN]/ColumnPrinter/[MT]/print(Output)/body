{
  Column firstColumn=columns.get(0);
  for (int line=0; line < firstColumn.size(); line++) {
    if (!rawColumn.print(out,line)) {
      firstColumn.print(out,line);
      for (int col=1; col < columns.size(); col++) {
        columns.get(col).print(out,line);
      }
    }
    out.println();
  }
}
