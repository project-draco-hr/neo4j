{
  StringBuilder builder=new StringBuilder("Node[").append(getId()).append(",used=").append(inUse()).append(",rel=").append(nextRel).append(",prop=").append(getNextProp()).append(",labels=").append(getLabelField()).append(",").append(isLight ? "light" : "heavy");
  if (!isLight && !dynamicLabelRecords.isEmpty()) {
    builder.append(",dynlabels=").append(dynamicLabelRecords);
  }
  return builder.append("]").toString();
}
