{
  return new TxExtractor(){
    @Override public ReadableLogChannel extract(){
      return data;
    }
    @Override public void extract(    LogBuffer buffer){
      try {
        while (data.hasMoreData()) {
          buffer.put(data.get());
        }
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
;
}
