{
  TestLogger logger=new TestLogger();
  Throwable cause=new Throwable("This is a throwable!");
  logger.debug("Debug");
  logger.debug("Debug",cause);
  logger.info("Info");
  logger.info("Info",cause);
  logger.warn("Warn");
  logger.warn("Warn",cause);
  logger.error("Error");
  logger.error("Error",cause);
  logger.assertExactly(debug("Debug"),debug("Debug",cause),info("Info"),info("Info",cause),warn("Warn"),warn("Warn",cause),error("Error"),error("Error",cause));
}
