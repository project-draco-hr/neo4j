{
  TestLogger logger=new TestLogger();
  logger.debug("Debug");
  logger.debug("Debug 2");
  try {
    logger.assertExactly(debug("Debug"));
    fail("Should have failed.");
  }
 catch (  AssertionError e) {
    assertThat(e.getMessage(),equalTo("Got more log calls than expected. The remaining log calls were: \n" + "LogCall{level=DEBUG, message='Debug 2', cause=null, flush=false, marker=LogMarker{name='no_mark'}}\n"));
  }
}
