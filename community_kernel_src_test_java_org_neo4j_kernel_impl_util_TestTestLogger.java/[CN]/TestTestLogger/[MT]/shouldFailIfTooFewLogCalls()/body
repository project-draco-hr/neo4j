{
  TestLogger logger=new TestLogger();
  try {
    logger.assertExactly(debug("Debug"));
    fail("Should have failed.");
  }
 catch (  AssertionError e) {
    assertThat(e.getMessage(),equalTo("Got fewer log calls than expected. The missing log calls were: \n" + "LogCall{level=DEBUG, message='Debug', cause=null, flush=false, marker=LogMarker{name='no_mark'}}\n"));
  }
}
