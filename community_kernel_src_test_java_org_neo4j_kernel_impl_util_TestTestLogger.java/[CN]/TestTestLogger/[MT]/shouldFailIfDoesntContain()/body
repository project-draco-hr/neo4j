{
  TestLogger logger=new TestLogger();
  logger.debug("Debug 1");
  logger.debug("Debug 3");
  try {
    logger.assertAtLeastOnce(debug("Debug 2"),debug("Debug 1"));
  }
 catch (  AssertionError e) {
    assertThat(e.getMessage(),equalTo("These log calls were expected, but never occurred: \n" + "LogCall{ DEBUG, message='Debug 2', cause=null}\n\nActual log calls were:\nLogCall{ DEBUG, " + "message='Debug 1', cause=null}\nLogCall{ DEBUG, message='Debug 3', cause=null}\n"));
  }
}
