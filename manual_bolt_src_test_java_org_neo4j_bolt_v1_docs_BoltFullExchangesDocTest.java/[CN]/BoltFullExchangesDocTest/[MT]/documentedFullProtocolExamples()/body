{
  Collection<Object[]> mappings=new ArrayList<>();
  HostnamePort address=new HostnamePort("localhost:7687");
  for (  DocExchangeExample ex : DocsRepository.docs().read("dev/transport.asciidoc","code[data-lang=\"bolt_exchange\"]",DocExchangeExample.exchange_example)) {
    mappings.add(new Object[]{"Socket    - " + ex.name(),ex,new SecureSocketConnection(),address});
    mappings.add(new Object[]{"WebSocket - " + ex.name(),ex,new SecureWebSocketConnection(),address});
  }
  for (  DocExchangeExample ex : DocsRepository.docs().read("dev/examples.asciidoc","code[data-lang=\"bolt_exchange\"]",DocExchangeExample.exchange_example)) {
    mappings.add(new Object[]{"Socket    - " + ex.name(),ex,new SecureSocketConnection(),address});
    mappings.add(new Object[]{"WebSocket - " + ex.name(),ex,new SecureWebSocketConnection(),address});
  }
  return mappings;
}
