{
  RaftState<RaftTestMember> state=newState();
  final long candidateTerm=state.term() + 1;
  Outcome<RaftTestMember> outcome=role.handler.handle(voteRequest().from(member1).term(candidateTerm).lastLogIndex(0).lastLogTerm(-1).build(),state,log());
  assertEquals(Role.FOLLOWER,outcome.getNewRole());
}
