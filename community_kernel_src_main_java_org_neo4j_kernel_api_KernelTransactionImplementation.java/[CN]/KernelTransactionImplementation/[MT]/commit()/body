{
  try {
    try {
      if (hasTxStateWithChanges()) {
        persistenceCache.apply(this.txState());
      }
    }
  finally {
      try {
        lockHolder.releaseLocks();
      }
 catch (      ReleaseLocksFailedKernelException e) {
        throw new TransactionFailureException(new RuntimeException(e.getMessage(),e));
      }
    }
    close();
  }
  finally {
    closing=false;
  }
}
