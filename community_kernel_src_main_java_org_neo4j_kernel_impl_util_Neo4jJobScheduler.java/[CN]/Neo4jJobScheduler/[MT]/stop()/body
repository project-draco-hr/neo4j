{
  RuntimeException exception=null;
  try {
    if (executor != null) {
      executor.shutdown();
      executor=null;
    }
  }
 catch (  RuntimeException e) {
    exception=e;
  }
  try {
    if (timer != null) {
      timer.cancel();
      timer=null;
    }
  }
 catch (  RuntimeException e) {
    exception=e;
  }
  if (exception != null) {
    throw new RuntimeException("Unable to shut down job scheduler properly.",exception);
  }
}
