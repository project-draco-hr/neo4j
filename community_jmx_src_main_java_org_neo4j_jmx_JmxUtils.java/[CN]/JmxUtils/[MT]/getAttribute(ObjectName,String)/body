{
  try {
    return (T)mbeanServer.getAttribute(objectName,attribute);
  }
 catch (  MBeanException e) {
    throw new RuntimeException(e);
  }
catch (  AttributeNotFoundException e) {
    throw new RuntimeException(e);
  }
catch (  InstanceNotFoundException e) {
    throw new RuntimeException(e);
  }
catch (  ReflectionException e) {
    throw new RuntimeException(e);
  }
}
