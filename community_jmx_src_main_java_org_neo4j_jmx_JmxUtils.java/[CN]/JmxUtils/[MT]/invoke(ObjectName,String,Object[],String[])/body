{
  try {
    return (T)mbeanServer.invoke(objectName,attribute,params,signatur);
  }
 catch (  MBeanException e) {
    throw new RuntimeException(e);
  }
catch (  InstanceNotFoundException e) {
    throw new RuntimeException(e);
  }
catch (  ReflectionException e) {
    throw new RuntimeException(e);
  }
}
