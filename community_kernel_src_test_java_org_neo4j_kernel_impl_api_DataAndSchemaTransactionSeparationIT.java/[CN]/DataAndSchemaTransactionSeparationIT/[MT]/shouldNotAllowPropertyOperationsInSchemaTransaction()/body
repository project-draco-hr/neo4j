{
  Pair<Node,Node> nodes=db.transaction(aPairOfNodes());
  Relationship relationship=db.transaction(relate(nodes));
  for (  Function<GraphDatabaseService,?> operation : new Function[]{propertyRead(Node.class,nodes.first(),"key1"),propertyWrite(Node.class,nodes.first(),"key1","value1"),propertyRead(Relationship.class,relationship,"key1"),propertyWrite(Relationship.class,relationship,"key1","value1")}) {
    db.failingTransaction(afterSchemaOperation(operation));
  }
}
