{
  String name="Mattias";
  Relationship relationship=createRelationshipAndLoadFresh(map("age",5,"name",name));
  assertEquals(withRelationshipOverhead(withArrayOverheadIncludingReferences(withObjectOverhead(8 + 8) + withObjectOverhead(8 + REFERENCE_SIZE + 4+ sizeOf(name)),2)),sizeOfRelationship(relationship));
}
