{
  int index=findNextWhiteSpace(this.line,0);
  this.appName=index == -1 ? this.line : this.line.substring(0,index);
  try {
    this.app=this.server.findApp(this.appName);
  }
 catch (  Exception e) {
    throw new ShellException(e);
  }
  if (this.app == null) {
    throw new ShellException("Unknown command '" + this.appName + "'");
  }
}
