{
  TestServerBuilder serverBuilder=EnterpriseTestServerBuilders.newInProcessBuilder();
  serverBuilder.withConfig(ServerSettings.tls_key_file.name(),ServerTestUtils.getRelativePath(getSharedTestTemporaryFolder(),ServerSettings.tls_key_file));
  serverBuilder.withConfig(ServerSettings.tls_certificate_file.name(),ServerTestUtils.getRelativePath(getSharedTestTemporaryFolder(),ServerSettings.tls_certificate_file));
  return serverBuilder;
}
