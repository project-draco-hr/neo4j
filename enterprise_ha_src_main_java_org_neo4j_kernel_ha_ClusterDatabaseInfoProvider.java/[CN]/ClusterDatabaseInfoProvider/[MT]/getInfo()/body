{
  ClusterMember self=members.getSelf();
  if (self == null)   return null;
  return new ClusterDatabaseInfo(new ClusterMemberInfo(self.getClusterUri().toString(),self.getHAUri() != null,true,self.getHARole(),Iterables.toArray(String.class,Iterables.map(Functions.TO_STRING,self.getRoleURIs()))),0,0);
}
