{
  for (  MemberInfo member : members.getMembers())   if (member.getClusterUri().equals(me)) {
    ClusterMemberInfo info=HighAvailabilityBean.clusterMemberInfo(member);
    return new ClusterDatabaseInfo(info.getInstanceId(),info.isAvailable(),info.getHaRole(),info.getClusterRoles(),info.getUris(),0,0);
  }
  throw new IllegalStateException("Couldn't find any information about myself, can't be right");
}
