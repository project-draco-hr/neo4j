{
  long numberOfDenseNodes=0;
  for (long i=0; i < highestSeenNodeId; i++) {
    if (nodeRelationshipLink.isDense(i)) {
      numberOfDenseNodes++;
    }
  }
  logger.info("# dense nodes: " + numberOfDenseNodes + ", which is "+ round(100D * numberOfDenseNodes / highestSeenNodeId)+ " %");
  super.done();
}
