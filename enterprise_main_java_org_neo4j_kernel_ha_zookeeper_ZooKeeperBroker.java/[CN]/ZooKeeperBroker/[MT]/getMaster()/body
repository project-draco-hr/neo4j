{
  if (masterClient != null) {
    return masterClient;
  }
  Machine master=zooClient.getMaster();
  if (master != null && master.getMachineId() == machineId) {
    throw new ZooKeeperException("I am master, so can't call getMaster() here",new Exception());
  }
  invalidateMaster();
  createMaster(master);
  return masterClient;
}
