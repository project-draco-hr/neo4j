{
  GraphDatabaseAPI graphdb=graphdb("graphdb",false,0);
  try {
    graphdb.getDependencyResolver().resolveDependency(KernelExtensions.class).resolveDependency(DummyExtension.class);
    fail("Extensions were loaded despite configured not to");
  }
 catch (  IllegalArgumentException ex) {
  }
 finally {
    graphdb.shutdown();
  }
}
