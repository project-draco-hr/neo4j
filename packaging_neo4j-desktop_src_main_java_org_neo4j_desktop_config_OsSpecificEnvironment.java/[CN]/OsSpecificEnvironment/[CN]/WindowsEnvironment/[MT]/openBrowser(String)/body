{
  Desktop desktop=getDesktop(Action.BROWSE);
  if (desktop != null) {
    try {
      desktop.browse(new URI(link));
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
catch (    URISyntaxException e) {
      e.printStackTrace();
    }
  }
 else {
    throw new UnsupportedOperationException();
  }
}
