{
  out.printf("[options=\"header\", cols=\"%s\"]%n",cols);
  out.printf("|===%n");
  for (  String columnHeader : header) {
    out.printf("|%s ",columnHeader);
  }
  out.printf("%n");
  for (  Object[] row : rows) {
    for (    Object cell : row) {
      out.printf("|%s ",cell);
    }
    out.printf("%n");
  }
  out.printf("|===%n");
}
