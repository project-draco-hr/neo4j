{
  Map<String,String> customConfig=new HashMap<>();
  if (customThreshold != null) {
    customConfig.put(GraphDatabaseSettings.dense_node_threshold.name(),"" + customThreshold);
  }
  return new StoreFactory(directory,new Config(customConfig),new DefaultIdGeneratorFactory(fs),pageCache,fs,LowLimitV3_0.RECORD_FORMATS,NullLogProvider.getInstance());
}
