{
  jvms=new ArrayList<StandaloneDbCom>();
  try {
    createDeadDbs(numSlaves);
    startUpMaster();
    for (int i=1; i <= numSlaves; i++) {
      File slavePath=dbPath(i);
      StandaloneDbCom slaveJvm=spawnJvm(slavePath,MASTER_PORT + i,i);
      jvms.add(slaveJvm);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
