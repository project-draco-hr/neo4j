{
  final NeoTransactionStoreApplier applier=new NeoTransactionStoreApplier(neoStore,indexingService,cacheAccess,lockService,transactionId,false);
  final PropertyKeyTokenRecord record=new PropertyKeyTokenRecord(42);
  final boolean result=applier.visitPropertyKeyTokenCommand(new Command.PropertyKeyTokenCommand().init(record));
  assertTrue(result);
  verify(propertyKeyTokenStore,times(1)).updateRecord(record);
}
