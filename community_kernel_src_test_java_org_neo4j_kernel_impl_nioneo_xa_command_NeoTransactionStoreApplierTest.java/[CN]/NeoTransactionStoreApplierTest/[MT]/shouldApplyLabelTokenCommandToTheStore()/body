{
  final NeoTransactionStoreApplier applier=new NeoTransactionStoreApplier(neoStore,indexingService,cacheAccess,lockService,transactionId,false);
  final LabelTokenRecord record=new LabelTokenRecord(42);
  final boolean result=applier.visitLabelTokenCommand(new Command.LabelTokenCommand().init(record));
  assertTrue(result);
  verify(labelTokenStore,times(1)).updateRecord(record);
}
