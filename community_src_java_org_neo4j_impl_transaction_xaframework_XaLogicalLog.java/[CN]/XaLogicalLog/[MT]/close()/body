{
  if (xidIdentMap.size() > 0) {
    log.info("Active transactions: " + xidIdentMap.size());
    log.info("Closing dirty log: " + fileName);
    if (writeBuffer != null) {
      writeBuffer.force();
    }
    writeBuffer=null;
    fileChannel.close();
    return;
  }
  if (writeBuffer != null) {
    writeBuffer.force();
  }
  WeakReference<MappedByteBuffer> bufferWeakRef=new WeakReference<MappedByteBuffer>(writeBuffer.getMappedBuffer());
  writeBuffer=null;
  fileChannel.close();
  File file=new File(fileName);
  if (!file.exists()) {
    throw new IOException("Logical log[" + fileName + "] not found");
  }
  boolean deleted=false;
  try {
    deleted=file.delete();
  }
 catch (  Exception e) {
  }
  for (int i=0; i < 3 && !deleted; i++) {
    try {
      Thread.sleep(500);
    }
 catch (    InterruptedException e) {
    }
    try {
      System.gc();
      deleted=file.delete();
    }
 catch (    Exception e) {
    }
  }
  if (!deleted) {
    try {
      clean(bufferWeakRef.get());
      deleted=file.delete();
    }
 catch (    Exception e) {
    }
  }
  if (!deleted) {
    log.warning("Unable to delete clean logical log[" + fileName + "]");
  }
}
