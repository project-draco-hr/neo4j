{
  buffer.clear();
  buffer.limit(4);
  if (fileChannel.read(buffer) != buffer.limit()) {
    return false;
  }
  buffer.flip();
  int identifier=buffer.getInt();
  Xid xid=xidIdentMap.get(identifier);
  if (xid == null) {
    return false;
  }
  if (xaRm.injectPrepare(xid)) {
    xidIdentMap.remove(identifier);
    recoveredTxMap.remove(identifier);
  }
  return true;
}
