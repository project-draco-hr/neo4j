{
  File storeDir=new File("dir");
  fs.get().mkdirs(storeDir);
  StoreFactory storeFactory=new StoreFactory(storeDir,Config.empty(),new DefaultIdGeneratorFactory(fs.get()),pageCacheRule.getPageCache(fs.get()),fs.get(),LowLimitV3_0.RECORD_FORMATS,NullLogProvider.getInstance());
  neoStores=storeFactory.openAllNeoStores(true);
  store=neoStores.getPropertyStore();
  converter=new PropertyPhysicalToLogicalConverter(store);
}
