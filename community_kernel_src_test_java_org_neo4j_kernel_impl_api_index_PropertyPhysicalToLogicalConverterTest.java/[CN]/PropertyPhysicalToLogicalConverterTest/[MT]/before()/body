{
  File storeDir=new File("dir");
  fs.get().mkdirs(storeDir);
  StoreFactory storeFactory=new StoreFactory(StoreFactory.configForStoreDir(new Config(),storeDir),new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),fs.get(),StringLogger.DEV_NULL,new DefaultTxHook());
  storeFactory.createPropertyStore();
  store=storeFactory.newPropertyStore();
  converter=new PropertyPhysicalToLogicalConverter(store);
}
