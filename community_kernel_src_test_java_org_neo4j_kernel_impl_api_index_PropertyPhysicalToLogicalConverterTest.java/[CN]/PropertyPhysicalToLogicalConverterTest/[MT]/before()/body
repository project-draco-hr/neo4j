{
  File storeDir=new File("dir");
  fs.get().mkdirs(storeDir);
  Monitors monitors=new Monitors();
  StoreFactory storeFactory=new StoreFactory(storeDir,new Config(),new DefaultIdGeneratorFactory(),pageCacheRule.getPageCache(fs.get()),fs.get(),NullLogProvider.getInstance(),monitors);
  storeFactory.createPropertyStore();
  store=storeFactory.newPropertyStore();
  converter=new PropertyPhysicalToLogicalConverter(store);
}
