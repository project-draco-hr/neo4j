{
  try {
    return rethrow.exception(this.getCause());
  }
 catch (  HeuristicMixedException e) {
    throw withCause(new SystemException(rethrow.xaCode),e);
  }
catch (  HeuristicRollbackException e) {
    throw withCause(new SystemException(rethrow.xaCode),e);
  }
catch (  RollbackException e) {
    throw withCause(new SystemException(rethrow.xaCode),e);
  }
}
