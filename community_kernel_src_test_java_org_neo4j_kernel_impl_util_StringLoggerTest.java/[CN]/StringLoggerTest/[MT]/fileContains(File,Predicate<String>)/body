{
  BufferedReader reader=new BufferedReader(fileSystem.openAsReader(file,Charset.defaultCharset().name()));
  try {
    String line=null;
    while ((line=reader.readLine()) != null) {
      if (predicate.accept(line)) {
        return true;
      }
    }
    return false;
  }
  finally {
    reader.close();
  }
}
