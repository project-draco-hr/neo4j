{
  if (random.random().nextFloat() < 0.1f) {
    return new InputNode(null,0,0,randomId(random),NO_PROPERTIES,abs(random.random().nextLong()),NO_LABELS,abs(random.random().nextLong()));
  }
  return new InputNode(null,0,0,randomGroup(random,0),randomId(random),randomProperties(random),null,randomLabels(random),null);
}
