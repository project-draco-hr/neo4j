{
  if (random.random().nextFloat() < 0.1f) {
    return new InputRelationship(null,0,0,NO_PROPERTIES,abs(random.random().nextLong()),randomGroup(random,0),randomId(random),randomGroup(random,1),randomId(random),null,abs(random.random().nextInt(Short.MAX_VALUE)));
  }
  return new InputRelationship(null,0,0,randomProperties(random),null,randomGroup(random,0),randomId(random),randomGroup(random,1),randomId(random),randomType(random),null);
}
