def to_docbook(self):
    '\n        Use asciidoc to convert asciidoc_file to DocBook.\n        args is a string containing additional asciidoc arguments.\n        '
    docbook_file = self.dst_path('.xml')
    if self.skip_asciidoc:
        if (not os.path.isfile(docbook_file)):
            die(('missing docbook file: %s' % docbook_file))
        return
    shell(('"%s" --backend docbook -a "a2x-format=%s" %s --out-file "%s" "%s"' % (self.asciidoc, self.format, self.asciidoc_opts, docbook_file, self.asciidoc_file)))
    if ((not self.no_xmllint) and XMLLINT):
        shell(('"%s" --nonet --noout --valid "%s"' % (XMLLINT, docbook_file)))
