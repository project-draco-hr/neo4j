{
  List<TraversalBranch> level=new LinkedList<TraversalBranch>();
  Integer depth=null;
  for (  TraversalBranch source : queue) {
    if (depth == null) {
      depth=source.depth();
    }
 else     if (source.depth() != depth) {
      break;
    }
    while (true) {
      TraversalBranch next=source.next();
      if (next == null) {
        break;
      }
      level.add(next);
    }
  }
  return level;
}
