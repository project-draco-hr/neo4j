{
  Transaction tx=database.getGraph().beginTx();
  try {
    Collection<Long> result=new ArrayList<>();
    for (    Node node : database.getGraph().index().forNodes(indexName).get(key,value)) {
      result.add(node.getId());
    }
    tx.success();
    return result;
  }
  finally {
    tx.finish();
  }
}
