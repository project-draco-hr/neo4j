{
  try (Transaction tx=database.getGraph().beginTransaction(KernelTransaction.Type.implicit,AccessMode.Static.WRITE)){
    try {
      Node referenceNode=database.getGraph().getNodeById(0l);
      tx.success();
      return referenceNode.getId();
    }
 catch (    NotFoundException e) {
      Node newNode=database.getGraph().createNode();
      tx.success();
      return newNode.getId();
    }
  }
 }
