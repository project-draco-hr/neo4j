{
  String query=String.format("CREATE CONSTRAINT ON ()-[r:%s]-() ASSERT exists(r.%s)",typeName,propertyKey);
  database.getGraph().execute(query);
  awaitIndexes();
  return new RelationshipPropertyExistenceConstraintDefinition(mock(InternalSchemaActions.class),DynamicRelationshipType.withName(typeName),propertyKey);
}
