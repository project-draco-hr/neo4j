{
  Tx head=null;
  for (  KernelTransactionImplementation tx : allTransactions) {
    if (tx.isOpen()) {
      Tx current=new Tx(tx,tx.getReuseCount());
      if (head != null) {
        current.next=head;
        head=current;
      }
 else {
        head=current;
      }
    }
  }
  relevantTransactions=head;
  this.snapshotTime=snapshotTime;
}
