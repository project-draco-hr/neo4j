{
  super(fileName,conf,IdType.NEOSTORE_BLOCK,idGeneratorFactory,pageCache,fileSystemAbstraction,stringLogger,versionMismatchHandler,monitors);
  this.relTypeStore=relTypeStore;
  this.labelTokenStore=labelTokenStore;
  this.propStore=propStore;
  this.relStore=relStore;
  this.nodeStore=nodeStore;
  this.schemaStore=schemaStore;
  this.relGroupStore=relGroupStore;
  relGrabSize=conf.get(Configuration.relationship_grab_size);
  setRecovered();
  try {
    if (getCreationTime() != 0 && getStoreVersion() == 0) {
      setStoreVersion(versionStringToLong(CommonAbstractStore.ALL_STORES_VERSION));
      updateHighId();
    }
  }
  finally {
    unsetRecovered();
  }
}
