{
  if (relTypeStore != null) {
    relTypeStore.close();
    relTypeStore=null;
  }
  if (propStore != null) {
    propStore.close();
    propStore=null;
  }
  if (relStore != null) {
    relStore.close();
    relStore=null;
  }
  if (nodeStore != null) {
    nodeStore.close();
    nodeStore=null;
  }
}
