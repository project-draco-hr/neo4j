{
  long pageId=pageIdForRecord(VERSION_POSITION);
  try (PageCursor cursor=storeFile.io(pageId,PF_EXCLUSIVE_LOCK)){
    if (cursor.next()) {
      incrementVersion(cursor);
    }
    return versionField;
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
 finally {
    try {
      storeFile.flush();
    }
 catch (    IOException e) {
      throw new UnderlyingStorageException(e);
    }
  }
}
