{
  if (relTypeStore == null || labelTokenStore == null || propStore == null || relStore == null || nodeStore == null || schemaStore == null || relGroupStore == null) {
    return;
  }
  setRecord(LATEST_TX_POSITION,lastCommittingTx.get());
  super.flushAll();
  relTypeStore.flushAll();
  labelTokenStore.flushAll();
  propStore.flushAll();
  relStore.flushAll();
  nodeStore.flushAll();
  schemaStore.flushAll();
  relGroupStore.flushAll();
}
