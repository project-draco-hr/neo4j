{
  long id;
  try (Transaction tx=db.beginTx()){
    Node theNode=db.createNode();
    id=theNode.getId();
    for (int i=0; i < 1; i++) {
      Node node=db.createNode();
      node.setProperty("" + i,"" + i);
    }
    tx.success();
  }
   try (Transaction tx=db.beginTx()){
    Node node=db.getNodeById(id);
    node.setProperty("0",0);
    tx.success();
  }
   return id;
}
