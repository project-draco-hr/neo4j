{
  long recordId=record.getId();
  return TYPE_FIELD_BYTES + length(record.getNextProp(),NULL) + length(record.getFirstNode())+ length(record.getSecondNode())+ length(getFirstPrevReference(record,recordId))+ getRelativeReferenceLength(record.getFirstNextRel(),recordId)+ length(getSecondPrevReference(record,recordId))+ getRelativeReferenceLength(record.getSecondNextRel(),recordId);
}
