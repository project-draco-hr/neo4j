{
  long id=record.getId();
  return Short.BYTES + length(record.getNextProp(),NULL) + length(record.getFirstNode())+ length(record.getSecondNode())+ length(toRelative(record.getFirstPrevRel(),id))+ length(toRelative(record.getFirstNextRel(),id),NULL)+ length(toRelative(record.getSecondPrevRel(),id))+ length(toRelative(record.getSecondNextRel(),id),NULL);
}
