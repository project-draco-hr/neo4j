{
  super(management);
  NeoStoreXaDataSource nioneodb=KernelBean.getNeoDataSource(management.getKernelData());
  File path;
  try {
    path=new File(nioneodb.getStoreDir()).getCanonicalFile().getAbsoluteFile();
  }
 catch (  IOException e) {
    path=new File(nioneodb.getStoreDir()).getAbsoluteFile();
  }
  this.storePath=path;
}
