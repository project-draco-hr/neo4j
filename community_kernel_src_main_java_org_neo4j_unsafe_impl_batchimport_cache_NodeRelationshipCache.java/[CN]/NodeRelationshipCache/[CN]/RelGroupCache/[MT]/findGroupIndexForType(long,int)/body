{
  long currentIndex=relGroupIndex;
  while (currentIndex != EMPTY) {
    long index=rebase(currentIndex);
    int foundType=array.getShort(index,TYPE_OFFSET);
    if (foundType == type) {
      return currentIndex;
    }
 else     if (foundType > type) {
      break;
    }
    currentIndex=all48Bits(array,index,0);
  }
  return EMPTY;
}
