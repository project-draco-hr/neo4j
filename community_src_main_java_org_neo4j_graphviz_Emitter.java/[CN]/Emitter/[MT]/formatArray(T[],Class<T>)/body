{
  PropertyType formatter=typeMap.get(type);
  StringBuilder result=new StringBuilder("[");
  boolean addComma=false;
  for (  T item : items) {
    if (addComma) {
      result.append(", ");
    }
    result.append(formatter.format(item));
    addComma=true;
  }
  result.append("]");
  return result.toString();
}
