{
  try {
    searcher.incRef();
    Sort sorting=additionalParametersOrNull != null ? additionalParametersOrNull.sorting : null;
    Hits hits=new Hits(searcher.getSearcher(),query,null,sorting);
    return new SearchResult(new HitsIterator(hits),hits.length());
  }
 catch (  IOException e) {
    throw new RuntimeException("Unable to query " + this + " with "+ query,e);
  }
}
