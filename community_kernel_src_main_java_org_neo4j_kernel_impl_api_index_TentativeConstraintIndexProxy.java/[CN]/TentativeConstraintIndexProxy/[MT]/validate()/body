{
  Iterator<IndexEntryConflictException> iterator=failures.iterator();
  if (iterator.hasNext()) {
    Set<ConstraintVerificationFailedKernelException.Evidence> evidence=new HashSet<ConstraintVerificationFailedKernelException.Evidence>();
    do {
      evidence.add(new ConstraintVerificationFailedKernelException.Evidence(iterator.next()));
    }
 while (iterator.hasNext());
    IndexDescriptor descriptor=getDescriptor();
    throw new IndexPopulationFailedKernelException(descriptor,new ConstraintVerificationFailedKernelException(new UniquenessConstraint(descriptor.getLabelId(),descriptor.getPropertyKeyId()),evidence));
  }
}
