{
  Iterator<IndexEntryConflictException> iterator=failures.iterator();
  if (iterator.hasNext()) {
    Set<Evidence> evidences=new HashSet<>();
    do {
      evidences.add(Evidence.of(iterator.next()));
    }
 while (iterator.hasNext());
    IndexDescriptor descriptor=getDescriptor();
    throw new ConstraintVerificationFailedKernelException(new UniquenessConstraint(descriptor.getLabelId(),descriptor.getPropertyKeyId()),evidences);
  }
}
