@staticmethod
def list(args):
    '\n        List all plugin directories (global and local).\n        '
    for d in [os.path.join(d, (Plugin.type + 's')) for d in config.get_load_dirs()]:
        if os.path.isdir(d):
            for f in os.walk(d).next()[1]:
                message.stdout(os.path.join(d, f))
