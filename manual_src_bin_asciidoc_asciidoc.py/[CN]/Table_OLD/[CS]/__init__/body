def __init__(self):
    AbstractBlock.__init__(self)
    self.CONF_ENTRIES += ('template', 'fillchar', 'format', 'colspec', 'headrow', 'footrow', 'bodyrow', 'headdata', 'footdata', 'bodydata')
    self.fillchar = None
    self.format = None
    self.colspec = None
    self.headrow = None
    self.footrow = None
    self.bodyrow = None
    self.headdata = None
    self.footdata = None
    self.bodydata = None
    self.underline = None
    self.isnumeric = False
    self.tablewidth = None
    self.columns = []
    self.check_msg = ''
