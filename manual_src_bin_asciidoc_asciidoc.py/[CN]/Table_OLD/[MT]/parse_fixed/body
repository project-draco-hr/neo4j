def parse_fixed(self, rows):
    'Parse the list of source table rows. Each row item in the returned\n        list contains a list of cell data elements.'
    result = []
    for row in rows:
        data = []
        start = 0
        row = char_decode(row)
        for c in self.columns:
            end = (start + c.rulerwidth)
            if (c is self.columns[(-1)]):
                data.append(char_encode(row[start:]).strip())
            else:
                data.append(char_encode(row[start:end]).strip())
            start = end
        result.append(data)
    return result
