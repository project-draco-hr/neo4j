def isnext(self):
    'Check if this block is next in document reader.'
    result = False
    reader.skip_blank_lines()
    if reader.read_next():
        if (not self.delimiter_reo):
            self.delimiter_reo = re.compile(self.delimiter)
        mo = self.delimiter_reo.match(reader.read_next())
        if mo:
            self.mo = mo
            result = True
    return result
