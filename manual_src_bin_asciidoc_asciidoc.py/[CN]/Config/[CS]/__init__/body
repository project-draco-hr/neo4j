def __init__(self):
    self.sections = OrderedDict()
    self.verbose = False
    self.header_footer = True
    self.tabsize = 8
    self.textwidth = 70
    self.newline = '\r\n'
    self.pagewidth = None
    self.pageunits = None
    self.outfilesuffix = ''
    self.subsnormal = SUBS_NORMAL
    self.subsverbatim = SUBS_VERBATIM
    self.tags = {}
    self.specialchars = {}
    self.specialwords = {}
    self.replacements = OrderedDict()
    self.replacements2 = OrderedDict()
    self.replacements3 = OrderedDict()
    self.specialsections = {}
    self.quotes = OrderedDict()
    self.fname = ''
    self.conf_attrs = {}
    self.cmd_attrs = {}
    self.loaded = []
    self.include1 = {}
    self.dumping = False
    self.filters = []
