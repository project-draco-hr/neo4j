def load_from_dirs(self, filename, dirs=None, include=[]):
    '\n        Load conf file from dirs list.\n        If dirs not specified try all the well known locations.\n        Return False if no file was sucessfully loaded.\n        '
    count = 0
    for f in self.find_in_dirs(filename, dirs):
        if self.load_file(f, include=include):
            count += 1
    return (count != 0)
