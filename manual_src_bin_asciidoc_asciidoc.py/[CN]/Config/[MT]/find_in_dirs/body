def find_in_dirs(self, filename, dirs=None):
    '\n        Find conf files from dirs list.\n        Return list of found file paths.\n        Return empty list if not found in any of the locations.\n        '
    result = []
    if (dirs is None):
        dirs = self.get_load_dirs()
    for d in dirs:
        f = os.path.join(d, filename)
        if os.path.isfile(f):
            result.append(f)
    return result
