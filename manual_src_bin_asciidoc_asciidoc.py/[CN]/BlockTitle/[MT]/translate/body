@staticmethod
def translate():
    assert (Lex.next() is BlockTitle)
    reader.read()
    if (not Title.subs):
        Title.subs = config.subsnormal
    s = Lex.subs((BlockTitle.title,), Title.subs)
    s = writer.newline.join(s)
    if (not s):
        message.warning('blank block title')
    BlockTitle.title = s
