@staticmethod
def dosubs(title):
    '\n        Perform title substitutions.\n        '
    if (not Title.subs):
        Title.subs = config.subsnormal
    title = Lex.subs((title,), Title.subs)
    title = writer.newline.join(title)
    if (not title):
        message.warning('blank section title')
    return title
