@staticmethod
def translate(skipsubs=False):
    'Parse the Title.attributes and Title.level from the reader. The\n        real work has already been done by parse().'
    assert (Lex.next() in (Title, FloatingTitle))
    for i in range(Title.linecount):
        reader.read()
    Title.setsectname()
    if (not skipsubs):
        Title.attributes['title'] = Title.dosubs(Title.attributes['title'])
