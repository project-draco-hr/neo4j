def write(self, *args, **kwargs):
    'Iterates arguments, writes tuple and list arguments one line per\n        element, else writes argument as single line. If no arguments writes\n        blank line. If argument is None nothing is written. self.newline is\n        appended to each line.'
    if (('trace' in kwargs) and (len(args) > 0)):
        trace(kwargs['trace'], args[0])
    if (len(args) == 0):
        self.write_line()
        self.lines_out = (self.lines_out + 1)
    else:
        for arg in args:
            if is_array(arg):
                for s in arg:
                    self.write_line(s)
            elif (arg is not None):
                self.write_line(arg)
