@staticmethod
def translate():
    assert (Lex.next() is AttributeList)
    reader.read()
    attrs = {}
    d = AttributeList.match.groupdict()
    for (k, v) in d.items():
        if (v is not None):
            if (k == 'attrlist'):
                v = subs_attrs(v)
                if v:
                    parse_attributes(v, attrs)
            else:
                AttributeList.attrs[k] = v
    AttributeList.subs(attrs)
    AttributeList.attrs.update(attrs)
