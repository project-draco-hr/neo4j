@staticmethod
def consume(d={}):
    "Add attribute list to the dictionary 'd' and reset the list."
    if AttributeList.attrs:
        d.update(AttributeList.attrs)
        AttributeList.attrs = {}
        if ('options' in d):
            options = parse_options(d['options'], (), 'illegal option name')
            for option in options:
                d[(option + '-option')] = ''
