def eof(self):
    'Returns True if all lines have been read.'
    if (len(self.next) == 0):
        if self.parent:
            self.closefile()
            assign(self, self.parent)
            document.attributes['infile'] = self.infile
            document.attributes['indir'] = self.indir
            return Reader1.eof(self)
        else:
            return True
    else:
        return False
