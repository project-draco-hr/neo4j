def load_tags(self, sections):
    '\n        Load listtags-* conf file sections to self.tags.\n        '
    for section in sections.keys():
        mo = re.match('^listtags-(?P<name>\\w+)$', section)
        if mo:
            name = mo.group('name')
            if (name in self.tags):
                d = self.tags[name]
            else:
                d = AttrDict()
            parse_entries(sections.get(section, ()), d)
            for k in d.keys():
                if (k not in self.TAGS):
                    message.warning(('[%s] contains illegal list tag: %s' % (section, k)))
            self.tags[name] = d
