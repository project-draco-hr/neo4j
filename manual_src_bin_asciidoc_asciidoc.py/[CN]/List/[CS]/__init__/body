def __init__(self):
    AbstractBlock.__init__(self)
    self.CONF_ENTRIES += ('type', 'tags')
    self.PARAM_NAMES += ('tags',)
    self.type = None
    self.tags = None
    self.tag = None
    self.label = None
    self.text = None
    self.index = None
    self.type = None
    self.ordinal = None
    self.number_style = None
