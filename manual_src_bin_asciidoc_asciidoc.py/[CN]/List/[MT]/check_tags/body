def check_tags(self):
    ' Check that all necessary tags are present. '
    tags = set(Lists.TAGS)
    if (self.type != 'labeled'):
        tags = tags.difference(['entry', 'label', 'term'])
    missing = tags.difference(self.tag.keys())
    if missing:
        self.error(('missing tag(s): %s' % ','.join(missing)), halt=True)
