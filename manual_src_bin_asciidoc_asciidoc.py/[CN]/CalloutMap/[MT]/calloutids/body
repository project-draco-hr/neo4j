def calloutids(self, listindex):
    if (listindex in self.comap):
        result = ''
        for coindex in self.comap[listindex]:
            result += (' ' + self.calloutid(self.listnumber, coindex))
        return result.strip()
    else:
        message.warning(('no callouts refer to list item ' + str(listindex)))
        return ''
