@staticmethod
def parse_align_spec(align_spec):
    '\n        Parse AsciiDoc cell alignment specifier and return 2-tuple with\n        horizonatal and vertical alignment names. Unspecified alignments\n        set to None.\n        '
    result = (None, None)
    if align_spec:
        mo = re.match('^([<\\^>])?(\\.([<\\^>]))?$', align_spec)
        if mo:
            result = (Table.ALIGN.get(mo.group(1)), Table.VALIGN.get(mo.group(3)))
    return result
