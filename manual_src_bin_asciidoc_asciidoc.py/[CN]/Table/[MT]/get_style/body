def get_style(self, prefix):
    "\n        Return the style dictionary whose name starts with 'prefix'.\n        "
    if (prefix is None):
        return None
    names = self.styles.keys()
    names.sort()
    for name in names:
        if name.startswith(prefix):
            return self.styles[name]
    else:
        self.error(('missing style: %s*' % prefix))
        return None
