def parse_entries(entries, dict, unquote=False, unique_values=False, allow_name_only=False, escape_delimiter=True):
    "Parse name=value entries from  from lines of text in 'entries' into\n    dictionary 'dict'. Blank lines are skipped."
    entries = config.expand_templates(entries)
    for entry in entries:
        if (entry and (not parse_entry(entry, dict, unquote, unique_values, allow_name_only, escape_delimiter))):
            raise EAsciiDoc, ('malformed section entry: %s' % entry)
