def re_join(relist):
    'Join list of regular expressions re1,re2,... to single regular\n    expression (re1)|(re2)|...'
    if (len(relist) == 0):
        return None
    result = []
    for s in relist:
        result.append(re.sub('\\?P<\\S+?>', '', s))
    result = ')|('.join(result)
    result = (('(' + result) + ')')
    return result
