def parse_named_attributes(s, attrs):
    'Update a attrs dictionary with name="value" attributes from the s string.\n    Returns False if invalid syntax.\n    Example:\n    attrs: \'star="sun",planet="earth"\'\n    dict: {\'planet\':\'earth\', \'star\':\'sun\'}\n    '

    def f(**keywords):
        return keywords
    try:
        d = {}
        d = get_kwargs(s)
        attrs.update(d)
        return True
    except Exception:
        return False
