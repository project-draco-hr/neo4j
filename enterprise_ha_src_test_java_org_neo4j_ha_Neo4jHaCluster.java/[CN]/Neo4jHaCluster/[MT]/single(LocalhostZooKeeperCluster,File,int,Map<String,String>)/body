{
  return (HighlyAvailableGraphDatabase)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(storeDir.getAbsolutePath()).setConfig(HaSettings.server_id,"1").setConfig(HaSettings.coordinators,zk.getConnectionString()).setConfig(HaSettings.server,"127.0.0.1:" + haPort).setConfig(ShellSettings.remote_shell_enabled,GraphDatabaseSetting.TRUE).setConfig(GraphDatabaseSettings.keep_logical_logs,GraphDatabaseSetting.TRUE).setConfig(config).newGraphDatabase();
}
