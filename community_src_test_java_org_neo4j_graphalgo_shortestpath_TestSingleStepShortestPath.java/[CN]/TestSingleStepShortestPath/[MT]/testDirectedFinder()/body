{
  graph.makeEdgeChain("a,b,c,d,e,f,m");
  graph.makeEdgeChain("a,g,h,i,j,k,l,m");
  PathFinder finder=new SingleStepShortestPathsFinder(graphDb,4,TraversalFactory.expanderForTypes(MyRelTypes.R1,Direction.OUTGOING));
  assertPaths(finder.findPaths(graph.getNode("a"),graph.getNode("j")),"a,g,h,i,j");
}
