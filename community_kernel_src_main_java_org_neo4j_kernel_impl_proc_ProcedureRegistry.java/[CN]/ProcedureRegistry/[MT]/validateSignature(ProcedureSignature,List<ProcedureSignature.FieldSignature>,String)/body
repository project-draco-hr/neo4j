{
  Set<String> names=new HashSet<>();
  for (  ProcedureSignature.FieldSignature field : fields) {
    if (!names.add(field.name())) {
      throw new ProcedureException(Status.Procedure.FailedRegistration,"Procedure `%s` cannot be registered, because it contains a duplicated " + fieldType + " field, '%s'. "+ "You need to rename or remove one of the duplicate fields.",signature.toString(),field.name());
    }
  }
}
