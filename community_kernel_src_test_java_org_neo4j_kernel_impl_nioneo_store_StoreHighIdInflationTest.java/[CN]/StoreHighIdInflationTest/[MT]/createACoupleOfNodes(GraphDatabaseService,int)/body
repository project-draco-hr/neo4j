{
  Transaction tx=db.beginTx();
  try {
    long last=0;
    for (int i=0; i < count; i++) {
      Node node=db.createNode();
      node.setProperty("key","A very very very loooooooooooooooooooooooooooooooooooooooooooooong string " + "that should spill over into a dynamic record");
      last=node.getId();
    }
    tx.success();
    return last;
  }
  finally {
    tx.finish();
  }
}
