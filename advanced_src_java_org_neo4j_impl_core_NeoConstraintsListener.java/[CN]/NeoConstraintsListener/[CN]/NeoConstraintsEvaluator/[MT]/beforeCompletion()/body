{
  getListener().removeThisEvaluator();
  if (deletedNodes != null) {
    Iterator<NodeImpl> itr=deletedNodes.values().iterator();
    while (itr.hasNext()) {
      NodeImpl node=itr.next();
      if (node.hasRelationships()) {
        log.severe("Deleted Node[" + node + "] still has relationship.");
        setRollbackOnly();
      }
    }
  }
}
