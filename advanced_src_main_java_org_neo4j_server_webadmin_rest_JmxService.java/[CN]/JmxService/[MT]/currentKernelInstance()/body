{
  if (DatabaseLocator.isLocalDatabase()) {
    Kernel kernelBean=((EmbeddedGraphDatabase)getGraphDatabase()).getManagementBean(Kernel.class);
    return addHeaders(Response.ok("\"" + kernelBean.getMBeanQuery().toString() + "\"",JsonRenderers.DEFAULT.getMediaType())).build();
  }
 else {
    return addHeaders(Response.ok("null",JsonRenderers.DEFAULT.getMediaType())).build();
  }
}
