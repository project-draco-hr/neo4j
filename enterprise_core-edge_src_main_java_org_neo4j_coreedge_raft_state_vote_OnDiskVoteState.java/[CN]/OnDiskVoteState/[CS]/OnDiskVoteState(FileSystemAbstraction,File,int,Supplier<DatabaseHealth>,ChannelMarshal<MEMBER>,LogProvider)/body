{
  File fileA=new File(stateDir,FILENAME + "a");
  File fileB=new File(stateDir,FILENAME + "b");
  InMemoryVoteStateChannelMarshal<MEMBER> marshal=new InMemoryVoteStateChannelMarshal<>(memberByteBufferMarshal);
  VoteStateRecoveryManager<MEMBER> recoveryManager=new VoteStateRecoveryManager<>(fileSystemAbstraction,marshal);
  StateRecoveryManager.RecoveryStatus recoveryStatus=recoveryManager.recover(fileA,fileB);
  this.inMemoryVoteState=recoveryManager.readLastEntryFrom(fileSystemAbstraction,recoveryStatus.previouslyActive());
  this.statePersister=new StatePersister<>(fileA,fileB,fileSystemAbstraction,numberOfEntriesBeforeRotation,marshal,recoveryStatus.previouslyInactive(),databaseHealthSupplier);
  logProvider.getLog(getClass()).info("State restored, last term is %d",inMemoryVoteState.term());
}
