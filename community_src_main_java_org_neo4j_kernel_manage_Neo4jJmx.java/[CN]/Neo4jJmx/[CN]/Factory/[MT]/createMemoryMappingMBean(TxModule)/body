{
  NeoStoreXaDataSource datasource=(NeoStoreXaDataSource)txModule.getXaDataSourceManager().getXaDataSource("nioneodb");
  if (!register(new MemoryMappingMonitor.MXBeanImplementation(instanceId,datasource))) {
    if (!register(new MemoryMappingMonitor.MemoryMapping(instanceId,datasource)))     failedToRegister("MemoryMappingMBean");
  }
}
