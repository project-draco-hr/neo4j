{
  if (files.isEmpty()) {
    return;
  }
  System.out.println(name + ":");
  int i=0;
  String indent="  ";
  for (  Option<File[]> group : files) {
    if (i++ > 0) {
      System.out.println();
    }
    if (group.metadata() != null) {
      System.out.println(indent + ":" + group.metadata());
    }
    for (    File file : group.value()) {
      System.out.println(indent + file);
    }
  }
  System.out.println();
}
