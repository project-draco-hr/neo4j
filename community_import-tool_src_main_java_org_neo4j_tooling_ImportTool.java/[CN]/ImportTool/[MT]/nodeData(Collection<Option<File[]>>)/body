{
  return new IterableWrapper<DataFactory<InputNode>,Option<File[]>>(nodesFiles){
    @Override protected DataFactory<InputNode> underlyingObjectToObject(    Option<File[]> input){
      Function<InputNode,InputNode> decorator=input.metadata() != null ? additiveLabels(input.metadata().split(":")) : NO_NODE_DECORATOR;
      return data(decorator,input.value());
    }
  }
;
}
