{
  return new IterableWrapper<DataFactory<InputNode>,Option<File[]>>(files){
    @Override protected DataFactory<InputNode> underlyingObjectToObject(    Option<File[]> group){
      Function<InputNode,InputNode> decorator=group.metadata() != null ? additiveLabels(group.metadata().split(":")) : Functions.<InputNode>identity();
      return data(decorator,group.value());
    }
  }
;
}
