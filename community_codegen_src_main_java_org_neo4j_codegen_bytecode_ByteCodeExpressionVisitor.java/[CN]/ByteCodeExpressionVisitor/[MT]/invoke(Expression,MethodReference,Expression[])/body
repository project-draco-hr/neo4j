{
  target.accept(this);
  for (  Expression argument : arguments) {
    argument.accept(this);
  }
  if (Modifier.isInterface(method.owner().modifiers())) {
    methodVisitor.visitMethodInsn(Opcodes.INVOKEINTERFACE,byteCodeName(method.owner()),method.name(),desc(method),true);
  }
 else   if (method.isConstructor()) {
    methodVisitor.visitMethodInsn(Opcodes.INVOKESPECIAL,byteCodeName(method.owner()),method.name(),desc(method),false);
  }
 else {
    methodVisitor.visitMethodInsn(Opcodes.INVOKEVIRTUAL,byteCodeName(method.owner()),method.name(),desc(method),false);
  }
}
