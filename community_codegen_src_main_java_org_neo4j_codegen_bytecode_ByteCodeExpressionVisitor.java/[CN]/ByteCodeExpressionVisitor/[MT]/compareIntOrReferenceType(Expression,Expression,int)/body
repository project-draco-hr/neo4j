{
  lhs.accept(this);
  rhs.accept(this);
  Label l0=new Label();
  methodVisitor.visitJumpInsn(opcode,l0);
  methodVisitor.visitInsn(ICONST_1);
  Label l1=new Label();
  methodVisitor.visitJumpInsn(GOTO,l1);
  methodVisitor.visitLabel(l0);
  methodVisitor.visitInsn(ICONST_0);
  methodVisitor.visitLabel(l1);
}
