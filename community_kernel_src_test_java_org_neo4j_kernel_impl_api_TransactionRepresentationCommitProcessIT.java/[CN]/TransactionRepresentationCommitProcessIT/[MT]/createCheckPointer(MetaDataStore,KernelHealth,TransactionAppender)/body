{
  CountCommittedTransactionThreshold committedTransactionThreshold=new CountCommittedTransactionThreshold(1);
  final StoreFlusher storeFlusher=new StoreFlusher(neoStores,mock(IndexingService.class),mock(LabelScanStore.class),Iterables.<IndexImplementation>empty());
