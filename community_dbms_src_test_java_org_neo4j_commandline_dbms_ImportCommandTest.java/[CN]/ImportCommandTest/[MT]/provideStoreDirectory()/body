{
  File storeDir=testDir.graphDbDir();
  GraphDatabaseService db=null;
  try {
    db=new TestGraphDatabaseFactory().newEmbeddedDatabase(storeDir);
    try (Transaction transaction=db.beginTx()){
      db.createNode();
      transaction.success();
    }
   }
  finally {
    if (db != null) {
      db.shutdown();
    }
  }
  return storeDir;
}
