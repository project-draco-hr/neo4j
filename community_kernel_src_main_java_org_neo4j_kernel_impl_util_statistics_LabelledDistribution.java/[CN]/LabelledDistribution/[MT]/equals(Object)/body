{
  if (this == obj) {
    return true;
  }
  if (null == obj || getClass() != obj.getClass()) {
    return false;
  }
  LabelledDistribution<T> other=(LabelledDistribution<T>)obj;
  return equalityTolerance == other.equalityTolerance && MapUtil.approximatelyEqual(this.distribution,other.distribution,equalityTolerance);
}
