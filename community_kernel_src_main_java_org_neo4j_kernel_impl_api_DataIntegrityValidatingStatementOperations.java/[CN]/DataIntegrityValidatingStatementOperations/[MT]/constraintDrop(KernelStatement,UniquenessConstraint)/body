{
  try {
    assertConstraintExists(constraint,schemaReadDelegate.constraintsGetForLabelAndPropertyKey(state,constraint.label(),constraint.propertyKeyId()));
  }
 catch (  NoSuchConstraintException e) {
    throw new DropConstraintFailureException(constraint,e);
  }
  schemaWriteDelegate.constraintDrop(state,constraint);
}
