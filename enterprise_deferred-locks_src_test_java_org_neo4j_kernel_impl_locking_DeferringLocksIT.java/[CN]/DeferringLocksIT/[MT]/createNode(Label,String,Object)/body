{
  return new WorkerCommand<Void,Void>(){
    @Override public Void doWork(    Void state) throws Exception {
      try (Transaction tx=db.beginTx()){
        Node node=db.createNode(label);
        node.setProperty(propertyKey,propertyValue);
        tx.success();
      }
       return null;
    }
  }
;
}
