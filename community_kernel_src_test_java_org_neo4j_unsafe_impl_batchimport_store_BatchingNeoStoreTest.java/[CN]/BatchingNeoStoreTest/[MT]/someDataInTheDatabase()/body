{
  GraphDatabaseService db=new TestGraphDatabaseFactory().setFileSystem(fsr.get()).newImpermanentDatabase(storeDir.getAbsolutePath());
  try (Transaction tx=db.beginTx()){
    db.createNode().createRelationshipTo(db.createNode(),MyRelTypes.TEST);
    tx.success();
  }
  finally {
    db.shutdown();
  }
}
