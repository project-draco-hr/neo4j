{
synchronized (instances) {
    for (int i=0; i < instances.length; i++) {
      Pair<Instance,BreakPoint[]> instance=instances[i];
      if (instance != null) {
        Thread.currentThread().interrupt();
        SubProcess.kill(instance.first());
        Thread.interrupted();
      }
      instances[i]=null;
    }
  }
}
