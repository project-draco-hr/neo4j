{
  Map<String,String> customConfig=new HashMap<>();
  if (customThreshold != null) {
    customConfig.put(GraphDatabaseSettings.dense_node_threshold.name(),"" + customThreshold);
  }
  Monitors monitors=new Monitors();
  Config config=config(customConfig);
  return new StoreFactory(config,new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(monitors,config),new DefaultFileSystemAbstraction(),StringLogger.DEV_NULL,new DefaultTxHook(),monitors);
}
