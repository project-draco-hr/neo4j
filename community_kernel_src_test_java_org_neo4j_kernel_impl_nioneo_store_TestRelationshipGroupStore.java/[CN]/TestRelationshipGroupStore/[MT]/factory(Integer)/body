{
  Map<String,String> customConfig=new HashMap<>();
  if (customThreshold != null) {
    customConfig.put(GraphDatabaseSettings.dense_node_threshold.name(),"" + customThreshold);
  }
  return new StoreFactory(StoreFactory.configForStoreDir(config(customConfig),directory),new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),new DefaultFileSystemAbstraction(),StringLogger.DEV_NULL,new DefaultTxHook());
}
