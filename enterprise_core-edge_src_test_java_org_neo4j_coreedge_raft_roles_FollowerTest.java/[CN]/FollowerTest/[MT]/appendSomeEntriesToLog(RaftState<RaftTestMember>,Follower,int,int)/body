{
  for (int i=0; i < numberOfEntriesToAppend; i++) {
    if (i == 0) {
      raft.update(follower.handle(new AppendEntries.Request<>(myself,term,i - 1,-1,new RaftLogEntry[]{new RaftLogEntry(term,ContentGenerator.content())},-1),raft,log()));
    }
 else {
      raft.update(follower.handle(new AppendEntries.Request<>(myself,term,i - 1,term,new RaftLogEntry[]{new RaftLogEntry(term,ContentGenerator.content())},-1),raft,log()));
    }
  }
}
