{
  RaftState<RaftTestMember> state=raftState().myself(myself).votingMembers(asSet(myself,member1,member2)).build();
  Outcome<RaftTestMember> outcome=new Follower().handle(new Election<>(myself,storeId.storeId()),state,log(),storeId);
  state.update(outcome);
  assertEquals(CANDIDATE,outcome.getRole());
  assertNotNull(messageFor(outcome,member1));
  assertNotNull(messageFor(outcome,member2));
}
