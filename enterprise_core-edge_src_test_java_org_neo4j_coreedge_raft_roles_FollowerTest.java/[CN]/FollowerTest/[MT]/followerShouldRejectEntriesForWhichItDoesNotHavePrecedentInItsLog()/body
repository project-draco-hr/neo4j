{
  RaftState<RaftTestMember> state=raftState().myself(myself).votingMembers(asSet(myself,member1,member2)).build();
  Follower follower=new Follower();
  Outcome<RaftTestMember> outcome=follower.handle(new RaftMessages.AppendEntries.Request<>(myself,99,99,0,new RaftLogEntry[]{new RaftLogEntry(99,ContentGenerator.content())},99),state,log());
  Message<RaftTestMember> response=messageFor(outcome,myself);
  assertThat(response,instanceOf(AppendEntries.Response.class));
  assertFalse(((AppendEntries.Response)response).success());
}
