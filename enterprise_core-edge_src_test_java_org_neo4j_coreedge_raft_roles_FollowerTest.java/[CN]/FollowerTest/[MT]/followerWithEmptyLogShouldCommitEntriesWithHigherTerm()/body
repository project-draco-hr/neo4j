{
  RaftState<RaftTestMember> state=raftState().myself(myself).build();
  Follower follower=new Follower();
  Outcome<RaftTestMember> outcome=follower.handle(new AppendEntries.Request<>(myself,1,-1,-1,new RaftLogEntry[]{new RaftLogEntry(1,new ReplicatedString("commit this!"))},0),state,log());
  state.update(outcome);
  assertEquals(0,state.entryLog().commitIndex());
  assertEquals(1,state.term());
}
