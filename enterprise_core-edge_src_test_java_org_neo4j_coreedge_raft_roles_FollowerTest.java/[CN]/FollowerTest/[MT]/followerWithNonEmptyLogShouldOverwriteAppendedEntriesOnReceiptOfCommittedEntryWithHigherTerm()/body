{
  int term=1;
  RaftState<RaftTestMember> state=raftState().myself(myself).term(term).build();
  Follower follower=new Follower();
  appendSomeEntriesToLog(state,follower,9,term);
  Outcome<RaftTestMember> outcome=follower.handle(new AppendEntries.Request<>(member1,2,3,1,new RaftLogEntry[]{new RaftLogEntry(2,new ReplicatedString("commit this!"))},4),state,log());
  state.update(outcome);
  assertEquals(4,state.entryLog().commitIndex());
  assertEquals(2,state.term());
}
