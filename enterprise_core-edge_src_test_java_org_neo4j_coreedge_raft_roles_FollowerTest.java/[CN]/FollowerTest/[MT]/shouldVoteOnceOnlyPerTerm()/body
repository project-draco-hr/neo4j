{
  RaftState<RaftTestMember> state=raftState().myself(myself).votingMembers(asSet(myself,member1,member2)).build();
  Follower follower=new Follower();
  Outcome<RaftTestMember> outcome1=follower.handle(voteRequest().from(member1).term(1).build(),state,log());
  state.update(outcome1);
  Outcome<RaftTestMember> outcome2=follower.handle(voteRequest().from(member2).term(1).build(),state,log());
  assertEquals(new Vote.Response<>(myself,1,true),messageFor(outcome1,member1));
  assertEquals(new Vote.Response<>(myself,1,false),messageFor(outcome2,member2));
}
