{
  RaftState<RaftTestMember> state=raftState().build();
  Follower follower=new Follower();
  final long startingTerm=state.term() + 1;
  Outcome<RaftTestMember> outcome1=follower.handle(voteRequest().from(member1).term(startingTerm).lastLogIndex(0).lastLogTerm(-1).build(),state,log());
  state.update(outcome1);
  Outcome<RaftTestMember> outcome2=follower.handle(voteRequest().from(member2).term(state.term()).lastLogIndex(0).lastLogTerm(-1).build(),state,log());
  assertThat(messageFor(outcome1,member1),instanceOf(Vote.Response.class));
  assertFalse(((Vote.Response)messageFor(outcome2,member2)).voteGranted());
}
