{
  try (Transaction tx=db.beginTx()){
    ReadOperations statement=ctxProvider.instance().readOperations();
    DoubleLongRegister result=Registers.newDoubleLongRegister();
    counts.indexSample(statement.labelGetForName(label.name()),statement.propertyKeyGetForName(propertyKey),result);
    tx.success();
    return result;
  }
 }
