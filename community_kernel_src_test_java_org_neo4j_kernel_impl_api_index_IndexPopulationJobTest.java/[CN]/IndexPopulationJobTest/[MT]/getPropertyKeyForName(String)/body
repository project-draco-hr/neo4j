{
  Long result;
  Transaction tx=db.beginTx();
  try {
    StatementOperations context=ctxProvider.getCtxForWriting().asStatementOperations();
    result=context.propertyKeyGetForName(ctxProvider.statementForWriting(),name);
    tx.success();
  }
  finally {
    tx.finish();
  }
  return result;
}
