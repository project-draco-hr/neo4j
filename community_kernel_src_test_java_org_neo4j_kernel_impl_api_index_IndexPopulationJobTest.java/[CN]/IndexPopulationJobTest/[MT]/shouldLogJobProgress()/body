{
  createNode(map(name,"irrelephant"),FIRST);
  TestLogger logger=new TestLogger();
  FlippableIndexProxy index=mock(FlippableIndexProxy.class);
  NeoStoreIndexStoreView store=new NeoStoreIndexStoreView(db.getXaDataSourceManager().getNeoStoreDataSource().getNeoStore());
  IndexPopulationJob job=newIndexPopulationJob(FIRST,name,populator,index,store,logger);
  job.run();
  logger.assertExactly(info("Index population started for label id 0 on property id 0"),info("Index population completed for label id 0 on property id 0, index is now online."));
}
