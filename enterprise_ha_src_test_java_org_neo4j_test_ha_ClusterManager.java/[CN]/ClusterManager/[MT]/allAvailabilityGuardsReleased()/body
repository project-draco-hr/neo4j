{
  return new Predicate<ManagedCluster>(){
    @Override public boolean test(    ManagedCluster item){
      for (      HighlyAvailableGraphDatabase member : item.getAllMembers()) {
        try {
          member.beginTx().close();
        }
 catch (        TransactionFailureException e) {
          return false;
        }
      }
      return true;
    }
  }
;
}
