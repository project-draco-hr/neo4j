{
  if (value instanceof Node) {
    Node node=(Node)value;
    int nbrOfLabels=0;
    for (    Label l : node.getLabels()) {
      if (!labelNames.contains(l.name())) {
        return false;
      }
      ++nbrOfLabels;
    }
    return labelNames.size() == nbrOfLabels && propertyMatcher.matches(node.getAllProperties());
  }
  return false;
}
