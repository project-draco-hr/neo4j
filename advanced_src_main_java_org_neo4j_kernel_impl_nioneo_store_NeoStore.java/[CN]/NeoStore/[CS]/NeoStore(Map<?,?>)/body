{
  super((String)config.get("neo_store"),config);
  int relGrabSize=DEFAULT_REL_GRAB_SIZE;
  if (getConfig() != null) {
    String grabSize=(String)getConfig().get("relationship_grab_size");
    if (grabSize != null) {
      relGrabSize=Integer.parseInt(grabSize);
    }
  }
  REL_GRAB_SIZE=relGrabSize;
}
