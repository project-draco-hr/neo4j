{
  breakNodeStore();
  ConsistencyCheckService service=new ConsistencyCheckService();
  service.runFullConsistencyCheck(fixture.directory().getPath(),new Config(stringMap(),GraphDatabaseSettings.class,ConsistencyCheckSettings.class),ProgressMonitorFactory.NONE,StringLogger.DEV_NULL);
  assertTrue(new File(fixture.directory(),service.defaultLogFileName()).exists());
}
