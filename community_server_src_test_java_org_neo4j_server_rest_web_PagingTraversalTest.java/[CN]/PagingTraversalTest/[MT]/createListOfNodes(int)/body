{
  Transaction tx=database.getGraph().beginTx();
  try {
    long zerothNode=helper.createNode(MapUtil.map("name",String.valueOf(0)));
    long previousNodeId=zerothNode;
    for (int i=1; i < numberOfNodes; i++) {
      long currentNodeId=helper.createNode(MapUtil.map("name",String.valueOf(i)));
      database.getGraph().getNodeById(previousNodeId).createRelationshipTo(database.getGraph().getNodeById(currentNodeId),DynamicRelationshipType.withName("PRECEDES"));
    }
    tx.success();
    return zerothNode;
  }
  finally {
    tx.finish();
  }
}
