{
  Response response=createAPagedTraverser();
  assertEquals(201,response.getStatus());
  String responseUri=response.getMetadata().get("Location").get(0).toString();
  assertThat(responseUri,containsString("/node/1/paged/traverse/node/"));
  assertNotNull(response.getEntity());
  assertThat(response.getEntity().toString(),containsString("\"name\" : \"19\""));
}
