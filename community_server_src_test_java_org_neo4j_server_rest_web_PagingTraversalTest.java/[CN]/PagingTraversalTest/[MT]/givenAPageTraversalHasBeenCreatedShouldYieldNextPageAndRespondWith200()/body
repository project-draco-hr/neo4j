{
  Response response=createAPagedTraverser();
  String locationUri=response.getMetadata().get("Location").get(0).toString();
  String traverserId=parseTraverserIdFromLocationUri(locationUri);
  response=service.pagedTraverse(traverserId,TraverserReturnType.node);
  assertEquals(200,response.getStatus());
  assertNotNull(response.getEntity());
  assertThat(response.getEntity().toString(),not(containsString("\"name\" : \"19\"")));
  assertThat(response.getEntity().toString(),containsString("\"name\" : \"91\""));
}
