{
  GraphDatabaseFactory factory=new GraphDatabaseFactory();
  GraphDatabaseAPI db=(GraphDatabaseAPI)factory.newEmbeddedDatabase(testdir.absolutePath());
  DependencyResolver resolver=db.getDependencyResolver();
  PageCache cache=resolver.resolveDependency(PageCache.class);
  try {
    assertThat(cache,instanceOf(MuninnPageCache.class));
  }
  finally {
    db.shutdown();
  }
}
