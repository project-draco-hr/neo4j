{
  Integer port=parsePort((String)kernel.getParam(ENABLE_ONLINE_BACKUP));
  if (port != null) {
    TheBackupInterface backup=new BackupImpl(kernel.graphDatabase());
    BackupServer server=new BackupServer(backup,port.intValue(),(StringLogger)kernel.getConfig().getParams().get(StringLogger.class));
    return server;
  }
  return null;
}
