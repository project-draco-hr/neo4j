{
  return record1.getFirstIn() == record2.getFirstIn() && record1.getFirstLoop() == record2.getFirstLoop() && record1.getFirstOut() == record2.getFirstOut() && record1.getNext() == record2.getNext() && record1.getOwningNode() == record2.getOwningNode() && record1.getPrev() == record2.getPrev() && record1.getType() == record2.getType();
}
