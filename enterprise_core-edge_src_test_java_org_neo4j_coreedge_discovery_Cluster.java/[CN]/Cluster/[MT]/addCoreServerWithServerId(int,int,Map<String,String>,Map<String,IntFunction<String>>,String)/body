{
  Config config=firstOrNull(coreServers).getDependencyResolver().resolveDependency(Config.class);
  List<AdvertisedSocketAddress> advertisedAddress=config.get(CoreEdgeClusterSettings.initial_core_cluster_members);
  coreServers.add(startCoreServer(serverId,intendedClusterSize,advertisedAddress,extraParams,instanceExtraParams,recordFormat));
}
