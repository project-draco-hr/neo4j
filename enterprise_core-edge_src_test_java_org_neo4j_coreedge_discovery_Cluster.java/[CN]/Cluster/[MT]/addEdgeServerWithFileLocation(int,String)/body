{
  Config config=firstOrNull(coreServers.values()).getDependencyResolver().resolveDependency(Config.class);
  List<AdvertisedSocketAddress> advertisedAddresses=config.get(CoreEdgeClusterSettings.initial_core_cluster_members);
  edgeServers.put(serverId,startEdgeServer(serverId,advertisedAddresses,stringMap(),emptyMap(),recordFormat));
}
