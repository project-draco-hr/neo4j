{
  if (e.getCause() instanceof Replicator.ReplicationFailedException) {
    return true;
  }
 else   if (e instanceof IdGenerationException) {
    return true;
  }
 else   if (e.getCause() instanceof org.neo4j.kernel.api.exceptions.TransactionFailureException && ((org.neo4j.kernel.api.exceptions.TransactionFailureException)e.getCause()).status() == LockSessionExpired) {
    return true;
  }
  return false;
}
