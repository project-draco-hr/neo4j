{
  Config config=coreServers.values().iterator().next().getDependencyResolver().resolveDependency(Config.class);
  List<AdvertisedSocketAddress> advertisedAddresses=config.get(CoreEdgeClusterSettings.initial_core_cluster_members);
  edgeServers.put(999,startEdgeServer(999,edgeDatabaseStoreFileLocation,advertisedAddresses,stringMap(),emptyMap(),StandardV3_0.NAME));
}
