{
  emptyLock.lock();
  try {
    Iterable<IndexDescriptor> descriptors=jobQueue.pollAll();
    for (    IndexDescriptor descriptor : descriptors) {
      jobTracker.waitUntilCanExecuteMoreSamplingJobs();
      sampleIndexOnTracker(indexMap,descriptor);
    }
  }
  finally {
    emptyLock.unlock();
  }
}
