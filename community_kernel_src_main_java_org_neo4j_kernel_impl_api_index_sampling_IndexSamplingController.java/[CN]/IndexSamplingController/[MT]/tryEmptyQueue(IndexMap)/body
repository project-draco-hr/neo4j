{
  if (emptyLock.tryLock()) {
    try {
      while (jobTracker.canExecuteMoreSamplingJobs()) {
        IndexDescriptor descriptor=jobQueue.poll();
        if (descriptor == null) {
          return;
        }
        sampleIndex(indexMap,descriptor);
      }
    }
  finally {
      emptyLock.unlock();
    }
  }
}
