{
  File file;
  if (osIsWindows()) {
    file=new File("\\\\" + UUID.randomUUID().toString() + "\\");
  }
 else   if (osIsMacOS()) {
    file=new File("/Network/Servers/localhost/" + UUID.randomUUID().toString());
  }
 else {
    file=new File("/proc/" + UUID.randomUUID().toString() + "/random");
  }
  return file;
}
