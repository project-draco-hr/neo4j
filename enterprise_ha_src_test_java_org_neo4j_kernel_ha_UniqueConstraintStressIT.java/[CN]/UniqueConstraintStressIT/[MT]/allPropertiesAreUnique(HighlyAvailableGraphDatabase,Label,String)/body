{
  try (Transaction tx=master.beginTx()){
    Set<Object> values=new HashSet<>();
    for (    Node node : master.getAllNodes()) {
      if (node.hasLabel(label)) {
        Object value=node.getProperty(property);
        if (values.contains(value)) {
          return false;
        }
        values.add(value);
      }
    }
    tx.success();
  }
   return true;
}
