{
  String path=args[0];
  String configFile=args[1];
  final GraphDatabaseService graphDb=new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(path).loadPropertiesFromFile(configFile).newGraphDatabase();
  Runtime.getRuntime().addShutdownHook(new Thread(){
    @Override public void run(){
      graphDb.shutdown();
    }
  }
);
}
