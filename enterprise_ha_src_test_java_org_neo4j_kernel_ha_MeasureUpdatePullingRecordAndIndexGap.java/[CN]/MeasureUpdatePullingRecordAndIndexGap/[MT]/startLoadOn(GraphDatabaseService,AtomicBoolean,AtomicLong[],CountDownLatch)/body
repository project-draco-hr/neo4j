{
  for (int i=0; i < numberOfIndexes; i++) {
    final int x=i;
    new Thread(){
      @Override public void run(){
        try {
          while (!halter.get()) {
            long nodeId;
            try (Transaction tx=db.beginTx()){
              Node node=db.createNode(label(x));
              node.setProperty(key(x),nodeId=node.getId());
              tx.success();
            }
             highIdNodes[x].set(nodeId);
          }
        }
  finally {
          endLatch.countDown();
        }
      }
    }
.start();
  }
}
