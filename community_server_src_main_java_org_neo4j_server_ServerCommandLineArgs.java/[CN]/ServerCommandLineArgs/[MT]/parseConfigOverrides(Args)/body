{
  Collection<Pair<String,String>> options=arguments.interpretOptions("c",Converters.<Pair<String,String>>optional(),new Function<String,Pair<String,String>>(){
    @Override public Pair<String,String> apply(    String s){
      if (s.contains("=")) {
        String[] keyVal=s.split("=",2);
        return pair(keyVal[0],keyVal[1]);
      }
      return pair(s,"true");
    }
  }
);
  return options.toArray(new Pair[options.size()]);
}
