{
  boolean exists=cursor.next();
  id=cursor.getId();
  if (state.nodeIsDeletedInThisTx(id)) {
    this.id=-1;
    return false;
  }
  this.nodeIsAddedInThisTx=state.nodeIsAddedInThisTx(id);
  if (exists || nodeIsAddedInThisTx) {
    this.nodeState=state.getNodeState(cursor.getId());
    return true;
  }
 else {
    this.id=-1;
    this.nodeState=null;
    return false;
  }
}
