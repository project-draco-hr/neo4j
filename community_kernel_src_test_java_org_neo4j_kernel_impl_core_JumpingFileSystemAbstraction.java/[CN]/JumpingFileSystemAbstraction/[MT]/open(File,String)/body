{
  if (fileName.getName().equals("neostore.nodestore.db") || fileName.getName().equals("neostore.relationshipstore.db") || fileName.getName().equals("neostore.propertystore.db")|| fileName.getName().equals("neostore.propertystore.db.strings")|| fileName.getName().equals("neostore.propertystore.db.arrays")) {
    return new JumpingFileChannel(new RandomAccessFile(fileName,mode).getChannel(),recordSizeFor(fileName));
  }
  return new RandomAccessFile(fileName,mode).getChannel();
}
