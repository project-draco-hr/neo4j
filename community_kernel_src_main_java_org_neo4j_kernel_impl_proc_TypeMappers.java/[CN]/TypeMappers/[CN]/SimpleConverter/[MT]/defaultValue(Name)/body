{
  String defaultValue=parameter.defaultValue();
  if (defaultValue.equals(Name.DEFAULT_VALUE)) {
    return Optional.empty();
  }
 else {
    try {
      return Optional.of(defaultConverter.apply(defaultValue));
    }
 catch (    Exception e) {
      throw new ProcedureException(Status.Procedure.ProcedureRegistrationFailed,"Default value `%s` could not be parsed as a %s",parameter.defaultValue(),javaClass.getSimpleName());
    }
  }
}
