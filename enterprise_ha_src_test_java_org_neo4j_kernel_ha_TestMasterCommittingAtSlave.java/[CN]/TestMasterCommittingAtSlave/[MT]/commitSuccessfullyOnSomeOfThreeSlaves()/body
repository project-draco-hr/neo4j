{
  TransactionPropagator propagator=newPropagator(5,3,givenOrder(),false,true,true);
  propagator.committed(2,1);
  Iterator<Slave> slaveIt=slaves.iterator();
  assertCalls((FakeSlave)slaveIt.next(),2);
  slaveIt.next();
  slaveIt.next();
  assertCalls((FakeSlave)slaveIt.next(),2);
  assertCalls((FakeSlave)slaveIt.next(),2);
  assertNoFailureLogs();
}
