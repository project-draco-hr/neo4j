{
  return new XaCommandReader(){
    @Override public XaCommand read(    ReadableByteChannel channel) throws IOException {
      buffer.clear();
      buffer.limit(4);
      if (channel.read(buffer) == 4) {
        buffer.flip();
        return new DummyCommand(buffer.getInt());
      }
      return null;
    }
  }
;
}
