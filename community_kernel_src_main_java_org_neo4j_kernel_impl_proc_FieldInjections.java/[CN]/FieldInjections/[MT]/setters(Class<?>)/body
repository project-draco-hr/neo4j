{
  List<FieldSetter> setters=new LinkedList<>();
  Class<?> currentClass=cls;
  do {
    for (    Field field : currentClass.getDeclaredFields()) {
      if (Modifier.isStatic(field.getModifiers())) {
        if (field.isAnnotationPresent(Context.class)) {
          throw new ProcedureException(Status.Procedure.FailedRegistration,Messages.get(proc_static_field_annotated_as_context,field.getName(),cls.getSimpleName()));
        }
        continue;
      }
      assertValidForInjection(cls,field);
      setters.add(createInjector(cls,field));
    }
  }
 while ((currentClass=currentClass.getSuperclass()) != null);
  return setters;
}
