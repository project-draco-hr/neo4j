{
  LabelsReference labelsReference=new LabelsReference();
  final RecordStore.Processor<FAILURE> processor=new NodeIndexingProcessor<>(propertyStore,propertyKeyPredicate,labelsReference,visitor);
  final Predicate<NodeRecord> predicate=new NodeLabelFilterPredicate(nodeStore,labelPredicate,labelsReference);
  return new ProcessStoreScan<>(processor,predicate);
}
