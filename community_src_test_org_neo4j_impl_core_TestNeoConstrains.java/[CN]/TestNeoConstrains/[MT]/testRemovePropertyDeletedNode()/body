{
  Node node=getNeo().createNode();
  node.setProperty(key,new Integer(1));
  node.delete();
  try {
    node.removeProperty(key);
    Transaction tx=getTransaction();
    tx.success();
    tx.finish();
    fail("Remove property on deleted node should not validate");
  }
 catch (  Exception e) {
  }
  setTransaction(getNeo().beginTx());
}
