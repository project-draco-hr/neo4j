{
  Node node1=getNeo().createNode();
  Node node2=getNeo().createNode();
  node1.createRelationshipTo(node2,MyRelTypes.TEST);
  node2.delete();
  node1.delete();
  try {
    Transaction tx=getTransaction();
    tx.success();
    tx.finish();
    fail("Should not validate");
  }
 catch (  Exception e) {
  }
  setTransaction(getNeo().beginTx());
}
