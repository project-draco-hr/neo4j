{
  if (this.neoServer == null) {
    return;
  }
  try {
    this.neoServer.getClass().getMethod("shutdown").invoke(this.neoServer);
    if (this.neoDirectory != null) {
      this.neoServiceInstance.getClass().getMethod("shutdown").invoke(this.neoServiceInstance);
    }
    this.neoServiceInstance=null;
    this.neoServer=null;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
