{
  ClientResponse response=Client.create().resource(functionalTestHelper.nodeUri(theStartNode.getId()) + "/paged/traverse/node/").accept(MediaType.APPLICATION_JSON_TYPE).post(ClientResponse.class);
  assertEquals(201,response.getStatus());
  assertThat(response.getLocation().toString(),containsString("/db/data/node/" + theStartNode.getId() + "/paged/traverse/node/"));
  assertEquals("application/json",response.getType().toString().toLowerCase());
}
