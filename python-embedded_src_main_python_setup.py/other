from distutils.core import setup
with open('README.txt') as file:
    long_description = file.read()
setup(name='neo4j-embedded', version='${pythonic_version}', description='Bindings for the embedded version of the neo4j graph database.', long_description=long_description, author='Neo Technology', packages=('neo4j', 'neo4j.javalib'), package_data={'': ['*.jar'], }, classifiers=('License :: OSI Approved :: GNU General Public License (GPL)', 'Topic :: Database :: Database Engines/Servers', 'Programming Language :: Python :: 2.7', 'Programming Language :: Java'))
