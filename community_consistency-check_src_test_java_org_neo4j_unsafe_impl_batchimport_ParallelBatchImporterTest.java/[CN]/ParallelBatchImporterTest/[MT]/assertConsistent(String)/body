{
  ConsistencyCheckService consistencyChecker=new ConsistencyCheckService();
  Result result=consistencyChecker.runFullConsistencyCheck(storeDir,new Config(stringMap(GraphDatabaseSettings.pagecache_memory.name(),"8m")),ProgressMonitorFactory.NONE,StringLogger.DEV_NULL);
  assertTrue("Database contains inconsistencies, there should be a report in " + storeDir,result.isSuccessful());
}
