{
  ConsistencyCheckService consistencyChecker=new ConsistencyCheckService();
  Result result=consistencyChecker.runFullConsistencyCheck(storeDir,new Config(stringMap(GraphDatabaseSettings.pagecache_memory.name(),"8m",GraphDatabaseFacadeFactory.Configuration.record_format.name(),getFormatName())),ProgressMonitorFactory.NONE,NullLogProvider.getInstance(),false);
  assertTrue("Database contains inconsistencies, there should be a report in " + storeDir,result.isSuccessful());
}
