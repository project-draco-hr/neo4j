{
  for (int i=0; i < out; i++) {
    inserter.createRelationship(node,inserter.createNode(null),relType,null);
  }
  for (int i=0; i < out; i++) {
    inserter.createRelationship(inserter.createNode(null),node,relType,null);
  }
  for (int i=0; i < out; i++) {
    inserter.createRelationship(node,node,relType,null);
  }
}
