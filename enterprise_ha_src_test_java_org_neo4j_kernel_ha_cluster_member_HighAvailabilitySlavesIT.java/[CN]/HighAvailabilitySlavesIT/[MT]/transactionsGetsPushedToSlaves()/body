{
  clusterManager=new ClusterManager(clusterOfSize(3),DIR.cleanDirectory("dbs"),stringMap(tx_push_factor.name(),"2"));
  clusterManager.start();
  ManagedCluster cluster=clusterManager.getDefaultCluster();
  String name="a node";
  long node=createNode(cluster.getMaster(),name);
  for (  HighlyAvailableGraphDatabase db : cluster.getAllMembers()) {
    Transaction transaction=db.beginTx();
    try {
      assertEquals(node,getNodeByName(db,name));
    }
  finally {
      transaction.finish();
    }
  }
}
