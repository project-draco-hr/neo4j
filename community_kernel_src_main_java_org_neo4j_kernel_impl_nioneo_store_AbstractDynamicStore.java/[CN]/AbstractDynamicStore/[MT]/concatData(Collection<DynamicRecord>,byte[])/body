{
  int totalLength=0;
  for (  DynamicRecord record : records)   totalLength+=record.getLength();
  if (target.length < totalLength)   target=new byte[totalLength];
  ByteBuffer buffer=ByteBuffer.wrap(target);
  for (  DynamicRecord record : records)   buffer.put(record.getData());
  return ByteBuffer.wrap(target,0,totalLength);
}
