{
  if (!record.isLight()) {
    return;
  }
  if (record.getLength() == 0) {
    record.setData(NO_DATA);
  }
  try (PageCursor cursor=storeFile.io(pageIdForRecord(record.getId()),PF_SHARED_LOCK,record,offsetForId(record.getId()),encodeBlockSizeFlags(getBlockSize(),IO_READ_DATA))){
    cursor.next();
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
