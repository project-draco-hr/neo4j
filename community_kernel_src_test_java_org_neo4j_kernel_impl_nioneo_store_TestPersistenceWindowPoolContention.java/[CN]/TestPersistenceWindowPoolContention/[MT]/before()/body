{
  File file=new File("target/bigfile");
  assertTrue("delete " + file,file.delete());
  channel=new RandomAccessFile(file,"rw").getChannel();
  write(channel,fileSize);
  pool=new PersistenceWindowPool(new File("contention test"),recordSize,channel,mappingSize,true,false,new ConcurrentHashMap<Long,PersistenceRow>(),BrickElementFactory.DEFAULT,StringLogger.DEV_NULL);
