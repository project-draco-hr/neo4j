{
  while (!halted) {
    OperationType type=randomOperationTypeButFavoringReads(0.6f);
    long id=randomPosition();
    PersistenceWindow window=pool.acquire(id,type);
    try {
switch (type) {
case READ:
        readStuff(window,id);
      break;
case WRITE:
    writeStuff(window,id);
  break;
}
}
  finally {
pool.release(window);
}
count++;
}
}
