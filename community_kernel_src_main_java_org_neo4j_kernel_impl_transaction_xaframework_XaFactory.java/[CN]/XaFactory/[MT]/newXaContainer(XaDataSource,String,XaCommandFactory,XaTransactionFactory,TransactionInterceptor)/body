{
  if (logicalLog == null || cf == null || tf == null) {
    throw new IllegalArgumentException("Null parameter, " + "LogicalLog[" + logicalLog + "] CommandFactory["+ cf+ "TransactionFactory["+ tf+ "]");
  }
  XaResourceManager rm=new XaResourceManager(xaDataSource,tf,txIdGenerator,txManager,logicalLog);
  XaLogicalLog log;
  if ("true".equalsIgnoreCase((String)config.get(Config.INTERCEPT_DESERIALIZED_TRANSACTIONS)) && interceptors != null) {
    log=new InterceptingXaLogicalLog(logicalLog,rm,cf,tf,interceptors,logBufferFactory,fileSystemAbstraction,stringLogger);
  }
 else {
    log=new XaLogicalLog(logicalLog,rm,cf,tf,logBufferFactory,fileSystemAbstraction,stringLogger);
  }
  rm.setLogicalLog(log);
  tf.setLogicalLog(log);
  return new XaContainer(rm,log);
}
