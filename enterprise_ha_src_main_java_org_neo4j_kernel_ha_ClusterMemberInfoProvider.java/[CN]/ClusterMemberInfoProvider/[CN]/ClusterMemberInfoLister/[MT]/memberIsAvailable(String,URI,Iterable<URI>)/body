{
  int instanceId=ClusterMemberModeSwitcher.getServerId(ServerUtil.getUriForScheme("ha",instanceUris));
  List<String> uris=new ArrayList<String>(2);
  for (  URI instanceUri : instanceUris) {
    uris.add(instanceUri.toASCIIString());
  }
  memberInfo.put(instanceClusterUri,new ClusterMemberInfo(Integer.toString(instanceId),role,new String[]{role},uris.toArray(new String[uris.size()])));
}
