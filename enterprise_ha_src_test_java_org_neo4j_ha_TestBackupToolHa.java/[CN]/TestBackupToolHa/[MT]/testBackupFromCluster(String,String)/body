{
  startCluster(clusterName);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode(backupArguments(true,"ha://localhost:2181",BACKUP_PATH,clusterName)));
  assertEquals(representation,DbRepresentation.of(BACKUP_PATH));
  DbRepresentation newRepresentation=createSomeData(instances.get(2));
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode(backupArguments(false,"ha://localhost:2182",BACKUP_PATH,clusterName)));
  assertEquals(newRepresentation,DbRepresentation.of(BACKUP_PATH));
}
