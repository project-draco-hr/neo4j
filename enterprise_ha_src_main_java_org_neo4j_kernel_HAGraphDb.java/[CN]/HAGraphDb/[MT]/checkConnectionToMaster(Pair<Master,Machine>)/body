{
  Pair<String,Integer> connectionInfo=master.other().getServer();
  SocketAddress socketAddr=new InetSocketAddress(connectionInfo.first(),connectionInfo.other());
  Socket socket=new Socket();
  try {
    socket.connect(socketAddr);
  }
 catch (  Exception e) {
    getMessageLog().logMessage("COULD NOT CONNECT: " + socketAddr,e);
    return false;
  }
  return true;
}
