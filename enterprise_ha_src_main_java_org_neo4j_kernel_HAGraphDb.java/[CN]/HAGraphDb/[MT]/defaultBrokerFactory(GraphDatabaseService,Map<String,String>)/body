{
  return new BrokerFactory(){
    @Override public Broker create(    AbstractGraphDatabase graphDb,    Map<String,String> config){
      return new ZooKeeperBroker(graphDb,getClusterNameFromConfig(config),machineId,getCoordinatorsFromConfig(config),getHaServerFromConfig(config),getBackupPortFromConfig(config),getClientReadTimeoutFromConfig(config),getClientLockReadTimeoutFromConfig(config),getMaxConcurrentChannelsPerSlaveFromConfig(config),slaveUpdateMode.syncWithZooKeeper,HAGraphDb.this);
    }
  }
;
}
