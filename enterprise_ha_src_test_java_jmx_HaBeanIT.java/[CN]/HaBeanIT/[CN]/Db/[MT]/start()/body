{
  if (db != null)   return;
  db=(HighlyAvailableGraphDatabase)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(storeDir).setConfig(HaSettings.server_id,"" + id).setConfig("jmx.port","" + (9912 + id)).setConfig(HaSettings.ha_server,":" + (1136 + id)).setConfig(HaSettings.initial_hosts,":5001").newGraphDatabase();
}
