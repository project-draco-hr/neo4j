{
  populateDbAndIndexes(nodeCount,multiThreaded);
  ConsistencyCheckService cc=new ConsistencyCheckService();
  Result result=cc.runFullConsistencyCheck(directory.graphDbDir(),new Config(stringMap(GraphDatabaseSettings.pagecache_memory.name(),"8m")),NONE,NullLogProvider.getInstance(),false);
  assertTrue(result.isSuccessful());
  dropIndexes();
}
