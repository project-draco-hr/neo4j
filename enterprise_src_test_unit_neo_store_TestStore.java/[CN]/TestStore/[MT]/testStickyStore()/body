{
  try {
    Store.createStore("testStore.db").close();
    java.nio.channels.FileChannel fileChannel=new java.io.RandomAccessFile("testStore.db","rw").getChannel();
    fileChannel.truncate(fileChannel.size() - 2);
    fileChannel.close();
    try {
      new Store("testStore.db");
    }
 catch (    IOException e) {
    }
  }
 catch (  IOException e) {
    fail("" + e);
  }
 finally {
    File file=new File("testStore.db");
    if (file.exists()) {
      file.delete();
    }
    file=new File("testStore.db.id");
    if (file.exists()) {
      file.delete();
    }
  }
}
