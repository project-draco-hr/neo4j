{
  File serverDir=targetDir.directory("server-" + ports.other(),true);
  File serverConfig=new File(serverDir,"server.cfg");
  File dbConfig=new File(serverDir,"neo4j.cfg");
  File dbDir=new File(serverDir,"graph-database");
  config(serverConfig,Pair.of(Configurator.DATABASE_LOCATION_PROPERTY_KEY,dbDir.getAbsolutePath()),Pair.of(Configurator.WEBSERVER_PORT_PROPERTY_KEY,ports.other().toString()),Pair.of(Configurator.DB_TUNING_PROPERTY_FILE_KEY,dbConfig.getAbsolutePath()));
  config(dbConfig,Pair.of(HighlyAvailableGraphDatabase.CONFIG_KEY_HA_CLUSTER_NAME,name),Pair.of(HighlyAvailableGraphDatabase.CONFIG_KEY_HA_SERVER,"localhost:" + ports.first()),Pair.of(HighlyAvailableGraphDatabase.CONFIG_KEY_HA_MACHINE_ID,Integer.toString(id)),Pair.of(HighlyAvailableGraphDatabase.CONFIG_KEY_HA_ZOO_KEEPER_SERVERS,keeperCluster.getConnectionString()));
  return Pair.of(serverConfig.getAbsolutePath(),serverDir);
}
