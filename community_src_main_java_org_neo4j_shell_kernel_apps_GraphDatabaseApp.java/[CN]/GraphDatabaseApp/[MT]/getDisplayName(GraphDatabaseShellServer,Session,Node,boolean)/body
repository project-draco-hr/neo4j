{
  if (checkForMe && isCurrent(session,NodeOrRelationship.wrap(node))) {
    return getDisplayNameForCurrent(server,session);
  }
  StringBuffer result=new StringBuffer("(" + node.getId());
  String title=findTitle(server,session,node);
  if (title != null) {
    result.append(", " + title);
  }
  result.append(")");
  return result.toString();
}
