{
  if (new XaLogicalLogRecoveryCheck(toRecover).recoveryRequired()) {
    ReadableLogChannel recoveredDataChannel=new ReadAheadLogChannel(toRecover,NO_MORE_CHANNELS,ReadAheadLogChannel.DEFAULT_READ_AHEAD_SIZE);
    recoveredDataVisitor.visit(recoveredDataChannel);
    logRotationControl.forceEverything();
  }
}
