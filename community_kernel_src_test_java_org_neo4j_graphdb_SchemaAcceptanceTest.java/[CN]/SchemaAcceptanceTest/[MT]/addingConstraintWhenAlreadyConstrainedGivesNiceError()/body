{
  createConstraint(label,propertyKey);
  try {
    createConstraint(label,propertyKey);
    fail("Expected exception to be thrown");
  }
 catch (  ConstraintViolationException e) {
    assertEquals("Already constrained CONSTRAINT ON ( my_label:MY_LABEL ) ASSERT my_label.my_property_key IS" + " UNIQUE.",e.getMessage());
  }
}
