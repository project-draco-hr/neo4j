{
  createConstraint(label,propertyKey);
  try {
    createIndex(db,label,propertyKey);
    fail("Expected exception to be thrown");
  }
 catch (  ConstraintViolationException e) {
    assertEquals("Unable to add index on [label: MY_LABEL, my_property_key] : Already constrained CONSTRAINT" + " ON ( my_label:MY_LABEL ) ASSERT my_label.my_property_key IS UNIQUE.",e.getMessage());
  }
}
