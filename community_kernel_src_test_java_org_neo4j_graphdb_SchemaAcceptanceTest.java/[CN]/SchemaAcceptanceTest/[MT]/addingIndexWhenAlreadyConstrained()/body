{
  createConstraint(label,propertyKey);
  try {
    createIndex(label,propertyKey);
    fail("Expected exception to be thrown");
  }
 catch (  ConstraintViolationException e) {
    assertEquals(e.getMessage(),"Label 'MY_LABEL' and property 'my_property_key' have a unique constraint" + " defined on them, so an index is already created that matches this.");
  }
}
