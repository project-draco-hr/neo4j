{
  GraphDatabaseService beansAPI=dbRule.getGraphDatabaseService();
  Schema schema=beansAPI.schema();
  String property="my_property_key";
  Transaction tx=beansAPI.beginTx();
  try {
    schema.createIndex(Labels.MY_LABEL,property);
    schema.createIndex(Labels.MY_LABEL,property);
    tx.success();
  }
  finally {
    tx.finish();
  }
}
