{
  createIndex(db,label,propertyKey);
  try {
    createConstraint(label,propertyKey);
    fail("Expected exception to be thrown");
  }
 catch (  ConstraintViolationException e) {
    assertEquals(String.format("Unable to create CONSTRAINT ON ( my_label:MY_LABEL ) ASSERT my_label.my_property_key " + "IS UNIQUE:%nUnable to add index on [label: MY_LABEL, my_property_key] : " + "Already indexed :MY_LABEL(my_property_key)."),e.getMessage());
  }
}
