{
  createIndex(db,label,propertyKey);
  try {
    createIndex(db,label,propertyKey);
    fail("Expected exception to be thrown");
  }
 catch (  ConstraintViolationException e) {
    assertEquals("Unable to add index on [label: MY_LABEL, my_property_key] : Already indexed " + ":MY_LABEL(my_property_key).",e.getMessage());
  }
}
