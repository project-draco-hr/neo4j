{
  shutdownIfNecessary();
  if (brokerSaysIAmMaster()) {
    this.localGraph=new EmbeddedGraphDbImpl(storeDir,config,this,LockManagerFactory.DEFAULT,IdGeneratorFactory.DEFAULT,DefaultRelationshipTypeCreator.INSTANCE,TopLevelTransactionFactory.DEFAULT,TxIdGeneratorFactory.DEFAULT);
  }
 else {
    this.localGraph=new EmbeddedGraphDbImpl(storeDir,config,this,new SlaveLockManagerFactory(broker,this),new SlaveIdGeneratorFactory(broker,this),new SlaveRelationshipTypeCreator(broker,this),new SlaveTopLevelTransactionFactory(broker,this),new SlaveTxIdGeneratorFactory(broker,this));
  }
}
