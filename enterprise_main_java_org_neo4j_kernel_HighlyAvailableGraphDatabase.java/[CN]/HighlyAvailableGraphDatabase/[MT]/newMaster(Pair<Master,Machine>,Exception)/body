{
  try {
    msgLog.logMessage("newMaster( " + master + ") called",e,true);
    reevaluateMyself(master);
  }
 catch (  ZooKeeperException ee) {
    ee.printStackTrace();
  }
catch (  HaCommunicationException ee) {
    ee.printStackTrace();
  }
catch (  Throwable t) {
    t.printStackTrace();
    msgLog.logMessage("Reevaluation ended in unknown exception " + t + " so shutting down",true);
    shutdown();
  }
}
