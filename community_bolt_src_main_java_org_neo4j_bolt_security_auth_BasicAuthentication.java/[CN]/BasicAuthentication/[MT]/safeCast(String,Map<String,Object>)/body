{
  Object value=authToken.get(key);
  if (value == null || !(value instanceof String)) {
    throw new AuthenticationException(Status.Security.AuthenticationFailed,"The value associated with the key `" + key + "` must be a String but was: "+ (value == null ? "null" : value.getClass().getSimpleName()));
  }
  return (String)value;
}
