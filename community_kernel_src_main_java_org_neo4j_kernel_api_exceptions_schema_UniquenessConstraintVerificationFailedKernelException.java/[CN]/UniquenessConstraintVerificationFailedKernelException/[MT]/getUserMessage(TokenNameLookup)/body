{
  StringBuilder message=new StringBuilder();
  for (Iterator<IndexEntryConflictException> iterator=conflicts.iterator(); iterator.hasNext(); ) {
    IndexEntryConflictException conflict=iterator.next();
    message.append(conflict.evidenceMessage(tokenNameLookup.labelGetName(constraint.label()),tokenNameLookup.propertyKeyGetName(constraint.propertyKeyId())));
    if (iterator.hasNext()) {
      message.append(System.lineSeparator());
    }
  }
  return message.toString();
}
