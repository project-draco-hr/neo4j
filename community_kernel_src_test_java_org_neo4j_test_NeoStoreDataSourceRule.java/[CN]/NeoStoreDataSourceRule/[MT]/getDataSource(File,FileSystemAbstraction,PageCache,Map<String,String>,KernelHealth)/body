{
  if (theDs != null) {
    theDs.stop();
    theDs.shutdown();
  }
  final Config config=new Config(stringMap(additionalConfig),GraphDatabaseSettings.class);
  StoreFactory sf=new StoreFactory(storeDir,config,new DefaultIdGeneratorFactory(fs),pageCache,fs,NullLogProvider.getInstance(),new Monitors());
  Locks locks=mock(Locks.class);
  when(locks.newClient()).thenReturn(mock(Locks.Client.class));
  theDs=new NeoStoreDataSource(storeDir,config,sf,NullLogProvider.getInstance(),mock(JobScheduler.class,RETURNS_MOCKS),mock(TokenNameLookup.class),dependencyResolverForNoIndexProvider(),mock(PropertyKeyTokenHolder.class),mock(LabelTokenHolder.class),mock(RelationshipTypeTokenHolder.class),locks,mock(SchemaWriteGuard.class),mock(TransactionEventHandlers.class),IndexingService.NO_MONITOR,fs,mock(StoreUpgrader.class),mock(TransactionMonitor.class),kernelHealth,mock(PhysicalLogFile.Monitor.class),TransactionHeaderInformationFactory.DEFAULT,new StartupStatisticsProvider(),mock(NodeManager.class),null,null,CommunityEditionModule.createCommitProcessFactory(),mock(PageCache.class),mock(ConstraintSemantics.class),new Monitors(),new Tracers("null",NullLog.getInstance()));
  return theDs;
}
