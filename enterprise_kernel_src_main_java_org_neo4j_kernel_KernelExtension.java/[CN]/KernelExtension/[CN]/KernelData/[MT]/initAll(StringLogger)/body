{
  for (  KernelExtension extension : loadedExtensions) {
    try {
      extension.init(this);
      initialized(extension);
      msgLog.logMessage("Extension " + extension + " initialized ok",true);
    }
 catch (    Throwable t) {
      msgLog.logMessage("Failed to init extension " + extension,t,true);
    }
  }
}
