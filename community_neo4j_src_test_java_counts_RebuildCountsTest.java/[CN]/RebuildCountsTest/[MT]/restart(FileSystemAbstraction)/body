{
  if (db != null) {
    db.shutdown();
  }
  fs.mkdirs(storeDir);
  TestGraphDatabaseFactory dbFactory=new TestGraphDatabaseFactory();
  db=dbFactory.setLogging(logging).setFileSystem(fs).addKernelExtension(new InMemoryIndexProviderFactory(indexProvider)).newImpermanentDatabaseBuilder(storeDir.getAbsolutePath()).setConfig(index_background_sampling_enabled,"false").newGraphDatabase();
}
