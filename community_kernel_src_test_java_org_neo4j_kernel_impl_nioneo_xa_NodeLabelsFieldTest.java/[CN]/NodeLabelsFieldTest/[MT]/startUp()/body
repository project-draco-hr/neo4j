{
  Monitors monitors=new Monitors();
  Config config=new Config();
  StoreFactory storeFactory=new StoreFactory(config,new DefaultIdGeneratorFactory(),pageCacheRule.getPageCache(fs.get(),config),fs.get(),StringLogger.DEV_NULL,new DefaultTxHook(),monitors);
  File storeFile=new File("store");
  storeFactory.createNodeStore(storeFile);
  nodeStore=storeFactory.newNodeStore(storeFile);
}
