{
switch (mode) {
case PIPELINE:
    while (lastSentIndex <= prevLogIndex) {
      if (prevLogIndex - matchIndex <= maxAllowedShippingLag) {
        sendNewEntries(prevLogIndex,prevLogTerm,newLogEntries,leaderContext);
      }
 else {
        mode=Mode.CATCHUP;
        break;
      }
    }
  break;
}
lastLeaderContext=leaderContext;
}
