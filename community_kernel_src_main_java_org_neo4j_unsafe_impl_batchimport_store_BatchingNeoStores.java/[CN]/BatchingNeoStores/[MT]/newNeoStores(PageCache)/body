{
  BatchingIdGeneratorFactory idGeneratorFactory=new BatchingIdGeneratorFactory(fileSystem);
  StoreFactory storeFactory=new StoreFactory(storeDir,neo4jConfig,idGeneratorFactory,pageCache,fileSystem,RecordFormatSelector.select(neo4jConfig,LowLimitV3_0.RECORD_FORMATS,NullLogService.getInstance()),logProvider);
  return storeFactory.openAllNeoStores(true);
}
