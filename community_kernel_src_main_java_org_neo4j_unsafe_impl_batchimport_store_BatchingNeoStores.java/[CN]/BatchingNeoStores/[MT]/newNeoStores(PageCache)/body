{
  BatchingIdGeneratorFactory idGeneratorFactory=new BatchingIdGeneratorFactory(fileSystem);
  StoreFactory storeFactory=new StoreFactory(storeDir,neo4jConfig,idGeneratorFactory,pageCache,fileSystem,RecordFormatSelector.autoSelectFormat(neo4jConfig,NullLogService.getInstance()),logProvider);
  return storeFactory.openAllNeoStores(true);
}
