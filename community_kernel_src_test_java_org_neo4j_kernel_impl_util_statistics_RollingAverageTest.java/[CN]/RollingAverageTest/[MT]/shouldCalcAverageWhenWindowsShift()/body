{
  RollingAverage avg=new RollingAverage(new RollingAverage.Parameters(2,RollingAverage.Parameters.DEFAULT_EQUALITY_TOLERANCE));
  avg.record(10);
  avg.record(2);
  avg.record(2);
  avg.record(3);
  avg.record(2);
  avg.record(2);
  double result=avg.average();
  assertThat(result,closeTo(2.5,0.1));
}
