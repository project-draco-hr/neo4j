{
  int[] lengths=new int[serializables.size()];
  int totalLength=0;
  for (int i=0; i < serializables.size(); i++)   totalLength+=(lengths[i]=serializables.get(i).length());
  byte[] array=new byte[totalLength];
  ByteBuffer target=ByteBuffer.wrap(array);
  for (int i=0; i < serializables.size(); i++)   serializables.get(i).append(target);
  return array;
}
