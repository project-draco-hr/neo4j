{
  IndexReader reader=accessor.newReader();
  try {
    List<Long> list=new LinkedList<>();
    for (PrimitiveLongIterator iterator=reader.lookup(propertyValue); iterator.hasNext(); ) {
      list.add(iterator.next());
    }
    Collections.sort(list);
    return list;
  }
  finally {
    reader.close();
  }
}
