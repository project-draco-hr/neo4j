{
synchronized (this) {
    if (closed) {
      return;
    }
    closed=true;
    for (    IndexSearcherRef searcher : indexSearchers.values()) {
      try {
        searcher.dispose(true);
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
    indexSearchers.clear();
  }
  if (xaContainer != null) {
    xaContainer.close();
  }
  providerStore.close();
}
