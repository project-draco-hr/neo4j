{
synchronized (this) {
    if (closed) {
      return;
    }
    closed=true;
    for (    IndexReference searcher : indexSearchers.values()) {
      searcher.dispose(true);
    }
    indexSearchers.clear();
  }
  if (xaContainer != null) {
    xaContainer.close();
    unbindLogicalLog();
  }
  providerStore.close();
}
