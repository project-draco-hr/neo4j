{
  this.filesystemFacade=config.get(Configuration.ephemeral) ? LuceneFilesystemFacade.MEMORY : LuceneFilesystemFacade.FS;
  indexSearchers=new IndexClockCache(config.get(Configuration.lucene_searcher_cache_size));
  caching=new Cache();
