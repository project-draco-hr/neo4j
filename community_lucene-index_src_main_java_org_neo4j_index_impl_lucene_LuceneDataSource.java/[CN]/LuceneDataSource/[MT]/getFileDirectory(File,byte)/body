{
  File path=new File(storeDir,"lucene");
  String extra=null;
  if (entityType == LuceneCommand.NODE) {
    extra="node";
  }
 else   if (entityType == LuceneCommand.RELATIONSHIP) {
    extra="relationship";
  }
 else {
    throw new RuntimeException("" + entityType);
  }
  return new File(path,extra);
}
