{
  String path=getDbPath();
  Neo4jTestCase.deleteFileOrDirectory(new File(path));
  assertEquals(0,Runtime.getRuntime().exec(new String[]{"java","-cp",System.getProperty("java.class.path"),AddRelToIndex.class.getName(),getDbPath()}).waitFor());
  Map<Object,Object> params=MapUtil.genericMap("store_dir",getDbPath(),IndexStore.class,new IndexStore(getDbPath()),LogBufferFactory.class,CommonFactories.defaultLogBufferFactory(MapUtil.stringMap()));
  LuceneDataSource ds=new LuceneDataSource(params);
  ds.close();
}
