{
  KernelTransactionImplementation transaction=mock(KernelTransactionImplementation.class);
  when(transaction.shouldBeTerminated()).thenReturn(true);
  when(transaction.mode()).thenReturn(AccessMode.FULL);
  KernelStatement statement=new KernelStatement(transaction,null,null,null,null,null);
  statement.readOperations().nodeExists(0);
}
