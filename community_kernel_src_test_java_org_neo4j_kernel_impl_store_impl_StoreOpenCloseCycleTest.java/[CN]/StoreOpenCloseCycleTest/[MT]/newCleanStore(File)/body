{
  EphemeralFileSystemAbstraction fs=fsRule.get();
  StoreOpenCloseCycle cycle=new StoreOpenCloseCycle(StringLogger.DEV_NULL,storeFile,new TestHeaderlessStoreFormat(),fs);
  fs.create(storeFile);
  StoreChannel channel=fs.open(storeFile,"rw");
  cycle.openStore(channel);
  cycle.closeStore(channel,1);
  return channel;
}
