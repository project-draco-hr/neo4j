{
  Iterator<ConstraintDefinition> definitions=map(new Function<PropertyConstraint,ConstraintDefinition>(){
    @Override public ConstraintDefinition apply(    PropertyConstraint constraint){
      int labelId=constraint.label();
      try {
        Label label=label(readOperations.labelGetName(labelId));
        return new PropertyConstraintDefinition(actions,label,readOperations.propertyKeyGetName(constraint.propertyKeyId()),constraint.type());
      }
 catch (      PropertyKeyIdNotFoundKernelException e) {
        throw new ThisShouldNotHappenError("Mattias","Couldn't find property name for " + constraint.propertyKeyId(),e);
      }
catch (      LabelNotFoundKernelException e) {
        throw new ThisShouldNotHappenError("Stefan","Couldn't find label name for label id " + labelId,e);
      }
    }
  }
,constraints);
  return asCollection(definitions);
}
