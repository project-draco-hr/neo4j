{
  long nodeId=-1;
  Node node=getGraphDb().createNode();
  nodeId=node.getId();
  getGraphDb().getNodeById(nodeId);
  node.delete();
  Transaction tx=getTransaction();
  tx.success();
  tx.finish();
  setTransaction(getGraphDb().beginTx());
  try {
    getGraphDb().getNodeById(nodeId);
    fail("Node[" + nodeId + "] should be deleted.");
  }
 catch (  NotFoundException e) {
  }
}
