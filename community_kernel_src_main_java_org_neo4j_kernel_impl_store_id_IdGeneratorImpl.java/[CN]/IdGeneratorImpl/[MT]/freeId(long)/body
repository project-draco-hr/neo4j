{
  if (id == INTEGER_MINUS_ONE) {
    return;
  }
  if (fileChannel == null) {
    throw new IllegalStateException("Generator closed " + file);
  }
  if (id < 0 || id >= highId.get()) {
    throw new IllegalArgumentException("Illegal id[" + id + "], highId is "+ highId.get());
  }
  keeper.freeId(id);
}
