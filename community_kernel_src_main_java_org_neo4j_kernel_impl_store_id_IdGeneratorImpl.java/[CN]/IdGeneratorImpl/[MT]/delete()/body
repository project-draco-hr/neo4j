{
  if (!isClosed()) {
    try {
      closeChannel();
    }
 catch (    IOException e) {
      throw new UnderlyingStorageException("Unable to safe close id generator " + file,e);
    }
  }
  if (!fs.deleteFile(file)) {
    throw new UnderlyingStorageException("Unable to delete id generator " + file);
  }
}
