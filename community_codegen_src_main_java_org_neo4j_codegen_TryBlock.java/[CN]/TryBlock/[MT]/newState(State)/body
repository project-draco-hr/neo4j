{
  return new State(){
    private final List<Consumer<MethodEmitter>> actions=new LinkedList<>();
    @Override public void addAction(    Consumer<MethodEmitter> action){
      actions.add(action);
    }
    @Override public List<Consumer<MethodEmitter>> actions(){
      return actions;
    }
    @Override public State nextState(){
      return parent;
    }
  }
;
}
