{
  StampedLock lock=new StampedLock();
  lock.writeLock();
  StampedLock clone=serialClone(lock);
  assertTrue(lock.isWriteLocked());
  assertFalse(clone.isWriteLocked());
  long s=clone.writeLock();
  assertTrue(clone.isWriteLocked());
  clone.unlockWrite(s);
  assertFalse(clone.isWriteLocked());
}
