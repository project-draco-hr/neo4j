{
  StoreFile.deleteIdFile(fileSystem,migrationDir,allExcept(StoreFile.RELATIONSHIP_GROUP_STORE));
  StoreFile.deleteStoreFile(fileSystem,migrationDir,allExcept(StoreFile.NODE_STORE,StoreFile.RELATIONSHIP_STORE,StoreFile.RELATIONSHIP_GROUP_STORE));
  StoreFile.move(fileSystem,storeDir,leftOversDir,IteratorUtil.<StoreFile>asIterable(StoreFile.NODE_STORE,StoreFile.RELATIONSHIP_STORE),false,false,StoreFileType.STORE);
  StoreFile.move(fileSystem,migrationDir,storeDir,StoreFile.currentStoreFiles(),true,true,StoreFileType.values());
  StoreFile.ensureStoreVersion(fileSystem,storeDir,StoreFile.currentStoreFiles());
}
