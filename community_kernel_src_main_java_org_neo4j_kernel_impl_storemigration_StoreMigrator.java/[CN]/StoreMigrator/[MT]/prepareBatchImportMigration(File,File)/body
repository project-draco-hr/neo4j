{
  BatchingNeoStore.createStore(fileSystem,migrationDir.getPath());
  Iterable<StoreFile> storeFiles=iterable(StoreFile.NODE_LABEL_STORE);
  StoreFile.fileOperation(COPY,fileSystem,storeDir,migrationDir,storeFiles,true,false,StoreFileType.values());
  StoreFile.ensureStoreVersion(fileSystem,pageCache,migrationDir,storeFiles);
}
