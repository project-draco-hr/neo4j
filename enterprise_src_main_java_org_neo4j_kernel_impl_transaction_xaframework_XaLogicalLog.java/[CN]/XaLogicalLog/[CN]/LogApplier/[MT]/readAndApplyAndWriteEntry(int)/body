{
  LogEntry entry=LogIoUtils.readEntry(buffer,byteChannel,cf);
  if (entry != null) {
    entry.setIdentifier(newXidIdentifier);
    if (entry instanceof LogEntry.Commit) {
      LogIoUtils.writeLogEntry(entry,writeBuffer);
      applyEntry(entry);
    }
 else {
      applyEntry(entry);
      LogIoUtils.writeLogEntry(entry,writeBuffer);
    }
    return true;
  }
  return false;
}
