{
  File path=new File("/tmp/asd");
  if (path.exists()) {
    FileUtils.deleteRecursively(path);
  }
  db=new GraphDatabaseFactory().newEmbeddedDatabaseBuilder(path).setConfig(GraphDatabaseSettings.auth_store,new File(path,"auth").getAbsolutePath()).setConfig(ServerSettings.tls_certificate_file,new File(path,"cert.pem").getAbsolutePath()).setConfig(ServerSettings.tls_key_file,new File(path,"key.pem").getAbsolutePath()).newGraphDatabase();
  try (Transaction tx=db.beginTx()){
    db.execute("CREATE INDEX ON :User(name)");
    tx.success();
  }
   try (Transaction tx=db.beginTx()){
    db.schema().awaitIndexesOnline(10,TimeUnit.SECONDS);
    tx.success();
  }
 }
