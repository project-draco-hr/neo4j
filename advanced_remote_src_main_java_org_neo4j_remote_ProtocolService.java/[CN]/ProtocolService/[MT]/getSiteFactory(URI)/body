{
  Transport result=loadSiteFactory(resourceUri);
  if (result == null) {
    for (    Transport factory : factories) {
      register(factory);
    }
    result=loadSiteFactory(resourceUri);
  }
  if (result != null) {
    return result;
  }
  throw new RuntimeException("No implementation available to handle resource URI: " + resourceUri + "\nSupported protocolls are: "+ allProtocols());
}
