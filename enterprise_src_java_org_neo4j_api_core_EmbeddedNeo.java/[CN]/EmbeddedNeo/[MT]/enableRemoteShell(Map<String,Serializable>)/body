{
  try {
    this.shellServer=new NeoShellServer(this);
    Object port=initialProperties.get("port");
    Object name=initialProperties.get("name");
    this.shellServer.makeRemotelyAvailable(port != null ? (Integer)port : 1337,name != null ? (String)name : "neoshell");
  }
 catch (  RemoteException e) {
    throw new IllegalStateException("Can't start remote neo shell: " + e);
  }
}
