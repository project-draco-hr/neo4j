{
  StatementContext context=statementCtxProvider.getCtxForReading();
  try {
    return context.nodeHasLabel(getId(),context.labelGetForName(label.name()));
  }
 catch (  LabelNotFoundKernelException e) {
    return false;
  }
catch (  EntityNotFoundException e) {
    return false;
  }
 finally {
    context.close();
  }
}
