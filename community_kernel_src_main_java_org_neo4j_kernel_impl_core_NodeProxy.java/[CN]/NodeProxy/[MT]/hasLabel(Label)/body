{
  StatementContext ctx=statementCtxProvider.getCtxForReading();
  try {
    return ctx.nodeHasLabel(getId(),ctx.labelGetForName(label.name()));
  }
 catch (  LabelNotFoundKernelException e) {
    return false;
  }
catch (  EntityNotFoundException e) {
    return false;
  }
 finally {
    ctx.close();
  }
}
