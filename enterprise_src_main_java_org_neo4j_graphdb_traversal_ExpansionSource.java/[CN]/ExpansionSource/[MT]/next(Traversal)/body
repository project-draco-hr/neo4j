{
  if (relationships == null) {
    if (traversal.uniquness.type == PrimitiveTypeFetcher.RELATIONSHIP || traversal.okToProceed(this)) {
      expandRelationships(traversal,false);
      return this;
    }
 else {
      return null;
    }
  }
  while (relationships.hasNext()) {
    Relationship relationship=relationships.next();
    if (relationship.equals(howIGotHere)) {
      continue;
    }
    Node node=relationship.getOtherNode(source);
    ExpansionSource next=new ExpansionSource(traversal,this,node,expander,relationship);
    if (traversal.okToProceed(next)) {
      return next;
    }
  }
  return null;
}
