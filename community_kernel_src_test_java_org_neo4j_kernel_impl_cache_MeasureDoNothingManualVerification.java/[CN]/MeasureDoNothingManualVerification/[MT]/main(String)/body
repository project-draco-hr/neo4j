{
  Log log=FormattedLog.toOutputStream(System.out);
  new Thread(new MeasureDoNothing("GC Monitor",log,100,1)).start();
  ExecutorService executorService=Executors.newFixedThreadPool(4);
  for (  Future<Object> objectFuture : executorService.invokeAll(asList(new GCHeavyJob(),new GCHeavyJob(),new GCHeavyJob(),new GCHeavyJob()))) {
    objectFuture.get();
  }
}
