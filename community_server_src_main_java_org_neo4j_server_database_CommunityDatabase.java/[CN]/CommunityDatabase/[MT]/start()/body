{
  try {
    this.graph=(AbstractGraphDatabase)new org.neo4j.graphdb.factory.GraphDatabaseFactory().newEmbeddedDatabaseBuilder(serverConfiguration.getString(DATABASE_LOCATION_PROPERTY_KEY,DEFAULT_DATABASE_LOCATION_PROPERTY_KEY)).setConfig(getDbTuningPropertiesWithServerDefaults()).newGraphDatabase();
    log.info("Successfully started database");
  }
 catch (  Exception e) {
    log.error("Failed to start database.",e);
    throw e;
  }
}
