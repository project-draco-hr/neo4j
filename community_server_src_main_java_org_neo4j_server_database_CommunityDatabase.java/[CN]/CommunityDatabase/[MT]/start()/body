{
  try {
    this.graph=(GraphDatabaseAPI)new org.neo4j.graphdb.factory.GraphDatabaseFactory().newEmbeddedDatabaseBuilder(serverConfig.getString(Configurator.DATABASE_LOCATION_PROPERTY_KEY,Configurator.DEFAULT_DATABASE_LOCATION_PROPERTY_KEY)).setConfig(loadNeo4jProperties()).newGraphDatabase();
    log.info("Successfully started database");
  }
 catch (  Exception e) {
    log.error("Failed to start database.",e);
    throw e;
  }
}
