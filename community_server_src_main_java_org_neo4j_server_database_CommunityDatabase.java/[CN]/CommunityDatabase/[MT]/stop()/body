{
  try {
    if (this.graph != null) {
      this.graph.shutdown();
      isRunning=false;
      this.graph=null;
      log.info("Successfully stopped database");
    }
  }
 catch (  Exception e) {
    log.error(String.format("Database did not stop cleanly. Reason [%s]",e.getMessage()));
    throw e;
  }
}
