{
  String location=POST(getDataUri() + "transaction",set()).location();
  ResponseEntity response=gen.get().expectedStatus(200).payload(createJsonFrom(set(map("statement","CREATE n RETURN id(n)")))).post(location + "/commit");
  Map<String,Object> result=jsonToMap(response.entity());
  assertNoErrors(result);
  Integer id=resultCell(result,0,0);
  assertThat(GET(getNodeUri(id)).status(),is(200));
}
