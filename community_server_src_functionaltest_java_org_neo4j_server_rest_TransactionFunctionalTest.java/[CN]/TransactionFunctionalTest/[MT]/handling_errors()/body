{
  String location=POST(getDataUri() + "transaction",set()).location();
  ResponseEntity response=gen.get().expectedStatus(200).payload(createJsonFrom(set(map("statement","This is not a valid Cypher Statement.")))).post(location + "/commit");
  Map<String,Object> result=jsonToMap(response.entity());
  assertErrors(result,Neo4jError.Code.UNKNOWN_STATEMENT_ERROR);
}
