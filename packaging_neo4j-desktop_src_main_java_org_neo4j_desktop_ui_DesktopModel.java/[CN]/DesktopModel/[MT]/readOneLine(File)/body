{
  try {
    if (file.exists() && file.isFile() && file.canRead()) {
      FileReader reader=new FileReader(file);
      try {
        BufferedReader buffered=new BufferedReader(reader);
        try {
          return buffered.readLine();
        }
  finally {
          buffered.close();
        }
      }
  finally {
        reader.close();
      }
    }
  }
 catch (  IOException e) {
    return null;
  }
  return null;
}
