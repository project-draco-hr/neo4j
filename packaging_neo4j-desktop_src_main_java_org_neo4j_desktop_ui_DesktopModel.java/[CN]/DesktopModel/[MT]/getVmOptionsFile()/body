{
  try {
    File jarFile=new File(MainWindow.class.getProtectionDomain().getCodeSource().getLocation().toURI());
    return new File(jarFile.getParentFile(),".vmoptions");
  }
 catch (  URISyntaxException e) {
    if (printStackTraces) {
      e.printStackTrace(System.out);
    }
  }
  return null;
}
