{
  logger.getLogger(getClass()).logMessage("Got master rebound");
  Listeners.notifyListeners(haListeners,new Listeners.Notification<ClusterMemberListener>(){
    @Override public void notify(    ClusterMemberListener listener){
      listener.memberIsAvailable(HighAvailabilityModeSwitcher.MASTER,new InstanceId(client.getCachedMaster().getMachineId()),URI.create("ha://" + client.getCachedMaster().getServerAsString()));
    }
  }
);
}
