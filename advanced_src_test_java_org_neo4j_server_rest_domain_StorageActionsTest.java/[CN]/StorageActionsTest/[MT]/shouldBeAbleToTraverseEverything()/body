{
  long startNode=createBasicTraversableGraph();
  List<Representation> hits=actions.traverseAndCollect(startNode,MapUtil.map("return filter",MapUtil.map("language","javascript","body","true;"),"max depth",10),TraverserReturnType.node);
  assertEquals(6,hits.size());
  hits=actions.traverseAndCollect(startNode,MapUtil.map("return filter",MapUtil.map("language","builtin","name","all"),"max depth",10),TraverserReturnType.node);
  assertEquals(6,hits.size());
}
