{
  if (item instanceof WebDriver) {
    WebDriver d=(WebDriver)item;
    try {
      return !d.findElement(by).getCssValue("display").equals("none");
    }
 catch (    NoSuchElementException e) {
      return false;
    }
catch (    StaleElementReferenceException e) {
      return false;
    }
  }
  return false;
}
