{
  Result r=sh("cd " + neo4jInstallDir() + " && sudo chmod +x bin/neo4j-backup && sudo bin/neo4j-backup"+ " -"+ mode+ " -from ha://"+ coordinatorAddresses+ " -to "+ BACKUP_DIR+ "/"+ backupName);
  if (!r.getOutput().contains("Done")) {
    throw new RuntimeException("Performing backup failed. Expected output to say 'Done', got this instead: \n" + r.getOutput());
  }
}
