{
  AssertableLogProvider log=new AssertableLogProvider(true);
  Config config=new Config(stringMap(connector(0,enabled).name(),"true"));
  life=new LifeSupport();
  life.add(new BoltKernelExtension().newInstance(mock(KernelContext.class),stubDependencies(config,log)));
  life.start();
  log.assertAtLeastOnce(inLog("io.netty.buffer.PooledByteBufAllocator").debug(equalTo("-Dio.netty.allocator.cacheTrimInterval: %s"),greaterThan(0)));
}
