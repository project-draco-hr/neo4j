{
  return new BoltKernelExtension.Dependencies(){
    @Override public LogService logService(){
      return new SimpleLogService(log,log);
    }
    @Override public Config config(){
      return config;
    }
    @Override public GraphDatabaseService db(){
      return mock(GraphDatabaseAPI.class);
    }
    @Override public JobScheduler scheduler(){
      return mock(JobScheduler.class);
    }
    @Override public UsageData usageData(){
      return new UsageData();
    }
    @Override public Monitors monitors(){
      return new Monitors();
    }
    @Override public AuthManager authManager(){
      return mock(AuthManager.class);
    }
    @Override public ThreadToStatementContextBridge txBridge(){
      return mock(ThreadToStatementContextBridge.class);
    }
    @Override public QueryExecutionEngine queryEngine(){
      return mock(QueryExecutionEngine.class);
    }
  }
;
}
