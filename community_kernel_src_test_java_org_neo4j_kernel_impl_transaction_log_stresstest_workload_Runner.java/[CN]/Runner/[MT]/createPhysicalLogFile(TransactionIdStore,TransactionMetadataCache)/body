{
  DefaultFileSystemAbstraction fs=new DefaultFileSystemAbstraction();
  PhysicalLogFiles logFiles=new PhysicalLogFiles(workingDirectory,fs);
  long rotateAtSize=Settings.BYTES.apply(GraphDatabaseSettings.logical_log_rotation_threshold.getDefaultValue());
  DeadSimpleLogVersionRepository logVersionRepository=new DeadSimpleLogVersionRepository(0);
  return new PhysicalLogFile(fs,logFiles,rotateAtSize,transactionIdStore,logVersionRepository,NOOP_LOGFILE_MONITOR,transactionMetadataCache);
}
