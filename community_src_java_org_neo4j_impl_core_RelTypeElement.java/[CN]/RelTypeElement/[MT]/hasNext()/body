{
  if (nextElement != null) {
    return true;
  }
  while (!srcTraversed && position < src.length()) {
    int value=src.get(position++);
    if (position >= src.length()) {
      srcTraversed=true;
      position=0;
    }
    if (!remove.contains(value)) {
      nextElement=value;
      return true;
    }
  }
  while (!addTraversed && position < add.length()) {
    int value=add.get(position++);
    if (position >= add.length()) {
      addTraversed=true;
      position=0;
    }
    if (!remove.contains(value)) {
      nextElement=value;
      return true;
    }
  }
  return false;
}
