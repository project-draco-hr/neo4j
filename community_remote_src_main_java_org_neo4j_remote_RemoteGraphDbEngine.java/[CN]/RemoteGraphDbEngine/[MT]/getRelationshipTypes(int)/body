{
  return new BatchIterable<String>(){
    @Override IterableSpecification<String> init(){
      return receive(connection.getRelationshipTypes(txId));
    }
    @Override IterableSpecification<String> more(    int requestToken){
      return receive(connection.getMoreRelationshipTypes(txId,requestToken));
    }
    @Override void done(    int requestToken){
      receive(connection.closeRelationshipTypeIterator(txId,requestToken));
    }
  }
;
}
