{
  keeper=new ZooKeeperMember[ports.length];
  boolean success=false;
  try {
    ZooKeeperProcess subprocess=new ZooKeeperProcess(null);
    for (int i=0; i < keeper.length; i++)     keeper[i]=subprocess.start(new String[]{config(target,i + 1,ports[i])});
    verify(null);
    success=true;
  }
 catch (  Exception e) {
    throw Exceptions.launderedException(e);
  }
 finally {
    if (!success)     shutdown();
  }
}
