{
  String url=createCsvFile("13");
  exception.expect(QueryExecutionException.class);
  exception.expectMessage("Cannot use periodic commit in a non-updating query (line 1, column 1 (offset: 0))");
  db.execute("USING PERIODIC COMMIT 1 " + "LOAD CSV FROM '" + url + "' AS line "+ "CALL org.neo4j.procedure.simpleArgument(toInt(line[0])) YIELD someVal as val "+ "RETURN val");
}
