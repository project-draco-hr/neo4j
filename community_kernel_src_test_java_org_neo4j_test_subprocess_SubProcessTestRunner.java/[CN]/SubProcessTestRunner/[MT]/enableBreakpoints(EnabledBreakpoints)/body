{
  Set<String> enabled=new HashSet<String>();
  if (breakpoints != null)   for (  String name : breakpoints.value()) {
    enabled.add(name);
  }
  for (  Map.Entry<String,BreakPoint> bp : this.breakpoints.entrySet()) {
    if (enabled.contains(bp.getKey())) {
      bp.getValue().enable();
    }
 else {
      bp.getValue().disable();
    }
  }
}
