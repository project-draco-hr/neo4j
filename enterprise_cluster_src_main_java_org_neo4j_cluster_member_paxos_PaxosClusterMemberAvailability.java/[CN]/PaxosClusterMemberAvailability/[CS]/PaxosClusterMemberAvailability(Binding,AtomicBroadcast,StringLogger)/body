{
  this.binding=binding;
  this.atomicBroadcast=atomicBroadcast;
  this.logger=logger;
  bindingListener=new BindingListener(){
    @Override public void listeningAt(    URI me){
      serverClusterId=me;
      PaxosClusterMemberAvailability.this.logger.logMessage("Listening at:" + me);
    }
  }
;
}
