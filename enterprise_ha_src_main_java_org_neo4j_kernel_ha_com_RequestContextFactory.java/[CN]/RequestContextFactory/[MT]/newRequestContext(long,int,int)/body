{
  long latestTxId=txIdStore.getLastCommittingTransactionId();
  TransactionMetadataCache.TransactionMetadata txMetadata=null;
  try {
    txMetadata=txStore.getMetadataFor(latestTxId);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  if (txMetadata != null) {
    return new RequestContext(sessionId,machineId,eventIdentifier,latestTxId,txMetadata.getMasterId(),txMetadata.getAuthorId());
  }
 else {
    return new RequestContext(sessionId,machineId,eventIdentifier,latestTxId,-1,-1);
  }
}
