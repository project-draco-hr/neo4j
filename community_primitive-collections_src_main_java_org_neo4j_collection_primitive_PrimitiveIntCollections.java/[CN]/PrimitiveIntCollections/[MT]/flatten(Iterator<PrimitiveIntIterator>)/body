{
  return new PrimitiveIntBaseIterator(){
    private PrimitiveIntIterator current;
    @Override protected boolean fetchNext(){
      while (current == null || !current.hasNext()) {
        if (!source.hasNext()) {
          return false;
        }
        current=source.next();
      }
      return source.hasNext() ? next(current.next()) : false;
    }
  }
;
}
