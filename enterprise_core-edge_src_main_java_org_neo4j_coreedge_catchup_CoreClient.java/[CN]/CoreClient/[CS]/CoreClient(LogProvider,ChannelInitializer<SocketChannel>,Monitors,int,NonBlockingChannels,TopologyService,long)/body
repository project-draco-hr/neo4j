{
  this.logProvider=logProvider;
  this.senderService=new SenderService(channelInitializer,logProvider,monitors,maxQueueSize,nonBlockingChannels);
  this.outbound=new CoreOutbound(discoveryService,senderService,logProvider,logThresholdMillis);
  this.pullRequestMonitor=monitors.newMonitor(PullRequestMonitor.class);
}
