{
  Iterator<PropertyConstraint> constraints=storeLayer.constraintsGetAll();
  while (constraints.hasNext()) {
    PropertyConstraint constraint=constraints.next();
    if (constraint.type() == ConstraintType.MANDATORY_NODE_PROPERTY || constraint.type() == ConstraintType.MANDATORY_RELATIONSHIP_PROPERTY) {
      return new MandatoryPropertyEnforcer(operations.entityReadOperations(),txStateToRecordStateVisitor,this,storeLayer,storeStatement);
    }
  }
  return txStateToRecordStateVisitor;
}
