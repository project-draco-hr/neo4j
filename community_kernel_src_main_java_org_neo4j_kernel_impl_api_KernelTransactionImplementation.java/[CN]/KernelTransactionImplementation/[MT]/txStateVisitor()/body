{
  Iterator<PropertyConstraint> constraints=storeLayer.constraintsGetAll();
  while (constraints.hasNext()) {
    PropertyConstraint constraint=constraints.next();
    if (constraint.type() == ConstraintType.NODE_PROPERTY_EXISTENCE || constraint.type() == ConstraintType.RELATIONSHIP_PROPERTY_EXISTENCE) {
      return new PropertyExistenceEnforcer(operations.entityReadOperations(),txStateToRecordStateVisitor,this,storeLayer,storeStatement);
    }
  }
  return txStateToRecordStateVisitor;
}
