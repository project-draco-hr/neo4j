{
  try {
    markAsClosed(txId);
    if (beforeHookInvoked) {
      hooks.afterCommit(txState,this,hooksState);
    }
  }
  finally {
    transactionMonitor.transactionFinished(true,hasTxStateWithChanges());
    System.out.println(Thread.currentThread().getName() + ": Committed " + super.toString());
  }
}
