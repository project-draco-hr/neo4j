{
  assertTransactionOpen();
  if (currentStatement == null) {
    IndexReaderFactory.Caching caching=new IndexReaderFactory.Caching(storageEngine.indexingService());
    currentStatement=new KernelStatement(this,caching,storageEngine.labelScanStore(),this,locks,operations,storeStatement);
  }
  currentStatement.acquire();
  return currentStatement;
}
