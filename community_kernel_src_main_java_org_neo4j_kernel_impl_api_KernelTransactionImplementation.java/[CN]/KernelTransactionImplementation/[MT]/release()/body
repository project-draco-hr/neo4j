{
  terminationReleaseLock.lock();
  try {
    statementLocks.close();
    statementLocks=null;
    terminationReason=null;
    type=null;
    accessMode=null;
    transactionEvent=null;
    legacyIndexTransactionState=null;
    txState=null;
    hooksState=null;
    closeListener=null;
    reuseCount++;
    pool.release(this);
  }
  finally {
    terminationReleaseLock.unlock();
  }
}
