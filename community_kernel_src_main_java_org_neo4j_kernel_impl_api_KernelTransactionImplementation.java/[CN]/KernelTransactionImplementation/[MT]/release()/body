{
  terminationReleaseLock.lock();
  try {
    locks.close();
    locks=null;
    terminated=false;
    terminationReason=null;
    pool.release(this);
    if (storeStatement != null) {
      storeStatement.close();
      storeStatement=null;
    }
  }
  finally {
    reuseCount++;
    terminationReleaseLock.unlock();
  }
}
