{
  terminationReleaseLock.lock();
  try {
    locks.close();
    locks=null;
    terminated=false;
    type=null;
    accessMode=null;
    transactionEvent=null;
    legacyIndexTransactionState=null;
    txState=null;
    hooksState=null;
    closeListener=null;
    reuseCount++;
    pool.release(this);
  }
  finally {
    terminationReleaseLock.unlock();
  }
}
