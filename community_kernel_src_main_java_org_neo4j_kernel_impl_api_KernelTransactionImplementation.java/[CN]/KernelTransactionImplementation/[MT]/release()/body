{
  terminationReuseLock.lock();
  try {
    locks.close();
    locks=null;
    terminationReason=null;
    type=null;
    accessMode=null;
    transactionEvent=null;
    legacyIndexTransactionState=null;
    txState=null;
    hooksState=null;
    closeListener=null;
    reuseCount++;
    pool.release(this);
  }
  finally {
    terminationReuseLock.unlock();
  }
}
