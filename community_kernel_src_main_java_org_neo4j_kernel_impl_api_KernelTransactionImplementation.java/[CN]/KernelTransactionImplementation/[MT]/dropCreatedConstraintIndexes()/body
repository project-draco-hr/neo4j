{
  if (hasTxStateWithChanges()) {
    for (    IndexDescriptor createdConstraintIndex : txState().constraintIndexesCreatedInTx()) {
      try {
        constraintIndexCreator.dropUniquenessConstraintIndex(createdConstraintIndex);
      }
 catch (      DropIndexFailureException e) {
        throw new IllegalStateException("Constraint index that was created in a transaction should be " + "possible to drop during rollback of that transaction.",e);
      }
catch (      TransactionFailureException e) {
        throw e;
      }
    }
  }
}
