{
  return new IteratorWrapper<InputNode,InputNode>(nodes){
    private long lastSeenId=-1;
    @Override protected InputNode underlyingObjectToObject(    InputNode node){
      if (lastSeenId != -1 && node.id() < lastSeenId) {
        throw new IllegalArgumentException("Cannot go backwards in node id sequence, last seen was " + lastSeenId + ", given id is "+ node.id());
      }
      lastSeenId=node.id();
      return node;
    }
  }
;
}
