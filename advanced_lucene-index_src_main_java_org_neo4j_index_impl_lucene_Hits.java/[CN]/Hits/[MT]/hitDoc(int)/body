{
  if (n >= lengthAtStart) {
    throw new IndexOutOfBoundsException("Not a valid hit number: " + n);
  }
  if (n >= hitDocs.size()) {
    getMoreDocs(n);
  }
  if (n >= length) {
    throw new ConcurrentModificationException("Not a valid hit number: " + n);
  }
  return (HitDoc)hitDocs.elementAt(n);
}
