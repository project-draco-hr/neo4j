{
  RemoteNode node=null;
  if (nodeCache != null) {
    node=nodeCache.get(nodeId);
  }
  if (node == null) {
    if (verified || engine.hasNodeWithId(id,nodeId)) {
      node=newNode(nodeId);
    }
 else {
      throw new NotFoundException("TODO: exception message. No node with id=" + nodeId);
    }
  }
  return node;
}
