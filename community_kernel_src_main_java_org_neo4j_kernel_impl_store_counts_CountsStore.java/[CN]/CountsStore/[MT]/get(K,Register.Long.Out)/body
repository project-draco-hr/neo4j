{
  int min=header.headerRecords();
  int max=min + totalRecords - 1;
  int mid;
  try (PageCursor cursor=pages.io(0,PF_SHARED_LOCK)){
    while (min <= max) {
      mid=min + (max - min) / 2;
      int cmp=compareKeyAndReadValue(cursor,key,mid,value);
      if (cmp == 0) {
        return;
      }
 else       if (cmp < 0) {
        max=mid - 1;
      }
 else {
        min=mid + 1;
      }
    }
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
  value.write(0);
}
