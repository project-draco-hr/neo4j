{
  try {
    int offset=page.getOffset();
    if (offset >= pages.pageSize()) {
      page.next();
      offset=0;
    }
    do {
      page.setOffset(offset);
      page.putInt(startLabelId);
      page.putInt(typeId);
      page.putInt(endLabelId);
      page.putLong(count);
    }
 while (page.shouldRetry());
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
