{
  if (data != null) {
    TransactionEntry entry=data.read(buffer);
    if (entry != null) {
      data=null;
      return addEntry(entry);
    }
  }
 else   if (buffer.readableBytes() >= HEADER_SIZE) {
    data=tryInitStream(buffer.getUnsignedShort(buffer.readerIndex()),buffer);
  }
  return null;
}
