{
  File path=path(11);
  startAndStopDb(path);
  setOlderNeoStoreVersion(path);
  try {
    startDb(path);
    fail("Shouldn't be able to upgrade if not told to");
  }
 catch (  TransactionFailureException e) {
    if (!(e.getCause() instanceof NotCurrentStoreVersionException)) {
      throw e;
    }
  }
}
