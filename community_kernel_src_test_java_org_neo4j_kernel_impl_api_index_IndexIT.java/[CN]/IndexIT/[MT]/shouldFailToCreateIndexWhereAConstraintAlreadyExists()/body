{
  newTransaction();
  statement.uniquenessConstraintCreate(getState(),labelId,propertyKey);
  commit();
  try {
    newTransaction();
    statement.indexCreate(getState(),labelId,propertyKey);
    commit();
    fail("expected exception");
  }
 catch (  SchemaKernelException e) {
    assertEquals("Unable to add index :label[5](property[8]) : " + "Already constrained CONSTRAINT ON ( n:label[5] ) " + "ASSERT n.property[8] IS UNIQUE.",e.getMessage());
  }
}
