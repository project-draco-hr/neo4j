{
  newTransaction();
  IndexDescriptor index=statement.indexCreate(getState(),labelId,propertyKey);
  commit();
  newTransaction();
  statement.indexDrop(getState(),index);
  commit();
  try {
    newTransaction();
    statement.indexDrop(getState(),index);
    commit();
  }
 catch (  SchemaKernelException e) {
    assertEquals("Unable to drop index on :label[5](property[8]): No such INDEX ON :label[5](property[8]).",e.getMessage());
  }
}
