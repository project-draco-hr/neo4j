{
  newTransaction();
  statement.addConstraintIndex(labelId,propertyKey);
  commit();
  try {
    newTransaction();
    statement.addIndex(labelId,propertyKey);
    commit();
    fail("expected exception");
  }
 catch (  SchemaKernelException e) {
    assertEquals(format("Already constrained CONSTRAINT ON ( n:label[%s] ) ASSERT n.property[%s] IS UNIQUE.",labelId,propertyKey),e.getMessage());
  }
}
