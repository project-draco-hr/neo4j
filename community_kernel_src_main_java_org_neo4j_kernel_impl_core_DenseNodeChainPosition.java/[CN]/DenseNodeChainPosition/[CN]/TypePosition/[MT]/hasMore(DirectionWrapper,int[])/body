{
  if (direction == DirectionWrapper.BOTH) {
    return directions.get(DirectionWrapper.OUTGOING).hasMore(direction,types) || directions.get(DirectionWrapper.INCOMING).hasMore(direction,types) || directions.get(DirectionWrapper.BOTH).hasMore(direction,types);
  }
 else {
    return directions.get(direction).hasMore(direction,types) || directions.get(DirectionWrapper.BOTH).hasMore(direction,types);
  }
}
