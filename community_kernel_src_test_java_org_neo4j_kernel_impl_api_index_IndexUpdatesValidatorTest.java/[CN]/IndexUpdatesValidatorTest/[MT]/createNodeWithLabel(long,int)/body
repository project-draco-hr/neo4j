{
  NodeRecord before=new NodeRecord(nodeId,false,false,NO_NEXT_RELATIONSHIP.intValue(),NO_NEXT_PROPERTY.intValue(),NO_LABELS_FIELD.intValue());
  NodeRecord after=new NodeRecord(nodeId,true,false,NO_NEXT_RELATIONSHIP.intValue(),NO_NEXT_PROPERTY.intValue(),NO_LABELS_FIELD.intValue());
  new InlineNodeLabels(after.getLabelField(),after).put(new long[]{label},null,null);
  return new NodeCommand().init(before,after);
}
