{
  if (io_flags == IO_FLAGS_VERIFY_RECORDS) {
    verifyRecordsMatchExpected(pageId,page);
  }
 else   if (io_flags == IO_FLAGS_WRITE_RECORDS) {
    writeRecords(pageId,page);
  }
 else   if (io_flags == IO_FLAGS_CORRUPT_RECORDS) {
    corruptRecords(page);
  }
  this.pageId=pageId;
  this.page=page;
}
