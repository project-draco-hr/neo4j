{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  try (PagedFile pagedFile=pageCache.map(file("a"),filePageSize);PageCursor cursor=pagedFile.io(0,PF_SHARED_WRITE_LOCK)){
    assertTrue(cursor.next());
  }
   try (PagedFile pagedFile=pageCache.map(file("a"),filePageSize);PageCursor cursor=pagedFile.io(0,PF_SHARED_WRITE_LOCK | PF_NO_GROW)){
    assertTrue(cursor.next());
    assertFalse(cursor.next());
    action.apply(cursor);
  }
 }
