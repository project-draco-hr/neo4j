{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  long startPageId=0;
  long endPageId=recordCount / recordsPerFilePage;
  File file=file("a");
  try (PagedFile pagedFile=pageCache.map(file,filePageSize);PageCursor cursor=pagedFile.io(startPageId,PF_EXCLUSIVE_LOCK)){
    while (cursor.getCurrentPageId() < endPageId && cursor.next()) {
      do {
        writeRecords(cursor);
      }
 while (cursor.shouldRetry());
    }
  }
  finally {
    pageCache.close();
  }
  verifyRecordsInFile(file,recordCount);
}
