{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheMonitor.NULL);
  File fileA=new File("a");
  File fileB=new File("b");
  try (PagedFile pfA=pageCache.map(fileA,filePageSize);PagedFile pfB=pageCache.map(fileB,filePageSize);PageCursor a=pfA.io(0,PF_EXCLUSIVE_LOCK);PageCursor b=pfB.io(0,PF_SHARED_LOCK)){
    fail("The second io() call should have thrown");
  }
