{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  try (PagedFile pagedFile=pageCache.map(file("a"),filePageSize);PageCursor writer=pagedFile.io(0,PF_SHARED_WRITE_LOCK);PageCursor reader=pagedFile.io(0,PF_SHARED_READ_LOCK)){
    assertTrue(writer.next());
    writer.raiseOutOfBounds();
    assertTrue(writer.checkAndClearBoundsFlag());
    assertTrue(reader.next());
    reader.raiseOutOfBounds();
    assertTrue(reader.checkAndClearBoundsFlag());
  }
 }
