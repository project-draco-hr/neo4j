{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  try (PagedFile pf=pageCache.map(file("a"),filePageSize,StandardOpenOption.READ,StandardOpenOption.WRITE,StandardOpenOption.APPEND,StandardOpenOption.SPARSE);PageCursor cursor=pf.io(0,PF_EXCLUSIVE_LOCK)){
    assertTrue(cursor.next());
  }
 }
