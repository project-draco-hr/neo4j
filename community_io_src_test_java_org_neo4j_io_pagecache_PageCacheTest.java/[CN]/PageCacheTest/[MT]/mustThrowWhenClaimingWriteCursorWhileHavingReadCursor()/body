{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  try (PagedFile pfA=pageCache.map(existingFile("a"),filePageSize);PagedFile pfB=pageCache.map(existingFile("b"),filePageSize);PageCursor a=pfA.io(0,PF_SHARED_LOCK);PageCursor b=pfB.io(0,PF_EXCLUSIVE_LOCK)){
    fail("The second io() call should have thrown");
  }
 }
