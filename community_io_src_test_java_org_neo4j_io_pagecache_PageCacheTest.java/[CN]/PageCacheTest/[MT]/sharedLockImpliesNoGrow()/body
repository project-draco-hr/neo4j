{
  int initialPages=3;
  generateFileWithRecords(file("a"),recordsPerFilePage * initialPages,recordSize);
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  int pagesChecked=0;
  try (PagedFile pagedFile=pageCache.map(file("a"),filePageSize);PageCursor cursor=pagedFile.io(0L,PF_SHARED_LOCK)){
    while (cursor.next()) {
      pagesChecked++;
    }
  }
   assertThat(pagesChecked,is(initialPages));
}
