{
  generateFileWithRecords(file,1,recordSize);
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheMonitor.NULL);
  try (PagedFile pagedFile=pageCache.map(file,filePageSize);PageCursor cursor=pagedFile.io(0,PF_EXCLUSIVE_LOCK)){
    cursor.next();
    for (int i=0; i < 100000; i++) {
      action.apply(cursor);
    }
  }
