{
  getPageCache(fs,maxPages,pageCachePageSize,PageCacheTracer.NULL);
  File file=file("a");
  try (PagedFile pf=pageCache.map(file,filePageSize);PageCursor parent=pf.io(0,PF_SHARED_WRITE_LOCK)){
    PageCursor linked=parent.openLinkedCursor(1);
    assertTrue(parent.next());
    assertTrue(linked.next());
    writeRecords(parent);
    writeRecords(linked);
  }
   verifyRecordsInFile(file,recordsPerFilePage * 2);
}
