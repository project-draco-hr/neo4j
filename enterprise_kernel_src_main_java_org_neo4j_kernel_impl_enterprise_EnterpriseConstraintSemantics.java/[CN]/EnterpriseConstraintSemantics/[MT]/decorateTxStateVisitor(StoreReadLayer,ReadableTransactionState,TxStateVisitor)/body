{
  Iterator<PropertyConstraint> constraints=storeLayer.constraintsGetAll();
  while (constraints.hasNext()) {
    PropertyConstraint constraint=constraints.next();
    if (constraint instanceof NodePropertyExistenceConstraint || constraint instanceof RelationshipPropertyExistenceConstraint) {
      return new PropertyExistenceEnforcer(visitor,txState,storeLayer);
    }
  }
  return visitor;
}
