{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final pathObject other=(pathObject)obj;
  if (node == null) {
    if (other.node != null)     return false;
  }
 else   if (!node.equals(other.node))   return false;
  return true;
}
