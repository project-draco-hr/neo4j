{
  AssertableLogProvider provider=new AssertableLogProvider();
  ErrorTranslator translator=new ErrorTranslator(provider.getLog("userlog"),provider.getLog("internallog"));
  Throwable cause=new Throwable("This is not an error we know how to handle.");
  Neo4jError translated=translator.translate(cause);
  assertThat(translated.status(),equalTo((Status)Status.General.UnknownFailure));
  provider.assertExactly(inLog("userlog").error(both(containsString("START OF REPORT")).and(containsString("END OF REPORT")),equalTo(cause)),inLog("internallog").error(containsString("Client triggered unexpected error, reference"),equalTo(cause)));
}
