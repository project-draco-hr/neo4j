{
  Node node=getGraphDb().createNode();
  node.setProperty("test","test");
  Transaction tx=getTransaction();
  tx.success();
  tx.finish();
  tx=getGraphDb().beginTx();
  node.setProperty("test2","test2");
  node.delete();
  tx.success();
  tx.finish();
  setTransaction(getGraphDb().beginTx());
}
