{
  assertEquals(path.length,finder.getPathAsNodes().size());
  int counter=0;
  for (  Node pathNode : finder.getPathAsNodes()) {
    Node graphNode=graph.getNode(path[counter++]);
    assertEquals(graphNode.getProperty(SimpleGraphBuilder.KEY_ID) + ", " + pathNode.getProperty(SimpleGraphBuilder.KEY_ID),graphNode,pathNode);
  }
}
