{
  if (idGenerator == null || !storeOk) {
    try {
      storeFile.close();
    }
 catch (    IOException e) {
      throw new UnderlyingStorageException("Failed to close store file: " + getStorageFileName(),e);
    }
    return;
  }
  try {
    storeFile.close();
    idGenerator.close();
  }
 catch (  IOException|IllegalStateException e) {
    throw new UnderlyingStorageException("Failed to close store file: " + getStorageFileName(),e);
  }
}
