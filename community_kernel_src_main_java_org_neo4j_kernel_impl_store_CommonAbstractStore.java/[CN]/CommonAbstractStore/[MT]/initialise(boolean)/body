{
  try {
    checkStorage(createIfNotExists);
    loadStorage();
  }
 catch (  Exception e) {
    if (storeFile != null) {
      try {
        closeStoreFile();
      }
 catch (      IOException failureToClose) {
        e.addSuppressed(failureToClose);
      }
    }
    throw launderedException(e);
  }
}
