{
  BatchingTransactionAppender appender=life.add(new BatchingTransactionAppender(logFile,logRotation,checkPointer,Consumers.LNOOP,transactionMetadataCache,transactionIdStore,legacyIndexTransactionOrdering,kernelHealth));
  life.start();
  appender.forceAfterAppend(logAppendEvent);
  assertThat(channelCommandQueue.take(),is(ChannelCommand.emptyBufferIntoChannelAndClearIt));
  assertThat(channelCommandQueue.take(),is(ChannelCommand.force));
  assertTrue(channelCommandQueue.isEmpty());
}
