{
  TraversalDescription traversalDescription=Traversal.description().depthFirst().relationships(NEXT,Direction.INCOMING).relationships(STATUS,Direction.INCOMING).filter(Traversal.returnWhereLastRelationshipTypeIs(STATUS));
  Traverser traverser=traversalDescription.traverse(getUnderlyingNode());
  return IteratorUtil.singleOrNull(traverser.iterator()).endNode();
}
