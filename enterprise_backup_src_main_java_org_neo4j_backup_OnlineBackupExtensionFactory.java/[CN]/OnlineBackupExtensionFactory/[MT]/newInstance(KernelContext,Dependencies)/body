{
  if (context.customIOConfigurationUsed(dependencies.getConfig())) {
    throw new IllegalArgumentException(CUSTOM_IO_EXCEPTION_MESSAGE);
  }
  return new OnlineBackupKernelExtension(dependencies.getConfig(),dependencies.getGraphDatabaseAPI(),dependencies.logService().getInternalLogProvider(),dependencies.monitors(),dependencies.neoStoreDataSource(),dependencies.checkPointer(),dependencies.transactionIdStoreSupplier(),dependencies.logicalTransactionStoreSupplier(),dependencies.logFileInformationSupplier(),dependencies.fileSystemAbstraction());
}
