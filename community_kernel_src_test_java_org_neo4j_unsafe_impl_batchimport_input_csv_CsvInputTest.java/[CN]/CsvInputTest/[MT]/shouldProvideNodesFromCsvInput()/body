{
  IdType idType=IdType.ACTUAL;
  Input input=new CsvInput(dataIterable(data("123,Mattias Persson,HACKER")),header(entry("id",Type.ID,idType.extractor(extractors)),entry("name",Type.PROPERTY,extractors.string()),entry("labels",Type.LABEL,extractors.string())),null,null,idType,COMMAS);
  Iterator<InputNode> nodes=input.nodes().iterator();
  assertNode(nodes.next(),123L,properties("name","Mattias Persson"),labels("HACKER"));
  assertFalse(nodes.hasNext());
}
