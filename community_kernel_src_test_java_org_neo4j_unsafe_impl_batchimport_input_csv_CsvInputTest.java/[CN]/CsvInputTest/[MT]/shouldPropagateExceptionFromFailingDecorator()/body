  Input input=new CsvInput(data,defaultFormatNodeFileHeader(),null,null,IdType.INTEGER,COMMAS,silentBadCollector(0),getRuntime().availableProcessors());
  try (InputIterator<InputNode> nodes=input.nodes().iterator()){
    nodes.next();
  }
 catch (  RuntimeException e) {
    assertTrue(e == failure);
  }
}
