{
  if (SystemUtils.IS_OS_WINDOWS)   return;
  startDb(null);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://" + ip,"-to",backupPath.getPath()));
  assertEquals(getDbRepresentation(),getBackupDbRepresentation());
  createSomeData(db);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://" + ip,"-to",backupPath.getPath()));
  assertEquals(getDbRepresentation(),getBackupDbRepresentation());
}
