{
  if (osIsWindows())   return;
  startDb(null);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://localhost","-to",BACKUP_PATH.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
  createSomeData(db);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://localhost","-to",BACKUP_PATH.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
}
