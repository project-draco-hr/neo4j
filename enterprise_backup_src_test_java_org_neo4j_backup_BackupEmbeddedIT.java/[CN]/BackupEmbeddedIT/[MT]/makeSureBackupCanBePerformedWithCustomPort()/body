{
  if (osIsWindows())   return;
  int port=4445;
  startDb("" + port);
  assertEquals(1,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://" + ip,"-to",backupPath.getPath()));
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://" + ip+ ":"+ port,"-to",backupPath.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(backupPath));
  createSomeData(db);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://" + ip+ ":"+ port,"-to",backupPath.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(backupPath));
}
