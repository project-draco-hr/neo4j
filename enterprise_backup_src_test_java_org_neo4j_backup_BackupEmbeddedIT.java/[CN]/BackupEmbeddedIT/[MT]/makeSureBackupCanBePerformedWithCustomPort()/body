{
  if (osIsWindows())   return;
  int port=4445;
  startDb("" + port);
  assertEquals(1,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://localhost","-to",BACKUP_PATH.getPath()));
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://localhost:" + port,"-to",BACKUP_PATH.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
  createSomeData(db);
  assertEquals(0,runBackupToolFromOtherJvmToGetExitCode("-from",BackupTool.DEFAULT_SCHEME + "://localhost:" + port,"-to",BACKUP_PATH.getPath()));
  assertEquals(DbRepresentation.of(db),DbRepresentation.of(BACKUP_PATH));
}
