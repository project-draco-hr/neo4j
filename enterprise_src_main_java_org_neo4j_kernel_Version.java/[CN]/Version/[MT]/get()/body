{
  StringBuilder version=new StringBuilder("neo4j-kernel");
  if (true) {
    version.append("-SNAPSHOT");
  }
  if (REVISION != null || DATE != null) {
    version.append(" (");
    if (REVISION != null) {
      version.append("revision ");
      version.append(REVISION);
    }
    if (DATE != null) {
      if (REVISION != null) {
        version.append(", ");
      }
      version.append(DATE);
    }
    version.append(")");
  }
  return version.toString();
}
