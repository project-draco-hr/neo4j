{
  if (fixtureStatements.size() > 0) {
    Client client=new Client();
    ClientRequest req=ClientRequest.create().accept(MediaType.APPLICATION_JSON).entity(createJsonFrom(map("statements",statementPayload())),MediaType.APPLICATION_JSON_TYPE).build(serverBaseURI.resolve(TX_COMMIT_ENDPOINT),"POST");
    ClientResponse response=client.handle(req);
    ensureInstallSuccessful(response);
  }
}
