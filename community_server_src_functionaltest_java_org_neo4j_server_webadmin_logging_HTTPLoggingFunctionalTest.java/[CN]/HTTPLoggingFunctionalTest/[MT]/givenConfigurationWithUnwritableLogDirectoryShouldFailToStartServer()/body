{
  server=ServerBuilder.server().withDefaultDatabaseTuning().withStartupHealthCheckRules(new HTTPLoggingPreparednessRule()).withProperty(Configurator.HTTP_LOGGING,"true").withProperty(Configurator.HTTP_LOG_LOCATION,"/not/writable").withProperty(Configurator.HTTP_LOG_CONFIG_LOCATION,getClass().getResource("/neo4j-server-test-logback.xml").getFile()).build();
  try {
    server.start();
    fail("should have thrown exception");
  }
 catch (  StartupHealthCheckFailedException e) {
    assertThat(e.getMessage(),containsString("HTTP log directory [/not/writable] cannot be created"));
  }
}
