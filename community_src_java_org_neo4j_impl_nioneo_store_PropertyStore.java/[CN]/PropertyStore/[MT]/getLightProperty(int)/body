{
  PersistenceWindow window=acquireWindow(id,OperationType.READ);
  PropertyStoreData storeData=null;
  try {
    storeData=getProperty(id,window.getBuffer());
  }
  finally {
    releaseWindow(window);
  }
  String key=null;
  try {
    key=keyPropertyStore.getString(storeData.keyStoreBlockId());
  }
 catch (  IOException e) {
    Logger.getLogger(PropertyStore.class.getName()).severe("Failed to get key string on property[" + id + "] type= "+ storeData.type()+ " keyStoreBlockId= "+ storeData.keyStoreBlockId()+ " propertyStoreBlockId= "+ storeData.propertyStoreBlockId()+ " previousPropertyId= "+ storeData.previousPropertyId()+ " nextPropertyId= "+ storeData.nextPropertyId());
    throw e;
  }
  return new PropertyData(id,key,null,storeData.nextPropertyId());
}
