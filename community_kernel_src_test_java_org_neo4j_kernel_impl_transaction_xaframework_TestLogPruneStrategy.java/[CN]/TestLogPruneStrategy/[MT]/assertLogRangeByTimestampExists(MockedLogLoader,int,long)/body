{
  long lowerLimit=lastTimestamp - millisToKeep;
  for (long version=log.getCurrentLogVersion() - 1; version >= 0; version--) {
    Long firstTimestamp=log.getFirstStartRecordTimestamp(version + 1);
    if (firstTimestamp == null) {
      break;
    }
    assertTrue("Log " + version + " should've been deleted by now. first of "+ (version + 1)+ ":"+ firstTimestamp+ ", highestVersion:"+ log.getCurrentLogVersion()+ ", lowerLimit:"+ lowerLimit+ ", timestamp:"+ lastTimestamp,firstTimestamp >= lowerLimit);
  }
}
