{
switch (command) {
case FlushCache:
    return flushCache();
case FlushFile:
  return flushFile();
case MapFile:
return mapFile();
case UnmapFile:
return unmapFile();
case ReadRecord:
return readRecord();
case WriteRecord:
return writeRecord();
case ReadMulti:
return readMulti();
case WriteMulti:
return writeMulti();
default :
throw new IllegalArgumentException("Unknown command: " + command);
}
}
