{
  return new BatchIterable<NodeSpecification>(){
    @Override IterableSpecification<NodeSpecification> init(){
      return receive(ixConnection.getIndexNodes(txId,serviceId,key,value));
    }
    @Override IterableSpecification<NodeSpecification> more(    int requestToken){
      return receive(connection.getMoreNodes(txId,requestToken));
    }
  }
;
}
