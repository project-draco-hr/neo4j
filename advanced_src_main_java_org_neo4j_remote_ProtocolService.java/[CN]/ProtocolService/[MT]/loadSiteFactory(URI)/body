{
  String protocol=resourceUri.getScheme();
  Iterable<RemoteSiteFactory> factories=implementations.get(protocol);
  if (factories == null) {
    return null;
  }
  for (  RemoteSiteFactory factory : factories) {
    try {
      if (factory.handlesUri(resourceUri)) {
        return factory;
      }
    }
 catch (    Exception ex) {
    }
  }
  return null;
}
