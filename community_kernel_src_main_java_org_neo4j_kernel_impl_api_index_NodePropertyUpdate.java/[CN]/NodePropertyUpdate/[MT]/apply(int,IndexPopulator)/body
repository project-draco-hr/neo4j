{
switch (getUpdateMode()) {
case ADDED:
    indexManipulator.add(n,getNodeId(),getValueAfter());
  break;
case CHANGED:
indexManipulator.remove(n,getNodeId(),getValueBefore());
indexManipulator.add(n,getNodeId(),getValueAfter());
break;
case REMOVED:
indexManipulator.remove(n,getNodeId(),getValueBefore());
break;
}
}
