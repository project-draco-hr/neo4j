{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  NodePropertyUpdate other=(NodePropertyUpdate)obj;
  if (!Arrays.equals(labelsBefore,other.labelsBefore))   return false;
  if (!Arrays.equals(labelsAfter,other.labelsAfter))   return false;
  if (nodeId != other.nodeId)   return false;
  if (propertyKeyId != other.propertyKeyId)   return false;
  if (updateMode != other.updateMode)   return false;
  if (valueAfter == null) {
    if (other.valueAfter != null)     return false;
  }
 else   if (!valueAfter.equals(other.valueAfter))   return false;
  if (valueBefore == null) {
    if (other.valueBefore != null)     return false;
  }
 else   if (!valueBefore.equals(other.valueBefore))   return false;
  return true;
}
