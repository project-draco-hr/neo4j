{
  master.shutdown();
  Thread.sleep(1000);
  while (!master.isMaster()) {
    ;
  }
  while (!slaves[0].getInstanceState().equals(ClusterMemberState.SLAVE)) {
    ;
  }
  while (!slaves[1].getInstanceState().equals(ClusterMemberState.SLAVE)) {
    ;
  }
  Transaction tx=slaves[0].beginTx();
  slaves[0].createNode();
  tx.success();
  tx.finish();
}
