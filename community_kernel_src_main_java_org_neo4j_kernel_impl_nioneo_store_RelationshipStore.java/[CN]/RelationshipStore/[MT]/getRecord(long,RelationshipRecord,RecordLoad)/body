{
  PageCursor cursor=pageCache.newPageCursor();
  try {
    storeFile.pin(cursor,PageLock.SHARED,pageIdForRecord(id));
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
  try {
    return getRecord(id,cursor,loadMode,target);
  }
  finally {
    storeFile.unpin(cursor);
  }
}
