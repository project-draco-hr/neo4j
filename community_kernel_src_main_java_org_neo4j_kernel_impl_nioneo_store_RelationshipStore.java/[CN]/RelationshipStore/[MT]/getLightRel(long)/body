{
  PersistenceWindow window=null;
  try {
    window=acquireWindow(id,OperationType.READ);
  }
 catch (  InvalidRecordException e) {
    return null;
  }
  try {
    RelationshipRecord record=getRecord(id,window,RecordLoad.CHECK);
    return record;
  }
  finally {
    releaseWindow(window);
  }
}
