{
  super(fileName,configuration,IdType.RELATIONSHIP,idGeneratorFactory,windowPoolFactory,fileSystemAbstraction,stringLogger,versionMismatchHandler,monitors);
  pageCache=new WindowPoolPageCache(windowPoolFactory,fileSystemAbstraction);
  try {
    storeFile=pageCache.map(fileName,RECORD_SIZE * 128);
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
  pageSize=storeFile.pageSize();
}
