{
  try {
    commandExecutor.submit(new Runnable(){
      @Override public void run(){
        try {
          cmd.doWork(state);
        }
 catch (        Exception e) {
          exception=e;
        }
      }
    }
).get();
  }
 catch (  InterruptedException e) {
    throw new RuntimeException(e);
  }
catch (  ExecutionException e) {
    throw new RuntimeException(e);
  }
}
