{
  Node node=graphDb.createNode();
  index.add(node,"name","Thomas Anderson");
  assertContains(index.query("name","\"Thomas Anderson\""),node);
  assertContains(index.query("name","\"thoMas ANDerson\""),node);
  restartTx();
  assertContains(index.query("name","\"Thomas Anderson\""),node);
  assertContains(index.query("name","\"thoMas ANDerson\""),node);
}
