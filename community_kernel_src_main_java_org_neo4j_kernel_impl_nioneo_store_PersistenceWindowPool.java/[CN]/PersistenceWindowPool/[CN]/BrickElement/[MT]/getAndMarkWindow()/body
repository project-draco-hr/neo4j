{
  LockableWindow candidate=window;
  if (candidate != null && !candidate.markAsInUse()) {
    candidate=null;
  }
 else {
    return candidate;
  }
  incrementRowCount();
  candidate=window;
  if (candidate != null && !candidate.markAsInUse()) {
    candidate=null;
  }
  if (candidate != null) {
    unLock();
  }
  setHit();
  return candidate;
}
