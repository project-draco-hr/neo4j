{
  IterableWrapper<StatusUpdate,Relationship> statusIterator=new IterableWrapper<StatusUpdate,Relationship>(underlyingNode.getRelationships(NEXT)){
    @Override protected StatusUpdate underlyingObjectToObject(    Relationship nextRel){
      return new StatusUpdate(nextRel.getOtherNode(underlyingNode));
    }
  }
;
  if (!statusIterator.iterator().hasNext()) {
    return null;
  }
  return statusIterator.iterator().next();
}
