{
  try {
    client=new SSHClient();
    client.addHostKeyVerifier(ALLOW_ALL);
    client.connect(cfg.getHost(),cfg.getPort());
    client.authPublickey(cfg.getUser(),client.loadKeys(cfg.getPrivateKeyPath()));
  }
 catch (  Throwable e) {
    e.printStackTrace();
    throw new ShellException(e);
  }
}
