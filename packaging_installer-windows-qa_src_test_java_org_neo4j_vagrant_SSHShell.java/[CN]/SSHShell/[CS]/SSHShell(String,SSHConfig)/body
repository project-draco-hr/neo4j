{
  this.vmName=vmName;
  try {
    client=new SSHClient();
    client.addHostKeyVerifier(ALLOW_ALL);
    client.connect(cfg.host(),cfg.port());
    client.authPublickey(cfg.user(),client.loadKeys(cfg.privateKeyPath()));
  }
 catch (  Throwable e) {
    e.printStackTrace();
    throw new ShellException(e);
  }
}
