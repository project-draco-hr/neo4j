{
  upgrader(new StoreMigrator(monitor,fs,DevNullLoggingService.DEV_NULL)).migrateIfNeeded(find20FormatStoreDirectory(storeDir.directory()));
  assertEquals(100,monitor.eventSize());
  assertTrue(monitor.isStarted());
  assertTrue(monitor.isFinished());
  GraphDatabaseService database=cleanup.add(new GraphDatabaseFactory().newEmbeddedDatabase(storeDir.absolutePath()));
  try {
    verifyDatabaseContents(database);
  }
  finally {
    database.shutdown();
  }
  NeoStore neoStore=cleanup.add(storeFactory.newNeoStore(true,false));
  verifyNeoStore(neoStore);
  neoStore.close();
  assertConsistentStore(storeDir.directory());
}
