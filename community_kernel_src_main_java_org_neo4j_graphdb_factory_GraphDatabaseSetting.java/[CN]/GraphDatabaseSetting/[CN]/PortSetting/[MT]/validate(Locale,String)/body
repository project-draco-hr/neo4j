{
  String[] ports=values.split("-");
  for (  String value : ports) {
    if (value == null) {
      throw illegalValue(locale,value);
    }
    int val;
    try {
      val=Integer.parseInt(value);
    }
 catch (    Exception e) {
      throw illegalValue(locale,value);
    }
    rangeCheck(val);
  }
}
