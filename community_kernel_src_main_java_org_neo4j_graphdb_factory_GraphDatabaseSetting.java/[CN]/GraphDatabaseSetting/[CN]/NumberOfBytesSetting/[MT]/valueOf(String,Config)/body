{
  String mem=rawValue.toLowerCase();
  long multiplier=1;
  if (mem.endsWith("k")) {
    multiplier=1024;
    mem=mem.substring(0,mem.length() - 1);
  }
 else   if (mem.endsWith("m")) {
    multiplier=1024 * 1024;
    mem=mem.substring(0,mem.length() - 1);
  }
 else   if (mem.endsWith("g")) {
    multiplier=1024 * 1024 * 1024;
    mem=mem.substring(0,mem.length() - 1);
  }
  return Long.parseLong(mem.trim()) * multiplier;
}
