{
  fullRange.validate(locale,value);
  Long bytes=fullRange.valueOf(value,null);
  if (bytes.longValue() > Integer.MAX_VALUE)   throw illegalValue(locale,value,"Size too big, keep withing interger range (2^32-1)","" + bytes);
  int result=bytes.intValue();
  if (result < atLeast)   throw illegalValue(locale,value,"Size too low, must be at least " + atLeast);
}
