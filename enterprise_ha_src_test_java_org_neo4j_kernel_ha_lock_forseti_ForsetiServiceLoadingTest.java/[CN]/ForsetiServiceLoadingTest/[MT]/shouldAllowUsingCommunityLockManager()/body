{
  GraphDatabaseAPI db=(GraphDatabaseAPI)new GraphDatabaseFactory().newEmbeddedDatabaseBuilder(dir.absolutePath()).setConfig(InternalAbstractGraphDatabase.Configuration.lock_manager,"community").newGraphDatabase();
  assertThat(db.getDependencyResolver().resolveDependency(Locks.class),instanceOf(CommunityLockManger.class));
  db.shutdown();
}
