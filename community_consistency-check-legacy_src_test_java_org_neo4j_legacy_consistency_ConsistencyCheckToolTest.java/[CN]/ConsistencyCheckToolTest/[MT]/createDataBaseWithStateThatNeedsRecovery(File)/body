{
  EphemeralFileSystemAbstraction fileSystem=fs.get();
  final GraphDatabaseService db=new TestGraphDatabaseFactory().setFileSystem(fileSystem).newImpermanentDatabase(storeDir);
  try (Transaction tx=db.beginTx()){
    db.createNode();
    tx.success();
  }
   EphemeralFileSystemAbstraction snapshot=fileSystem.snapshot();
  db.shutdown();
  return snapshot;
}
