{
  OutsideWorld outsideWorld=mock(OutsideWorld.class);
  CheckConsistencyCommand checkConsistencyCommand=new CheckConsistencyCommand(testDir.directory("home").toPath(),testDir.directory("conf").toPath(),outsideWorld);
  String[] arguments={""};
  try {
    checkConsistencyCommand.execute(arguments);
    fail("Should have thrown an exception.");
  }
 catch (  IncorrectUsage e) {
    assertThat(e.getMessage(),containsString("database"));
  }
}
