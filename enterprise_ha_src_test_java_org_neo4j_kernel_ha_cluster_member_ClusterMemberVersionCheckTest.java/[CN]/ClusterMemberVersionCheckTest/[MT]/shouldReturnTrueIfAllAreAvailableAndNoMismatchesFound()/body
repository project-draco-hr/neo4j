{
  ClusterMembers members=clusterMembersWith(member(ID_1,MASTER,true,EXPECTED_STORE_ID),member(ID_3,SLAVE,true,EXPECTED_STORE_ID),member(ID_4,SLAVE,true,EXPECTED_STORE_ID),member(ID_5,SLAVE,true,EXPECTED_STORE_ID));
  boolean versionCheckPassed=new ClusterMemberVersionCheck(members,ID_2,new FakeClock()).doVersionCheck(EXPECTED_STORE_ID,1,SECONDS);
  assertTrue(versionCheckPassed);
  verify(members,times(0)).waitForEvent(anyLong());
}
