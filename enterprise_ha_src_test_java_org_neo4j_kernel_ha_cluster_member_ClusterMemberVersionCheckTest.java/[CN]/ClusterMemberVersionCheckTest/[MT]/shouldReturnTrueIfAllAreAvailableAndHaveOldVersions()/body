{
  ClusterMembers members=clusterMembersWith(member(ID_1,MASTER,true,StoreId.DEFAULT),member(ID_2,SLAVE,true,StoreId.DEFAULT),member(ID_3,SLAVE,true,StoreId.DEFAULT),member(ID_4,SLAVE,true,StoreId.DEFAULT));
  boolean versionCheckPassed=new ClusterMemberVersionCheck(members,ID_5,new FakeClock()).doVersionCheck(EXPECTED_STORE_ID,1,SECONDS);
  assertTrue(versionCheckPassed);
  verify(members,times(0)).waitForEvent(anyLong());
}
