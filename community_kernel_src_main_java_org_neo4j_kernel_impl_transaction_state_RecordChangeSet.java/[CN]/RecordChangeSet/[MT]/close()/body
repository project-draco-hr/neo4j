{
  if (hasChanges()) {
    nodeRecords.close();
    propertyRecords.close();
    relRecords.close();
    schemaRuleChanges.close();
    relGroupRecords.close();
    propertyKeyTokenChanges.close();
    labelTokenChanges.close();
    relationshipTypeTokenChanges.close();
    changeCounter.clear();
  }
}
