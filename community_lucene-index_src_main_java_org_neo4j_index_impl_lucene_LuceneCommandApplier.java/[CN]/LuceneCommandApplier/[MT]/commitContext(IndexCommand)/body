{
  Map<Byte,CommitContext> contextMap=commitContextMap(command.getEntityType());
  byte indexNameId=command.getIndexNameId();
  CommitContext context=contextMap.get(indexNameId);
  if (context == null) {
    IndexIdentifier identifier=new IndexIdentifier(IndexEntityType.byId(command.getEntityType()),definitions.getIndexName(indexNameId));
    context=new CommitContext(dataSource,identifier,dataSource.getType(identifier,recovery),recovery);
    contextMap.put(indexNameId,context);
  }
  return context;
}
