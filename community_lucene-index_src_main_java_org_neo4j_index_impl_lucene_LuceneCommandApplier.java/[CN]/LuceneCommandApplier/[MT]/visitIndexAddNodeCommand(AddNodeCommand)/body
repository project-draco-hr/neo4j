{
  CommitContext context=commitContext(command);
  String key=definitions.getKey(command.getKeyId());
  Object value=command.getValue();
  if (value != null) {
    context.ensureWriterInstantiated();
    context.indexType.addToDocument(context.getDocument(new IdData(command.getEntityId()),true).document,key,value);
  }
  return false;
}
