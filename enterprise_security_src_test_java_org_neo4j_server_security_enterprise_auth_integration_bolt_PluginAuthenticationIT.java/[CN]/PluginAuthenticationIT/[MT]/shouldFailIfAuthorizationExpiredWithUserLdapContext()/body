{
  assertConnectionSucceeds(authToken("neo4j","neo4j","plugin-TestCacheableAdminAuthPlugin"));
  client.send(TransportTestUtil.chunk(run("CALL dbms.security.clearAuthCache() MATCH (n) RETURN n"),pullAll()));
  assertThat(client,eventuallyReceives(msgSuccess(),msgFailure(Status.Security.AuthorizationExpired,"Plugin 'plugin-TestCacheableAdminAuthPlugin' authorization info expired.")));
}
