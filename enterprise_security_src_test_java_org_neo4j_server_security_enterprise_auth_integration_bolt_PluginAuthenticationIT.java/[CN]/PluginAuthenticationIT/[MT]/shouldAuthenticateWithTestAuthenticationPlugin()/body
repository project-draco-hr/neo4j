{
  client.connect(address).send(TransportTestUtil.acceptedVersions(1,0,0,0)).send(TransportTestUtil.chunk(init("TestClient/1.1",map("principal","neo4j","credentials","neo4j","scheme","basic","realm","TestAuthenticationPlugin"))));
  assertThat(client,eventuallyReceives(new byte[]{0,0,0,1}));
  assertThat(client,eventuallyReceives(msgSuccess()));
}
