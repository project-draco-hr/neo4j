{
  Exception closeException=null;
  for (  T closeable : closeables) {
    if (closeable != null) {
      try {
        closeable.close();
      }
 catch (      Exception e) {
        if (closeException == null) {
          closeException=e;
        }
 else {
          closeException.addSuppressed(e);
        }
      }
    }
  }
  if (closeException != null) {
    throw new IOException("Exception closing multiple resources",closeException);
  }
}
