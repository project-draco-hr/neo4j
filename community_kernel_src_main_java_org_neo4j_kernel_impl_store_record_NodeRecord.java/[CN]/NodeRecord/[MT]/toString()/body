{
  String denseInfo=(dense ? "group" : "rel") + "=" + nextRel;
  String lightHeavyInfo=isLight ? "light" : dynamicLabelRecords.isEmpty() ? "heavy" : "heavy,dynlabels=" + dynamicLabelRecords;
  return "Node[" + getId() + ",used="+ inUse()+ ","+ denseInfo+ ",prop="+ getNextProp()+ ",labels="+ parseLabelsField(this)+ ","+ lightHeavyInfo+ ",secondaryUnitId="+ getSecondaryUnitId()+ "]";
}
