{
  StringBuilder builder=new StringBuilder("Node[").append(getId()).append(",used=").append(inUse()).append("," + (dense ? "group" : "rel") + "=").append(nextRel).append(",prop=").append(getNextProp()).append(",labels=").append(parseLabelsField(this)).append(",").append(isLight ? "light" : "heavy");
  if (!isLight && !dynamicLabelRecords.isEmpty()) {
    builder.append(",dynlabels=").append(dynamicLabelRecords);
  }
  return builder.append("]").toString();
}
