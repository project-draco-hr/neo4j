{
  int offset=(id - buffer.position()) * getRecordSize();
  buffer.setOffset(offset);
  byte inUse=buffer.get();
  boolean inUseFlag=((inUse & Record.IN_USE.byteValue()) == Record.IN_USE.byteValue());
  if (!inUseFlag) {
    throw new IOException("Record[" + id + "] not in use["+ inUse+ "]");
  }
  return new RelationshipData(id,buffer.getInt(),buffer.getInt(),buffer.getInt(),buffer.getInt(),buffer.getInt(),buffer.getInt(),buffer.getInt(),buffer.getInt());
}
