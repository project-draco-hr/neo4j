{
  String key="key";
  node1.setProperty(key,value);
  newTransaction();
  clearCache();
  Object readValue=node1.getProperty(key);
  Array.set(readValue,0,poison);
  assertTrue(format("Expected %s, but was %s",ArrayUtil.toString(value),ArrayUtil.toString(readValue)),ArrayUtil.equals(value,node1.getProperty(key)));
}
