{
  try {
    KeyStoreInformation keyStore=createKeyStore(config,log);
    return SslContextBuilder.forServer(keyStore.getCertificatePath(),keyStore.getPrivateKeyPath()).build();
  }
 catch (  IOException|OperatorCreationException|GeneralSecurityException e) {
    throw new RuntimeException("Failed to initilize SSL encryption support, which is required to start this " + "connector. Error was: " + e.getMessage(),e);
  }
}
