{
  RecordingByteChannel channel=new RecordingByteChannel();
  MessageFormat.Reader reader=new PackStreamMessageFormatV1.Reader(new Neo4jPack.Unpacker(new BufferedChannelInput(16).reset(channel)));
  MessageFormat.Writer writer=new PackStreamMessageFormatV1.Writer(new Neo4jPack.Packer(new BufferedChannelOutput(channel)),NO_OP);
  writer.write(msg).flush();
  channel.eof();
  return unpack(reader,channel);
}
