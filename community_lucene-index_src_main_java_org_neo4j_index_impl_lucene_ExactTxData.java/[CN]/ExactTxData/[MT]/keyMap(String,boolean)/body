{
  if (data == null) {
    if (create) {
      data=new HashMap<String,Map<Object,Set<Object>>>();
    }
 else {
      return null;
    }
  }
  Map<Object,Set<Object>> inner=data.get(key);
  if (inner == null && create) {
    inner=new HashMap<Object,Set<Object>>();
    data.put(key,inner);
    if (key == null) {
      hasOrphans=true;
    }
  }
  return inner;
}
