{
  Map<Object,Set<Object>> keyMap=keyMap(key,create);
  if (keyMap == null) {
    return null;
  }
  Set<Object> ids=keyMap.get(value);
  if (ids == null && create) {
    ids=new HashSet<Object>();
    keyMap.put(value,ids);
    if (value == null) {
      hasOrphans=true;
    }
  }
  return ids;
}
