{
  if (ids.isEmpty())   return Collections.emptySet();
  if (ids.iterator().next() instanceof Long) {
    return (Collection)ids;
  }
 else {
    Collection<Long> longs=new ArrayList<Long>(ids.size());
    for (    Object id : ids) {
      longs.add(((RelationshipId)id).id);
    }
    return longs;
  }
}
