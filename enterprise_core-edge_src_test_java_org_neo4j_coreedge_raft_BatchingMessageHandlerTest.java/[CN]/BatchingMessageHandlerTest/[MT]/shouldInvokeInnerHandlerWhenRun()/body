{
  @SuppressWarnings("unchecked") Inbound.MessageHandler<RaftMessages.RaftMessage> innerHandler=mock(Inbound.MessageHandler.class);
  BatchingMessageHandler batchHandler=new BatchingMessageHandler(innerHandler,NullLogProvider.getInstance(),QUEUE_SIZE,MAX_BATCH);
  RaftMessages.NewEntry.Request message=new RaftMessages.NewEntry.Request(null,null);
  batchHandler.handle(message);
  verifyZeroInteractions(innerHandler);
  batchHandler.run();
  verify(innerHandler).handle(message);
}
