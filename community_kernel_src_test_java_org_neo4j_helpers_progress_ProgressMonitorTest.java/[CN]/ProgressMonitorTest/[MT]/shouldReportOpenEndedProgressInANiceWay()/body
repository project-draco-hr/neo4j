{
  StringWriter buffer=new StringWriter();
  ProgressListener progress=ProgressMonitorFactory.textual(buffer).openEnded(testName.getMethodName(),10);
  for (int i=0; i < 25; i++) {
    progress.add(50);
  }
  progress.done();
  assertEquals(testName.getMethodName() + "\n" + "....................     200\n"+ "....................     400\n"+ "....................     600\n"+ "....................     800\n"+ "....................    1000\n"+ "....................    1200\n"+ ".....                   done\n",buffer.toString());
}
