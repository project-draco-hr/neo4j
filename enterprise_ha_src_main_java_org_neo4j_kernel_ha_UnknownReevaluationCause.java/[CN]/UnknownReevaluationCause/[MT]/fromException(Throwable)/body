{
  if (cause instanceof ComException && cause.getCause() instanceof BranchedDataException)   return KnownReevaluationCauses.BRANCHED_DATA;
  return new ReevaluationCause(){
    @Override public String name(){
      return UNKNOWN;
    }
    @Override public String getDescription(){
      return "See exception";
    }
    @Override public void log(    StringLogger logger,    String message){
      logger.logMessage(formLogLine(message,this),cause,true);
    }
  }
;
}
