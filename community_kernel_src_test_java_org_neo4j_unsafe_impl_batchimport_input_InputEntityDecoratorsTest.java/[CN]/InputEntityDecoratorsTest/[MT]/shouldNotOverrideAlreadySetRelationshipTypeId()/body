{
  String defaultType="TYPE";
  Function<InputRelationship,InputRelationship> decorator=InputEntityDecorators.defaultRelationshipType(defaultType);
  Integer typeId=5;
  InputRelationship relationship=new InputRelationship(0,InputEntity.NO_PROPERTIES,null,"start","end",null,typeId);
  relationship=decorator.apply(relationship);
  assertEquals(null,relationship.type());
  assertEquals(typeId.intValue(),relationship.typeId());
}
