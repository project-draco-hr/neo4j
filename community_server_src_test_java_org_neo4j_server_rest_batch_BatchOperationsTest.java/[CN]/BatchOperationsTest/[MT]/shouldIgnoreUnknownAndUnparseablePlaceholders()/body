{
  assertEquals("foo {00000000010001010001001100111000100101010111001101110111}",ops.replaceLocationPlaceholders("foo {00000000010001010001001100111000100101010111001101110111}",Collections.<Integer,String>emptyMap()));
  assertEquals("foo {2147483648}",ops.replaceLocationPlaceholders("foo {2147483648}",Collections.<Integer,String>emptyMap()));
}
