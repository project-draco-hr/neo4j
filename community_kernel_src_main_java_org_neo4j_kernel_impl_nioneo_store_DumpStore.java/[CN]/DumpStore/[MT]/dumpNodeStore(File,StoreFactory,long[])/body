{
  NodeStore store=storeFactory.newNodeStore(file);
  try {
    new DumpStore<NodeRecord,NodeStore>(System.out){
      @Override protected Object transform(      NodeRecord record) throws Exception {
        return record.inUse() ? record : "";
      }
    }
.dump(store,ids);
  }
  finally {
    store.close();
  }
}
