{
  try {
    new DumpStore<T,AbstractNameStore<T>>(System.out){
      @Override protected Object transform(      T record) throws Exception {
        if (record.inUse()) {
          store.makeHeavy(record);
          return record.getId() + ": \"" + store.getStringFor(record)+ "\": "+ record;
        }
        return null;
      }
    }
.dump(store,ids);
  }
  finally {
    store.close();
  }
}
