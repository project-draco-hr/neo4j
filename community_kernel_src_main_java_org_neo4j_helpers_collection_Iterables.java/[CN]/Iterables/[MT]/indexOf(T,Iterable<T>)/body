{
  if (itemToFind == null) {
    int index=0;
    for (    T item : iterable) {
      if (item == null) {
        return index;
      }
      index++;
    }
  }
 else {
    int index=0;
    for (    T item : iterable) {
      if (itemToFind.equals(item)) {
        return index;
      }
      index++;
    }
  }
  return -1;
}
