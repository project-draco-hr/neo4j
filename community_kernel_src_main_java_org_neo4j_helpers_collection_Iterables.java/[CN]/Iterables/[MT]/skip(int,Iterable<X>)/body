{
  return new Iterable<X>(){
    @Override public Iterator<X> iterator(){
      Iterator<X> iterator=iterable.iterator();
      for (int i=0; i < skip; i++) {
        if (iterator.hasNext())         iterator.next();
 else         return Iterables.<X>empty().iterator();
      }
      return iterator;
    }
  }
;
}
