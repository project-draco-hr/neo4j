{
  if (currentIterator == null) {
    if (iterator.hasNext()) {
      I next=iterator.next();
      currentIterator=next.iterator();
    }
 else {
      return false;
    }
  }
  while (!currentIterator.hasNext() && iterator.hasNext()) {
    currentIterator=iterator.next().iterator();
  }
  return currentIterator.hasNext();
}
