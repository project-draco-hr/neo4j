{
  String result=query;
  for (  String keyword : keywordsToBreakOn) {
    String upperKeyword=keyword.toUpperCase();
    result=result.replace(keyword + " ",upperKeyword + " ").replace(keyword + " ",upperKeyword + " ").replace(" " + upperKeyword + " ","\n" + upperKeyword + " ");
  }
  return result;
}
