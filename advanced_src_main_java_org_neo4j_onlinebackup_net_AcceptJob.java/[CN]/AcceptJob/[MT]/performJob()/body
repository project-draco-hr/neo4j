{
  try {
    if (serverChannel.isOpen()) {
      acceptedChannel=serverChannel.accept();
    }
 else {
      setNoRequeue();
    }
  }
 catch (  IOException e) {
    throw new SocketException("Unable to accept incoming connection",e);
  }
  if (acceptedChannel != null) {
    executeCallback();
    acceptedChannel=null;
    return true;
  }
  return false;
}
