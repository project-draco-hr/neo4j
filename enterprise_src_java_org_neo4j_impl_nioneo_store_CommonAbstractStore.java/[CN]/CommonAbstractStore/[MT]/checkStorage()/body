{
  if (!new File(storageFileName).exists()) {
    throw new IOException("No such store[" + storageFileName + "]");
  }
  this.fileChannel=new RandomAccessFile(storageFileName,"rw").getChannel();
  this.fileLock=this.fileChannel.tryLock();
  if (fileLock == null) {
    fileChannel.close();
    throw new IOException("Unable to lock store [" + storageFileName + "], other program running?");
  }
}
