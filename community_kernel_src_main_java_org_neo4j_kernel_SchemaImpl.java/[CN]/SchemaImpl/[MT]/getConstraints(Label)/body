{
  final StatementContext context=ctxProvider.getCtxForReading();
  try {
    Iterator<UniquenessConstraint> constraints=context.constraintsGetForLabel(context.labelGetForName(label.name()));
    return asConstraintDefinitions(context,constraints);
  }
 catch (  LabelNotFoundKernelException e) {
    return emptyList();
  }
 finally {
    context.close();
  }
}
