{
  int opsLeft=numberOfOps;
  long start=System.nanoTime();
  while (opsLeft-- > 0) {
    runOperation();
  }
  double delta=(System.nanoTime() - start);
  delta=delta / (1000.0 * 1000.0);
  double opsPerUnit=numberOfOps / delta;
  return new BenchmarkResult(getName(),new BenchmarkResult.Metric("result",opsPerUnit,OPERATION.per(Units.MILLISECOND)));
}
