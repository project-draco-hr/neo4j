{
  TxDataBoth data=getTxData(index,true);
  TxDataHolder added=data.added(false);
  if (added != null) {
    added.clear();
  }
  TxDataHolder removed=data.removed(true);
  if (removed != null) {
    removed.clear();
    removed.setRemoveAll();
  }
  queueCommand(new ClearCommand(index.identifier));
}
