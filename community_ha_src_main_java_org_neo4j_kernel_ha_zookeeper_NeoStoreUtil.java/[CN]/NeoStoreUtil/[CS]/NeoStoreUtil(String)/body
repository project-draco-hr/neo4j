{
  try {
    FileChannel fileChannel=new RandomAccessFile(storeDir + File.separator + "neostore","r").getChannel();
    ByteBuffer buf=ByteBuffer.allocate(4 * 9);
    if (fileChannel.read(buf) != 4 * 9) {
      throw new RuntimeException("Unable to read neo store header information");
    }
    buf.flip();
    buf.get();
    creationTime=buf.getLong();
    buf.get();
    storeId=buf.getLong();
    buf.get();
    version=buf.getLong();
    buf.get();
    txId=buf.getLong();
    fileChannel.close();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
