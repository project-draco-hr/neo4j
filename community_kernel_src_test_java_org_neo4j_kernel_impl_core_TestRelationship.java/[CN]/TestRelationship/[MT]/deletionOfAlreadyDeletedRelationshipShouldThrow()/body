{
  GraphDatabaseService db=getGraphDb();
  Node node1=db.createNode();
  Node node2=db.createNode();
  Relationship relationship=node1.createRelationshipTo(node2,TEST);
  commit();
  try (Transaction tx=db.beginTx()){
    relationship.delete();
    tx.success();
  }
   try (Transaction tx=db.beginTx()){
    relationship.delete();
    tx.success();
  }
 }
