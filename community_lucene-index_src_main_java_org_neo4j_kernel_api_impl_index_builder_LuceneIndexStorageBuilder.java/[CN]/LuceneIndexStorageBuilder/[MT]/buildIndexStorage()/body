{
  if (indexStorage == null) {
    Objects.requireNonNull(directoryFactory);
    Objects.requireNonNull(fileSystem);
    Objects.requireNonNull(indexRootFolder);
    Objects.requireNonNull(indexIdentifier);
    indexStorage=new PartitionedIndexStorage(directoryFactory,fileSystem,indexRootFolder,indexIdentifier);
  }
  return indexStorage;
}
