{
  KernelAPI kernel=db.getDependencyResolver().resolveDependency(KernelAPI.class);
  KernelTransaction transaction=kernel.newTransaction(Type.implicit,READ);
  try (Statement statement=transaction.acquireStatement()){
    return asList(statement.readOperations().procedureCallRead(procedureName("dbms","cluster",DiscoverEndpointAcquisitionServersProcedure.NAME),new Object[0]));
  }
 }
