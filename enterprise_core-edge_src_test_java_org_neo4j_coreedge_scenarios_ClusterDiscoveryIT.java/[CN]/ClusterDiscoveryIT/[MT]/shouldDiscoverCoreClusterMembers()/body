{
  File dbDir=dir.directory();
  cluster=Cluster.start(dbDir,3,0);
  List<Object[]> currentMembers;
  for (int i=0; i < 3; i++) {
    currentMembers=discoverClusterMembers(cluster.getCoreServerById(i));
    assertThat(currentMembers,containsInAnyOrder(new Object[]{"127.0.0.1:8000"},new Object[]{"127.0.0.1:8001"},new Object[]{"127.0.0.1:8002"}));
  }
}
