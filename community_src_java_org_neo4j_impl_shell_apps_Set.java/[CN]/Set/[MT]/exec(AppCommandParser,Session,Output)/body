{
  if (parser.arguments().size() < 2) {
    throw new ShellException("Must supply key and value, " + "like: set -t String title \"This is a neo node\"");
  }
  String key=parser.arguments().get(0);
  Class<?> type=this.getValueType(parser);
  Object value=null;
  try {
    value=type.getConstructor(String.class).newInstance(parser.arguments().get(1));
  }
 catch (  Exception e) {
    throw new ShellException(e);
  }
  Node node=this.getCurrentNode(session);
  NodeOrRelationship thing=getNodeOrRelationship(node,parser);
  thing.setProperty(key,value);
  return null;
}
