{
  Node node1=graphDb.createNode();
  Node node2=graphDb.createNode();
  SingleSourceSingleSinkShortestPath<Double> sp;
  sp=new Dijkstra<Double>(0.0,node1,node2,new DoubleEvaluator("cost"),new DoubleAdder(),new DoubleComparator(),Direction.BOTH,DynamicRelationshipType.withName("MY_TYPE"));
  sp.getCost();
  List<PropertyContainer> path=sp.getPath();
}
