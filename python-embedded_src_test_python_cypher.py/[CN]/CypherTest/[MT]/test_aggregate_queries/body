def test_aggregate_queries(self):
    db = self.graphdb
    with db.transaction:
        node = db.query('CREATE node-[:Depends_on]->port<-[:Has]-parent1<-[:Has]-parent2 \n                                 RETURN node').single['node']
    result = db.query('START node=node({id})\n                MATCH p=node-[:Depends_on]->port<-[:Has*1..]-parent\n                RETURN node, COLLECT(p) as end_points', id=node.id)
    collected = result['end_points']
    for val in collected:
        pass
