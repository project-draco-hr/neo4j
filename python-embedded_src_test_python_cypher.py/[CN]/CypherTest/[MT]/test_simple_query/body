def test_simple_query(self):
    db = self.graphdb
    result = db.query('START n=node(0) RETURN n')
    col = result['n']
    node = col.single
    self.assertEquals(0, node.id)
    node = result['n'].single
    self.assertEquals(0, node.id)
    for row in result:
        node = row['n']
        self.assertEquals(0, node.id)
