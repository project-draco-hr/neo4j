{
  for (  URI server : clusterContext.getConfiguration().getMembers()) {
    if (!clusterContext.isMe(server)) {
      clusterContext.timeouts.setTimeout(HeartbeatMessage.i_am_alive + "-" + server,timeout(HeartbeatMessage.timed_out,message,server));
      clusterContext.timeouts.setTimeout(HeartbeatMessage.sendHeartbeat + "-" + server,timeout(HeartbeatMessage.sendHeartbeat,message,server));
    }
  }
}
