{
  Config config=new Config(new HashMap<String,String>(),GraphDatabaseSettings.class);
  StoreFactory sf=new StoreFactory(storeDir,config,new DefaultIdGeneratorFactory(fs.get()),pageCache,fs.get(),NullLogProvider.getInstance());
  NeoStores neoStores=sf.openNeoStores(true,NeoStores.StoreType.NODE_LABEL);
  exception.expect(IllegalStateException.class);
  exception.expectMessage("Specified store was not initialized. Please specify " + NeoStores.StoreType.META_DATA.name() + " as one of the stores types that should be open to be able to use it.");
  neoStores.getMetaDataStore();
}
