{
synchronized (logFile) {
    logRotation.rotateLogIfNeeded();
    try {
      if (transactionQueue.accept(batchAppender) > 0) {
        appender.force();
        transactionQueue.accept(batchCommitterAndApplier);
      }
    }
 catch (    IOException e) {
      kernelHealth.panic(e);
      throw e;
    }
 finally {
      transactionQueue.clear();
    }
  }
}
