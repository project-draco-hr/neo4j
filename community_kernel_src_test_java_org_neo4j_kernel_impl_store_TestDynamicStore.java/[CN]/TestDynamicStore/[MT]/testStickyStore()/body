{
  try {
    createEmptyStore(dynamicStoreFile(),30);
    StoreChannel fileChannel=fs.get().open(dynamicStoreFile(),"rw");
    fileChannel.truncate(fileChannel.size() - 2);
    fileChannel.close();
    DynamicArrayStore store=newStore();
    store.makeStoreOk();
    store.close();
  }
  finally {
    deleteBothFiles();
  }
}
