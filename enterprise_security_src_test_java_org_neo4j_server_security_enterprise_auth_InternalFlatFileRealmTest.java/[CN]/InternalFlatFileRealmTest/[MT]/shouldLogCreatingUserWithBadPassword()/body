{
  try {
    testRealm.newUser("andres","",true);
  }
 catch (  InvalidArgumentsException e) {
  }
  try {
    testRealm.newUser("mats",null,true);
  }
 catch (  InvalidArgumentsException e) {
  }
  log.assertExactly(error("User creation failed for user `%s`: %s","andres","A password cannot be empty."),error("User creation failed for user `%s`: %s","mats","A password cannot be empty."));
}
