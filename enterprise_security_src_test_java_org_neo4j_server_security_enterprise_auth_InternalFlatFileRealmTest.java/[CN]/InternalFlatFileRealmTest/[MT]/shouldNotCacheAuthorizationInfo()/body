{
  EnterpriseAuthSubject mike=authManager.login(authToken("mike","123"));
  mike.allowsReads();
  assertThat("Test realm did not receive a call",testRealm.takeAuthorizationFlag(),is(true));
  mike.allowsWrites();
  assertThat("Test realm did not receive a call",testRealm.takeAuthorizationFlag(),is(true));
}
