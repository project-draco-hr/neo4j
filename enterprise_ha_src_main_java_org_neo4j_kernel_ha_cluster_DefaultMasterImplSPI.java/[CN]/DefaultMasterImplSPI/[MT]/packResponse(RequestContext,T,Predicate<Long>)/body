{
  try {
    AccumulatorVisitor<CommittedTransactionRepresentation> accumulator=new AccumulatorVisitor<>(wrapLongFilter(filter));
    exhaust(txStore.getCursor(context.lastAppliedTransaction() + 1,accumulator));
    Iterable<CommittedTransactionRepresentation> txs=accumulator.getAccumulator();
    return new Response<>(response,storeId(),txs,ResourceReleaser.NO_OP);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
