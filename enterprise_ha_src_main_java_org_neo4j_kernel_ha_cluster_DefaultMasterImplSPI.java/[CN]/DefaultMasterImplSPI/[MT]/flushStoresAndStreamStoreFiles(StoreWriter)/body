{
  NeoStoreDataSource dataSource=dependencyResolver.resolveDependency(DataSourceManager.class).getDataSource();
  StoreCopyServer streamer=new StoreCopyServer(transactionIdStore,dataSource,dependencyResolver.resolveDependency(LogRotationControl.class),fileSystem,storeDir,monitors.newMonitor(StoreCopyServer.Monitor.class));
  return streamer.flushStoresAndStreamStoreFiles(writer,false);
}
