{
  if (xidIdentMap.size() > 0) {
    log.info("Active transactions: " + xidIdentMap.size());
    log.info("Closing dirty log: " + fileName);
    force();
    fileChannel.close();
    return;
  }
  force();
  fileChannel.close();
  File file=new File(fileName);
  if (!file.exists()) {
    throw new IOException("Logical log[" + fileName + "] not found");
  }
  file.delete();
}
