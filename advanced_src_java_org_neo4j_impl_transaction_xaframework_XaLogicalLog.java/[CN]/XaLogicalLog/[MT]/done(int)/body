{
  validate(identifier);
  try {
    buffer.clear();
    buffer.put(DONE).putInt(identifier);
    buffer.flip();
    fileChannel.write(buffer);
    xidIdentMap.remove(identifier);
  }
 catch (  IOException e) {
    throw new XAException("Logical log unable to mark as done [" + identifier + "] "+ e);
  }
}
