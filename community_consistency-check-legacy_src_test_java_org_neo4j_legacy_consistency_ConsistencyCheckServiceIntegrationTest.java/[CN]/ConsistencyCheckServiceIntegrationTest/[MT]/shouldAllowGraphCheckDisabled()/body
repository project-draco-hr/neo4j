{
  GraphDatabaseService gds=getGraphDatabaseService();
  try (Transaction tx=gds.beginTx()){
    gds.createNode();
    tx.success();
  }
   gds.shutdown();
  ConsistencyCheckService service=new ConsistencyCheckService();
  Config configuration=new Config(settings(),GraphDatabaseSettings.class,ConsistencyCheckSettings.class);
  configuration.applyChanges(MapUtil.stringMap(ConsistencyCheckSettings.consistency_check_graph.name(),Settings.FALSE));
  Result result=service.runFullConsistencyCheck(testDirectory.graphDbDir(),configuration,ProgressMonitorFactory.NONE,NullLogProvider.getInstance(),new DefaultFileSystemAbstraction());
  assertEquals(ConsistencyCheckService.Result.SUCCESS,result);
}
