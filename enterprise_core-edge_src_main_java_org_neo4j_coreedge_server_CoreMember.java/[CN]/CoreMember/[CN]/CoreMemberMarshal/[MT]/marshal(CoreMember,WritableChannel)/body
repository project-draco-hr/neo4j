{
  if (member == null) {
    channelMarshal.marshal(NULL_ADDRESS,channel);
    channelMarshal.marshal(NULL_ADDRESS,channel);
    channelMarshal.marshal(NULL_ADDRESS,channel);
  }
 else {
    channelMarshal.marshal(member.getCoreAddress(),channel);
    channelMarshal.marshal(member.getRaftAddress(),channel);
    channelMarshal.marshal(member.getBoltAddress(),channel);
  }
}
