{
  clusterManager=new OnlineBackupClusterManager(fromXml(getClass().getResource("/threeinstances.xml").toURI()),BACKUP_PATH,MapUtil.stringMap(OnlineBackupSettings.online_backup_enabled.name(),Settings.TRUE));
  clusterManager.start();
  cluster=clusterManager.getDefaultCluster();
  cluster.await(allSeesAllAsAvailable());
}
