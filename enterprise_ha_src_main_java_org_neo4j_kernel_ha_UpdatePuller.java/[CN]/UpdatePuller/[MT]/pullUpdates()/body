{
  if (availabilityGuard.isAvailable(5000)) {
    Response<Void> response=master.pullUpdates(requestContextFactory.newRequestContext(-3));
    new TransactionCommittingResponseUnpacker(resolver.resolveDependency(LogicalTransactionStore.class).getAppender(),resolver.resolveDependency(TransactionRepresentationStoreApplier.class),resolver.resolveDependency(TransactionIdStore.class)).unpackResponse(response);
    lastUpdateTime.setLastUpdateTime(System.currentTimeMillis());
  }
}
