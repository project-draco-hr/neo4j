{
  createHosting();
  configDb.newDatabase("lol","doesntexist",DatabaseHosting.Mode.EXTERNAL,new Config(stringMap(store_dir.name(),testDir.absolutePath() + "/nomatter")));
  restartHosting();
  logging.getMessagesLog(DatabaseHosting.class).assertExactly(error("Unable to start database 'lol', " + "because there is no database provider called 'doesntexist', which this database has been configured " + "to use."));
}
