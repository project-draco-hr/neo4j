{
  Predicate<Path> filter=new Predicate<Path>(){
    public boolean accept(    Path pos){
      return pos.endNode().equals(end);
    }
  }
;
  return Traversal.description().expand(expander).depthFirst().filter(filter).prune(Traversal.pruneAfterDepth(maxDepth)).uniqueness(uniqueness()).traverse(start);
}
