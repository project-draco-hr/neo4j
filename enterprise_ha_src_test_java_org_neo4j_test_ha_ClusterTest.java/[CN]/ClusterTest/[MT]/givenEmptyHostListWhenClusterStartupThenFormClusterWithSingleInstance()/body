{
  HighlyAvailableGraphDatabase db=(HighlyAvailableGraphDatabase)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(TargetDirectory.forTest(getClass()).directory("singleinstance",true).getAbsolutePath()).setConfig(ClusterSettings.server_id,"1").setConfig(ClusterSettings.initial_hosts,"").newGraphDatabase();
  try {
    System.out.println(db.isAvailable(10));
  }
  finally {
    db.shutdown();
  }
}
