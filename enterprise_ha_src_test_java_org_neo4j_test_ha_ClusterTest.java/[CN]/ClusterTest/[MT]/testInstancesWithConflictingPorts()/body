{
  ClusterManager clusterManager=null;
  try {
    clusterManager=new ClusterManager(fromXml(getClass().getResource("/threeinstancesconflictingports.xml").toURI()),TargetDirectory.forTest(getClass()).directory("testClusterConflictingPorts",true),MapUtil.stringMap());
    clusterManager.start();
  }
  finally {
    clusterManager.stop();
  }
}
