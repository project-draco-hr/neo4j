{
  ClusterManager clusterManager=new ClusterManager(getClass().getResource("/fourinstances.xml").toURI(),folder.getRoot(),MapUtil.stringMap());
  clusterManager.start();
  logging.getLogger().info("STOPPING MASTER");
  clusterManager.getMaster("neo4j.ha").stop();
  logging.getLogger().info("STOPPED MASTER");
  Thread.sleep(60000);
  logging.getLogger().info("STOPPING CLUSTER");
  clusterManager.stop();
}
