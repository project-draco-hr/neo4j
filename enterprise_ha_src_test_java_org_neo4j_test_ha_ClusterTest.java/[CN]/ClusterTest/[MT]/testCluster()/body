{
  ClusterManager clusterManager=new ClusterManager(fromXml(getClass().getResource("/threeinstances.xml").toURI()),folder.getRoot(),MapUtil.stringMap());
  clusterManager.start();
  GraphDatabaseService master=clusterManager.getDefaultCluster().getMaster();
  logging.getLogger().info("CREATE NODE");
  Transaction tx=master.beginTx();
  master.createNode();
  logging.getLogger().info("CREATED NODE");
  tx.success();
  tx.finish();
  clusterManager.stop();
}
