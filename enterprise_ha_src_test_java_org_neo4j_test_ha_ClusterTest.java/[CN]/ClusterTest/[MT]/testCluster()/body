{
  ClusterManager clusterManager=new ClusterManager(fromXml(getClass().getResource("/threeinstances.xml").toURI()),TargetDirectory.forTest(getClass()).directory("testCluster",true),MapUtil.stringMap());
  clusterManager.start();
  GraphDatabaseAPI master=clusterManager.getDefaultCluster().getMaster();
  Transaction tx=master.beginTx();
  master.createNode();
  tx.success();
  tx.finish();
  clusterManager.stop();
}
