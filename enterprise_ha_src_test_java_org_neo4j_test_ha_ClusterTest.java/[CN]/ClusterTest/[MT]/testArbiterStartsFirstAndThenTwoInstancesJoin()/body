{
  ClusterManager clusterManager=new ClusterManager(ClusterManager.clusterWithAdditionalArbiters(2,1),TargetDirectory.forTest(getClass()).directory("testCluster",true),MapUtil.stringMap());
  try {
    clusterManager.start();
    clusterManager.getDefaultCluster().await(allSeesAllAsAvailable());
    GraphDatabaseAPI master=clusterManager.getDefaultCluster().getMaster();
    Transaction tx=master.beginTx();
    master.createNode();
    tx.success();
    tx.finish();
  }
  finally {
    clusterManager.stop();
  }
}
