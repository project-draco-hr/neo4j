{
  ClusterManager clusterManager=new ClusterManager(ClusterManager.clusterWithAdditionalArbiters(2,1),testDirectory.directory("testCluster"),MapUtil.stringMap());
  try {
    clusterManager.start();
    clusterManager.getDefaultCluster().await(allSeesAllAsAvailable());
    HighlyAvailableGraphDatabase master=clusterManager.getDefaultCluster().getMaster();
    try (Transaction tx=master.beginTx()){
      master.createNode();
      tx.success();
    }
   }
  finally {
    clusterManager.stop();
  }
}
