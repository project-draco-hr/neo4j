{
  ClusterManager clusterManager=new ClusterManager.Builder(testDirectory.directory("testCluster")).withCluster(clusterWithAdditionalArbiters(2,1)).build();
  try {
    clusterManager.start();
    clusterManager.getCluster().await(allSeesAllAsAvailable());
    HighlyAvailableGraphDatabase master=clusterManager.getCluster().getMaster();
    try (Transaction tx=master.beginTx()){
      master.createNode();
      tx.success();
    }
   }
  finally {
    clusterManager.safeShutdown();
  }
}
