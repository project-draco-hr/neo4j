{
  File storeDir=directory.directory();
  File outputFPath=Files.createTempDirectory("output").toFile();
  CsvOutput output=life.add(new CsvOutput(config(MetricsSettings.csvEnabled.name(),"true",MetricsSettings.csvInterval.name(),"10ms",MetricsSettings.csvPath.name(),outputFPath.getAbsolutePath()),new MetricRegistry(),NullLog.getInstance(),kerneContext(storeDir)));
  life.start();
  waitForFileToAppear(outputFPath);
}
