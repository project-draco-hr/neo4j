{
  File storeDir=directory.directory();
  CsvOutput output=life.add(new CsvOutput(config(MetricsSettings.csvEnabled.name(),"true",MetricsSettings.csvInterval.name(),"10ms",MetricsSettings.csvPath.name(),"test.csv"),new MetricRegistry(),NullLog.getInstance(),kerneContext(storeDir)));
  life.start();
  waitForFileToAppear(new File(storeDir,"test.csv"));
}
