{
  File storeDir=directory.directory();
  String name="metrics.csv";
  CsvOutput output=life.add(new CsvOutput(config(MetricsSettings.csvEnabled.name(),"true",MetricsSettings.csvInterval.name(),"10ms",MetricsSettings.csvPath.name(),name),new MetricRegistry(),NullLog.getInstance(),kernelContext(storeDir)));
  life.start();
  waitForFileToAppear(new File(storeDir,name));
}
