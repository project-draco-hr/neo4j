{
  GraphDatabaseAPI db=(GraphDatabaseAPI)new TestGraphDatabaseFactory().setFileSystem(fs.get()).newImpermanentDatabase(storeDir);
  XaDataSourceManager xaDs=db.getDependencyResolver().resolveDependency(XaDataSourceManager.class);
  XaDataSource additionalDs=new DummyXaDataSource("dummy","dummy".getBytes(),new FakeXAResource("dummy"));
  xaDs.registerDataSource(additionalDs);
  Node node=createNodeWithOneRelationshipToIt(db);
  deleteNodeButNotItsRelationshipsInATwoPhaseTransaction(db,additionalDs,node);
  db.shutdown();
  filterNeostoreLogicalLog(fs.get(),new File(storeDir,LOGICAL_LOG_DEFAULT_NAME + ".v0"),new VerificationLogHook());
}
