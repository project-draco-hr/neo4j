{
  int elementCount=0;
  long actualSize=0;
  long registeredSize=0;
  for (int i=0; i < cache.length(); i++) {
    EntityWithSizeObject obj=cache.get(i);
    if (obj != null) {
      elementCount++;
      actualSize+=obj.sizeOfObjectInBytesIncludingOverhead();
      registeredSize+=obj.getRegisteredSize();
    }
  }
  logger.info(name + " purge (nr " + purgeCount+ "): elementCount:"+ elementCount+ " and sizes actual:"+ getSize(actualSize)+ ", perceived:"+ getSize(currentSize.get())+ " (diff:"+ getSize(currentSize.get() - actualSize)+ "), registered:"+ getSize(registeredSize),true);
}
