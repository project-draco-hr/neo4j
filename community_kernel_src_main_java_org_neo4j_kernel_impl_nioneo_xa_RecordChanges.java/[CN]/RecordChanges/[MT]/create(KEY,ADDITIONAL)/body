{
  if (recordChanges.containsKey(key))   throw new IllegalStateException(key + " already exists");
  RECORD record=loader.newUnused(key,additionalData);
  RecordChange<KEY,RECORD,ADDITIONAL> change=new RecordChange<KEY,RECORD,ADDITIONAL>(recordChanges,key,record,loader,manageBeforeState,true);
  recordChanges.put(key,change);
  return change;
}
