{
  super(zooKeeperServers,msgLog,Client.DEFAULT_READ_RESPONSE_TIMEOUT_SECONDS,Client.DEFAULT_READ_RESPONSE_TIMEOUT_SECONDS,Client.DEFAULT_MAX_NUMBER_OF_CONCURRENT_CHANNELS_PER_CLIENT,sessionTimeout);
  this.clusterName=clusterName;
  try {
    zooKeeper=new ZooKeeper(zooKeeperServers,SESSION_TIME_OUT,new WatcherImpl());
  }
 catch (  IOException e) {
    throw new ZooKeeperException("Unable to create zoo keeper client",e);
  }
}
