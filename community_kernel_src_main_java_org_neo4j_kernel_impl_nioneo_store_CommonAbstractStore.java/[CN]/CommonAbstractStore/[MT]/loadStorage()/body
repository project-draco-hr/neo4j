{
  try {
    readAndVerifyBlockSize();
    verifyFileSizeAndTruncate();
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException("Unable to load storage " + getStorageFileName(),e);
  }
  loadIdGenerator();
  this.windowPool=windowPoolFactory.create(getStorageFileName(),getEffectiveRecordSize(),getFileChannel(),configuration,stringLogger,getNumberOfReservedLowIds());
