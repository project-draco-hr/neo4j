{
  CacheType newType=config.getEnum(CacheType.class,GraphDatabaseSettings.cache_type);
  if (!cacheConfigSame(newType,config)) {
    node=newType.node(logger,config);
    relationship=newType.relationship(logger,config);
  }
 else {
    node.clear();
    relationship.clear();
  }
  this.type=newType;
  this.config=config;
}
