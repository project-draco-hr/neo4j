{
  long endTime=currentTimeMillis() + interval;
  while (currentTimeMillis() < endTime) {
    if (!execution.stillExecuting()) {
      break;
    }
    try {
      sleep(min(10,max(0,endTime - currentTimeMillis())));
    }
 catch (    InterruptedException e) {
      execution.panic(e);
      break;
    }
  }
}
