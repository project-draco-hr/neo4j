{
  if (type == null) {
    throw new NoSuchFieldException(fieldName);
  }
  try {
    Field field=type.getDeclaredField(fieldName);
    if (field != null) {
      return field;
    }
  }
 catch (  NoSuchFieldError e) {
  }
catch (  NoSuchFieldException e) {
  }
  return getField(fieldName,type.getSuperclass());
}
