{
  tx=getCurrentTransaction(tx);
  TxLockElement tle=getOrCreateLockElement(tx);
  try {
    tle.movedOn=false;
    while (totalWriteCount > tle.writeCount) {
      deadlockGuardedWait(tx,tle,READ);
    }
    registerReadLockAcquired(tx,tle);
  }
  finally {
    tle.movedOn=true;
    marked--;
  }
}
