{
  long nodeId;
  Transaction tx=database.graph.beginTx();
  try {
    Node node=database.graph.createNode();
    for (    Map.Entry<String,Object> entry : properties.entrySet()) {
      node.setProperty(entry.getKey(),entry.getValue());
    }
    nodeId=node.getId();
    tx.success();
  }
  finally {
    tx.finish();
  }
  return nodeId;
}
