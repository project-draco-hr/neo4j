{
  long nodeId;
  try (Transaction tx=database.getGraph().beginTx()){
    Node node=database.getGraph().createNode(LABEL);
    for (    Map.Entry<String,Object> entry : properties.entrySet()) {
      node.setProperty(entry.getKey(),entry.getValue());
    }
    nodeId=node.getId();
    tx.success();
  }
   return nodeId;
}
