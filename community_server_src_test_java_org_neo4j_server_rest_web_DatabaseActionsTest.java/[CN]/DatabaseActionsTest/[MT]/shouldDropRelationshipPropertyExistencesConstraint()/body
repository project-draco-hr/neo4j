{
  String typeName="PROGRAMS", propertyKey="since";
  ConstraintDefinition index=graphdbHelper.createRelationshipPropertyExistenceConstraint(typeName,asList(propertyKey));
  actions.dropRelationshipPropertyExistenceConstraint(typeName,asList(propertyKey));
  assertFalse("Constraint should have been dropped",asSet(graphdbHelper.getRelationshipPropertyExistenceConstraints(typeName,propertyKey)).contains(index));
}
