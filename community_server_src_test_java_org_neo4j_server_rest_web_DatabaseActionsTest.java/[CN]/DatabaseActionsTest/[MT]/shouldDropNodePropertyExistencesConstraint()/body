{
  String labelName="user", propertyKey="login";
  ConstraintDefinition index=graphdbHelper.createNodePropertyExistenceConstraint(labelName,asList(propertyKey));
  actions.dropNodePropertyExistenceConstraint(labelName,asList(propertyKey));
  assertFalse("Constraint should have been dropped",asSet(graphdbHelper.getNodePropertyExistenceConstraints(labelName,propertyKey)).contains(index));
}
