{
  String labelName="user", propertyKey="login";
  ConstraintDefinition index=graphdbHelper.createPropertyExistenceConstraint(labelName,asList(propertyKey));
  actions.dropPropertyExistenceConstraint(labelName,asList(propertyKey));
  assertFalse("Constraint should have been dropped",asSet(graphdbHelper.getPropertyExistenceConstraints(labelName,propertyKey)).contains(index));
}
