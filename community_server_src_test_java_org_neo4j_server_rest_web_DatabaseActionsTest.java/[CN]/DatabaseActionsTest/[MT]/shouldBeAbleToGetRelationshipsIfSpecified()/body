{
  long startNode=createBasicTraversableGraph();
  ListRepresentation traverse=actions.traverse(startNode,new HashMap<String,Object>(),TraverserReturnType.relationship);
  List<Object> hits=serialize(traverse);
  for (  Object hit : hits) {
    @SuppressWarnings("unchecked") Map<String,Object> map=(Map<String,Object>)hit;
    RelationshipRepresentationTest.verifySerialisation(map);
  }
}
