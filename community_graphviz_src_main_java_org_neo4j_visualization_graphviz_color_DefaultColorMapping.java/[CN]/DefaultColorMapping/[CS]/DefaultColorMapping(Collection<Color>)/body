{
  Color[] existingColors=Color.values();
  for (  Color color : existingColors) {
    if (!reservedColors.contains(color)) {
      availableColors.add(color.dark);
    }
  }
  for (  Color color : existingColors) {
    if (!reservedColors.contains(color)) {
      availableColors.add(color.light);
    }
  }
}
