{
  try {
    Method runMethod=groovyScriptEngine.getClass().getMethod("run",String.class,groovyBinding.getClass());
    runMethod.invoke(groovyScriptEngine,scriptName + ".groovy",groovyBinding);
  }
 catch (  Exception e) {
    throw new ShellException("Groovy exception: " + this.findProperMessage(e),e);
  }
}
