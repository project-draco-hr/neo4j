{
  try {
    String env=(String)session.get(PATH_STRING);
    List<String> list=new ArrayList<String>();
    if (env != null && env.trim().length() > 0) {
      for (      String path : env.split(":")) {
        list.add(path);
      }
    }
    list.add(".");
    list.add("script");
    list.add("src/script");
    return list;
  }
 catch (  RemoteException e) {
    throw new ShellException(e);
  }
}
