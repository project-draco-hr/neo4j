{
  Resource statement=description.statementFactory.newInstance();
  TraverserIterator iterator=new TraverserIterator(statement,description.uniqueness.create(description.uniquenessParameter),description.expander,description.branchOrdering,description.evaluator,startNodes,description.initialState);
  return description.sorting != null ? new SortingTraverserIterator(statement,this,iterator) : iterator;
}
