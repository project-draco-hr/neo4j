{
  when(store.nodesGetFromIndexSeek(state,indexDescriptor,value)).then(answerAsPrimitiveLongIteratorFrom(asList(2l,3l)));
  state.txState().nodeDoReplaceProperty(1l,noNodeProperty(1l,propertyKeyId),stringProperty(propertyKeyId,value));
  txContext.nodeAddLabel(state,asNode(1l,asPropertyCursor(stringProperty(propertyKeyId,value)),Cursors.<LabelItem>empty()),labelId);
  PrimitiveLongIterator result=txContext.nodesGetFromIndexSeek(state,indexDescriptor,value);
  assertThat(asSet(result),equalTo(asSet(1l,2l,3l)));
}
