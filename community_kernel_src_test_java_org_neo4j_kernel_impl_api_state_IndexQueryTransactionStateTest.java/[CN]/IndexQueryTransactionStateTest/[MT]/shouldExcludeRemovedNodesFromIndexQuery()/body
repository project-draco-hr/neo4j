{
  when(store.nodesGetFromIndexSeek(state,indexDescriptor,value)).then(answerAsPrimitiveLongIteratorFrom(asList(1l,2l,3l)));
  txContext.nodeDelete(state,asNode(2l));
  PrimitiveLongIterator result=txContext.nodesGetFromIndexSeek(state,indexDescriptor,value);
  assertThat(asSet(result),equalTo(asSet(1l,3l)));
}
