{
  when(store.nodeGetUniqueFromIndexSeek(state,indexDescriptor,value)).thenReturn(asPrimitiveResourceIterator(1l));
  when(statement.acquireSingleNodeCursor(1l)).thenReturn(asNodeCursor(2l,PropertyCursor.EMPTY,LabelCursor.EMPTY));
  txContext.nodeDelete(state,1l);
  long result=txContext.nodeGetUniqueFromIndexSeek(state,indexDescriptor,value);
  assertNoSuchNode(result);
}
