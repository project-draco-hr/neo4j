{
  when(store.nodeGetFromUniqueIndexSeek(state,indexDescriptor,value)).thenReturn(asPrimitiveResourceIterator(1l));
  txContext.nodeRemoveProperty(state,asNode(1l,asPropertyCursor(stringProperty(propertyKeyId,value)),asLabelCursor(labelId)),propertyKeyId);
  long result=txContext.nodeGetFromUniqueIndexSeek(state,indexDescriptor,value);
  assertNoSuchNode(result);
}
