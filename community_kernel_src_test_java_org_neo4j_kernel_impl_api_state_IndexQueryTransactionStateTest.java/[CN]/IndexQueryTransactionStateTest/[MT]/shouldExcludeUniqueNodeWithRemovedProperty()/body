{
  when(store.nodeGetUniqueFromIndexLookup(state,indexDescriptor,value)).thenReturn(asPrimitiveResourceIterator(1l));
  when(store.nodeHasLabel(state,1l,labelId)).thenReturn(true);
  when(oldTxState.getNodesWithChangedProperty(propertyKeyId,value)).thenReturn(new DiffSets<>(Collections.<Long>emptySet(),asSet(1l)));
  when(oldTxState.hasChanges()).thenReturn(true);
  long result=txContext.nodeGetUniqueFromIndexLookup(state,indexDescriptor,value);
  assertNoSuchNode(result);
}
