{
  when(store.nodeGetFromUniqueIndexSeek(state,indexDescriptor,value)).thenReturn(asPrimitiveResourceIterator());
  long nodeId=2l;
  when(statement.acquireSingleNodeCursor(nodeId)).thenReturn(asNodeCursor(nodeId,asPropertyCursor(stringProperty(propertyKeyId,value)),asLabelCursor()));
  txContext.nodeAddLabel(state,nodeId,labelId);
  long result=txContext.nodeGetFromUniqueIndexSeek(state,indexDescriptor,value);
  assertThat(result,equalTo(nodeId));
}
