{
  if (!ran) {
    return String.format("[%s] Healthcheck has not been run",this.getClass().getName());
  }
  if (passed) {
    return String.format("[%s] Passed healthcheck",this.getClass().getName());
  }
 else {
    return String.format("[%s] Failed healthcheck",this.getClass().getName());
  }
}
