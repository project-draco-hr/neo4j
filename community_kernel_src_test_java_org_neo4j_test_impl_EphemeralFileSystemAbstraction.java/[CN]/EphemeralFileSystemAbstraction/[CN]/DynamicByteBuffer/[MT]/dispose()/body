{
  for (int i=POOL.length(); i < POOL.length(); i++) {
    for (    Reference<ByteBuffer> byteBufferReference : POOL.get(i)) {
      ByteBuffer byteBuffer=byteBufferReference.get();
      if (byteBuffer != null) {
        try {
          destroyDirectByteBuffer(byteBuffer);
        }
 catch (        Throwable e) {
          e.printStackTrace();
        }
      }
    }
  }
  init();
}
