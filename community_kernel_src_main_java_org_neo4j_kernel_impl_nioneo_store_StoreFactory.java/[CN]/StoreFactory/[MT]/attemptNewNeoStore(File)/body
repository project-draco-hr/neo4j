{
  return new NeoStore(fileName,config,idGeneratorFactory,windowPoolFactory,fileSystemAbstraction,stringLogger,txHook,newRelationshipTypeStore(new File(fileName.getPath() + ".relationshiptypestore.db")),newPropertyStore(new File(fileName.getPath() + ".propertystore.db")),newRelationshipStore(new File(fileName.getPath() + ".relationshipstore.db")),newNodeStore(new File(fileName.getPath() + ".nodestore.db")),newOrCreateSchemaStore(new File(fileName.getPath() + ".schemastore.db")));
}
