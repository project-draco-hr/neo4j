{
  ByteBuffer firstRecord=ByteBuffer.allocate(RelationshipGroupStore.RECORD_SIZE).putInt(denseNodeThreshold);
  firstRecord.flip();
  firstRecord.limit(firstRecord.capacity());
  createEmptyStore(storeFileName(RELATIONSHIP_GROUP_STORE_NAME),buildTypeDescriptorAndVersion(RelationshipGroupStore.TYPE_DESCRIPTOR),firstRecord,IdType.RELATIONSHIP_GROUP);
}
