{
  File labelsFileName=new File(baseFileName.getPath() + LABELS_PART);
  DynamicArrayStore dynamicLabelStore=new DynamicArrayStore(labelsFileName,config,IdType.NODE_LABELS,idGeneratorFactory,windowPoolFactory,fileSystemAbstraction,stringLogger,versionMismatchHandler,monitors);
  return new NodeStore(baseFileName,config,idGeneratorFactory,windowPoolFactory,fileSystemAbstraction,stringLogger,dynamicLabelStore,versionMismatchHandler,monitors);
}
