{
  createEmptyStore(storeFileName(LABEL_TOKEN_STORE_NAME),buildTypeDescriptorAndVersion(LabelTokenStore.TYPE_DESCRIPTOR));
  createDynamicStringStore(storeFileName(LABEL_TOKEN_NAMES_STORE_NAME),TokenStore.NAME_STORE_BLOCK_SIZE,IdType.LABEL_TOKEN_NAME);
  LabelTokenStore store=newLabelTokenStore();
  store.close();
}
