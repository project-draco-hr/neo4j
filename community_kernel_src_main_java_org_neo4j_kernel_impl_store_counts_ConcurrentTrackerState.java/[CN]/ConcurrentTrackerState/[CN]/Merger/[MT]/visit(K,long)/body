{
  while (next < updates.length) {
    Update<K> nextUpdate=updates[next];
    int cmp=key.compareTo(nextUpdate.key);
    if (cmp == 0) {
      next++;
      value=nextUpdate.value;
    }
 else     if (cmp > 0) {
      next++;
      target.visit(nextUpdate.key,nextUpdate.value);
      continue;
    }
    break;
  }
  target.visit(key,value);
}
