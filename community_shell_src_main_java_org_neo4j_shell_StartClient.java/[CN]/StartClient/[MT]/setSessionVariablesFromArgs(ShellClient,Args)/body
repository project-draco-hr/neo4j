{
  String profile=args.get("profile",null);
  if (profile != null) {
    applyProfileFile(new File(profile),client);
  }
  for (  Map.Entry<String,String> entry : args.asMap().entrySet()) {
    String key=entry.getKey();
    if (key.startsWith("D")) {
      key=key.substring(1);
      client.session().set(key,entry.getValue());
    }
  }
}
