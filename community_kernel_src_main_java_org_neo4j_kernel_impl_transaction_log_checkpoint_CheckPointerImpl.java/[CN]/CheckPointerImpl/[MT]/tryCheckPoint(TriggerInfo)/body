{
  if (lock.tryLock()) {
    try {
      return doCheckPoint(info,LogCheckPointEvent.NULL);
    }
  finally {
      lock.unlock();
    }
  }
 else {
    lock.lock();
    try {
      msgLog.info(info.describe(lastCheckPointedTx) + " Check pointing was already running, completed now");
      return lastCheckPointedTx;
    }
  finally {
      lock.unlock();
    }
  }
}
