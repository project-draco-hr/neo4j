{
  LogProvider logProvider=FormattedLogProvider.toOutputStream(System.out);
  try {
    ConsistencyCheckService.Result result=new ConsistencyCheckService().runFullConsistencyCheck(testDirectory.graphDbDir(),new Config(),ProgressMonitorFactory.textual(System.err),logProvider);
    assertTrue(result.isSuccessful());
  }
 catch (  ConsistencyCheckIncompleteException e) {
    fail(e.getMessage());
  }
}
