{
  @SuppressWarnings("unchecked") T[] result=(type != null) ? (T[])Array.newInstance(type,servers.length) : null;
  for (int i=0; i < servers.length; i++) {
    if (servers[i] != null) {
      SubProcess.stop(servers[i].first());
      if (result != null)       result[i]=servers[i].other();
    }
  }
  return result;
}
