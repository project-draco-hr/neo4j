{
  channelGroup.add(channel);
  if (slave == null) {
    return;
  }
synchronized (connectedSlaveChannels) {
    Set<SlaveContext> txs=connectedSlaveChannels.get(channel);
    if (txs == null) {
      txs=new HashSet<SlaveContext>();
      connectedSlaveChannels.put(channel,txs);
    }
    txs.add(slave);
  }
}
