{
  Long result=null;
  while (result == null) {
    if (!source.hasNext()) {
      endReached();
      break;
    }
    Document doc=source.next();
    Long id=Long.parseLong(doc.getField(LuceneIndex.KEY_DOC_ID).stringValue());
    if (exclude == null || !exclude.contains(id)) {
      result=id;
    }
  }
  return result;
}
