{
  indexStorage=new PartitionedIndexStorage(DirectoryFactory.PERSISTENT,new DefaultFileSystemAbstraction(),testDir.directory(),1);
  LuceneIndex luceneIndex=new LuceneIndex(indexStorage,new IndexConfiguration(false),new IndexSamplingConfig(new Config()));
  luceneIndex.prepare();
  luceneIndex.open();
  accessor=new LuceneIndexAccessor(luceneIndex);
}
