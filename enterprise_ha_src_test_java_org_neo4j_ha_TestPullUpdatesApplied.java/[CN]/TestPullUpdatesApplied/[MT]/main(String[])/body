{
  int i=Integer.parseInt(args[1]);
  HighlyAvailableGraphDatabase db=new HighlyAvailableGraphDatabase(args[0],stringMap(HaConfig.CONFIG_KEY_SERVER_ID,"" + i,HaConfig.CONFIG_KEY_SERVER,"localhost:" + (6666 + i),HaConfig.CONFIG_KEY_COORDINATORS,args[2],HaConfig.CONFIG_KEY_PULL_INTERVAL,0 + "ms"));
  db.pullUpdates();
}
