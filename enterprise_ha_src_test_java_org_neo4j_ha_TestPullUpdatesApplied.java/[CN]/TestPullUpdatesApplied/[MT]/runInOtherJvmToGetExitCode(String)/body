{
  List<String> allArgs=new ArrayList<String>(Arrays.asList("java","-cp",System.getProperty("java.class.path"),TestPullUpdatesApplied.class.getName()));
  allArgs.addAll(Arrays.asList(args));
  Process p=Runtime.getRuntime().exec(allArgs.toArray(new String[allArgs.size()]));
  List<Thread> threads=new LinkedList<Thread>();
  launchStreamConsumers(threads,p);
  Thread.sleep(5000);
  p.destroy();
  for (  Thread t : threads)   t.join();
  return 0;
}
