{
  return (HighlyAvailableGraphDatabase)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(path).setConfig(ClusterSettings.cluster_server,"127.0.0.1:" + (5001 + serverId)).setConfig(ClusterSettings.initial_hosts,"127.0.0.1:5001").setConfig(ClusterSettings.server_id,Integer.toString(serverId)).setConfig(HaSettings.ha_server,"localhost:" + (6666 + serverId)).setConfig(HaSettings.pull_interval,"0ms").newGraphDatabase();
}
