{
  return (HighlyAvailableGraphDatabase)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(dir.directory("" + i,clear).getAbsolutePath()).setConfig(ClusterSettings.cluster_server,"127.0.0.1:" + (5001 + i)).setConfig(ClusterSettings.initial_hosts,"127.0.0.1:5001").setConfig(HaSettings.server_id,"" + i).setConfig(HaSettings.ha_server,"localhost:" + (6666 + i)).setConfig(HaSettings.pull_interval,"0ms").newGraphDatabase();
}
