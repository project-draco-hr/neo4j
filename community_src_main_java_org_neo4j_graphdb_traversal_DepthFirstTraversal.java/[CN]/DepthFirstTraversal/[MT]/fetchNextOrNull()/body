{
  Position position=null;
  while (position == null) {
    if (current == null) {
      return null;
    }
    ExpansionSource next=current.next(this);
    if (next == null) {
      current=current.parent;
      continue;
    }
 else {
      current=next;
    }
    if (current != null) {
      position=current.position(this);
    }
  }
  return position;
}
