{
  long[] nodes=createMoreComplexGraph();
  String noHitsJson="{\"to\":\"" + functionalTestHelper.nodeUri(nodes[1]) + "\", \"max depth\":3, \"relationships\":{\"type\":\"to\", \"direction\":\"in\"}, \"algorithm\":\"shortestPath\"}";
  ClientResponse response=postPathQuery(nodes,noHitsJson,"/path");
  assertEquals(404,response.getStatus());
}
