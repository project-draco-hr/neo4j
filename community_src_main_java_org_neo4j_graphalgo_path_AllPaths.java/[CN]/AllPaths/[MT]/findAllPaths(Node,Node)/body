{
  ReturnFilter filter=new ReturnFilter(){
    public boolean shouldReturn(    Position pos){
      return pos.node().equals(end);
    }
  }
;
  return TraversalFactory.createTraversalDescription().expand(expander).depthFirst().filter(filter).prune(TraversalFactory.pruneAfterDepth(maxDepth)).uniqueness(uniqueness()).traverse(start).paths();
}
