{
  for (int i=0; i < 250; i++) {
    final int x=i;
    new Thread(){
      public void run(){
        try {
          Assert.assertEquals(x + "",json.format(GremlinPluginTest.executeTestScript("x=" + x + "; x",null)));
        }
 catch (        BadInputException e) {
          e.printStackTrace();
        }
      }
    }
.start();
  }
}
