{
  try (ResourceIterator<File> files=ds.listLogicalLogs()){
    while (files.hasNext()) {
      File storeFile=files.next();
      try {
        copyFile(writer,fs,baseDir,temporaryBuffer,storeFile);
      }
 catch (      FileNotFoundException ignored) {
      }
    }
  }
 catch (  IOException e) {
    throw new ServerFailureException(e);
  }
}
