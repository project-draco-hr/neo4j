{
  List<Triplet<String,Long,TxExtractor>> stream=new ArrayList<>();
  XaDataSourceManager dsManager=dsManager(graphDb);
  final XaDataSource dataSource=dsManager.getXaDataSource(dataSourceName);
  if (dataSource == null) {
    throw new RuntimeException("No data source '" + dataSourceName + "' found");
  }
  List<LogExtractor> extractors=startTx < endTx ? Collections.singletonList(getTransactionStreamForDatasource(dataSource,startTx,endTx,stream,ServerUtil.ALL)) : Collections.<LogExtractor>emptyList();
  return new Response<>(null,graphDb.storeId(),createTransactionStream(Collections.singletonList(dataSourceName),stream,extractors),ResourceReleaser.NO_OP);
}
