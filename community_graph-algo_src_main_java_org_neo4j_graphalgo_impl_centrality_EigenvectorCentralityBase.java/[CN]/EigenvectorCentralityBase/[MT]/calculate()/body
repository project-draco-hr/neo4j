{
  if (doneCalculation) {
    return;
  }
  doneCalculation=true;
  values=new HashMap<Node,Double>();
  totalIterations=0;
  Random random=new Random(System.currentTimeMillis());
  for (  Node node : nodeSet) {
    values.put(node,random.nextDouble());
  }
  normalize(values);
  runIterations(maxIterations);
}
