{
  String msg;
  if (arguments != null) {
    try {
      msg=format(message,arguments);
    }
 catch (    IllegalFormatException e) {
      msg=format("IllegalFormat{message: \"%s\", arguments: %s}",message,Arrays.toString(arguments));
    }
  }
 else {
    msg=message;
  }
  return format("%s @ %s: %s",level,context,msg);
}
