{
synchronized (logCalls) {
    assertEquals(logCalls.size(),logSkipCount + throwables.length);
    for (int i=0; i < throwables.length; i++) {
      LogCall logCall=logCalls.get(logSkipCount + i);
      Throwable throwable=throwables[i];
      if (logCall.throwable == null && throwable != null || logCall.throwable != null && logCall.throwable.getClass() != throwable.getClass()) {
        fail(format("Expected %s, but was:\n%s",throwable,logCall.throwable));
      }
    }
  }
}
