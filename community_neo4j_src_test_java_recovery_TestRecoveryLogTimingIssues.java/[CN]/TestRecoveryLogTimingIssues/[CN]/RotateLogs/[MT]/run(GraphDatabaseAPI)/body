{
  try {
    if (dataSources.isEmpty())     dsManager(graphdb).getNeoStoreDataSource().rotateLogicalLog();
 else {
      for (      XaDataSource ds : dsManager(graphdb).getAllRegisteredDataSources()) {
        if (dataSources.contains(ds.getName()))         ds.rotateLogicalLog();
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
