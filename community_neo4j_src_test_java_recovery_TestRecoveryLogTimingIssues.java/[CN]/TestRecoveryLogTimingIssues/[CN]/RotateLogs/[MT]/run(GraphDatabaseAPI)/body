{
  try {
    if (dataSources.isEmpty())     graphdb.getXaDataSourceManager().getNeoStoreDataSource().rotateLogicalLog();
 else {
      for (      XaDataSource ds : graphdb.getXaDataSourceManager().getAllRegisteredDataSources()) {
        if (dataSources.contains(ds.getName()))         ds.rotateLogicalLog();
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
