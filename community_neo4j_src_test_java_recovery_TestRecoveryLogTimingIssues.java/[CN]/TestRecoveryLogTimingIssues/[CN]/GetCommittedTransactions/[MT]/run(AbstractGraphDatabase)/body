{
  try {
    XaDataSource dataSource=graphdb.getConfig().getTxModule().getXaDataSourceManager().getXaDataSource(DEFAULT_DATA_SOURCE_NAME);
    for (long logVersion=0; logVersion < highestLogVersion; logVersion++) {
      dataSource.getLogicalLog(logVersion);
    }
    for (long txId=2; txId <= highestTxId; txId++) {
      dataSource.getCommittedTransaction(txId);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
