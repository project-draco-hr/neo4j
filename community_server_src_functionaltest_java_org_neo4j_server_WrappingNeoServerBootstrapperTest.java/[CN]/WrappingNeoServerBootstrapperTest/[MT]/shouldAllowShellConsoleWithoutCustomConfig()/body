{
  WrappingNeoServerBootstrapper srv;
  srv=new WrappingNeoServerBootstrapper(getGraphDb());
  srv.start();
  String response=gen.get().payload("{\"command\" : \"ls\",\"engine\":\"shell\"}").expectedStatus(Status.OK.getStatusCode()).post("http://127.0.0.1:7474/db/manage/server/console/").entity();
  assertTrue(response.contains("neo4j-sh (0)$"));
  srv.stop();
}
