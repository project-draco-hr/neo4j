{
  EmbeddedServerConfigurator config=new EmbeddedServerConfigurator(myDb);
  String hostAddress=InetAddress.getLocalHost().getHostAddress();
  config.configuration().setProperty(Configurator.WEBSERVER_ADDRESS_PROPERTY_KEY,hostAddress);
  WrappingNeoServerBootstrapper srv=new WrappingNeoServerBootstrapper(myDb,config);
  srv.start();
  try {
    gen.get().expectedStatus(Status.OK.getStatusCode()).get("http://127.0.0.1:7474/db/data/");
    fail();
  }
 catch (  ClientHandlerException cee) {
  }
  gen.get().expectedStatus(Status.OK.getStatusCode()).get("http://" + hostAddress + ":7474/db/data/");
  srv.stop();
}
