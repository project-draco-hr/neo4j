{
  deletionFailureOk=true;
  theDatabase.start();
  CommunityDatabase db=new CommunityDatabase(configuratorWithServerProperties(stringMap(Configurator.DATABASE_LOCATION_PROPERTY_KEY,databaseDirectory.getAbsolutePath())));
  try {
    db.start();
  }
 catch (  RuntimeException e) {
    assertThat(e.getCause().getCause(),instanceOf(StoreLockException.class));
  }
}
