def test_write_in_one_thread_read_in_another(self):
    db = self.graphdb
    with db.transaction:
        node = db.node(name='Bob the node')

    def read_method():
        self.assertEquals(db.node[node.id]['name'], 'Bob the node')
    thread = threading.Thread(target=read_method)
    thread.start()
    thread.join()
