{
  NodeOrRelationship result=null;
  if (typedId.isNode()) {
    try {
      result=NodeOrRelationship.wrap(server.getNeo().getNodeById(typedId.getId()));
    }
 catch (    NotFoundException e) {
      throw new ShellException("Node " + typedId.getId() + " not found",e);
    }
  }
 else {
    try {
      result=NodeOrRelationship.wrap(server.getNeo().getRelationshipById(typedId.getId()));
    }
 catch (    NotFoundException e) {
      throw new ShellException("Relationship " + typedId.getId() + " not found",e);
    }
  }
  return result;
}
