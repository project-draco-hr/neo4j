{
  List<Tester> testers=new ArrayList<Tester>(20);
  for (int i=0; i < 20; i++) {
    int x=r.nextInt(3);
    Tester t;
    if (x == 0) {
      t=new Reader();
    }
 else     if (x == 1) {
      t=new Committer();
    }
 else     if (x == 2) {
      t=new Rollbacker();
    }
 else {
      throw new Exception("oh noes");
    }
    testers.add(t);
  }
  return testers;
}
