{
  List<Tester> testers=new ArrayList<>(20);
  for (int i=0; i < 20; i++) {
    int x=r.nextInt(3);
    Tester t;
    if (x == 0) {
      t=new Reader("Reader-" + i);
    }
 else     if (x == 1) {
      t=new Committer("Committer-" + i);
    }
 else     if (x == 2) {
      t=new Rollbacker("Rollbacker-" + i);
    }
 else {
      throw new Exception("oh noes");
    }
    testers.add(t);
  }
  return testers;
}
