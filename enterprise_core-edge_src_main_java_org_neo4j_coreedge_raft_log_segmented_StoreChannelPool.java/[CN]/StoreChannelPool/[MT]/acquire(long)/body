{
  if (closed) {
    throw new RuntimeException(CLOSED_ERROR_MESSAGE);
  }
  if (markedForDisposal) {
    throw new DisposedException();
  }
  StoreChannel storeChannel=channels.pollFirst();
  if (storeChannel == null) {
    storeChannel=create();
  }
  storeChannel.position(byteOffset);
  return storeChannel;
}
