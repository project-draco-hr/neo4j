{
  super();
  boolean isMaster=args.getBoolean("master",null).booleanValue();
  String storeDir=args.get("path",null);
  AbstractBroker broker=isMaster ? new FakeMasterBroker() : new FakeSlaveBroker(args.getNumber("id",null).intValue());
  this.db=new HighlyAvailableGraphDatabase(storeDir,new HashMap<String,String>(),broker);
  broker.setDb(this.db);
  this.location=location;
  this.location.ensureRegistryCreated();
  this.location.bind(this);
}
