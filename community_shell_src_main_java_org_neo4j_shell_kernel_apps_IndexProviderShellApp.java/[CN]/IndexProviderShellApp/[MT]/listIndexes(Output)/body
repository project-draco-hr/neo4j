{
  out.println("Node indexes:");
  for (  String name : getServer().getDb().index().nodeIndexNames()) {
    out.println("  " + name);
  }
  out.println("");
  out.println("Relationship indexes:");
  for (  String name : getServer().getDb().index().relationshipIndexNames()) {
    out.println("  " + name);
  }
}
