{
  FileSystemAbstraction fs=new EphemeralFileSystemAbstraction();
  Map<String,String> configParams=stringMap(GraphDatabaseSettings.neo_store.name(),"graph.db/neostore");
  life=new LifeSupport();
  life.start();
  PageCache pageCache=StandalonePageCacheFactory.createPageCache(fs,getClass().getName(),life);
  storeFactory=new StoreFactory(new Config(configParams),new DefaultIdGeneratorFactory(),pageCache,fs,StringLogger.DEV_NULL,new Monitors());
}
