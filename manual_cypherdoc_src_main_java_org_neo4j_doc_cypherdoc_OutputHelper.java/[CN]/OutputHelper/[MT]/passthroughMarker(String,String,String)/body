{
  StringBuilder sb=new StringBuilder(64);
  sb.append('<').append(htmlElement).append(" class=\"").append(role).append("\"></").append(htmlElement).append('>');
  String html=sb.toString();
  sb=new StringBuilder(64);
  sb.append('<').append(docbookElement).append(" role=\"").append(role).append("\"></").append(docbookElement).append('>');
  String docbook=sb.toString();
  return passthroughHtmlAndDocbook(html,docbook);
}
