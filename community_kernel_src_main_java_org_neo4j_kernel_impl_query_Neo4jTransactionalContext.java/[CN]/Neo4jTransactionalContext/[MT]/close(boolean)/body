{
  if (isOpen) {
    try {
      statement.metaOperations().unregisterExecutingQuery(executingQuery);
      statement.close();
      if (success) {
        transaction.success();
      }
 else {
        transaction.failure();
      }
      transaction.close();
    }
  finally {
      statement=null;
      transaction=null;
      isOpen=false;
    }
  }
}
