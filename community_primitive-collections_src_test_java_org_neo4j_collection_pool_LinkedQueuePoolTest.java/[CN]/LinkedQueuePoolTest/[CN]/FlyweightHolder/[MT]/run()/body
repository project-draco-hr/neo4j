{
  try {
    R acquired=pool.acquire();
    onAcquire.countDown();
    try {
      latch.acquire();
    }
 catch (    InterruptedException e) {
      throw new RuntimeException(e);
    }
    if (release.get()) {
      pool.release(acquired);
      released.countDown();
    }
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
