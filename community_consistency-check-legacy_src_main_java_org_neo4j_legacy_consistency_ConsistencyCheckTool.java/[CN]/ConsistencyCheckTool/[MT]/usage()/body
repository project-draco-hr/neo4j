{
  return joinAsLines(jarUsage(getClass(),"[-propowner] [-recovery] [-config <neo4j.conf>] <storedir>"),"WHERE:   <storedir>      is the path to the store to check","         -recovery       to perform recovery on the store before checking","         <neo4j.conf>    is the location of an optional properties file containing tuning","                         parameters for the consistency check");
}
