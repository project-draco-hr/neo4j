{
  try {
    int eventIdentifier=((TxManager)txManager).getEventIdentifier();
    return responseReceiver.receive(broker.getMaster().first().createRelationshipType(responseReceiver.getSlaveContext(eventIdentifier),name));
  }
 catch (  ZooKeeperException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
catch (  ComException e) {
    clusterReceiver.newMaster(e);
    throw e;
  }
}
