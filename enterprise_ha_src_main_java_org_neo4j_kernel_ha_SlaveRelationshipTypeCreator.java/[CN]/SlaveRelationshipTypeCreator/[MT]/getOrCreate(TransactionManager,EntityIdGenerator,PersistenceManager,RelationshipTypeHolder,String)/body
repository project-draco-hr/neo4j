{
  try {
    int eventIdentifier=((TxManager)txManager).getEventIdentifier();
    return databaseOperations.receive(broker.getMaster().first().createRelationshipType(databaseOperations.getSlaveContext(eventIdentifier),name));
  }
 catch (  RuntimeException e) {
    databaseOperations.exceptionHappened(e);
    throw e;
  }
}
