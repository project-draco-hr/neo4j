{
  boolean foundGroup=false;
synchronized (this) {
    List<GroupRecord> newGroups=new ArrayList<>();
    for (    GroupRecord other : groups) {
      if (other.name().equals(group.name())) {
        foundGroup=true;
      }
 else {
        newGroups.add(other);
      }
    }
    if (foundGroup) {
      groups=newGroups;
      saveGroupsToFile();
      groupsByName.remove(group.name());
    }
    removeFromUserMap(group);
  }
  return foundGroup;
}
