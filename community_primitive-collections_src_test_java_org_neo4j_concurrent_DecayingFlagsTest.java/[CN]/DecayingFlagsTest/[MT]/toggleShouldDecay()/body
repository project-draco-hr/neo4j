{
  DecayingFlags.Key featureA=new DecayingFlags.Key(1);
  DecayingFlags.Key featureB=new DecayingFlags.Key(3);
  DecayingFlags set=new DecayingFlags(2);
  set.flag(featureA);
  set.flag(featureA);
  set.flag(featureB);
  set.sweep();
  assertEquals("4000",set.asHex());
  set.sweep();
  assertEquals("0000",set.asHex());
}
