{
  if (opcode == Opcodes.NEW) {
    super.visitTypeInsn(opcode,typeName);
    ++outstandingAllocs;
  }
 else   if (opcode == Opcodes.ANEWARRAY) {
    super.visitInsn(Opcodes.DUP);
    super.visitTypeInsn(opcode,typeName);
    invokeRecordAllocation(typeName);
  }
 else {
    super.visitTypeInsn(opcode,typeName);
  }
}
