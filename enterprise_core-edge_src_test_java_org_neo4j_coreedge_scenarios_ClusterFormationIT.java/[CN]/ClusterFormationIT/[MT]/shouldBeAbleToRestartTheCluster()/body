{
  assertEquals(3,cluster.numberOfCoreMembersReportedByTopology());
  cluster.shutdown();
  cluster.start();
  assertEquals(3,cluster.numberOfCoreMembersReportedByTopology());
  cluster.removeCoreMemberWithMemberId(1);
  cluster.addCoreMemberWithId(3).start();
  cluster.shutdown();
  cluster.start();
  assertEquals(3,cluster.numberOfCoreMembersReportedByTopology());
}
