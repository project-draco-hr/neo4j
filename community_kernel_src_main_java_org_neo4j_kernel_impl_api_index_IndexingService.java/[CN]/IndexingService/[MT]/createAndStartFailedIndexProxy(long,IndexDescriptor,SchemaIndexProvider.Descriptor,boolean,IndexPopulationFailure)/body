{
  IndexPopulator indexPopulator=getPopulatorFromProvider(providerDescriptor,ruleId,new IndexConfiguration(unique));
  String indexUserDescription=indexUserDescription(descriptor,providerDescriptor);
  IndexProxy result=new FailedIndexProxy(descriptor,providerDescriptor,indexUserDescription,indexPopulator,populationFailure);
  result=contractCheckedProxy(result,true);
  return serviceDecoratedProxy(ruleId,result);
}
