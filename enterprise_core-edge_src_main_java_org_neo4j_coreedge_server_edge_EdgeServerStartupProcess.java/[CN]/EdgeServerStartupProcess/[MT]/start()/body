{
  dataSourceManager.start();
  CoreMember coreMember=findCoreMemberToCopyFrom();
  if (localDatabase.isEmpty()) {
    localDatabase.stop();
    localDatabase.copyStoreFrom(coreMember,storeFetcher);
    localDatabase.start();
  }
 else {
    localDatabase.ensureSameStoreId(coreMember,storeFetcher);
  }
  txPulling.start();
}
