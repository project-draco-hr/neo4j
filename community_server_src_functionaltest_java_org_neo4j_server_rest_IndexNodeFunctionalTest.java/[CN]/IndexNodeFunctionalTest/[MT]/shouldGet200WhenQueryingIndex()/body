{
  String indexName="bobTheIndex";
  String key="bobsKey";
  String value="bobsValue";
  long node=helper.createNode();
  helper.addNodeToIndex(indexName,key,value,node);
  ClientResponse response=Client.create().resource(functionalTestHelper.indexNodeUri(indexName) + "?query=" + key+ ":"+ value).accept(MediaType.APPLICATION_JSON).get(ClientResponse.class);
  assertEquals(200,response.getStatus());
  response.close();
}
