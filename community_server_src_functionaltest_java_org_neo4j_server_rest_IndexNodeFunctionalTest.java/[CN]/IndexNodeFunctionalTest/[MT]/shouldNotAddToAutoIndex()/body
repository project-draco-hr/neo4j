{
  String indexName=server.getDatabase().graph.index().getNodeAutoIndexer().getAutoIndex().getName();
  String key="key";
  String value="the value";
  value=URIHelper.encode(value);
  int nodeId=0;
  Response r=RestRequest.req().post(functionalTestHelper.indexNodeUri(indexName,key,value),JsonHelper.createJsonFrom(functionalTestHelper.nodeUri(nodeId)));
  assertEquals(403,r.getStatus());
}
