{
  final String index="people", key="name", value="Mattias";
  helper.createNodeIndex(index);
  gen().expectedStatus(201).payloadType(MediaType.APPLICATION_JSON_TYPE).payload("{\"key\": \"" + key + "\", \"value\": \""+ value+ "\", \"uri\":\""+ functionalTestHelper.nodeUri(helper.createNode())+ "\"}").post(functionalTestHelper.nodeIndexUri() + index + "?unique");
}
