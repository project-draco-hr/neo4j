{
  if (readOnlyIndexesCreated == 0) {
    gen.get().expectedStatus(204).get(functionalTestHelper.nodeIndexUri());
  }
 else {
    String entity=gen.get().expectedStatus(200).get(functionalTestHelper.nodeIndexUri()).entity();
    Map<String,Object> map=JsonHelper.jsonToMap(entity);
    assertEquals(readOnlyIndexesCreated,map.size());
  }
}
