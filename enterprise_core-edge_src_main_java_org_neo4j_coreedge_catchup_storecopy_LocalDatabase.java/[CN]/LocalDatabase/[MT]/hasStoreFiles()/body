{
  for (  StoreType storeType : StoreType.values()) {
    StoreFile storeFile=storeType.getStoreFile();
    if (storeFile != null) {
      boolean exists=fileSystemAbstraction.fileExists(new File(storeDir,storeFile.storeFileName()));
      if (exists) {
        return true;
      }
    }
  }
  return false;
}
