{
  if (storeId == null) {
    org.neo4j.kernel.impl.store.StoreId kernelStoreId=dataSourceManager.getDataSource().getStoreId();
    storeId=new StoreId(kernelStoreId.getCreationTime(),kernelStoreId.getRandomId(),kernelStoreId.getUpgradeTime(),kernelStoreId.getUpgradeId());
    log.info("My StoreId is: " + storeId);
  }
  return storeId;
}
