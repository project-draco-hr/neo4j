{
  try {
    return lookup.unreflectConstructor(procDefinition.getConstructor());
  }
 catch (  IllegalAccessException|NoSuchMethodException e) {
    throw new ProcedureException(Status.Procedure.FailedRegistration,e,"Unable to find a usable public no-argument constructor in the class `%s`. " + "Please add a valid, public constructor, recompile the class and try again.",procDefinition.getSimpleName());
  }
}
