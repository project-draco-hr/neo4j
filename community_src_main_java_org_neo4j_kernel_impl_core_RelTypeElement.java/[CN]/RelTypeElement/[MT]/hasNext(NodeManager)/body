{
  if (nextElement != null) {
    return true;
  }
  while (!addTraversed && position < add.length()) {
    int value=add.get(position++);
    if (position >= add.length()) {
      addTraversed=true;
      position=0;
    }
    if (!remove.contains(value)) {
      nextElement=value;
      return true;
    }
  }
  while (!srcTraversed) {
    if (position >= src.length()) {
      while (getNode().getMoreRelationships(nodeManager) && position >= src.length())       ;
      if (position >= src.length()) {
        srcTraversed=true;
        position=0;
        return false;
      }
    }
    int value=src.get(position++);
    if (!remove.contains(value)) {
      nextElement=value;
      return true;
    }
  }
  return false;
}
