{
  long at=0;
  for (int i=0; i < groupCounts.length; i++) {
    at+=groupCounts[i];
    if (nodeInOrder < at) {
      return groups[i];
    }
  }
  throw new IllegalArgumentException("Strange, couldn't find group for node (import order) " + nodeInOrder + ", counted to "+ at+ " as total number of "+ at);
}
