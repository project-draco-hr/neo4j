{
  Transaction tx=neo.beginTx();
  SingleSourceShortestPath<Integer> singleSourceShortestPath=new SingleSourceShortestPathBFS(graph.getRandomNode(null),Direction.BOTH,MyRelTypes.R1);
  new ClosenessCentrality<Integer>(singleSourceShortestPath,new IntegerAdder(),0,graph2.getNodes(),new CostDivider<Integer>(){
    public Integer divideByCost(    Double d,    Integer c){
      return (int)(d / c);
    }
    public Integer divideCost(    Integer c,    Double d){
      return (int)(c / d);
    }
  }
).calculate();
  tx.finish();
}
