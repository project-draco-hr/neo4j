{
  try (Transaction transaction=db.beginTx()){
    String output="";
    Traverser traverser=td.traverse(A);
    PathPrinter pathPrinter=new PathPrinter("name");
    for (    Path path : traverser) {
      output+=Paths.pathToString(path,pathPrinter);
    }
    output+="\n";
    return output;
  }
 }
