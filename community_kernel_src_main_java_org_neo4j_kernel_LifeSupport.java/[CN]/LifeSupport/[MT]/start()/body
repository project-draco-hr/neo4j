{
  init();
  if (status == Status.STOPPED) {
    status=Status.STARTING;
    for (    LifecycleInstance instance : instances) {
      try {
        instance.start();
      }
 catch (      LifecycleException e) {
        status=Status.STARTED;
        try {
          stop();
        }
 catch (        LifecycleException e1) {
          throw causedBy(e1,e);
        }
        throw e;
      }
    }
    status=Status.STARTED;
  }
}
