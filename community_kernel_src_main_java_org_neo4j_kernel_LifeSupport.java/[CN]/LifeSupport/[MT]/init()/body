{
  if (status == Status.NONE) {
    status=Status.INITIALIZING;
    for (    LifecycleInstance instance : instances) {
      try {
        instance.init();
      }
 catch (      LifecycleException e) {
        status=Status.STOPPED;
        try {
          shutdown();
        }
 catch (        LifecycleException e1) {
          throw causedBy(e1,e);
        }
        throw e;
      }
    }
    status=Status.STOPPED;
  }
}
