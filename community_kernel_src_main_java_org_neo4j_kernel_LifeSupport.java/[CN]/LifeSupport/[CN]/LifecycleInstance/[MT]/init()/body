{
  if (currentStatus == Status.NONE) {
    currentStatus=Status.INITIALIZING;
    try {
      instance.init();
      currentStatus=Status.STOPPED;
    }
 catch (    Throwable e) {
      currentStatus=Status.NONE;
      throw new LifecycleException(instance,Status.NONE,Status.STOPPED,e);
    }
  }
}
