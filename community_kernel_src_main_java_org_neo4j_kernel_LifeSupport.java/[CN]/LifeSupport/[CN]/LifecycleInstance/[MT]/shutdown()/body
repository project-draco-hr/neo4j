{
  if (currentStatus == Status.STOPPED) {
    currentStatus=Status.SHUTTING_DOWN;
    try {
      instance.shutdown();
    }
 catch (    Throwable e) {
      throw new LifecycleException(instance,Status.STOPPED,Status.SHUTTING_DOWN,e);
    }
 finally {
      currentStatus=Status.SHUTDOWN;
    }
  }
}
