{
  if (currentStatus == Status.STOPPED) {
    currentStatus=Status.STARTING;
    try {
      instance.start();
      currentStatus=Status.STARTED;
    }
 catch (    Throwable e) {
      currentStatus=Status.STOPPED;
      throw new LifecycleException(instance,Status.STOPPED,Status.STARTED,e);
    }
  }
}
