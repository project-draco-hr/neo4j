{
  client.connect(address).send(TransportTestUtil.acceptedVersions(1,0,0,0)).send(TransportTestUtil.chunk(init("TestClient/1.1",emptyMap()),run("RETURN point({x:13, y:37, crs:'cartesian'}) as p"),pullAll()));
  assertThat(client,eventuallyReceives(new byte[]{0,0,0,1}));
  assertThat(client,TransportTestUtil.eventuallyReceives(msgSuccess(),msgSuccess(map("fields",singletonList("p"))),msgRecord(eqRecord(nullValue())),msgFailure(Status.Request.Invalid,"Point is not yet supported as a return type in Bolt")));
}
