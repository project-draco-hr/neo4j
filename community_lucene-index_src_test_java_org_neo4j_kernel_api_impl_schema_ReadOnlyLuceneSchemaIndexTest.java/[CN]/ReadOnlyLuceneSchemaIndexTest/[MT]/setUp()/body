{
  PartitionedIndexStorage indexStorage=new PartitionedIndexStorage(DirectoryFactory.PERSISTENT,new DefaultFileSystemAbstraction(),testDirectory.directory(),"1");
  Config config=Config.empty();
  IndexSamplingConfig samplingConfig=new IndexSamplingConfig(config);
  luceneSchemaIndex=new ReadOnlyDatabaseSchemaIndex(indexStorage,IndexConfiguration.NON_UNIQUE,samplingConfig,new ReadOnlyIndexPartitionFactory());
}
