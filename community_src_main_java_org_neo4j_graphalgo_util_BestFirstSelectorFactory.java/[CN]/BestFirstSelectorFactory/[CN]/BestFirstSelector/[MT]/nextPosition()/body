{
  while (true) {
    ExpansionSource next=current.next();
    if (next != null) {
      queue.put(next,currentAggregatedValue + calculateValue(next));
    }
 else {
      break;
    }
  }
  Entry<ExpansionSource,Double> entry=queue.pop();
  if (entry != null) {
    current=entry.getEntity();
    currentAggregatedValue=entry.getPriority();
    return current;
  }
  return null;
}
