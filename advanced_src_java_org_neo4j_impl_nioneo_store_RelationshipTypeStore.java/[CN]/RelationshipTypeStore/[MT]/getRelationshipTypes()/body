{
  LinkedList<RelationshipTypeData> typeDataList=new LinkedList<RelationshipTypeData>();
  for (int i=0; ; i++) {
    int blockId=-1;
    try {
      PersistenceWindow window=acquireWindow(i,OperationType.READ);
      try {
        blockId=getRelationshipTypeBlockId(i,window.getBuffer());
      }
  finally {
        releaseWindow(window);
      }
    }
 catch (    IOException e) {
      break;
    }
    if (blockId != Record.RESERVED.intValue()) {
      typeDataList.add(new RelationshipTypeData(i,typeNameStore.getString(blockId)));
    }
  }
  return typeDataList.toArray(new RelationshipTypeData[typeDataList.size()]);
}
