{
  ResourceObject r1=new ResourceObject("R1");
  Thread stressThreads[]=new Thread[100];
  StressThread.go=false;
  for (int i=0; i < 100; i++) {
    stressThreads[i]=new StressThread("Thread" + i,100,9,0.50f,r1);
  }
  for (int i=0; i < 100; i++) {
    stressThreads[i].start();
  }
  StressThread.go=true;
}
