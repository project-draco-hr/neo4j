{
  StringBuilder sb=new StringBuilder();
  sb.append("http");
  if (jettyPort == 443) {
    sb.append("s");
  }
  sb.append("://");
  try {
    sb.append(InetAddress.getLocalHost().getHostName());
  }
 catch (  UnknownHostException e) {
    sb.append("localhost");
  }
  if (jettyPort != 80) {
    sb.append(":");
    sb.append(jettyPort);
  }
  sb.append("/");
  return new URI(sb.toString());
}
