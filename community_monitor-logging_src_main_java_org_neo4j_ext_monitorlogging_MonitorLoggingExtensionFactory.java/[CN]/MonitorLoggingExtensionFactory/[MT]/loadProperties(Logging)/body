{
  Properties props=new Properties();
  try {
    InputStream resource=getClass().getResourceAsStream(filename);
    if (resource != null) {
      props.load(resource);
    }
  }
 catch (  Exception e) {
    logging.getMessagesLog(getClass()).warn("Unable to read the log monitors property file: " + filename,e);
  }
  return props;
}
