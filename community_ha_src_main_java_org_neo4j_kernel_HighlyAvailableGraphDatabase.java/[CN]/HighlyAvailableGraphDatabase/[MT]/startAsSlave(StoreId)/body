{
  msgLog.logMessage("Starting[" + machineId + "] as slave",true);
  this.localGraph=new EmbeddedGraphDbImpl(storeDir,storeId,config,this,new SlaveLockManagerFactory(broker,this),new SlaveIdGeneratorFactory(broker,this),new SlaveRelationshipTypeCreator(broker,this),new SlaveTxIdGeneratorFactory(broker,this),new SlaveTxRollbackHook(broker,this),new ZooKeeperLastCommittedTxIdSetter(broker),CommonFactories.defaultFileSystemAbstraction());
  instantiateAutoUpdatePullerIfConfigSaysSo();
  msgLog.logMessage("Started as slave",true);
}
