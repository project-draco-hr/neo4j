{
  ByteBuf buffer=Unpooled.buffer();
  ReplicatedContent message=new ReplicatedIdAllocationRequest(new CoreMember(new AdvertisedSocketAddress("host_a:1"),new AdvertisedSocketAddress("host_a:2")),IdType.PROPERTY,100,200);
  marshal.serialize(message,buffer);
  assertThat(marshal.deserialize(buffer),equalTo(message));
}
