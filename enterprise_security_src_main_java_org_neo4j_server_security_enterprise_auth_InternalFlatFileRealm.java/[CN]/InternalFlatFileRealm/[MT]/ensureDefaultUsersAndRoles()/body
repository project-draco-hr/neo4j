{
  if (authenticationEnabled || authorizationEnabled) {
    if (numberOfRoles() == 0) {
      for (      String role : roles.keySet()) {
        newRole(role);
      }
    }
    if (numberOfUsers() == 0) {
      newUser("neo4j","neo4j",true);
      addUserToRole("neo4j",PredefinedRolesBuilder.ADMIN);
    }
  }
}
