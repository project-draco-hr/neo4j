{
  Node node1=createNode();
  Node node2=createNode();
  Relationship rel;
  rel=createRelationship(node1,node2,TestTypes.TEST);
  long id=rel.getId();
  Transaction tx=graphdb.beginTx();
  try {
    rel.delete();
    tx.success();
  }
  finally {
    tx.finish();
  }
  graphdb.getRelationshipById(id);
}
