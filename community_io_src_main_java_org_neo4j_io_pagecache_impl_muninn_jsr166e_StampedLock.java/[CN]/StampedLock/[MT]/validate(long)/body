{
  if (MH_loadFence == null) {
    return (stamp & SBITS) == (U.getLongVolatile(this,STATE) & SBITS);
  }
 else {
    try {
      MH_loadFence.invokeExact(U);
      return (stamp & SBITS) == (state & SBITS);
    }
 catch (    Throwable throwable) {
      throw new AssertionError(throwable);
    }
  }
}
