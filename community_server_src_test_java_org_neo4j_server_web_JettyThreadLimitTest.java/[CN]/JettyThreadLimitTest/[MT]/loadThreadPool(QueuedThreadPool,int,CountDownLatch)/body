{
  final CountDownLatch endLatch=new CountDownLatch(1);
  for (int i=0; i < tasksToSubmit; i++) {
    threadPool.execute(new Runnable(){
      @Override public void run(){
        startLatch.countDown();
        try {
          endLatch.await();
        }
 catch (        InterruptedException e) {
          e.printStackTrace();
        }
      }
    }
);
  }
  return endLatch;
}
