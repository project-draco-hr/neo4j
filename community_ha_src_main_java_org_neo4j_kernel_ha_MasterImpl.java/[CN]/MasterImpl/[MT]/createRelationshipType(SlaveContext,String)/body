{
  Integer id=graphDbConfig.getRelationshipTypeHolder().getIdFor(name);
  if (id != null) {
    return packResponse(context,id);
  }
  id=graphDbConfig.getRelationshipTypeCreator().getOrCreate(graphDbConfig.getTxModule().getTxManager(),graphDbConfig.getIdGeneratorModule().getIdGenerator(),graphDbConfig.getPersistenceModule().getPersistenceManager(),graphDbConfig.getRelationshipTypeHolder(),name);
  return packResponse(context,id);
}
