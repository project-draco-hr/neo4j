{
  StringBuilder builder=new StringBuilder();
  String host=getHost(defaultHost);
  if (host != null) {
    builder.append(host);
  }
  if (getPort() != 0) {
    builder.append(":");
    builder.append(getPort());
    if (isRange()) {
      builder.append("-").append(getPorts()[1]);
    }
  }
  return builder.toString();
}
