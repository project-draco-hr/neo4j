{
  VirtualMachine win1=vm(Neo4jVM.WIN_1);
  VirtualMachine win2=vm(Neo4jVM.WIN_2);
  VirtualMachine win3=vm(Neo4jVM.WIN_3);
  Object[][] ps=new Object[][]{{new EnterpriseDriver[]{new WindowsEnterpriseDriver(win1,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER),new WindowsEnterpriseDriver(win2,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER),new WindowsEnterpriseDriver(win3,SharedConstants.WINDOWS_ENTERPRISE_INSTALLER,SharedConstants.WINDOWS_COORDINATOR_INSTALLER)}}};
  return Arrays.asList(ps);
}
