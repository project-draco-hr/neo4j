{
  return new GraphDatabaseFactory().setKernelExtensions(kernelExtensions).newEmbeddedDatabaseBuilder(tempStore.getAbsolutePath()).setConfig(GraphDatabaseSettings.keep_logical_logs,Settings.TRUE).setConfig(GraphDatabaseSettings.allow_store_upgrade,config.get(GraphDatabaseSettings.allow_store_upgrade).toString()).setConfig(InternalAbstractGraphDatabase.Configuration.log_configuration_file,"neo4j-backup-logback.xml").newGraphDatabase();
}
