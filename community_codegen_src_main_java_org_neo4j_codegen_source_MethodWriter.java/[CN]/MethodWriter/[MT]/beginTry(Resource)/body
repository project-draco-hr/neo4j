{
  indent().append("try");
  if (resources.length > 0) {
    String sep=" ( ";
    for (    Resource resource : resources) {
      append(sep).append(resource.type().name()).append(" ").append(resource.name()).append(" = ");
      resource.producer().accept(this);
      sep="; ";
    }
    append(" )");
  }
  append("\n");
  indent().append("{\n");
  level++;
}
