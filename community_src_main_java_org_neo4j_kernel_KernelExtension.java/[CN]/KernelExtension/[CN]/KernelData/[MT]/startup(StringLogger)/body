{
  for (  KernelExtension extension : Service.load(KernelExtension.class)) {
    try {
      extension.load(this);
      loaded(extension);
      msgLog.logMessage("Extension " + extension + " loaded ok");
    }
 catch (    Exception ex) {
      msgLog.logMessage("Failed to load extension " + extension,ex);
    }
  }
}
