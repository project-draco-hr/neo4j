{
  File seedDir;
  try {
    seedDir=directory.cleanDirectory("seed");
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  GraphDatabaseService db=null;
  try {
    db=new EnterpriseGraphDatabaseFactory().newEmbeddedDatabase(seedDir);
    createSomeData(db);
  }
  finally {
    if (db != null) {
      db.shutdown();
    }
  }
  deleteAllLogsOn(seedDir);
  return seedDir;
}
