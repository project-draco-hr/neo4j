{
  Response response=http.POST("/db/data/transaction/commit",quotedJson("{ 'statements': [ { 'statement': 'USING PERIODIC COMMIT CREATE (n {id: 23}) RETURN n' }, { 'statement': 'RETURN 1' } ] }"));
  assertThat(response.status(),equalTo(200));
  assertThat(response,hasErrors(Status.Statement.InvalidSemantics));
}
