{
  ServerTestUtils.withCSVFile(1,new ServerTestUtils.BlockWithCSVFileURL(){
    @Override public void execute(    String url){
      Response response=http.POST("/db/data/transaction/commit",quotedJson("{ 'statements': [ { 'statement': 'CREATE ()' }, " + "{ 'statement': 'USING PERIODIC COMMIT LOAD CSV FROM \\\"" + url + "\\\" AS line "+ "CREATE ()' } ] }"));
      assertThat(response,hasErrors(Status.Statement.InvalidSemantics));
    }
  }
);
}
