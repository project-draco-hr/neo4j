{
  try (Transaction ignored=db.beginTx()){
    Node nodeByUniqueStringProperty=db.findNode(Label.label(LABEL),getUniqueStringProperty(),getLastNodePropertyValue(totalNodes) + "");
    Node nodeByStringProperty=db.findNode(Label.label(LABEL),getStringProperty(),(getLastNodePropertyValue(totalNodes) - 1) + "");
    assertNotNull("Should find last inserted node",nodeByStringProperty);
    assertEquals("Both nodes should be the same last inserted node",nodeByStringProperty,nodeByUniqueStringProperty);
    Node nodeByUniqueLongProperty=db.findNode(Label.label(LABEL),getUniqueLongProperty(),getLastNodePropertyValue(totalNodes));
    Node nodeByLongProperty=db.findNode(Label.label(LABEL),getLongProperty(),(getLastNodePropertyValue(totalNodes) - 1));
    assertNotNull("Should find last inserted node",nodeByLongProperty);
    assertEquals("Both nodes should be the same last inserted node",nodeByLongProperty,nodeByUniqueLongProperty);
  }
 }
