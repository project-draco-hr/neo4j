  config=ServerConfigFactory.loadConfig(null,installation.getServerConfigurationsFile(),getDatabaseConfigurationFile(),FormattedLog.toOutputStream(System.out),pair(Configurator.AUTH_STORE_FILE_KEY,new File(databaseDirectory,"./dbms/auth").getAbsolutePath()),pair(Configurator.DATABASE_LOCATION_PROPERTY_KEY,databaseDirectory.getAbsolutePath()));
