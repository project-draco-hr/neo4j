{
  if (error.status().code().classification().refersToLog()) {
    userLog.error("Client triggered an unexpected error [%s]: %s. " + "See debug.log for more details, reference %s.",error.status(),error.message(),error.reference());
    debugLog.error(format("Client triggered an unexpected error [%s]: %s, reference %s.",error.status(),error.message(),error.reference()),error.cause());
  }
}
