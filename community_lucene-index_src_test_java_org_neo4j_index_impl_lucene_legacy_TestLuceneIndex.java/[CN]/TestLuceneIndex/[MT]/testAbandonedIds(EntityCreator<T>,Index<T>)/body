{
  T a=creator.create();
  T b=creator.create();
  T c=creator.create();
  String key="name";
  String value="value";
  index.add(a,key,value);
  index.add(b,key,value);
  index.add(c,key,value);
  restartTx();
  creator.delete(b);
  restartTx();
  IteratorUtil.count((Iterator<Node>)index.get(key,value));
  rollbackTx();
  beginTx();
  IteratorUtil.count((Iterator<Node>)index.get(key,value));
  index.add(c,"something","whatever");
  restartTx();
  IteratorUtil.count((Iterator<Node>)index.get(key,value));
}
