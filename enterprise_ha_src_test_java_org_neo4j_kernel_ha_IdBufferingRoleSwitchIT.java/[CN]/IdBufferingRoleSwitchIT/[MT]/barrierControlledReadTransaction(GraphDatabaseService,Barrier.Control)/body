{
  return new WorkerCommand<Void,Void>(){
    @Override public Void doWork(    Void state) throws Exception {
      try (Transaction tx=slave.beginTx()){
        barrier.reached();
        tx.success();
      }
 catch (      Exception e) {
      }
 finally {
        barrier.release();
      }
      return null;
    }
  }
;
}
