{
  return new Function<RelationshipRecord,Check<RelationshipRecord,ConsistencyReport.RelationshipConsistencyReport>>(){
    @Override public Check<RelationshipRecord,ConsistencyReport.RelationshipConsistencyReport> apply(    RelationshipRecord relationship){
      int[] propertyKeys=relationships.get(relationship.getType());
      if (propertyKeys != null) {
        PrimitiveIntSet keys=Primitive.intSet(propertyKeys.length);
        for (        int key : propertyKeys) {
          keys.add(key);
        }
        return new RealCheck<>(relationship,ConsistencyReport.RelationshipConsistencyReport.class,reporter,RecordType.RELATIONSHIP,keys);
      }
      return noCheck();
    }
  }
;
}
