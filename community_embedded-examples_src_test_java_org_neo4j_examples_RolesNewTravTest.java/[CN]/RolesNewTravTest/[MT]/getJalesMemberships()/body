{
  Transaction tx=graphDb.beginTx();
  try {
    System.out.println("Jale's memberships:");
    Node jale=getUserByName("Jale");
    TraversalDescription td=Traversal.description().depthFirst().relationships(RoleRels.MEMBER_OF,Direction.OUTGOING).relationships(RoleRels.PART_OF,Direction.OUTGOING).evaluator(Evaluators.excludeStartPosition());
    for (    Path path : td.traverse(jale)) {
      Node membership=path.endNode();
      System.out.println(membership.getProperty(NAME) + " " + (path.length() - 1));
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
}
