{
  Transaction tx=graphDb.beginTx();
  try {
    System.out.println("All groups:");
    Node referenceNode=graphDb.getReferenceNode();
    TraversalDescription td=Traversal.description().breadthFirst().relationships(RoleRels.ROOT,Direction.INCOMING).relationships(RoleRels.PART_OF,Direction.INCOMING).evaluator(Evaluators.excludeStartPosition());
    for (    Node group : td.traverse(referenceNode).nodes()) {
      System.out.println(group.getProperty(NAME));
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
}
