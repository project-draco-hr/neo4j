{
  String[] args=new String[]{"-host",host,"-port",String.valueOf(port),"-to","/var/backup/graph"};
  newBackupTool().run(args);
  verify(backupService).doIncrementalBackupOrFallbackToFull(eq(host),eq(port),eq(new File("/var/backup/graph")),eq(ConsistencyCheck.FULL),any(Config.class),eq(BackupClient.BIG_READ_TIMEOUT),eq(false));
}
