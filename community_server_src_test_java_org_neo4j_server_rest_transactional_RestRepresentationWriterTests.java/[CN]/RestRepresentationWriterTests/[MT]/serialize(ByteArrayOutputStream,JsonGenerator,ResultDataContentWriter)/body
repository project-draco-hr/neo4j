{
  json.writeStartObject();
  resultDataContentWriter.write(json,asList("the column"),new MapRow(map("the column",map("one",map("two",asList(true,map("three",42)))))));
  json.writeEndObject();
  json.flush();
  json.close();
  String jsonAsString=out.toString();
  return jsonNode(jsonAsString).get("rest");
}
