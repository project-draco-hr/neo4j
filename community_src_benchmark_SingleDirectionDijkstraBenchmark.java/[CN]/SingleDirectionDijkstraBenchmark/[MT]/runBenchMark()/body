{
  Transaction tx=neo.beginTx();
  startNode=graph.getRandomNode(null);
  endNode=graph.getRandomNode(startNode);
  new SingleDirectionDijkstra<Double>(0.0,startNode,endNode,new CostEvaluator<Double>(){
    public Double getCost(    Relationship relationship,    boolean backwards){
      return 1.0;
    }
  }
,new DoubleAdder(),new DoubleComparator(),Direction.BOTH,MyRelTypes.R1).getCost();
  tx.finish();
}
