{
  try {
    ScriptEngine engine=new GremlinScriptEngine();
    TransactionalGraph graph=getGremlinWrappedGraph();
    engine.getBindings(ScriptContext.ENGINE_SCOPE).put("$_g",graph);
    try {
      engine.getBindings(ScriptContext.ENGINE_SCOPE).put("$_",graph.getVertex(0l));
    }
 catch (    Exception e) {
    }
    return engine;
  }
 catch (  Throwable e) {
    e.printStackTrace();
    return null;
  }
}
