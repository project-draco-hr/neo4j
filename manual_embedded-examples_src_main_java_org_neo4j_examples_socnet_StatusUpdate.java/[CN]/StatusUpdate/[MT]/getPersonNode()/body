{
  TraversalDescription traversalDescription=underlyingNode.getGraphDatabase().traversalDescription().depthFirst().relationships(NEXT,Direction.INCOMING).relationships(STATUS,Direction.INCOMING).evaluator(Evaluators.includeWhereLastRelationshipTypeIs(STATUS));
  Traverser traverser=traversalDescription.traverse(getUnderlyingNode());
  return singleOrNull(traverser.iterator()).endNode();
}
