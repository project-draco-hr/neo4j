{
  Transaction tx=graphDatabaseService.beginTx();
  try {
    for (int i=0; i < 10; i++) {
      graphDatabaseService.getReferenceNode().setProperty("key" + i,value);
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
  tx=graphDatabaseService.beginTx();
  try {
    for (int i=0; i < 10; i++) {
      graphDatabaseService.getReferenceNode().removeProperty("key" + i);
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
}
