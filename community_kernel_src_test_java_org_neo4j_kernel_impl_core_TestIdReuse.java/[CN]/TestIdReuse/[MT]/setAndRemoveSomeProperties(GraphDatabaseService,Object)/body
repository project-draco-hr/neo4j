{
  Node commonNode;
  try (Transaction transaction=graphDatabaseService.beginTx()){
    commonNode=graphDatabaseService.createNode();
    for (int i=0; i < 10; i++) {
      commonNode.setProperty("key" + i,value);
    }
    transaction.success();
  }
   try (Transaction transaction=graphDatabaseService.beginTx()){
    for (int i=0; i < 10; i++) {
      commonNode.removeProperty("key" + i);
    }
    transaction.success();
  }
 }
