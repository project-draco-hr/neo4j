{
  Transaction tx=node.getGraphDatabase().beginTx();
  try {
    for (int i=0; i < 10; i++) {
      node.setProperty("key" + i,value);
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
  tx=node.getGraphDatabase().beginTx();
  try {
    for (int i=0; i < 10; i++) {
      node.removeProperty("key" + i);
    }
    tx.success();
  }
  finally {
    tx.finish();
  }
}
