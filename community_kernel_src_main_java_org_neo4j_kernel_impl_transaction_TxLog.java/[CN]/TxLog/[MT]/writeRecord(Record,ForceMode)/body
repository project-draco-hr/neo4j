{
switch (record.getType()) {
case TX_START:
    txStart(record.getGlobalId());
  break;
case BRANCH_ADD:
addBranch(record.getGlobalId(),record.getBranchId());
break;
case MARK_COMMIT:
markAsCommitting(record.getGlobalId(),forceMode);
break;
default :
throw new IOException("Illegal record type[" + record.getType() + "]");
}
}
