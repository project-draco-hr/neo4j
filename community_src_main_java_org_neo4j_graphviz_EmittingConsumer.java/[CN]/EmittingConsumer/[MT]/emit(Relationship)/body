{
  long start=relation.getStartNode().getId();
  long end=relation.getEndNode().getId();
  Emitter emitter=emitRelationship(relation.getId(),relation.getType(),start,end);
  for (  String key : relation.getPropertyKeys()) {
    emitter.emitProperty(SourceType.RELATIONSHIP,key,relation.getProperty(key));
  }
  emitter.done();
}
