{
  into.position(0);
  into.limit(filePageSize);
  long position=pageIdToPosition(pageId);
  int readBytes=-1;
  if (position <= channel.size()) {
    readBytes=channel.read(into,position);
  }
  if (readBytes < filePageSize) {
    for (int i=Math.max(readBytes,0); i < filePageSize; i++) {
      into.put(i,(byte)0);
    }
  }
}
