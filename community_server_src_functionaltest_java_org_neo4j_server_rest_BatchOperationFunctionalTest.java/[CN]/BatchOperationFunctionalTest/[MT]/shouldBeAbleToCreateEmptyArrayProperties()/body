{
  int originalNodeCount=helper.getNumberOfNodes();
  JaxRsResponse response=RestRequest.req().post(functionalTestHelper.dataUri() + "batch",new PrettyJSON().array().object().key("method").value("POST").key("to").value("/node").key("body").object().key("age").array().endArray().endObject().endObject().endArray().toString());
  assertEquals(200,response.getStatus());
  assertEquals(originalNodeCount + 1,helper.getNumberOfNodes());
}
