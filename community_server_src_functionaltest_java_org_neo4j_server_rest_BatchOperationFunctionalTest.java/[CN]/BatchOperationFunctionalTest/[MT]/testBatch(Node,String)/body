{
  String jsonString;
  try {
    jsonString=new PrettyJSON().array().object().key("method").value("GET").key("to").value("/node/" + anode.getId() + "/properties").endObject().endArray().toString();
    String entity=gen.get().expectedStatus(200).payload(jsonString).post(batchUri()).entity();
    assertTrue(entity.contains(asian));
  }
 catch (  JSONException e) {
    e.printStackTrace();
  }
}
