{
  Benchmark bench=new SimpleBenchmark(){
    @Override public void runOperation() throws Exception {
    }
  }
;
  BenchmarkResult result=bench.run();
  assertThat(result,not(nullValue()));
  assertThat(result.getMetrics().size(),is(1));
  assertThat(result.getMetrics().get(0).getValue(),not(nullValue()));
}
