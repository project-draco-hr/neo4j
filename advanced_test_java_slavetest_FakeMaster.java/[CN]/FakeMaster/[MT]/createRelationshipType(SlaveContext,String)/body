{
  Integer id=getConfig().getRelationshipTypeHolder().getIdFor(name);
  if (id != null) {
    return new Response<Integer>(id,new TransactionStreams());
  }
  Config config=getConfig();
  id=config.getRelationshipTypeCreator().getOrCreate(txManager,config.getIdGeneratorModule().getIdGenerator(),config.getPersistenceModule().getPersistenceManager(),name);
  return packResponse(context,id,ALL);
}
