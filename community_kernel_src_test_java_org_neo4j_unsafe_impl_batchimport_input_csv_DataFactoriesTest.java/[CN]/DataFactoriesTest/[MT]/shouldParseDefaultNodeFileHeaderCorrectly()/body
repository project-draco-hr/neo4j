{
  CharSeeker seeker=new BufferedCharSeeker(new StringReader("ID:ID,label-one:label,also-labels:LABEL,name,age:long"));
  IdType idType=IdType.STRING;
  Header header=DataFactories.defaultFormatNodeFileHeader().create(seeker,COMMAS,idType.extractor());
  Extractors extractors=new Extractors(',');
  assertArrayEquals(array(entry("ID",Type.ID,idType.extractor()),entry("label-one",Type.LABEL,extractors.stringArray()),entry("also-labels",Type.LABEL,extractors.stringArray()),entry("name",Type.PROPERTY,Extractors.STRING),entry("age",Type.PROPERTY,Extractors.LONG)),header.entries());
}
