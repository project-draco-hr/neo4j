{
  CharSeeker dataSeeker=mock(CharSeeker.class);
  Header.Factory headerFactory=defaultFormatNodeFileHeader(new StringReader("id:ID\tname:String\tbirth_date:long"));
  Extractors extractors=new Extractors(';');
  Header header=headerFactory.create(dataSeeker,TABS,IdType.ACTUAL);
  assertArrayEquals(array(entry("id",Type.ID,extractors.long_()),entry("name",Type.PROPERTY,extractors.string()),entry("birth_date",Type.PROPERTY,extractors.long_())),header.entries());
  verifyZeroInteractions(dataSeeker);
  dataSeeker.close();
}
