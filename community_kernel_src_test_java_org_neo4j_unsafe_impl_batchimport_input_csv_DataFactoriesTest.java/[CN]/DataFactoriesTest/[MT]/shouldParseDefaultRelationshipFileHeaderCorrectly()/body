{
  CharSeeker seeker=new BufferedCharSeeker(new StringReader("node one\tnode two\ttype\tdate:long\tmore:long[]"));
  IdType idType=IdType.ACTUAL;
  Extractors extractors=new Extractors('\t');
  Header header=DataFactories.defaultFormatRelationshipFileHeader().create(seeker,TABS,idType);
  assertArrayEquals(array(entry("node one",Type.START_NODE,idType.extractor(extractors)),entry("node two",Type.END_NODE,idType.extractor(extractors)),entry("type",Type.RELATIONSHIP_TYPE,extractors.string()),entry("date",Type.PROPERTY,extractors.long_()),entry("more",Type.PROPERTY,extractors.longArray())),header.entries());
  seeker.close();
}
