{
  addUpdate(index,1,"1");
  addUpdate(index,2,"2");
  updatePopulator(index,asList(change(1,"1","1a"),change(2,"2","2a")),indexStoreView);
  addUpdate(index,3,"3");
  addUpdate(index,4,"4");
  updatePopulator(index,asList(change(1,"1a","1b"),change(4,"4","4a")),indexStoreView);
  assertIndexedValues(no("1"),no("1a"),hit("1b",1),no("2"),hit("2a",2),hit("3",3),no("4"),hit("4a",4));
}
