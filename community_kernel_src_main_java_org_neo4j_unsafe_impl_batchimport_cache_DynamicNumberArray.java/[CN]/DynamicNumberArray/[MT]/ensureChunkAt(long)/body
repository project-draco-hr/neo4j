{
  if (index >= length()) {
synchronized (this) {
      if (index >= length()) {
        NumberArray[] newChunks=Arrays.copyOf(chunks,chunkIndex(index) + 1);
        for (int i=chunks.length; i < newChunks.length; i++) {
          newChunks[i]=addChunk(chunkSize);
        }
        chunks=newChunks;
      }
    }
  }
  return chunkAt(index);
}
