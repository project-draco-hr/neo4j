{
  newUpgrader(alwaysAllowed(),new StoreMigrator(new SilentMigrationProgressMonitor()),new DatabaseFiles(fs.get())).attemptUpgrade(new File(workingDirectory,NeoStore.DEFAULT_NAME));
  verifyFilesHaveSameContent(fs.get(),MigrationTestUtils.findOldFormatStoreDirectory(),new File(workingDirectory,"upgrade_backup"));
}
