{
  BatchInserter graphDb=newBatchInserter();
  long startNode=graphDb.createNode(properties);
  assertProperties(graphDb.getGraphDbService().getNodeById(startNode));
  graphDb.setNodeProperties(startNode,properties);
  assertProperties(graphDb.getGraphDbService().getNodeById(startNode));
  graphDb.setNodeProperties(startNode,properties);
  assertProperties(graphDb.getGraphDbService().getNodeById(startNode));
  graphDb.shutdown();
}
