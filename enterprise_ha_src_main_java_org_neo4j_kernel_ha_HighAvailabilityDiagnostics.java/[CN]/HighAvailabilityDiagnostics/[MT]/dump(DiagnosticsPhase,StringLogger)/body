{
  StringBuilder builder=new StringBuilder();
  builder.append("High Availability diagnostics\n").append("Member state:").append(memberStateMachine.getCurrentState().name()).append("\n").append("State machines:\n");
  for (  StateMachine stateMachine : stateMachines.getStateMachines()) {
    builder.append("   ").append(stateMachine.getMessageType().getSimpleName()).append(":").append(stateMachine.getState().toString()).append("\n");
  }
  builder.append("Current timeouts:\n");
  for (  Map.Entry<Object,Timeouts.Timeout> objectTimeoutEntry : stateMachines.getTimeouts().getTimeouts().entrySet()) {
    builder.append(objectTimeoutEntry.getKey().toString()).append(":").append(objectTimeoutEntry.getValue().getTimeoutMessage().toString());
  }
  log.logMessage(builder.toString());
}
