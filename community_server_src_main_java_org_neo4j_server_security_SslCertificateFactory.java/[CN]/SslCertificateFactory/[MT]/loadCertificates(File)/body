{
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(certFile);
    Collection<? extends Certificate> certificates=CertificateFactory.getInstance(CERTIFICATE_TYPE).generateCertificates(fis);
    return certificates.toArray(new Certificate[]{});
  }
  finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
}
