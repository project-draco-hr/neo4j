{
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(certFile);
    return CertificateFactory.getInstance(CERTIFICATE_TYPE).generateCertificate(fis);
  }
  finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
}
