{
  if (type == null)   throw new IllegalStateException();
switch (type) {
case BOOL:
case BYTE:
case SHORT:
case CHAR:
case INT:
{
      return header;
    }
case LONG:
{
    if ((header & 0x1L) > 0) {
      return header >>> 1;
    }
 else {
      remainingBlocksToRead--;
      return cursor.getLong();
    }
  }
default :
{
  throw new IllegalStateException("Not an integral type:" + type);
}
}
}
