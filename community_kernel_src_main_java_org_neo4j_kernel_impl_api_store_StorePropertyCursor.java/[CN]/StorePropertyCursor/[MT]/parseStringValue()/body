{
  assertReadingStatus();
  assertOfOneOfTypes(SHORT_STRING,STRING);
  try {
    readPropertyData();
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
  if (type == SHORT_STRING) {
    return LongerShortString.decode(block.toBits());
  }
 else {
    buffer.flip();
    return UTF8.decode(buffer.array(),0,buffer.limit());
  }
}
