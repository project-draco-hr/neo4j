{
  assertReadingStatus();
  assertOfOneOfTypes(SHORT_ARRAY,ARRAY);
  try {
    readPropertyData();
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
  if (type == SHORT_ARRAY) {
    return ShortArray.decode(block.toBits());
  }
 else {
    return getRightArray();
  }
}
