{
  final GraphDatabaseService db=new TestGraphDatabaseFactory().setFileSystem(fs.get()).newImpermanentDatabaseBuilder(storeDirectory.absolutePath()).newGraphDatabase();
  try (Transaction tx=db.beginTx()){
    db.createNode(label("FOO"));
    db.createNode(label("BAR"));
    tx.success();
  }
   fs.snapshot(shutdownDb(db));
}
