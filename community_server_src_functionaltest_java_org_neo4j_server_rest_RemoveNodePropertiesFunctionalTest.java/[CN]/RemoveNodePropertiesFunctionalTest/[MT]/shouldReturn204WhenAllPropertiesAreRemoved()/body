{
  long nodeId=helper.createNode();
  Map<String,Object> map=new HashMap<String,Object>();
  map.put("jim","tobias");
  helper.setNodeProperties(nodeId,map);
  ClientResponse response=removeNodePropertiesOnServer(nodeId);
  assertEquals(204,response.getStatus());
  doc.builder("Delete all properties from node").status(Response.Status.NO_CONTENT).delete(functionalTestHelper.nodePropertiesUri(nodeId));
}
