{
  Long result=null;
  while (result == null) {
    if (!searchResult.documents.hasNext()) {
      endReached();
      break;
    }
    Document doc=searchResult.documents.next();
    long id=Long.parseLong(doc.getField(LuceneIndex.KEY_DOC_ID).stringValue());
    if (exclude == null || !exclude.contains(id)) {
      result=id;
    }
  }
  return result;
}
