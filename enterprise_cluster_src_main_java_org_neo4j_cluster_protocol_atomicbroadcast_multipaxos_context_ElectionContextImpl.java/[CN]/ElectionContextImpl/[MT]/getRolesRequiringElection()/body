{
  return filter(new Predicate<String>(){
    @Override public boolean test(    String role){
      return clusterContext.getConfiguration().getElected(role) == null;
    }
  }
,map(new Function<ElectionRole,String>(){
    @Override public String apply(    ElectionRole role){
      return role.getName();
    }
  }
,roles));
}
