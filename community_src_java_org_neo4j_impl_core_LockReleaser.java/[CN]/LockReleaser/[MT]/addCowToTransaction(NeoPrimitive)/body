{
  Thread currentThread=Thread.currentThread();
  List<NeoPrimitive> cowElements=cowMap.get(currentThread);
  if (cowElements != null) {
    cowElements.add(element);
  }
 else {
    cowElements=new LinkedList<NeoPrimitive>();
    cowElements.add(element);
    cowMap.put(currentThread,cowElements);
  }
}
