{
  if (primitive instanceof NodeImpl) {
    ArrayMap<Integer,CowNodeElement> cowElements=cowMap.get(primitive.getCowTxId()).nodes;
    CowNodeElement element=cowElements.get(primitive.id);
    if (element != null) {
      return element.propertyAddMap;
    }
  }
 else   if (primitive instanceof RelationshipImpl) {
    ArrayMap<Integer,CowRelElement> cowElements=cowMap.get(primitive.getCowTxId()).relationships;
    CowRelElement element=cowElements.get(primitive.id);
    if (element != null) {
      return element.propertyAddMap;
    }
  }
  return null;
}
