{
  return new PrefetchingIterator<Document>(){
    private int docId;
    @Override protected Document fetchNextOrNull(){
      Document document=null;
      while (document == null && isPossibleDocId(docId)) {
        if (!deleted(docId)) {
          document=getDocument(docId);
        }
        docId++;
      }
      return document;
    }
  }
;
}
