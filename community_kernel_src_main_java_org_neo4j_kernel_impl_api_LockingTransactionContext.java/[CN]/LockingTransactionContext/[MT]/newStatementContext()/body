{
  StatementContextParts parts=delegate.newStatementContext();
  LockingStatementContext lockingContext=new LockingStatementContext(parts.entityWriteOperations(),parts.schemaReadOperations(),parts.schemaWriteOperations(),parts.schemaStateOperations(),lockHolder);
  parts.replace(null,null,null,lockingContext,lockingContext,lockingContext,lockingContext,null);
  return parts;
}
