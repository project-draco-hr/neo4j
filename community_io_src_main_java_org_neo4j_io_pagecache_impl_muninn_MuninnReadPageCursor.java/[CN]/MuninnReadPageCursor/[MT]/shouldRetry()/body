{
  boolean needsRetry=optimisticLock && !page.validate(lockStamp);
  if (needsRetry) {
    setOffset(0);
    optimisticLock=false;
    lockStamp=page.readLock();
    if (!page.isBoundTo(pagedFile.swapper,currentPageId)) {
      page.unlockRead(lockStamp);
      page=null;
      pin(currentPageId,false);
    }
  }
  return needsRetry;
}
