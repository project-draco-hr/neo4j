{
  FileUtils.deleteDirectory(BASE_ZOO_KEEPER_DATA_DIR);
  zooKeeperCluster=new LocalZooKeeperCluster(ZOO_KEEPER_CLUSTER_SIZE,LocalZooKeeperCluster.defaultDataDirectoryPolicy(BASE_ZOO_KEEPER_DATA_DIR),LocalZooKeeperCluster.defaultPortPolicy(2181),LocalZooKeeperCluster.defaultPortPolicy(2888),LocalZooKeeperCluster.defaultPortPolicy(3888));
}
