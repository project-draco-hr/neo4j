{
  testCallFail(subject,"CALL dbms.createUser('Craig', 'foo', false)",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(subject,"CALL dbms.createUser('Craig', '', false)",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(subject,"CALL dbms.createUser('', 'foo', false)",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
}
