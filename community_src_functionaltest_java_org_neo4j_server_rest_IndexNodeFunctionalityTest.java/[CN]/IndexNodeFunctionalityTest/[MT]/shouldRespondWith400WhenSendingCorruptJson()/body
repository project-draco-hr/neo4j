{
  long nodeId=helper.createNode();
  String key="key";
  String value="value";
  ClientResponse response=Client.create().resource(functionalTestHelper.indexUri() + "/node/" + key+ "/"+ value).type(MediaType.APPLICATION_JSON).entity(functionalTestHelper.nodeUri(nodeId)).post(ClientResponse.class);
  assertEquals(400,response.getStatus());
}
