{
  Args arguments=Args.parse(args);
  String storeDir=arguments.orphans().get(0);
  GraphDatabaseAPI db=(GraphDatabaseAPI)new HighlyAvailableGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(storeDir).setConfig(arguments.asMap()).newGraphDatabase();
  LegacyDatabaseImpl legacyDb=new LegacyDatabaseImpl(storeDir,db);
  rmiLocation(parseInt(arguments.orphans().get(1))).bind(legacyDb);
}
