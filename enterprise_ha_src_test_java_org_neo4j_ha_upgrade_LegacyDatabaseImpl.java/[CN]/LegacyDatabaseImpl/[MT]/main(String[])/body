{
  Args arguments=new Args(args);
  File storeDir=new File(arguments.orphans().get(0));
  GraphDatabaseService db=(GraphDatabaseService)LegacyDatabaseImpl.class.forName("org.neo4j.kernel.HighlyAvailableGraphDatabase").getConstructor(String.class,Map.class).newInstance(storeDir.getAbsolutePath(),arguments.asMap());
  LegacyDatabaseImpl legacyDb=new LegacyDatabaseImpl(storeDir.getAbsolutePath(),db);
  rmiLocation(parseInt(arguments.orphans().get(1))).bind(legacyDb);
}
