{
  if (value instanceof Node) {
    Node node=(Node)value;
    Iterable<Label> labels=node.getLabels();
    boolean match=true;
    int nbrOfLabels=0;
    for (    Label l : labels) {
      match&=labelNames.contains(l.name());
      ++nbrOfLabels;
    }
    match&=nbrOfLabels == labelNames.size();
    match&=propertyMatcher.matches(node.getAllProperties());
    return match;
  }
  return false;
}
