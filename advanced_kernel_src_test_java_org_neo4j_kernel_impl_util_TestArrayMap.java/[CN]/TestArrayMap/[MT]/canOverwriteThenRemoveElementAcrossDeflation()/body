{
  ArrayMap<String,Integer> map=new ArrayMap<String,Integer>(3,false,true);
  map.put("key1",1);
  map.put("key2",2);
  map.put("key3",3);
  map.put("key4",4);
  map.put("key5",5);
  map.put("key1",6);
  map.remove("key1");
  assertNull("removed element still found",map.get("key1"));
  map.remove("key2");
  assertNull("removed element still found",map.get("key1"));
  map.remove("key3");
  assertNull("removed element still found",map.get("key1"));
  map.remove("key4");
  assertNull("removed element still found",map.get("key1"));
}
