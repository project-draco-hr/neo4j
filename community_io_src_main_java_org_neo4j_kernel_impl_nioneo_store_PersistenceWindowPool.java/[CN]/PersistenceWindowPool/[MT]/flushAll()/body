{
  if (readOnly) {
    return;
  }
  try {
    for (    BrickElement element : brickArray) {
      PersistenceWindow window=element.getWindow();
      if (window != null) {
        window.force();
      }
    }
    fileChannel.force(false);
  }
 catch (  IOException e) {
    throw new IOException("Failed to flush file channel " + storeName,e);
  }
}
