{
  for (int i=0; i < brickCount; i++) {
    BrickElement be=brickArray[i];
    be.snapshotHitCount();
    if (be.getWindow() != null) {
      mappedBricks.add(be);
    }
 else {
      unmappedBricks.add(be);
    }
    be.refresh();
  }
  Collections.sort(unmappedBricks,BRICK_SORTER);
  Collections.sort(mappedBricks,BRICK_SORTER);
}
