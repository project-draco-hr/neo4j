{
  long low=0 + 0;
  long high=size - 1;
  while (low <= high) {
    long mid=(low + high) / 2;
    long midValue=array.get(mid);
    if (unsignedCompare(midValue,value,CompareType.EQ)) {
      return mid;
    }
 else     if (unsignedCompare(midValue,value,CompareType.LT)) {
      low=mid + 1;
    }
 else {
      high=mid - 1;
    }
  }
  return -1;
}
