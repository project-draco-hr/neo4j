{
  testCallEmpty(db,adminSubject,"CALL dbms.createUser('Henrik', 'bar', false)",null);
  testCallEmpty(db,adminSubject,"CALL dbms.deleteUser('Henrik')",null);
  try {
    testCallEmpty(db,adminSubject,"CALL dbms.addUserToRole('Henrik', '" + PredefinedRolesBuilder.PUBLISHER + "')",null);
    fail("Expected exception to be thrown");
  }
 catch (  QueryExecutionException e) {
    assertTrue("Exception should contain 'User Henrik does not exist'",e.getMessage().contains("User Henrik does not exist"));
  }
}
