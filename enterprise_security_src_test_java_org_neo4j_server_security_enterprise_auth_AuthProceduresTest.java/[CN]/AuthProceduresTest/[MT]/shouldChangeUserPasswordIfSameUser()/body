{
  testCallEmpty(readSubject,"CALL dbms.changeUserPassword( 'readSubject', '321' )");
  assertEquals(AuthenticationResult.FAILURE,neo.login("readSubject","123").getAuthenticationResult());
  assertEquals(AuthenticationResult.SUCCESS,neo.login("readSubject","321").getAuthenticationResult());
  testCallEmpty(adminSubject,"CALL dbms.changeUserPassword( 'adminSubject', 'cba' )");
  assertEquals(AuthenticationResult.FAILURE,neo.login("adminSubject","abc").getAuthenticationResult());
  assertEquals(AuthenticationResult.SUCCESS,neo.login("adminSubject","cba").getAuthenticationResult());
}
