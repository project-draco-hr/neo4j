{
  testFailRemoveUserFromRole(pwdSubject);
  testFailRemoveUserFromRole(readSubject);
  testFailRemoveUserFromRole(writeSubject);
  testCallFail(schemaSubject,"CALL dbms.removeUserFromRole('readSubject', '" + READER + "')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.removeUserFromRole('Olivia', '" + READER + "')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.removeUserFromRole('Olivia', 'thisRoleDoesNotExist')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
}
