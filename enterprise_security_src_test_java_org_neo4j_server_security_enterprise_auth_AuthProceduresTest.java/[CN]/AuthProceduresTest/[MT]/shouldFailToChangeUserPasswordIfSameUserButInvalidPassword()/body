{
  testCallFail(db,readSubject,"CALL dbms.changeUserPassword( 'readSubject', '123' )",QueryExecutionException.class,"Old password and new password cannot be the same");
  testCallFail(db,adminSubject,"CALL dbms.changeUserPassword( 'adminSubject', 'abc' )",QueryExecutionException.class,"Old password and new password cannot be the same");
}
