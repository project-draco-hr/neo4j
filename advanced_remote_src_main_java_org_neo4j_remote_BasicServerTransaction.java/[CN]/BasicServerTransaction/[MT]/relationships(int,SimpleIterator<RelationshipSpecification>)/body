{
  RelationshipSpecification[] result=consume(token,relIter,iterator,server.relationshipsBatchSize(iterator.count())).toArray(new RelationshipSpecification[0]);
  if (iterator.hasNext()) {
    return response().buildPartialRelationshipResponse(token,result);
  }
 else {
    return response().buildFinalRelationshipResponse(result);
  }
}
