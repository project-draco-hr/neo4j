{
  if (progress == null) {
    return;
  }
  try {
    for (    NodeLabelRange range : reader) {
      if (!continueScanning) {
        return;
      }
      LabelScanDocument document=new LabelScanDocument(range);
      reporter.forNodeLabelScan(document,this);
    }
  }
  finally {
    try {
      reader.close();
    }
 catch (    IOException e) {
      progress.failed(e);
    }
    progress.done();
  }
}
