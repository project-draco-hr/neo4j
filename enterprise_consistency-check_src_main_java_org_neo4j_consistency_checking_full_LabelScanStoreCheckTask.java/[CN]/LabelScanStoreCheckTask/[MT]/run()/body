{
  if (progress == null) {
    return;
  }
  try {
    if (scanSupport == null) {
      return;
    }
    try (NodeRangeReader reader=scanSupport.newRangeReader()){
      for (      NodeLabelRange range : reader) {
        if (continueScanning) {
          LabelScanDocument document=new LabelScanDocument(range.id(),range);
          reporter.forNodeLabelScan(document,this);
        }
 else {
          return;
        }
      }
    }
 catch (    IOException e) {
      progress.failed(e);
    }
  }
  finally {
    progress.done();
  }
}
