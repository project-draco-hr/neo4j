{
  final RecordingByteChannel rawData=new RecordingByteChannel();
  final MessageFormat.Writer packer=new PackStreamMessageFormatV1.Writer(new PackStream.Packer(new BufferedChannelOutput(rawData)),NO_OP);
  for (  Message message : messages) {
    packer.write(message);
  }
  packer.flush();
  return rawData.getBytes();
}
