{
  final File tempFile=File.createTempFile("neo4j","rrd");
  tempFile.delete();
  tempFile.mkdir();
  Runtime.getRuntime().addShutdownHook(new Thread(){
    @Override public void run(){
      try {
        FileUtils.deleteRecursively(tempFile);
      }
 catch (      IOException e) {
      }
    }
  }
);
  return tempFile.getAbsolutePath();
}
