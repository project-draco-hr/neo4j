{
  Sampleable[] primitives={new NodeIdsInUseSampleable((GraphDatabaseSPI)db.graph),new PropertyCountSampleable((GraphDatabaseSPI)db.graph),new RelationshipCountSampleable((GraphDatabaseSPI)db.graph)};
  Sampleable[] usage={};
  final String basePath=config.getString(RRDB_LOCATION_PROPERTY_KEY,getDefaultDirectory((GraphDatabaseSPI)db.graph));
  final RrdDb rrdb=createRrdb(basePath,join(primitives,usage));
  scheduler.scheduleAtFixedRate(new RrdJob(new RrdSamplerImpl(rrdb,primitives)),RRD_THREAD_NAME + "[primitives]",SECONDS.toMillis(0),SECONDS.toMillis(3));
  return rrdb;
}
