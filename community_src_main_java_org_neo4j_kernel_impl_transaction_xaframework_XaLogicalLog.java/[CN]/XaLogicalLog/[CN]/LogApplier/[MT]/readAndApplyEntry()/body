{
  buffer.clear();
  buffer.limit(1);
  if (byteChannel.read(buffer) != buffer.limit()) {
    return false;
  }
  buffer.flip();
  byte entry=buffer.get();
switch (entry) {
case LogEntry.TX_START:
    readTxStartEntry();
  return true;
case LogEntry.TX_PREPARE:
readTxPrepareEntry();
return true;
case LogEntry.TX_1P_COMMIT:
readAndApplyTxOnePhaseCommit();
return true;
case LogEntry.TX_2P_COMMIT:
readAndApplyTxTwoPhaseCommit();
return true;
case LogEntry.COMMAND:
readCommandEntry();
return true;
case LogEntry.DONE:
readDoneEntry();
return true;
case LogEntry.EMPTY:
return false;
default :
throw new IOException("Internal recovery failed, " + "unknown log entry[" + entry + "]");
}
}
