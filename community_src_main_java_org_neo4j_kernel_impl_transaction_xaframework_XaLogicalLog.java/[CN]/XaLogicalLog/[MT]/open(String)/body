{
  fileChannel=new RandomAccessFile(fileToOpen,"rw").getChannel();
  if (fileChannel.size() != 0) {
    doInternalRecovery(fileToOpen);
  }
 else {
    logVersion=xaTf.getCurrentVersion();
    buffer.clear();
    buffer.putLong(logVersion);
    buffer.flip();
    fileChannel.write(buffer);
    scanIsComplete=true;
  }
}
