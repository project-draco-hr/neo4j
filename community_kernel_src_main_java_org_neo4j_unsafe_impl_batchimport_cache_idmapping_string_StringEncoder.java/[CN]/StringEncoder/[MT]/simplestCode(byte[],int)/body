{
  int[] codes=new int[]{0,0};
  codes[0]=inputLength << 25;
  codes[1]=0;
  for (int i=0; i < 3 && i < inputLength; i++) {
    codes[0]=codes[0] | bytes[i] << ((2 - i) * 8);
  }
  for (int i=3; i < 7 && i < inputLength; i++) {
    codes[1]=codes[1] | (bytes[i]) << ((6 - i) * 8);
  }
  return codes;
}
