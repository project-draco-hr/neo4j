{
  if (length < 32) {
    return length;
  }
 else   if (length <= 96) {
    return length >> 1;
  }
 else   if (length <= 324) {
    return length >> 2;
  }
 else   if (length <= 580) {
    return length >> 3;
  }
 else   if (length <= 836) {
    return length >> 4;
  }
 else {
    return 127;
  }
}
