{
  NeoStore store=neoStore;
  if (store == null) {
synchronized (this) {
      store=neoStore;
      if (store == null) {
        NeoStoreProvider neoStoreProvider=graphdb.getDependencyResolver().resolveDependency(NeoStoreProvider.class);
        store=neoStoreProvider.evaluate();
        neoStore=store;
      }
    }
  }
  return store;
}
