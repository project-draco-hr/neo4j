{
  initializeStores();
  startTx();
  long nodeId=ds.nextId(Node.class);
  xaCon.getWriteTransaction().nodeCreate(nodeId);
  pStore.nextId();
  PropertyData prop=xaCon.getWriteTransaction().nodeAddProperty(nodeId,index("nisse"),new Integer(10));
  commitTx();
  ds.stop();
  initializeStores();
  startTx();
  xaCon.getWriteTransaction().nodeChangeProperty(nodeId,prop,new Integer(5));
  xaCon.getWriteTransaction().nodeRemoveProperty(nodeId,prop);
  xaCon.getWriteTransaction().nodeDelete(nodeId);
  commitTx();
  ds.stop();
}
