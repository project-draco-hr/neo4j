{
  Iterator<Token> itr=MyPropertyKeyToken.index(key).iterator();
  if (!itr.hasNext()) {
    int id=(int)ds.nextId(PropertyKeyTokenRecord.class);
    Token index=createDummyIndex(id,key);
    xaCon.getWriteTransaction().createPropertyKeyToken(key,id);
    return id;
  }
  return itr.next().id();
}
