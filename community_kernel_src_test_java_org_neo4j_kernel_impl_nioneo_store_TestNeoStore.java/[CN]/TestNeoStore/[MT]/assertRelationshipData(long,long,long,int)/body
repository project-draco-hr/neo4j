{
  try {
    storeLayer.visit(rel,new RelationshipVisitor(){
      @Override public void visit(      long relId,      long startNode,      long endNode,      int type){
        assertEquals(firstNode,startNode);
        assertEquals(secondNode,endNode);
        assertEquals(relType,type);
      }
    }
);
  }
 catch (  EntityNotFoundException e) {
    throw new RuntimeException(e);
  }
}
