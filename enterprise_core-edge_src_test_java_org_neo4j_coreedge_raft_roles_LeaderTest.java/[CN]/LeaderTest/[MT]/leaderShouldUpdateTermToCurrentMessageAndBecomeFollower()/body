{
  RaftState<RaftTestMember> state=raftState().build();
  Leader leader=new Leader();
  Outcome outcome=leader.handle(voteRequest().from(member1).term(HIGHEST_TERM).lastLogIndex(0).lastLogTerm(-1).build(),state,log());
  assertEquals(FOLLOWER,outcome.newRole);
  assertEquals(HIGHEST_TERM,outcome.term);
}
