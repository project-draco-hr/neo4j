{
  if (server != null) {
    server.stop();
    server.shutdown();
    server=null;
    try {
      ClusterMemberAvailability client=getClusterMemberAvailability();
      client.memberIsUnavailable(BACKUP);
    }
 catch (    NoClassDefFoundError e) {
    }
catch (    IllegalArgumentException e) {
    }
  }
}
