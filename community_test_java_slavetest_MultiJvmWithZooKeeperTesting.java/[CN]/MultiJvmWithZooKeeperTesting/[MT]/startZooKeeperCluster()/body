{
  FileUtils.deleteDirectory(BASE_ZOO_KEEPER_DATA_DIR);
  zooKeeperCluster=new ZooKeeperClusterLifecycle(ZOO_KEEPER_CLUSTER_SIZE,ZooKeeperClusterLifecycle.defaultDataDirectoryPolicy(BASE_ZOO_KEEPER_DATA_DIR),ZooKeeperClusterLifecycle.defaultPortPolicy(2181),ZooKeeperClusterLifecycle.defaultPortPolicy(2888),ZooKeeperClusterLifecycle.defaultPortPolicy(3888));
}
