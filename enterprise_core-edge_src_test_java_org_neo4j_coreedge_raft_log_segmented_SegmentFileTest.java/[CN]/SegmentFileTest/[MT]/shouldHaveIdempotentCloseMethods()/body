{
  SegmentFile segment=create(fsRule.get(),fileNames.getForVersion(0),readerPool,0,contentMarshal,logProvider,segmentHeader);
  IOCursor<EntryRecord> reader=segment.getReader(0);
  segment.closeWriter();
  reader.close();
  assertTrue(segment.tryClose());
  segment.close();
  assertTrue(segment.tryClose());
  segment.close();
}
