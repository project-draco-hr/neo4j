{
  endPreviousGroup();
synchronized (this) {
    sortBuckets=new ParallelSort(radix,dataCache,trackerCache,processorsForSorting).run();
  }
  if (detectAndMarkCollisions() > 0) {
    try (ResourceIterator<Object> idIterator=ids.iterator()){
      buildCollisionInfo(idIterator);
    }
   }
  readyForUse=true;
}
