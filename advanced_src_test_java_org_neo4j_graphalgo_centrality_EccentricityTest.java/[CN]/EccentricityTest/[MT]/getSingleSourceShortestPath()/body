{
  return new SingleSourceShortestPathDijkstra<Double>(0.0,null,new CostEvaluator<Double>(){
    public Double getCost(    Relationship relationship,    boolean backwards){
      return 1.0;
    }
  }
,new org.neo4j.graphalgo.shortestPath.std.DoubleAdder(),new org.neo4j.graphalgo.shortestPath.std.DoubleComparator(),Direction.BOTH,MyRelTypes.R1);
