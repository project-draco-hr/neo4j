{
  NodeRecord before=new NodeRecord(nodeId,true,false,NO_NEXT_RELATIONSHIP.intValue(),NO_NEXT_PROPERTY.intValue(),NO_LABELS_FIELD.intValue());
  NodeRecord after=new NodeRecord(nodeId,true,false,NO_NEXT_RELATIONSHIP.intValue(),NO_NEXT_PROPERTY.intValue(),ThreadLocalRandom.current().nextLong(100));
  NodeCommand command=new NodeCommand();
  command.init(before,after);
  return command;
}
