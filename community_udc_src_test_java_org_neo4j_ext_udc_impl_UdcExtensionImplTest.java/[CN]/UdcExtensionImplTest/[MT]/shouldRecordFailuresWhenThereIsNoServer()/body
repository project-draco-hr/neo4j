{
  GraphDatabaseService graphdb=new GraphDatabaseFactory().newEmbeddedDatabaseBuilder("should-record-failures").setConfig(GraphDatabaseSettings.first_delay,"100").setConfig(GraphDatabaseSettings.udc_host,"127.0.0.1:1").newGraphDatabase();
  assertGotFailureWithRetry(IS_GREATER_THAN_ZERO);
  destroy(graphdb);
}
