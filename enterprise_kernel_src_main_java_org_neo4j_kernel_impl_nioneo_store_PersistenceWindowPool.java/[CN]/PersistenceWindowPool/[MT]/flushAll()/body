{
  if (readOnly)   return;
  for (  BrickElement element : brickArray) {
    PersistenceWindow window=element.getWindow();
    if (window != null) {
      window.force();
    }
  }
  try {
    fileChannel.force(false);
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException("Failed to flush file channel " + storeName,e);
  }
}
