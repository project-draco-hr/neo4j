{
  if (fileSystem.fileExists(file.getAbsolutePath()))   throw new IllegalArgumentException(file + " already exist");
  FileChannel fileChannel=null;
  try {
    fileChannel=fileSystem.open(file.getAbsolutePath(),"rw");
    write(fileChannel,System.currentTimeMillis(),new Random(System.currentTimeMillis()).nextLong(),0,1,indexVersion);
  }
  finally {
    fileChannel.close();
  }
}
