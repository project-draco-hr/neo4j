{
  if (fileSystem.fileExists(file))   throw new IllegalArgumentException(file + " already exist");
  FileChannel fileChannel=null;
  try {
    fileChannel=fileSystem.open(file,"rw");
    write(fileChannel,System.currentTimeMillis(),new Random(System.currentTimeMillis()).nextLong(),0,1,indexVersion);
  }
  finally {
    if (fileChannel != null)     fileChannel.close();
  }
}
