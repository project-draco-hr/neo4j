{
  if (fileSystem == null) {
    fileSystem=new EphemeralFileSystemAbstraction();
  }
  File directory=new File("raft-log");
  fileSystem.mkdir(directory);
  SegmentedRaftLog newRaftLog=new SegmentedRaftLog(fileSystem,directory,rotateAtSize,new DummyRaftableContentSerializer(),NullLogProvider.getInstance(),1000,raft_log_pruning.getDefaultValue());
  life.add(newRaftLog);
  life.init();
  life.start();
  return newRaftLog;
}
