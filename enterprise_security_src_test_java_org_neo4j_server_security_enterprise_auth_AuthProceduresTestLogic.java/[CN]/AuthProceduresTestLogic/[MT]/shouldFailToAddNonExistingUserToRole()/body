{
  testCallFail(adminSubject,"CALL dbms.addUserToRole('Olivia', '" + PUBLISHER + "')",QueryExecutionException.class,"User Olivia does not exist");
  testCallFail(adminSubject,"CALL dbms.addUserToRole('Olivia', 'thisRoleDoesNotExist')",QueryExecutionException.class,"User Olivia does not exist");
  testCallFail(adminSubject,"CALL dbms.addUserToRole('Olivia', '')",QueryExecutionException.class,"User Olivia does not exist");
}
