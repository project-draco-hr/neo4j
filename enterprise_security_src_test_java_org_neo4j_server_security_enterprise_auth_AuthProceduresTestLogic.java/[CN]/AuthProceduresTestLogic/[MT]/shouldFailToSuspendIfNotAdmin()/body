{
  testCallFail(schemaSubject,"CALL dbms.suspendUser('readSubject')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.suspendUser('Craig')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.suspendUser('')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
}
