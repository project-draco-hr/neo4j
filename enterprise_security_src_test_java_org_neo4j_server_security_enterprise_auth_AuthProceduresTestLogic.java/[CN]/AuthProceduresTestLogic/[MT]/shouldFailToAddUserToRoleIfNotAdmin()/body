{
  testFailAddUserToRole(pwdSubject);
  testFailAddUserToRole(readSubject);
  testFailAddUserToRole(writeSubject);
  testCallFail(schemaSubject,"CALL dbms.addUserToRole('readSubject', '" + PUBLISHER + "')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.addUserToRole('Olivia', '" + PUBLISHER + "')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.addUserToRole('Olivia', 'thisRoleDoesNotExist')",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
}
