{
  testCallFail(adminSubject,"CALL dbms.listRolesForUser('Petra') YIELD value as roles RETURN roles",QueryExecutionException.class,"User Petra does not exist");
  testCallFail(adminSubject,"CALL dbms.listRolesForUser('') YIELD value as roles RETURN roles",QueryExecutionException.class,"User  does not exist");
}
