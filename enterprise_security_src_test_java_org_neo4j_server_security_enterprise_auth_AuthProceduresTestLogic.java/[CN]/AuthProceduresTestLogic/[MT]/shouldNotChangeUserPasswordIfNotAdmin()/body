{
  testCallFail(schemaSubject,"CALL dbms.changeUserPassword( 'readSubject', '321' )",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.changeUserPassword( 'jake', '321' )",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
  testCallFail(schemaSubject,"CALL dbms.changeUserPassword( 'readSubject', '' )",QueryExecutionException.class,AuthProcedures.PERMISSION_DENIED);
}
