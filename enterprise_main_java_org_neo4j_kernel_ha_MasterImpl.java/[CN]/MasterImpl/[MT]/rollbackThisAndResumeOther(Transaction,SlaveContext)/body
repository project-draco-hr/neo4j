{
  try {
    TransactionManager txManager=graphDbConfig.getTxModule().getTxManager();
    txManager.rollback();
    transactions.remove(txId);
    if (otherTx != null) {
      txManager.resume(otherTx);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
