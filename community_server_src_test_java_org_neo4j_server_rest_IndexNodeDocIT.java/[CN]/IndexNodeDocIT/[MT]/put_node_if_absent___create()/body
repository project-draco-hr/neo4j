{
  final String index="people", key="name", value="Mattias";
  helper.createNodeIndex(index);
  String uri=functionalTestHelper.nodeIndexUri() + index + "?unique";
  gen().expectedStatus(201).payloadType(MediaType.APPLICATION_JSON_TYPE).payload("{\"key\": \"" + key + "\", \"value\": \""+ value+ "\", \"uri\":\""+ functionalTestHelper.nodeUri(helper.createNode())+ "\"}").post(uri);
}
