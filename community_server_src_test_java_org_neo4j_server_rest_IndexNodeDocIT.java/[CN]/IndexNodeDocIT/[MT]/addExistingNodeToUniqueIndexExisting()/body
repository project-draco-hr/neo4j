{
  final String indexName="favorites";
  final String key="some-key";
  final String value="some value";
  long nodeId=createNode();
  gen().noGraph().expectedStatus(201).payload(JsonHelper.createJsonFrom(generateNodeIndexCreationPayload(key,value,functionalTestHelper.nodeUri(nodeId)))).post(functionalTestHelper.indexNodeUri(indexName) + "?uniqueness=create_or_fail");
  gen().noGraph().expectedStatus(409).payload(JsonHelper.createJsonFrom(generateNodeIndexCreationPayload(key,value,functionalTestHelper.nodeUri(nodeId)))).post(functionalTestHelper.indexNodeUri(indexName) + "?uniqueness=create_or_fail");
}
