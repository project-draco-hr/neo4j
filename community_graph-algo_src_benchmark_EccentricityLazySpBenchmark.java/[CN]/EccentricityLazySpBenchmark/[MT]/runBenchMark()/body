{
  Transaction tx=neo.beginTx();
  nodeSet=new HashSet<Node>();
  for (int i=0; i < 2; ++i) {
    nodeSet.add(graph.getRandomNode(null));
  }
  SingleSourceShortestPath<Integer> singleSourceShortestPath=new SingleSourceShortestPathBFS(graph.getRandomNode(null),Direction.BOTH,MyRelTypes.R1);
  new Eccentricity<Integer>(singleSourceShortestPath,0,nodeSet,new IntegerComparator()).calculate();
  SingleSourceShortestPath<Double> singleSourceShortestPath2=new SingleSourceShortestPathDijkstra<Double>(0.0,null,new DoubleEvaluator("cost"),new DoubleAdder(),new DoubleComparator(),Direction.BOTH,MyRelTypes.R1);
  new Eccentricity<Double>(singleSourceShortestPath2,0.0,nodeSet,new DoubleComparator()).calculate();
  tx.finish();
}
