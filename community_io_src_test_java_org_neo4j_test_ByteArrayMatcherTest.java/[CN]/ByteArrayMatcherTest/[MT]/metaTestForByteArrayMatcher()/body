{
  byte[] a=new byte[]{1,-2,3};
  byte[] b=new byte[]{1,3,-2};
  assertThat("a == a",a,byteArray(a));
  boolean caughtError=false;
  try {
    assertThat("a != b",a,byteArray(b));
  }
 catch (  AssertionError error) {
    caughtError=true;
  }
  assertTrue("should have thrown on a != b",caughtError);
}
