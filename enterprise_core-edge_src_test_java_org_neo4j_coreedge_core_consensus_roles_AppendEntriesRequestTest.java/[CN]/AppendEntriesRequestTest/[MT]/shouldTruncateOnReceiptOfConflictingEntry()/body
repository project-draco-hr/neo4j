{
  InMemoryRaftLog raftLog=new InMemoryRaftLog();
  RaftState state=raftState().myself(myself).term(5).entryLog(raftLog).build();
  long leaderTerm=state.term() + leaderTermDifference;
  raftLog.append(new RaftLogEntry(state.term() - 1,content()));
  Outcome outcome=role.handler.handle(appendEntriesRequest().from(leader).leaderTerm(leaderTerm).prevLogIndex(raftLog.appendIndex() - 1).prevLogTerm(-1).logEntry(new RaftLogEntry(leaderTerm,content())).build(),state,log());
  assertTrue(((Response)messageFor(outcome,leader)).success());
  assertThat(outcome.getLogCommands(),hasItem(new TruncateLogCommand(0)));
}
