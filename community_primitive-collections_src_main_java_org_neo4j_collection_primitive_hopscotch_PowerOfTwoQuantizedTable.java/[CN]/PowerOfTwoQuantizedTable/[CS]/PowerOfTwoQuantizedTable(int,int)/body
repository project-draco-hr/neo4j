{
  if (capacity <= 0) {
    throw new IllegalArgumentException("Illegal capacity " + capacity);
  }
  if (h < 4 || h > 32) {
    throw new IllegalArgumentException("h needs to be 4 <= h <= 32, was " + h);
  }
  this.h=h;
  this.capacity=quantize(capacity);
  this.tableMask=highestOneBit(this.capacity) - 1;
}
