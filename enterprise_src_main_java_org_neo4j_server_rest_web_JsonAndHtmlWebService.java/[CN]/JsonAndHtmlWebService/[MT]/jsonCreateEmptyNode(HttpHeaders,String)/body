{
  if (!isNullOrEmpty(body) && headers != null && (!headers.getMediaType().equals(MediaType.TEXT_PLAIN_TYPE) && !headers.getMediaType().equals(MediaType.MULTIPART_FORM_DATA_TYPE))) {
    return Response.status(Status.UNSUPPORTED_MEDIA_TYPE).build();
  }
  return createEmptyNode(body,JsonRenderers.DEFAULT);
}
