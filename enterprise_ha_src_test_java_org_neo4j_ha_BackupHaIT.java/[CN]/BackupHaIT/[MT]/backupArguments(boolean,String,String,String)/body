{
  List<String> args=new ArrayList<String>();
  args.add(trueForFull ? "-full" : "-incremental");
  args.add("-from");
  args.add(from);
  args.add("-to");
  args.add(to);
  if (clusterName != null) {
    args.add("-cluster");
    args.add(clusterName);
  }
  return args.toArray(new String[args.size()]);
}
