{
  FileUtils.deleteDirectory(PATH);
  FileUtils.deleteDirectory(BACKUP_PATH);
  clusterManager=new ClusterManager(fromXml(getClass().getResource("/threeinstances.xml").toURI()),PATH,MapUtil.stringMap(OnlineBackupSettings.online_backup_enabled.name(),Settings.TRUE)){
    @Override protected void config(    GraphDatabaseBuilder builder,    String clusterName,    InstanceId serverId){
      builder.setConfig(OnlineBackupSettings.online_backup_server,(":" + (4444 + serverId.toIntegerIndex())));
    }
  }
;
  clusterManager.start();
  cluster=clusterManager.getDefaultCluster();
  representation=createSomeData(cluster.getMaster());
}
