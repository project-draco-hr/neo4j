{
  FileUtils.deleteDirectory(new File(PATH));
  FileUtils.deleteDirectory(new File(BACKUP_PATH));
  clusterManager=new ClusterManager(fromXml(getClass().getResource("/threeinstances.xml").toURI()),new File(PATH),MapUtil.stringMap(OnlineBackupSettings.online_backup_enabled.name(),GraphDatabaseSetting.TRUE)){
    @Override protected void config(    GraphDatabaseBuilder builder,    String clusterName,    int serverId){
      builder.setConfig(OnlineBackupSettings.online_backup_port,(4444 + serverId) + "");
    }
  }
;
  clusterManager.start();
  cluster=clusterManager.getDefaultCluster();
  representation=createSomeData(cluster.getMaster());
}
