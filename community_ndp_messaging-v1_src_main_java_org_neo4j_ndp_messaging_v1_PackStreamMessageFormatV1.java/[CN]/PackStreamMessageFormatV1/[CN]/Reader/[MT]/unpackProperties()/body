{
  int numProps=(int)unpacker.unpackMapHeader();
  Map<String,Object> map;
  if (numProps > 0) {
    map=new HashMap<>(numProps,1);
    for (int j=0; j < numProps; j++) {
      String key=unpacker.unpackString();
      Object val=unpackValue();
      map.put(key,val);
    }
  }
 else {
    map=Collections.EMPTY_MAP;
  }
  return map;
}
