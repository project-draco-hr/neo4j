{
  Map<String,Object> map=unpackRawMap();
  String codeStr=map.containsKey("code") ? (String)map.get("code") : Status.General.UnknownFailure.name();
  String msg=map.containsKey("message") ? (String)map.get("message") : "<No message supplied>";
  output.handleFailureMessage(new Neo4jError(codeFromString(codeStr),msg));
}
