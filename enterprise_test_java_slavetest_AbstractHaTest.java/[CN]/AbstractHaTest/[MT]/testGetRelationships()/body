{
  initializeDbs(1);
  assertEquals((Integer)1,executeJob(new CommonJobs.CreateSubRefNodeWithRelCountJob(CommonJobs.REL_TYPE.name(),CommonJobs.REL_TYPE.name(),CommonJobs.KNOWS.name()),0));
  assertEquals((Integer)2,executeJob(new CommonJobs.CreateSubRefNodeWithRelCountJob(CommonJobs.REL_TYPE.name(),CommonJobs.REL_TYPE.name(),CommonJobs.KNOWS.name()),0));
  assertEquals((Integer)2,executeJob(new CommonJobs.GetRelationshipCountJob(CommonJobs.REL_TYPE.name(),CommonJobs.KNOWS.name()),0));
  assertEquals((Integer)2,executeJobOnMaster(new CommonJobs.GetRelationshipCountJob(CommonJobs.REL_TYPE.name(),CommonJobs.KNOWS.name())));
}
