{
  ConfigurationMigrator migrator=new GraphDatabaseConfigurationMigrator();
  assertThat(migrator.apply(stringMap("neo4j.ext.udc.disable","true"),NullLog.getInstance()),equalTo(stringMap("neo4j.ext.udc.enabled","false")));
  assertThat(migrator.apply(stringMap("neo4j.ext.udc.disable","false"),NullLog.getInstance()),equalTo(stringMap("neo4j.ext.udc.enabled","true")));
}
