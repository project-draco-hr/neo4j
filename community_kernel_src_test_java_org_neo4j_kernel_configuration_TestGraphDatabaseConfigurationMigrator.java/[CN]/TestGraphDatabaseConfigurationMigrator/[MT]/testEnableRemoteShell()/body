{
  ConfigurationMigrator migrator=new GraphDatabaseConfigurationMigrator();
  assertThat(migrator.apply(stringMap("enable_remote_shell","true"),NullLog.getInstance()),equalTo(stringMap("remote_shell_enabled","true")));
  assertThat(migrator.apply(stringMap("enable_remote_shell","false"),NullLog.getInstance()),equalTo(stringMap("remote_shell_enabled","false")));
  assertThat(migrator.apply(stringMap("enable_remote_shell","port=1234"),NullLog.getInstance()),equalTo(stringMap("remote_shell_enabled","true","remote_shell_port","1234","remote_shell_read_only","false","remote_shell_name","shell")));
}
