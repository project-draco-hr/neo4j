{
  Token token=null;
  for (int i=(id < tokens.length) ? id : tokens.length - 1; i >= 0; i--) {
    token=tokens[i];
    if (token.id() == id) {
      break;
    }
    if (token.id() < id) {
      token=null;
      break;
    }
  }
  if (token != null) {
    String name=token.name();
    result.append(pre).append(name).append(" [").append(handle).append("Id=").append(token.id()).append(']');
  }
 else {
    result.append(handle).append("Id=").append(id);
  }
  return result;
}
