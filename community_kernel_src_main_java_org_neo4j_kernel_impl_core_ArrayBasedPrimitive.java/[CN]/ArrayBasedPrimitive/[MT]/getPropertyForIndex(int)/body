{
  PropertyData[] local=properties;
  int low=0;
  int high=local.length - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    PropertyData midVal=local[mid];
    int midId=midVal.getIndex();
    if (midId < keyId)     low=mid + 1;
 else     if (midId > keyId)     high=mid - 1;
 else     return midVal;
  }
  return null;
}
