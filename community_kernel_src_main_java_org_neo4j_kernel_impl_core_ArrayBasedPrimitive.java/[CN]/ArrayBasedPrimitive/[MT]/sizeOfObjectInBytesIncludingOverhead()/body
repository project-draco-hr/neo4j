{
  int size=SizeOfs.REFERENCE_SIZE + 8;
  if (properties != null) {
    size=withArrayOverheadIncludingReferences(size,properties.length);
    for (    Property data : properties)     size+=data.asPropertyDataJustForIntegration().sizeOfObjectInBytesIncludingOverhead();
  }
  return withObjectOverhead(size);
}
