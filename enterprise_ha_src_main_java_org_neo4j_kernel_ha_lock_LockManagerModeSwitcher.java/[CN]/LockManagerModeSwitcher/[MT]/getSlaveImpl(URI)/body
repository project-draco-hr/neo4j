{
  SlaveLockManager.Configuration slaveConfig=new SlaveLockManager.Configuration(){
    @Override public long getStateSwitchTimeout(){
      return config.get(HaSettings.state_switch_timeout);
    }
  }
;
  return new SlaveLockManager(txManager,txHook,switchBlock,slaveConfig,new RagManager(),requestContextFactory,master,xaDsm);
}
