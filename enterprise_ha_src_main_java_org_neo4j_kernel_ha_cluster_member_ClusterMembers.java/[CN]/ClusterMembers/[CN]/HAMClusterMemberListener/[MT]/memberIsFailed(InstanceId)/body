{
  String oldMembers=members.values().toString();
  ClusterMember member=getMember(instanceId);
  for (  String role : member.getRoles()) {
    member=member.unavailableAs(role);
  }
  members.put(instanceId,member);
  log.debug("Member is failed. Instance: " + instanceId + ". OldMembers: "+ oldMembers+ ". NewMembers: "+ members.values());
}
