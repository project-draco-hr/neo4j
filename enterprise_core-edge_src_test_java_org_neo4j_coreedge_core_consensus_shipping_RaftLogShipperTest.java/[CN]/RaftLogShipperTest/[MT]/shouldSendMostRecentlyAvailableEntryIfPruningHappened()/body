{
  raftLog.append(entry0);
  raftLog.append(entry1);
  raftLog.append(entry2);
  raftLog.append(entry3);
  startLogShipper();
  raftLog.prune(2);
  outbound.clear();
  logShipper.onMismatch(0,new LeaderContext(0,0));
  RaftMessages.AppendEntries.Request expected=new RaftMessages.AppendEntries.Request(CURRENT_VERSION,leader,leaderTerm,2,entry2.term(),RaftLogEntry.empty,leaderCommit);
  assertThat(outbound.sentTo(follower),hasItem(expected));
}
