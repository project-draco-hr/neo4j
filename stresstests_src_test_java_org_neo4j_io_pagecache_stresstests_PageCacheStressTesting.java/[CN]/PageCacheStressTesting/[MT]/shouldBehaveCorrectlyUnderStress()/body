{
  PageCacheMonitor monitor=new SimpleMonitor();
  PageCacheStressTest runner=new PageCacheStressTest.Builder().with(timePeriod(durationInMinutes,MINUTES)).withNumberOfPages(numberOfPages).withRecordsPerPage(recordsPerPage).withNumberOfThreads(numberOfThreads).withCachePagePadding(cachePagePadding).withNumberOfCachePages(numberOfCachePages).with(monitor).build(pageCacheFactory);
  runner.run();
}
