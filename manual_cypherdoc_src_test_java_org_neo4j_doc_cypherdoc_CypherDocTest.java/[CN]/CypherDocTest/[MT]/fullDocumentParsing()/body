{
  String content=FileUtils.readFileToString(new File("src/test/resources/hello-world.asciidoc"));
  String output=CypherDoc.parse(content);
  assertThat(output,allOf(containsString("[[cypherdoc-hello-world]]"),containsString("<p class=\"cypherdoc-console\"></p>"),containsString("[source,cypher]"),containsString("[queryresult]"),containsString("{Person|name = \\'Adam\\'\\l}"),containsString("= Hello World =")));
  assertThat(output,allOf(containsString("<span class=\"hide-query\"></span>"),containsString("<span class=\"setup-query\"></span>"),containsString("<span class=\"query-output\"></span>"),containsString("<simpara role=\"query-output\"></simpara>")));
}
