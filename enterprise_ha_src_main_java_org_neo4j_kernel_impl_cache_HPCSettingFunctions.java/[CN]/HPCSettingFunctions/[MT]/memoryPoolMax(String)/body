{
  return new Optionals.LazyOptional<Long>(){
    @Override protected Long evaluate(){
      try {
        return (long)((CompositeDataSupport)ManagementFactory.getPlatformMBeanServer().getAttribute(new ObjectName(bean),"Usage")).get("max");
      }
 catch (      Exception e) {
        return null;
      }
    }
  }
;
}
