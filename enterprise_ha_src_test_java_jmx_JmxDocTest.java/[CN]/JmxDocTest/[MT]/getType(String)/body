{
  if (TYPES.containsKey(type)) {
    return TYPES.get(type);
  }
 else   if (type.endsWith(";")) {
    if (type.startsWith("[L")) {
      return type.substring(2,type.length() - 1) + "[]";
    }
 else {
      throw new IllegalArgumentException("Don't know how to parse this type: " + type);
    }
  }
  return type;
}
