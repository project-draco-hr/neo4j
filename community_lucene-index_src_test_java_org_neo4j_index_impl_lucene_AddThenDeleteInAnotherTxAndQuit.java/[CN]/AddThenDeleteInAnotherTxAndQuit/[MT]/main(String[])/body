{
  GraphDatabaseService db=new GraphDatabaseFactory().newEmbeddedDatabase(args[0]);
  Index<Node> index=db.index().forNodes("index");
  Transaction tx=db.beginTx();
  try {
    Node node=db.createNode();
    index.add(node,"key","value");
    tx.success();
  }
  finally {
    tx.finish();
  }
  tx=db.beginTx();
  try {
    index.delete();
    tx.success();
  }
  finally {
    tx.finish();
  }
  System.exit(0);
}
