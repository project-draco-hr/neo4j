{
  String batfile="dos-exec-" + RandomString.generate(5) + ".bat";
  System.out.println("EXECUTING: " + "echo '" + cmd + "' > "+ batfile+ " && chmod +x "+ batfile+ " && "+ batfile+ " && rm "+ batfile);
  sh.run("echo '" + cmd + "' > "+ batfile+ " && chmod +x "+ batfile+ " && ./"+ batfile+ " && rm "+ batfile);
}
