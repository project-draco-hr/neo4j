{
  Result result=db.execute("MATCH (user:User { userId: {userId} } ) -[:FRIEND]- () -[:FRIEND]- (distantFriend) " + "RETURN COUNT(distinct distantFriend)",singletonMap("userId",(Object)(long)random.nextInt(USERS)));
  return Pair.of((Long)single(single(result).values()),result.getExecutionPlanDescription());
}
