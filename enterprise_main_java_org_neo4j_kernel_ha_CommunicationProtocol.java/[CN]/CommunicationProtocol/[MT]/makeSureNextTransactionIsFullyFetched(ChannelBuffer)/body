{
  buffer.markReaderIndex();
  try {
    if (buffer.readByte() > 0) {
      buffer.skipBytes(8);
      int blockSize=0;
      while ((blockSize=buffer.readUnsignedByte()) == 0) {
        buffer.skipBytes(BlockLogBuffer.DATA_SIZE);
      }
      buffer.skipBytes(blockSize);
    }
  }
  finally {
    buffer.resetReaderIndex();
  }
}
