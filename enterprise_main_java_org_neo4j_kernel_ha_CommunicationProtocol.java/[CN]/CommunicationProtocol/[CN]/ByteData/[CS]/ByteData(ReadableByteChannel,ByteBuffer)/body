{
  int size=0, chunk=0;
  List<byte[]> data=new LinkedList<byte[]>();
  while ((chunk=channel.read(readBuffer)) >= 0) {
    size+=chunk;
    byte[] bytes=new byte[chunk];
    readBuffer.flip();
    readBuffer.get(bytes);
    readBuffer.clear();
    data.add(bytes);
  }
  this.data=data;
  this.size=size;
}
