{
  buffer.makeReadyForTransfer();
  try {
    if (getRecordSize() != getFileChannel().transferTo(((long)id) * getRecordSize(),getRecordSize(),buffer.getFileChannel())) {
      return false;
    }
    return true;
  }
 catch (  IOException e) {
    throw new StoreFailureException("Failed to transfer data to read from buffer",e);
  }
}
