{
  String webAdminManagementUri=configuration.getString(Configurator.WEB_ADMIN_PATH_PROPERTY_KEY);
  String dataUri=configuration.getString(Configurator.REST_API_PATH_PROPERTY_KEY);
  DiscoveryRepresentation dr=new DiscoveryRepresentation(new URI(webAdminManagementUri),new URI(dataUri));
  return outputFormat.ok(dr);
}
