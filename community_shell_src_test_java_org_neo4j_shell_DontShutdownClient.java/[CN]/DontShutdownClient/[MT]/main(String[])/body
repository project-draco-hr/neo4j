{
  String path=args[0];
  File propsFile=createDefaultPropertiesFile(path);
  GraphDatabaseShellServer server=new GraphDatabaseShellServer(path,false,propsFile.getAbsolutePath());
  new SameJvmClient(new HashMap<String,Serializable>(),server,new SystemOutput(),InterruptSignalHandler.getHandler());
  server.shutdown();
}
