{
  required:   for (int i=0; i < this.tags.length; i++) {
    String requiredTag=this.tags[i];
    for (int j=0; j < tags.length; j++) {
      String tag=tags[j];
      if (requiredTag.equals(tag)) {
        continue required;
      }
    }
    return;
  }
  super.invoke(proxy,method,args,tags);
}
