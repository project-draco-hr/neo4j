{
  Iterator<Map.Entry<Predicate<Method>,MonitorListenerInvocationHandler>> iter=monitorListeners.entrySet().iterator();
  while (iter.hasNext()) {
    Map.Entry<Predicate<Method>,MonitorListenerInvocationHandler> handlerEntry=iter.next();
    if (handlerEntry.getValue() instanceof UntaggedMonitorListenerInvocationHandler) {
      UntaggedMonitorListenerInvocationHandler handler=(UntaggedMonitorListenerInvocationHandler)handlerEntry.getValue();
      if (handler.getMonitorListener() == monitorListener) {
        iter.remove();
      }
    }
  }
  recalculateAllMethodListeners();
}
