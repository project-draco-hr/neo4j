{
  Map<String,Node> nodes=data.get();
  String nodeUri=getNodeUri(nodes.get("I"));
  gen.get().expectedStatus(204).payload(createJsonFrom(new String[]{"MyLabel","MyOtherLabel"})).post(nodeUri + "/labels");
  Collection<Label> actual=asCollection(nodes.get("I").getLabels());
  assertThat(actual.size(),is(2));
  assertThat("Labels should be correct",actual,containsInAnyOrder(label("MyLabel"),label("MyOtherLabel")));
}
