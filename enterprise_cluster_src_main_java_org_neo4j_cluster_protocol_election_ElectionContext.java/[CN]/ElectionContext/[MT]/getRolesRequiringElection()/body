{
  return filter(new Specification<String>(){
    @Override public boolean satisfiedBy(    String role){
      return clusterContext.getConfiguration().getElected(role) == null;
    }
  }
,map(new Function<ElectionRole,String>(){
    @Override public String map(    ElectionRole role){
      return role.getName();
    }
  }
,roles));
}
