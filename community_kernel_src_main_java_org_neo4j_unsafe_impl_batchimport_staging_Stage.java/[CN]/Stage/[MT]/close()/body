{
  Exception exception=null;
  try {
    for (    Step<?> step : pipeline) {
      try {
        step.close();
      }
 catch (      Exception e) {
        if (exception == null) {
          exception=e;
        }
 else {
          exception.addSuppressed(e);
        }
      }
    }
    if (exception != null) {
      throw launderedException(exception);
    }
  }
  finally {
    pipeline.clear();
  }
}
