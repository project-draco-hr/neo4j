{
  return new ClusterContextImpl(me,commonStateSnapshot,logProvider,timeouts,joiningInstances == null ? null : new ArrayList<>(toList(joiningInstances)),joinDeniedConfigurationResponseState == null ? null : joinDeniedConfigurationResponseState.snapshot(),executor,objectOutputStreamFactory,objectInputStreamFactory,snapshotLearnerContext,snapshotHeartbeatContext);
}
