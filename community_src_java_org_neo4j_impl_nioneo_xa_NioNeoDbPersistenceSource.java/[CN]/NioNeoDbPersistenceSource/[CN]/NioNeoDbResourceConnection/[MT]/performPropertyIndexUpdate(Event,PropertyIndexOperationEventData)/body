{
  try {
    if (event == Event.PROPERTY_INDEX_CREATE) {
      propIndexConsumer.createPropertyIndex(data.getIndex().getKeyId(),data.getIndex().getKey());
    }
 else {
      throw new PersistenceUpdateFailedException("Unkown event: " + event);
    }
  }
 catch (  IOException e) {
    throw new PersistenceUpdateFailedException("Event[" + event + "]",e);
  }
}
