{
  RelationshipData relData[]=nodeConsumer.getRelationships(nodeId);
  RawRelationshipData relationships[]=new RawRelationshipData[relData.length];
  for (int i=0; i < relData.length; i++) {
    relationships[i]=new RawRelationshipData(relData[i].getId(),relData[i].firstNode(),relData[i].secondNode(),relData[i].relationshipType());
  }
  return relationships;
}
