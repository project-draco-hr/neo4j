{
  LocalhostZooKeeperCluster zoo=LocalhostZooKeeperCluster.singleton().clearDataAndVerifyConnection();
  File dir=TargetDirectory.forTest(getClass()).directory("dbs",true);
  masterDir=new File(dir,"0").getAbsolutePath();
  master=(GraphDatabaseAPI)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(masterDir).setConfig(HaSettings.server,"localhost:6361").setConfig(HaSettings.server_id,"0").setConfig(HaSettings.coordinators,zoo.getConnectionString()).newGraphDatabase();
  slaveDir=new File(dir,"1").getAbsolutePath();
  slave=(GraphDatabaseAPI)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(slaveDir).setConfig(HaSettings.server,"localhost:6362").setConfig(HaSettings.server_id,"1").setConfig(HaSettings.coordinators,zoo.getConnectionString()).newGraphDatabase();
}
