{
  try (PageCursor cursor=storeFile.io(pageIdForRecord(id),PF_SHARED_LOCK)){
    if (cursor.next()) {
      RelationshipGroupRecord record;
      do {
        record=getRecord(id,cursor);
      }
 while (cursor.shouldRetry());
      if (record != null) {
        return record;
      }
    }
    return new RelationshipGroupRecord(id,-1);
  }
 catch (  IOException e) {
    return new RelationshipGroupRecord(id,-1);
  }
}
