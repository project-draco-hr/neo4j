{
  try (PageCursor cursor=storeFile.io(pageIdForRecord(id),PF_SHARED_LOCK)){
    if (cursor.next()) {
      do {
        readRecord(id,cursor,record);
      }
 while (cursor.shouldRetry());
    }
 else {
      record.setInUse(false);
    }
    return record;
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
