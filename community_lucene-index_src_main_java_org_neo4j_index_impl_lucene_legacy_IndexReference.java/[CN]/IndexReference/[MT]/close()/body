{
  try {
    if (this.searcherIsClosed || this.refCount.get() == 0) {
      return true;
    }
    boolean reallyClosed=false;
    if (this.refCount.decrementAndGet() <= 0 && this.detached) {
      disposeSearcher();
      reallyClosed=true;
    }
    return reallyClosed;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
