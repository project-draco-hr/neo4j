{
  if (retries > 20) {
    close();
  }
  if (!acquireWriteBuffer()) {
    retries++;
    return false;
  }
  buffer.put(HeaderConstants.MASTER_GREETING);
  buffer.putLong(master.getVersion(xaDsName));
  buffer.flip();
  log("Setup greeting");
  setStatus(Status.SEND_GREETING);
  retries=0;
  return true;
}
