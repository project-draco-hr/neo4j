{
  int adjustment=0;
  if (highId > idRange.getRangeStart()) {
    adjustment=(int)(highId - idRange.getRangeStart());
  }
  IdRange adjustedForLocalHighId=new IdRange(idRange.getDefragIds(),max(this.highId,idRange.getRangeStart()),idRange.getRangeLength() - adjustment);
  return adjustedForLocalHighId;
}
