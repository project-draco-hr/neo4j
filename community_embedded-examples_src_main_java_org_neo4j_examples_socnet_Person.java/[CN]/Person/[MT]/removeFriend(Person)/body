{
  Transaction tx=underlyingNode.getGraphDatabase().beginTx();
  try {
    if (!this.equals(otherPerson)) {
      Relationship friendRel=getFriendRelationshipTo(otherPerson);
      if (friendRel != null) {
        friendRel.delete();
      }
      tx.success();
    }
  }
  finally {
    tx.finish();
  }
}
