{
  label=label.replace("\\","\\\\");
  label=label.replace("\"","\\\"");
  label=label.replace("'","\\\\'");
  label=label.replace("\n","\\n");
  label=label.replace("<","\\<");
  label=label.replace(">","\\>");
  label=label.replace("[","\\[");
  label=label.replace("]","\\]");
  label=label.replace("{","\\{");
  label=label.replace("}","\\}");
  label=label.replace("|","\\|");
  return label;
}
