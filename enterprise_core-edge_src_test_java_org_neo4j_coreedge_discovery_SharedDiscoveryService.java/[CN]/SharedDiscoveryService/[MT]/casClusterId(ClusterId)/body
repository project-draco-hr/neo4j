{
  boolean success;
  lock.lock();
  try {
    if (this.clusterId == null) {
      success=true;
      this.clusterId=clusterId;
    }
 else {
      success=this.clusterId.equals(clusterId);
    }
    if (success) {
      notifyCoreClients();
    }
  }
  finally {
    lock.unlock();
  }
  return success;
}
