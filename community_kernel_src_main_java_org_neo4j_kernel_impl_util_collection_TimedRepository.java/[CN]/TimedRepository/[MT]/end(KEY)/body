{
  while (true) {
    Entry<VALUE> entry=repo.get(key);
    if (entry == null) {
      return;
    }
    if (entry.markForEndingIfInUse()) {
      return;
    }
    if (entry.acquire()) {
      end0(key,entry.value);
      return;
    }
    if (entry.isMarkedForEnding()) {
      return;
    }
  }
}
