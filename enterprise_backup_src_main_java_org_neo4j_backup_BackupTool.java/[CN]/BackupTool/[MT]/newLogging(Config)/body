{
  Logging logging;
  try {
    BackupTool.class.getClassLoader().loadClass("ch.qos.logback.classic.LoggerContext");
    LifeSupport life=new LifeSupport();
    LogbackService logbackService=life.add(new LogbackService(config,(LoggerContext)getSingleton().getLoggerFactory(),"neo4j-backup-logback.xml"));
    life.start();
    logging=logbackService;
  }
 catch (  Throwable e) {
    logging=new SystemOutLogging();
  }
  return logging;
}
