{
  try {
    if (!out.exists()) {
      out.createNewFile();
    }
    BufferedReader is=new BufferedReader(new FileReader(out));
    String line=is.readLine();
    if (line == null || !line.equals(START)) {
      is.close();
      fw=new FileWriter(out,false);
      line(START);
      line("REST API\n========");
      fw.close();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
