{
  ClientRequest request;
  try {
    System.out.println("URI syntax exception: '" + uri + "'");
    request=ClientRequest.create().accept(applicationJsonType).build(new URI(uri),method);
  }
 catch (  URISyntaxException e) {
    throw new RuntimeException(e);
  }
  return retrieveResponse(title,uri,responseCode,applicationJsonType,headerField,request);
}
