{
  try {
    if (data.title == null) {
      return;
    }
    File out=new File("target",data.title.replace(" ","_") + ".txt");
    out.createNewFile();
    fw=new FileWriter(out,false);
    line("[[" + data.title + "]]");
    line("== " + data.title + " ==");
    line("");
    line("*+" + data.method + " "+ data.relUri+ "+*");
    line("");
    if (data.payload != null) {
      line("_Example parameters (payload encoding = \"" + data.payloadencoding + "\")_");
      line("");
      line(data.payload);
      line("");
    }
    line("_Example Response_");
    line("");
    line("*+" + data.status.getStatusCode() + ": "+ data.status.name()+ "+*");
    line("");
    line("[source,javascript]");
    line("----");
    line(data.entity);
    line("----");
    line("");
    fw.flush();
    fw.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
