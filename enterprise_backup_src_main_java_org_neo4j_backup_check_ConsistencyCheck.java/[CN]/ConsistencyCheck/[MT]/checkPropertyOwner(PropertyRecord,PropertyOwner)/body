{
  if (propertyOwners == null)   return false;
  Long propId=Long.valueOf(prop.getId());
  PropertyOwner oldOwner=propertyOwners.put(propId,newOwner);
  if (oldOwner != null) {
    @SuppressWarnings("unchecked") RecordStore<PrimitiveRecord> oldStore=(RecordStore<PrimitiveRecord>)oldOwner.storeFrom(this), newStore=(RecordStore<PrimitiveRecord>)newOwner.storeFrom(this);
    return inconsistent(oldStore,oldStore.getRecord(oldOwner.id),newStore,newStore.getRecord(newOwner.id),MULTIPLE_OWNERS.forProperty(prop));
  }
 else {
    return false;
  }
}
