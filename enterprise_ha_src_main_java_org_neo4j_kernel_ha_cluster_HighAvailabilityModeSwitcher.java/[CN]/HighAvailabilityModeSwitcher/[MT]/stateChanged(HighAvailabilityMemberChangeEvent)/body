{
  availableMasterId=event.getServerHaUri();
  if (event.getNewState() == event.getOldState()) {
    return;
  }
switch (event.getNewState()) {
case TO_MASTER:
    life.shutdown();
  life=new LifeSupport();
switchToMaster();
break;
case TO_SLAVE:
life.shutdown();
life=new LifeSupport();
switchToSlave();
break;
case PENDING:
life.shutdown();
life=new LifeSupport();
break;
default :
}
}
