{
  if (event.getNewState() == event.getOldState()) {
    if (event.getServerHaUri() != null) {
      availableMasterId=event.getServerHaUri();
    }
    return;
  }
  availableMasterId=event.getServerHaUri();
  currentTargetState=event.getNewState();
switch (event.getNewState()) {
case TO_MASTER:
    switchToMaster();
  break;
case TO_SLAVE:
switchToSlave();
break;
case PENDING:
switchToPending(event.getOldState());
break;
default :
}
}
