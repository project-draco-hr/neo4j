{
  msgLog.info("I am %s, moving to pending",instanceId);
  startModeSwitching(new Runnable(){
    @Override public void run(){
      haCommunicationLife.shutdown();
      haCommunicationLife=new LifeSupport();
    }
  }
,new CancellationHandle());
