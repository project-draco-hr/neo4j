{
  return new RaftLogCursor(){
    private long currentIndex=fromIndex - 1;
    RaftLogEntry current=null;
    @Override public boolean next() throws IOException {
      currentIndex++;
      boolean hasNext=currentIndex <= appendIndex;
      if (hasNext) {
        try {
          current=readLogEntry(currentIndex);
        }
 catch (        RaftLogCompactedException e) {
          throw new IOException(e);
        }
      }
 else {
        current=null;
      }
      return hasNext;
    }
    @Override public void close() throws IOException {
    }
    @Override public long index(){
      return currentIndex;
    }
    @Override public RaftLogEntry get(){
      return current;
    }
  }
;
}
