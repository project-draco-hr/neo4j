{
  int i=0;
  for (; i <= commitIndex; i++) {
    ReplicatedContent content=readEntryContent(i);
    for (    Listener listener : listeners) {
      listener.onAppended(content);
      listener.onCommitted(content);
    }
  }
  for (; i <= appendIndex; i++) {
    ReplicatedContent content=readEntryContent(i);
    for (    Listener listener : listeners) {
      listener.onAppended(content);
    }
  }
}
