{
  client.connect(address).send(TransportTestUtil.acceptedVersions(1,0,0,0)).send(TransportTestUtil.chunk(init("TestClient/1.1",map("principal","neo4j","credentials","wrong","scheme","basic"))));
  assertThat(client,eventuallyRecieves(new byte[]{0,0,0,1}));
  assertThat(client,eventuallyRecieves(msgFailure(Status.Security.AuthenticationFailed,String.format("The client provided an incorrect username and/or password. (ID:%s)",server.uniqueIdentier()))));
}
