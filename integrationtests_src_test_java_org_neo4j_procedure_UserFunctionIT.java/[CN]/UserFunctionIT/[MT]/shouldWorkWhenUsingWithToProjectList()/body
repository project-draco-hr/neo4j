{
  try (Transaction ignore=db.beginTx()){
    db.createNode(Label.label("Person"));
    db.createNode(Label.label("Person"));
    Result res=db.execute("MATCH (n:Person) WITH collect(n) as persons RETURN org.neo4j.procedure.nodeListArgument(persons)" + " AS someVal");
    assertThat(res.next().get("someVal"),equalTo(2L));
  }
 }
