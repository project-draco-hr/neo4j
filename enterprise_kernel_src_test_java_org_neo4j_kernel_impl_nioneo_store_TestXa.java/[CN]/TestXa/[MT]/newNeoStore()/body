{
  Map<Object,Object> config=new HashMap<Object,Object>();
  config.putAll(Config.getDefaultParams());
  MapUtil.genericMap(config,LockManager.class,lockManager,LockReleaser.class,lockReleaser,IdGeneratorFactory.class,ID_GENERATOR_FACTORY,FileSystemAbstraction.class,CommonFactories.defaultFileSystemAbstraction(),LogBufferFactory.class,CommonFactories.defaultLogBufferFactory(config),TxIdGenerator.class,TxIdGenerator.DEFAULT,"store_dir",path(),"neo_store",file("neo"),"logical_log",file("nioneo_logical.log"));
  return new NeoStoreXaDataSource(config);
}
