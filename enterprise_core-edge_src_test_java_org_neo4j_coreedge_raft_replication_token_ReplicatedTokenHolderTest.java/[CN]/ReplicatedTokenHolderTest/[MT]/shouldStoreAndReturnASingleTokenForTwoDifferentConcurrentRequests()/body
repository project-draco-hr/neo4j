{
  IdGeneratorFactory idGeneratorFactory=mock(IdGeneratorFactory.class);
  IdGenerator idGenerator=mock(IdGenerator.class);
  when(idGenerator.nextId()).thenReturn(1L);
  when(idGeneratorFactory.get(any(IdType.class))).thenReturn(idGenerator);
  StorageEngine storageEngine=mockedStorageEngine();
  when(dependencies.resolveDependency(StorageEngine.class)).thenReturn(storageEngine);
  RaceConditionSimulatingReplicator replicator=new RaceConditionSimulatingReplicator();
  ReplicatedTokenHolder<Token,LabelTokenRecord> tokenHolder=new ReplicatedLabelTokenHolder(replicator,idGeneratorFactory,dependencies,TIMEOUT_MILLIS);
  tokenHolder.setLastCommittedIndex(-1);
  tokenHolder.start();
  replicator.injectLabelTokenBeforeOtherOneReplicates(new ReplicatedTokenRequest(LABEL,"Dog",createCommandBytes(createCommands(INJECTED_TOKEN_ID))));
  int tokenId=tokenHolder.getOrCreateId("Person");
  assertEquals(EXPECTED_TOKEN_ID,tokenId);
}
