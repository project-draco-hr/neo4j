def test_get_first_index_hit(self):
    with self.graphdb.transaction:
        idx = self.graphdb.node.indexes.create('test')
        for x in range(50):
            idx['akey']['avalue'] = self.graphdb.node(name=x)
    hits = iter(idx['akey']['avalue'])
    node = hits.next()
    hits.close()
    self.assertEquals(0, node['name'])
