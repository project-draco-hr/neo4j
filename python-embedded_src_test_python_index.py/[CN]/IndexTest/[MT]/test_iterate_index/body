def test_iterate_index(self):
    with self.graphdb.transaction:
        idx = self.graphdb.node.indexes.create('test')
        for x in range(50):
            idx['akey']['avalue'] = self.graphdb.node(name=x)
    hits = []
    for hit in idx['akey']['avalue']:
        hits.append(hit)
    self.assertEquals(50, len(hits))
