{
  List<IndexableField> fields=document.getFields();
  for (  IndexableField field : fields) {
    if (!(LuceneIndex.KEY_DOC_ID.equals(field.name()) || LuceneIndex.KEY_END_NODE_ID.equals(field.name()) || LuceneIndex.KEY_START_NODE_ID.equals(field.name()))) {
      return false;
    }
  }
  return true;
}
