{
  List<IndexableField> fields=document.getFields();
  for (  IndexableField field : fields) {
    if (!(LuceneLegacyIndex.KEY_DOC_ID.equals(field.name()) || LuceneLegacyIndex.KEY_END_NODE_ID.equals(field.name()) || LuceneLegacyIndex.KEY_START_NODE_ID.equals(field.name()))) {
      return false;
    }
  }
  return true;
}
