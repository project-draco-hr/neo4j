{
  File storeDir=new File("dir");
  fs.get().mkdirs(storeDir);
  StoreFactory storeFactory=new StoreFactory(storeDir,new Config(),new DefaultIdGeneratorFactory(fs.get()),pageCacheRule.getPageCache(fs.get()),fs.get(),NullLogProvider.getInstance());
  neoStores=storeFactory.openNeoStores(true);
  nodeStore=neoStores.getNodeStore();
}
