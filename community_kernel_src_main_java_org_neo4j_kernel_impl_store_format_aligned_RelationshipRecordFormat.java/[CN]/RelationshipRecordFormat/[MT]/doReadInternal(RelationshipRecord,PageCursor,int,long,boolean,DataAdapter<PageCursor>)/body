{
  int type=cursor.getShort() & 0xFFFF;
  long recordId=record.getId();
  record.initialize(inUse,decode(cursor,adapter,headerByte,HAS_PROPERTY_BIT,NULL),decode(cursor,adapter),decode(cursor,adapter),type,toAbsolute(decode(cursor,adapter),recordId),toAbsolute(decode(cursor,adapter,headerByte,HAS_START_NEXT_BIT,NULL),recordId),toAbsolute(decode(cursor,adapter),recordId),toAbsolute(decode(cursor,adapter,headerByte,HAS_END_NEXT_BIT,NULL),recordId),has(headerByte,FIRST_IN_START_BIT),has(headerByte,FIRST_IN_END_BIT));
}
