{
  try {
    DocValuesCollector docValuesCollector=new DocValuesCollector();
    searcher.search(query,docValuesCollector);
    return new LongValuesIterator(docValuesCollector.getMatchingDocs(),docValuesCollector.getTotalHits(),NODE_ID_KEY);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
