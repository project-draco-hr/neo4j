{
  try {
    MasterTransaction tx=transactions.get(txId);
    if (tx.finishAsap()) {
      finishThisAndResumeOther(otherTx,txId,false);
      return;
    }
    tx.updateTime();
    txManager.suspend();
    if (otherTx != null) {
      txManager.resume(otherTx);
    }
  }
 catch (  Exception e) {
    throw Exceptions.launderedException(e);
  }
}
