{
  Config config=getGraphDbConfig();
  Integer id=config.getRelationshipTypeHolder().getIdFor(name);
  if (id != null) {
    return packResponse(context,id);
  }
  id=config.getRelationshipTypeCreator().getOrCreate(config.getTxModule().getTxManager(),config.getIdGeneratorModule().getIdGenerator(),config.getPersistenceModule().getPersistenceManager(),config.getRelationshipTypeHolder(),name);
  return packResponse(context,id);
}
