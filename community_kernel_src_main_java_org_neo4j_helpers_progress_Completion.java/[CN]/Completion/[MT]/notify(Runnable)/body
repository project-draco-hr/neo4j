{
  if (callback == null) {
    throw new IllegalArgumentException("callback may not be null");
  }
  Collection<Runnable> callbacks=this.callbacks;
  if (callbacks != null) {
synchronized (callbacks) {
      if (this.callbacks == callbacks) {
        callbacks.add(callback);
        callback=null;
      }
    }
  }
  if (callback != null) {
    callback.run();
  }
}
