{
  final String mainClassName=EmbeddedNeo4jApp.class.getName();
  ObservablePlatform platform=buildPlatform(forMainClass(named(mainClassName)),includingTargetClasses(),includingTargetTestClasses(),provisioning(mavenBundle().groupId("org.apache.geronimo.specs").artifactId("geronimo-jta_1.1_spec").version("1.1.1"),mavenBundle().groupId("org.neo4j").artifactId("neo4j-kernel").version("1.2-1.2.M01")));
  platform.start();
}
