{
  TargetDirectory dir=TargetDirectory.forTest(getClass());
  return (HighlyAvailableGraphDatabase)new EnterpriseGraphDatabaseFactory().newHighlyAvailableDatabaseBuilder(dir.directory(clusterName + "-" + serverId,true).getAbsolutePath()).setConfig(HaSettings.server_id,String.valueOf(serverId)).setConfig(HaSettings.coordinators,zoo.getConnectionString()).setConfig(HaSettings.cluster_name,clusterName).setConfig(HaSettings.server,"localhost:" + serverPort).setConfig(HaSettings.read_timeout,"2").newGraphDatabase();
}
