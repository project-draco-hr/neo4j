{
  applicationProcess.start();
  raftLog.append(new RaftLogEntry(0,new NewLeaderBarrier()));
  raftLog.append(new RaftLogEntry(0,new NewLeaderBarrier()));
  raftLog.append(new RaftLogEntry(0,new NewLeaderBarrier()));
  applicationProcess.notifyCommitted(2);
  applier.sync(false);
  assertEquals(2,applicationProcess.lastApplied());
}
