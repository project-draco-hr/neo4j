{
  String hostname=ServerUtil.getHostString(masterServer.getSocketAddress()).contains("0.0.0.0") ? me.getHost() : ServerUtil.getHostString(masterServer.getSocketAddress());
  int port=masterServer.getSocketAddress().getPort();
  InstanceId serverId=config.get(ClusterSettings.server_id);
  return URI.create("ha://" + hostname + ":"+ port+ "?serverId="+ serverId);
}
