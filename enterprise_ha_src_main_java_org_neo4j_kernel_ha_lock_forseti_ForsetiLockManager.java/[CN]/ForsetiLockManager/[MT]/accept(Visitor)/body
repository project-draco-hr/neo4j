{
  for (int i=0; i < lockMaps.length; i++) {
    if (lockMaps[i] != null) {
      ResourceType type=resourceTypes[i];
      for (      Map.Entry<Long,Lock> entry : lockMaps[i].entrySet()) {
        Lock lock=entry.getValue();
        out.visit(type,entry.getKey(),lock.describeWaitList(),0);
      }
    }
  }
}
