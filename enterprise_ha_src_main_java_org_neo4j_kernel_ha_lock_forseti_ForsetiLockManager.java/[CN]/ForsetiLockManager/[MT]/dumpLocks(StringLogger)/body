{
  for (int i=0; i < lockMaps.length; i++) {
    if (lockMaps[i] != null) {
      out.info("ResouceType[" + i + "]");
      out.info("==============");
      for (      Object raw : lockMaps[i].entrySet()) {
        Map.Entry<Long,Lock> entry=(Map.Entry<Long,Lock>)raw;
        out.info("Resource[" + entry.getKey() + "]: "+ entry.getValue().describeWaitList());
      }
    }
  }
}
