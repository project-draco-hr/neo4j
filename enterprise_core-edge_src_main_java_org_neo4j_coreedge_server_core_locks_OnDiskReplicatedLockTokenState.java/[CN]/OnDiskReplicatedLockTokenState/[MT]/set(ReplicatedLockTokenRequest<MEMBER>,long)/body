{
  InMemoryReplicatedLockTokenState<MEMBER> temp=new InMemoryReplicatedLockTokenState<>(inMemoryReplicatedLockTokenState);
  temp.set(request,logIndex);
  try {
    statePersister.persistStoreData(temp);
    inMemoryReplicatedLockTokenState=temp;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
