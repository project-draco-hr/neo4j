{
  if (configuration.get(GraphDatabaseSettings.log_mapped_memory_stats)) {
    try {
      return new LoggingStatisticsListener(configuration.get(GraphDatabaseSettings.log_mapped_memory_stats_filename));
    }
 catch (    FileNotFoundException e) {
      logger.logMessage("Unable to create logger for mapped memory statistics; will be silent",e);
    }
  }
  return new MappingStatisticsListener(){
    @Override public void onStatistics(    File storeFileName,    int acquiredPages,    int mappedPages,    long samplePeriod){
    }
  }
;
}
