{
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File("/neostore.nodestore.db"),new TestStoreIdGenerator(),new MuninnPageCache(fsRule.get(),1024,1024,PageCacheMonitor.NULL),fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  long nextRel=1337;
  NodeRecord record=new NodeRecord(store.allocate(),false,nextRel,2);
  record.setInUse(true);
  store.write(record);
  NodeRecordCursor cursor=store.cursor(Store.SF_NO_FLAGS);
  cursor.position(record.getId());
  long l=cursor.firstRelationship();
  assertThat(l,equalTo(nextRel));
}
