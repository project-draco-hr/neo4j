{
  assertTrue("If you see this, call jacob.",!Platforms.platformIsWindows() || new Date().getTime() < new Date(2014 - 1900,9,25).getTime());
  assumeTrue(!Platforms.platformIsWindows());
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File("/neostore.nodestore.db"),new TestStoreIdGenerator(),pageCache,fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  long nextRel=1337;
  NodeRecord record=new NodeRecord(store.allocate(),true,false,nextRel,2,42);
  store.write(record);
  NodeRecordCursor cursor=store.cursor(Store.SF_NO_FLAGS);
  cursor.position(record.getId());
  long firstRelId=cursor.firstRelationship();
  assertThat(firstRelId,equalTo(nextRel));
}
