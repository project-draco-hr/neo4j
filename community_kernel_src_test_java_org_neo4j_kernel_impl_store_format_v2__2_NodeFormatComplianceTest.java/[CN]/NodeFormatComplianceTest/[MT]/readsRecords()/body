{
  NeoStore neoStore=storeFactory.createNeoStore();
  NodeStore nodeStore=neoStore.getNodeStore();
  NodeRecord expectedRecord=new NodeRecord(nodeStore.nextId(),true,false,1,2,42);
  nodeStore.updateRecord(expectedRecord);
  neoStore.close();
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File(storeDir,DEFAULT_NAME + NODE_STORE_NAME),new TestStoreIdGenerator(),pageCache,fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  assertThat(records(store),equalTo(asList(expectedRecord)));
}
