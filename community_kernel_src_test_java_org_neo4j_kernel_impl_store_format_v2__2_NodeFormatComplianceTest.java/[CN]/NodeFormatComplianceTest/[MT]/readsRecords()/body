{
  NeoStore neoStore=storeFactory.createNeoStore();
  NodeStore nodeStore=neoStore.getNodeStore();
  NodeRecord expectedRecord=new NodeRecord(nodeStore.nextId(),false,1,2);
  expectedRecord.setInUse(true);
  nodeStore.updateRecord(expectedRecord);
  neoStore.close();
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File("/neostore.nodestore.db"),new TestStoreIdGenerator(),new MuninnPageCache(fsRule.get(),1024,1024,PageCacheMonitor.NULL),fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  assertThat(records(store),equalTo(asList(expectedRecord)));
}
