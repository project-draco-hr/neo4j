{
  storeFactory.createNeoStore().close();
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File("/neostore.nodestore.db"),new TestStoreIdGenerator(),new MuninnPageCache(fsRule.get(),1024,1024,PageCacheMonitor.NULL),fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  NodeRecord expectedRecord=new NodeRecord(store.allocate(),false,1,2);
  expectedRecord.setInUse(true);
  store.write(expectedRecord);
  store.stop();
  store.shutdown();
  NodeStore nodeStore=storeFactory.newNodeStore();
  NodeRecord record=nodeStore.getRecord(expectedRecord.getId());
  assertThat(record,equalTo(expectedRecord));
}
