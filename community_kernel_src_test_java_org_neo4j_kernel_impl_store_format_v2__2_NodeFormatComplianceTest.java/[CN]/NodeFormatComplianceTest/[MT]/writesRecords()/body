{
  storeFactory.createNeoStore().close();
  StandardStore<NodeRecord,NodeRecordCursor> store=new StandardStore<>(new NodeStoreFormat_v2_2(),new File(storeDir,DEFAULT_NAME + NODE_STORE_NAME),new TestStoreIdGenerator(),pageCache,fsRule.get(),StringLogger.DEV_NULL);
  store.init();
  store.start();
  NodeRecord expectedRecord=new NodeRecord(store.allocate(),true,false,1,2,42);
  store.write(expectedRecord);
  store.stop();
  store.shutdown();
  NodeStore nodeStore=storeFactory.newNodeStore();
  NodeRecord record=nodeStore.getRecord(expectedRecord.getId());
  assertThat(record,equalTo(expectedRecord));
}
