{
  try {
    Store.createStore(storeFile()).close();
    java.nio.channels.FileChannel fileChannel=new java.io.RandomAccessFile(storeFile(),"rw").getChannel();
    fileChannel.truncate(fileChannel.size() - 2);
    fileChannel.close();
    Store store=new Store(storeFile());
    store.makeStoreOk();
    store.close();
  }
  finally {
    deleteBothFiles();
  }
}
