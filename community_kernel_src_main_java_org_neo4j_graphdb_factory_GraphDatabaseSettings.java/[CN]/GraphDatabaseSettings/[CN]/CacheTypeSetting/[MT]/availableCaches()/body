{
  List<String> available=new ArrayList<String>();
  for (  CacheProvider cacheProvider : Service.load(CacheProvider.class))   available.add(cacheProvider.getName());
  for (  String prioritized : new String[]{"soft","gcr"})   if (available.remove(prioritized))   available.add(0,prioritized);
  return available.toArray(new String[0]);
}
