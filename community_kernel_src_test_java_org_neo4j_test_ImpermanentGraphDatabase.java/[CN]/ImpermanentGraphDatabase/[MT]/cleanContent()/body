{
  Transaction tx=inner.beginTx();
  try {
    for (    Node node : inner.getAllNodes()) {
      for (      Relationship rel : node.getRelationships(Direction.OUTGOING)) {
        rel.delete();
      }
      if (!node.hasRelationship()) {
        node.delete();
      }
    }
    tx.success();
  }
 catch (  Exception e) {
    tx.failure();
  }
 finally {
    tx.finish();
  }
}
