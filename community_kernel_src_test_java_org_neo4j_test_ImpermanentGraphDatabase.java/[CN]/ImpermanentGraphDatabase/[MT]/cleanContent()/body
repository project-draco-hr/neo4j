{
  Transaction tx=beginTx();
  try {
    Iterable<Node> allNodes=GlobalGraphOperations.at(this).getAllNodes();
    for (    Node node : allNodes) {
      for (      Relationship rel : node.getRelationships(Direction.OUTGOING)) {
        rel.delete();
      }
      node.delete();
    }
    tx.success();
  }
 catch (  Exception e) {
    tx.failure();
  }
 finally {
    tx.finish();
  }
}
