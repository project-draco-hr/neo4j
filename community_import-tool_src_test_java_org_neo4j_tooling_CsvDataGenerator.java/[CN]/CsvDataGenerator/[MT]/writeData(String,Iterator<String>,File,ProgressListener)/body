{
  System.out.println("Writing " + file.getAbsolutePath());
  try (Writer out=new BufferedWriter(new FileWriter(file),102 * 1024 * 10)){
    out.write(header);
    out.append('\n');
    while (iterator.hasNext()) {
      out.write(iterator.next());
      out.append('\n');
      progress.add(1);
    }
  }
 }
