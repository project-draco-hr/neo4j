{
  if (nextPageId >= lastPageId && (pf_flags & PF_NO_GROW) != 0) {
    return false;
  }
  if (page != null) {
    pagedFile.unpin(this);
  }
  try {
    pagedFile.pin(this,getLockType(pf_flags),nextPageId);
    pageIO.apply(nextPageId,page,io_context,io_flags);
  }
 catch (  IOException e) {
    if (page != null) {
      pagedFile.unpin(this);
    }
    throw e;
  }
  nextPageId++;
  return true;
}
