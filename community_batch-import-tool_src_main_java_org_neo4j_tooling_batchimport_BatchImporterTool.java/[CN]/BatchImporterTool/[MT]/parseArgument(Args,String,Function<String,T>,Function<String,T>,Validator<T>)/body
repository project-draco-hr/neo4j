{
  T value;
  if (!args.has(key)) {
    value=defaultValue.apply(key);
  }
 else {
    String stringValue=args.get(key);
    value=converter.apply(stringValue);
  }
  if (value != null) {
    for (    Validator<T> validator : validators) {
      validator.validate(value);
    }
  }
  return value;
}
