{
  StandardEnterpriseAuthSubject enterpriseSubject=StandardEnterpriseAuthSubject.castOrFail(authSubject);
  EnterpriseUserManager userManager=enterpriseSubject.getUserManager();
  return Stream.of(new UserResult(enterpriseSubject.username(),userManager.getRoleNamesForUser(enterpriseSubject.username()),userManager.getUser(enterpriseSubject.username()).getFlags()));
}
