{
  for (int i=0; i < processors.length; i++) {
    StoreProcessor processor=processors[i];
    beforeProcessing(processor);
    try {
      processor.applyFiltered(store,progressListeners[i]);
    }
 catch (    Throwable e) {
      progressListeners[i].failed(e);
    }
 finally {
      afterProcessing(processor);
    }
  }
}
