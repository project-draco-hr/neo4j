{
  UserSerialization serialization=new UserSerialization();
  List<User> users=asList(new User("Steve","12345",Privileges.ADMIN,new Credentials("SomeSalt","SomeAlgo","1234321"),false),new User("Bob","54321",Privileges.ADMIN,new Credentials("OtherSalt","OtherAlgo","0987654"),false));
  byte[] serialized=serialization.serialize(users);
  assertThat(serialization.deserializeUsers(serialized),equalTo(users));
}
