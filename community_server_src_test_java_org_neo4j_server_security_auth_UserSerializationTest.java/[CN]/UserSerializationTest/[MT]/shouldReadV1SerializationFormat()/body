{
  UserSerialization serialization=new UserSerialization();
  List<User> deserialized=serialization.deserializeUsers(("Steve:12345:SomeAlgo,1234321,SomeSalt:false\n" + "Bob:54321:OtherAlgo,0987654,OtherSalt:false").getBytes(Charsets.UTF_8));
  assertThat(deserialized,equalTo(asList(new User("Steve","12345",Privileges.ADMIN,new Credentials("SomeSalt","SomeAlgo","1234321"),false),new User("Bob","54321",Privileges.ADMIN,new Credentials("OtherSalt","OtherAlgo","0987654"),false))));
}
