{
  StringBuilder sql=new StringBuilder(100);
  String fieldList=String.valueOf(Arrays.asList(fields));
  fieldList=fieldList.substring(1,fieldList.length() - 1);
  String valueList=new String(new char[fields.length - 1]).replace("\0",", ?");
  sql.append("INSERT INTO ").append(identifierQuoteString).append(tableName).append(identifierQuoteString).append(" (").append(fieldList).append(") VALUES (?").append(valueList).append(")");
  return sqldb.prepareStatement(sql.toString());
}
