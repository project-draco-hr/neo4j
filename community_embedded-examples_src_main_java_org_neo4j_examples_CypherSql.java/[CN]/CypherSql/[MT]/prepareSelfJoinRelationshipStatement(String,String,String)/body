{
  StringBuilder sql=new StringBuilder(100);
  sql.append("UPDATE ").append(identifierQuoteString).append(entity).append(identifierQuoteString).append(" SET ").append(targetAttribute).append(" = (SELECT ").append(identifierQuoteString).append(entity).append(identifierQuoteString).append(".id FROM ").append(identifierQuoteString).append(entity).append(identifierQuoteString).append(" WHERE ").append(identifierQuoteString).append(entity).append(identifierQuoteString).append(".").append(matchAttribute).append(" =? ) WHERE ").append(matchAttribute).append("=?");
  return sqldb.prepareStatement(sql.toString());
}
