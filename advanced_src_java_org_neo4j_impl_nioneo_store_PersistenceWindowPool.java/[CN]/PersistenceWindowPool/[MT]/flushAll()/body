{
synchronized (activeRowWindows) {
    for (    Set windowSet : txIdentifiers.values()) {
      if (windowSet != null) {
        Iterator itr=windowSet.iterator();
        while (itr.hasNext()) {
          ((MappedPersistenceWindow)itr.next()).force();
        }
      }
    }
  }
  fileChannel.force(false);
}
