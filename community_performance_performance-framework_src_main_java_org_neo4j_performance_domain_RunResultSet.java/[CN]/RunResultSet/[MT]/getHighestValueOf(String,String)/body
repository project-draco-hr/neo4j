{
  BenchmarkResult.Metric highest=null;
  RunResult bestRun=null;
  for (  RunResult result : results) {
    BenchmarkResult.Metric current=result.getMetric(caseName,metricName);
    if (current != null && (highest == null || current.compareTo(highest) < 0)) {
      highest=current;
      bestRun=result;
    }
  }
  return Pair.of(highest,bestRun);
}
