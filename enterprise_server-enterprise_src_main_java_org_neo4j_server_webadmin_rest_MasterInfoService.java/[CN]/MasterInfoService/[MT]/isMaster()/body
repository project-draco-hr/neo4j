{
  if (haDb == null) {
    return Response.status(Response.Status.FORBIDDEN).build();
  }
  if (haDb.isMaster()) {
    return Response.status(Response.Status.OK).entity(Boolean.toString(true).getBytes()).build();
  }
 else {
    return Response.status(Response.Status.SEE_OTHER).entity(Boolean.toString(false).getBytes()).header(HttpHeaders.LOCATION,getMasterUriAsString()).build();
  }
}
