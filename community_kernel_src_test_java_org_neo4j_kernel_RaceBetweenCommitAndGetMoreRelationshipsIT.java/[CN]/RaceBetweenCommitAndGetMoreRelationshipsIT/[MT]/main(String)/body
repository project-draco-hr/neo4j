{
  String path="target/commit-getMore-race";
  if (args != null && args.length >= 1) {
    path=args[0];
  }
  delete(new File(path));
  GraphDatabaseAPI graphdb=(GraphDatabaseAPI)new GraphDatabaseFactory().newEmbeddedDatabase(path);
  RaceBetweenCommitAndGetMoreRelationshipsIT race=instance=new RaceBetweenCommitAndGetMoreRelationshipsIT(graphdb,graphdb.getDependencyResolver().resolveDependency(NodeManager.class));
  try {
    race.execute();
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
 finally {
    graphdb.shutdown();
  }
}
