{
  EphemeralFileSystemAbstraction fsa=new EphemeralFileSystemAbstraction();
  fsa.mkdir(testDir.directory());
  StatePersister<AtomicInteger> statePersister=new StatePersister<>(stateFileA(),stateFileB(),fsa,100,new AtomicIntegerMarshal(),stateFileA(),mock(Supplier.class));
  statePersister.persistStoreData(new AtomicInteger(99));
  assertEquals(4,fsa.getFileSize(stateFileA()));
}
