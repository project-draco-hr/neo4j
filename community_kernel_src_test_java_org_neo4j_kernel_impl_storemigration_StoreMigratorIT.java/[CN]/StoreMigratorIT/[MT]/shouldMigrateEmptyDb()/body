{
  File legacyStoreDir=Unzip.unzip(LegacyStore.class,"emptydb.zip");
  new StoreMigrator(monitor).migrate(new LegacyStore(fs,new File(legacyStoreDir,NeoStore.DEFAULT_NAME)),storeFactory.createNeoStore(storeFileName));
