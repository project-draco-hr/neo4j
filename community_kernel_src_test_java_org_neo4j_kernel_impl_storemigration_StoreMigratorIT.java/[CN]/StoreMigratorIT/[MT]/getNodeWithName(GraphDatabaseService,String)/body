{
  Transaction tx=db.beginTx();
  try {
    for (    Node node : GlobalGraphOperations.at(db).getAllNodes()) {
      if (name.equals(node.getProperty("name",null))) {
        tx.success();
        return node;
      }
    }
  }
  finally {
    tx.finish();
  }
  throw new IllegalArgumentException(name + " not found");
}
