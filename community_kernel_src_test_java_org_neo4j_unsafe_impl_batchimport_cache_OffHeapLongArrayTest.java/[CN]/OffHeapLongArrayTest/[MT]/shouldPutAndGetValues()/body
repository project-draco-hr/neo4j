{
  int size=100;
  LongArray array=new OffHeapLongArray(size,-1);
  for (int i=0; i < size; i++) {
    assertEquals(-1L,array.get(i));
  }
  array.set(10,100);
  array.set(0,21);
  array.set(99,349389);
  assertEquals(100L,array.get(10));
  assertEquals(21L,array.get(0));
  assertEquals(349389L,array.get(99));
}
