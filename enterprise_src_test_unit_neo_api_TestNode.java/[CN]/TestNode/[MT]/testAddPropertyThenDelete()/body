{
  try {
    UserTransaction ut=TransactionFactory.getUserTransaction();
    Node node=NodeManager.getManager().createNode();
    node.setProperty("test","test");
    ut.commit();
    ut.begin();
    node.setProperty("test2","test2");
    node.delete();
    ut.commit();
    ut.begin();
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail("" + e);
  }
}
