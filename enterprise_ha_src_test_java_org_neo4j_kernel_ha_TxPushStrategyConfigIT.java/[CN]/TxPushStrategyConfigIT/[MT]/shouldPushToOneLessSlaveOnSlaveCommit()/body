{
  startCluster(4,2,"fixed");
  createTransactionOn(new InstanceId(FIRST_SLAVE));
  assertLastTransactions(lastTx(MASTER,BASE_TX_ID + 1),lastTx(FIRST_SLAVE,BASE_TX_ID + 1),lastTx(SECOND_SLAVE,BASE_TX_ID),lastTx(THIRD_SLAVE,BASE_TX_ID + 1));
  createTransactionOn(new InstanceId(SECOND_SLAVE));
  assertLastTransactions(lastTx(MASTER,BASE_TX_ID + 2),lastTx(FIRST_SLAVE,BASE_TX_ID + 1),lastTx(SECOND_SLAVE,BASE_TX_ID + 2),lastTx(THIRD_SLAVE,BASE_TX_ID + 2));
  createTransactionOn(new InstanceId(THIRD_SLAVE));
  assertLastTransactions(lastTx(MASTER,BASE_TX_ID + 3),lastTx(FIRST_SLAVE,BASE_TX_ID + 1),lastTx(SECOND_SLAVE,BASE_TX_ID + 3),lastTx(THIRD_SLAVE,BASE_TX_ID + 3));
}
