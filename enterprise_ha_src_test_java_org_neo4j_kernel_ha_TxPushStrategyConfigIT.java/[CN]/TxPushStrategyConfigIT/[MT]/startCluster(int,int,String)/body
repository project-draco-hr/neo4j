{
  ClusterManager clusterManager=life.add(new ClusterManager(clusterOfSize(memberCount),dir.cleanDirectory(name.getMethodName()),stringMap()){
    @Override protected void config(    GraphDatabaseBuilder builder,    String clusterName,    int serverId){
      builder.setConfig(HaSettings.tx_push_factor,"" + pushFactor);
      builder.setConfig(HaSettings.tx_push_strategy,pushStrategy);
    }
  }
);
  life.start();
  cluster=clusterManager.getDefaultCluster();
  cluster.await(allSeesAllAsAvailable());
  mapMachineIds();
}
