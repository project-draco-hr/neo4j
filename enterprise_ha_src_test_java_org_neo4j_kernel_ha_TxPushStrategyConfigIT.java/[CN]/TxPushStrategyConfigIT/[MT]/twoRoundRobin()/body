{
  startCluster(4,2,"round_robin");
  createTransactionOnMaster();
  assertLastTransactions(lastTx(FIRST_SLAVE,BASE_TX_ID + 1),lastTx(SECOND_SLAVE,BASE_TX_ID + 1),lastTx(THIRD_SLAVE,BASE_TX_ID));
  createTransactionOnMaster();
  assertLastTransactions(lastTx(FIRST_SLAVE,BASE_TX_ID + 1),lastTx(SECOND_SLAVE,BASE_TX_ID + 2),lastTx(THIRD_SLAVE,BASE_TX_ID + 2));
  createTransactionOnMaster();
  assertLastTransactions(lastTx(FIRST_SLAVE,BASE_TX_ID + 3),lastTx(SECOND_SLAVE,BASE_TX_ID + 2),lastTx(THIRD_SLAVE,BASE_TX_ID + 3));
}
