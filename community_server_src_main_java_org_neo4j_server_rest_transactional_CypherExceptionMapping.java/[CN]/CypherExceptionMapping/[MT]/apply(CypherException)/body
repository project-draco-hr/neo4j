{
  if (ParameterNotFoundException.class.isInstance(e))   return StatusCode.STATEMENT_MISSING_PARAMETER;
  if (SyntaxException.class.isInstance(e))   return StatusCode.STATEMENT_SYNTAX_ERROR;
  if (InternalException.class.isInstance(e))   return StatusCode.INTERNAL_STATEMENT_EXECUTION_ERROR;
  return StatusCode.STATEMENT_EXECUTION_ERROR;
}
