{
  StoreFactory storeFactory=new StoreFactory(new Config(),new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),(fs=new EphemeralFileSystemAbstraction()),StringLogger.SYSTEM,new DefaultTxHook());
  File storeFile=new File("store");
  storeFactory.createNodeStore(storeFile);
  nodeStore=storeFactory.newNodeStore(storeFile);
}
