{
  int blockCount=(int)(headerByte >>> 4);
  long recordId=record.getId();
  record.initialize(inUse,toAbsolute(Reference.decode(cursor,PAGE_CURSOR_ADAPTER),recordId),toAbsolute(Reference.decode(cursor,PAGE_CURSOR_ADAPTER),recordId));
  while (blockCount-- > 0) {
    record.addLoadedBlock(cursor.getLong());
  }
}
