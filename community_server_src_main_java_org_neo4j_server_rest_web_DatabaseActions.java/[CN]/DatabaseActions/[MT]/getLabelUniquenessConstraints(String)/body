{
  Transaction tx=database.getGraph().beginTx();
  try {
    ListRepresentation result=new ListRepresentation(CONSTRAINT_DEFINITION,map(CONSTRAINT_DEF_TO_REPRESENTATION,filteredConstraints(labelName,ConstraintType.UNIQUENESS)));
    tx.success();
    return result;
  }
  finally {
    tx.finish();
  }
}
