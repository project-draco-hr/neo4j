{
  final Set<String> propertyKeysSet=asSet(propertyKeys);
  Transaction tx=database.getGraph().beginTx();
  try {
    ConstraintDefinition constraint=singleOrNull(filteredConstraints(labelName,propertyUniquenessFilter(propertyKeysSet)));
    if (constraint != null)     constraint.drop();
    tx.success();
    return constraint != null;
  }
  finally {
    tx.finish();
  }
}
