{
  NodeUpdateCollectingVisitor visitor=new NodeUpdateCollectingVisitor();
  @SuppressWarnings("unchecked") Visitor<NodeLabelUpdate,Exception> labelVisitor=mock(Visitor.class);
  StoreScan<Exception> storeScan=storeView.visitNodes(new int[]{labelId},new int[]{propertyKeyId},visitor,labelVisitor);
  storeScan.run();
  assertEquals(asSet(NodePropertyUpdate.add(alistair.getId(),propertyKeyId,"Alistair",new long[]{labelId}),NodePropertyUpdate.add(stefan.getId(),propertyKeyId,"Stefan",new long[]{labelId})),visitor.getUpdates());
}
