{
  if (cmd == null || cmd.equals("q")) {
    return false;
  }
 else   if (cmd.equals("h")) {
    printHelp();
  }
 else   if (cmd.equals("l")) {
    listFiles(neoStore);
  }
 else   if (cmd.startsWith("r")) {
    read(cmd,neoStore);
  }
 else   if (!cmd.trim().isEmpty()) {
    console.printf("unrecognized command%n");
  }
  return true;
}
