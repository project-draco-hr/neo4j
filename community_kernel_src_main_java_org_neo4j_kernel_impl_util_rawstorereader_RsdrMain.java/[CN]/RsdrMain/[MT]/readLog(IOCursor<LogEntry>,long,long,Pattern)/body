{
  TimeZone timeZone=TimeZone.getDefault();
  long lineCount=-1;
  while (cursor.next()) {
    LogEntry logEntry=cursor.get();
    lineCount++;
    if (lineCount > toLine) {
      return;
    }
    if (lineCount < fromLine) {
      continue;
    }
    String str=logEntry.toString(timeZone);
    if (pattern == null || pattern.matcher(str).find()) {
      console.printf("%s%n",str);
    }
  }
}
