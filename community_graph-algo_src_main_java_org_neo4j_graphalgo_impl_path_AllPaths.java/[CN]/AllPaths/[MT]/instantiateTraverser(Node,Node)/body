{
  GraphDatabaseService db=start.getGraphDatabase();
  TraversalDescription base=db.traversalDescription().depthFirst().uniqueness(uniqueness());
  return bidirectionalTraversal().startSide(base.expand(expander).evaluator(toDepth(maxDepth / 2))).endSide(base.expand(expander.reverse()).evaluator(toDepth(maxDepth - maxDepth / 2))).traverse(start,end);
}
