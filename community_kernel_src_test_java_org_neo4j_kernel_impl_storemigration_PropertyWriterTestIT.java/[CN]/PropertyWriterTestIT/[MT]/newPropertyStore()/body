{
  Config config=MigrationTestUtils.defaultConfig();
  File outputDir=new File("target/outputDatabase");
  FileUtils.deleteRecursively(outputDir);
  assertTrue(outputDir.mkdirs());
  File fileName=new File(outputDir,"neostore");
  StoreFactory storeFactory=new StoreFactory(config,defaultIdGeneratorFactory(),defaultFileSystemAbstraction(),defaultLastCommittedTxIdSetter(),StringLogger.DEV_NULL,defaultTxHook());
  neoStore=storeFactory.createNeoStore(fileName.getAbsolutePath());
  return neoStore.getPropertyStore();
}
