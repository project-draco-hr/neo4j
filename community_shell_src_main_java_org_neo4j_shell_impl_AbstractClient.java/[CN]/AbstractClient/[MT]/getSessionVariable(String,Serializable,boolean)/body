{
  try {
    Serializable result=this.session().get(key);
    if (result == null) {
      result=defaultValue;
    }
    if (interpret && result != null) {
      result=this.getServer().interpretVariable(key,result,session());
    }
    return result;
  }
 catch (  RemoteException e) {
    throw new RuntimeException(e);
  }
}
