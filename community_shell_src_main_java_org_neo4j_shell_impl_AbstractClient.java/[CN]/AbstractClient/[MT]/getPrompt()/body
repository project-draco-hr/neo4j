{
  String result=null;
  try {
    result=(String)getSessionVariable(PROMPT_KEY,null,true);
  }
 catch (  Exception e) {
    try {
      result=(String)getSessionVariable(PROMPT_KEY,null,false);
    }
 catch (    ShellException e1) {
      throw new RuntimeException(e1);
    }
  }
  return result;
}
