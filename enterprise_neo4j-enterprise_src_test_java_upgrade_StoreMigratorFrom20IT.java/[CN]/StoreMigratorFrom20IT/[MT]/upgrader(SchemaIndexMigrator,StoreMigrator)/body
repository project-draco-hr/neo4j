{
  Map<String,String> params=config.getParams();
  params.put(GraphDatabaseSettings.allow_store_upgrade.name(),"true");
  Config config=new Config(params);
  StoreUpgrader upgrader=new StoreUpgrader(upgradableDatabase,monitor,config,fs,NullLogProvider.getInstance());
  upgrader.addParticipant(indexMigrator);
  upgrader.addParticipant(storeMigrator);
  return upgrader;
}
