{
  try {
    Object[] inputArray=new Object[1];
    inputArray[0]="newPassword";
    dbmsOperations(AccessMode.Static.NONE).procedureCallDbms(procedureName("dbms","security","changePassword"),inputArray);
    fail("Should have failed.");
  }
 catch (  Exception e) {
    assertThat(e.getClass(),equalTo(ProcedureException.class));
  }
}
