{
  Client client=Client.create();
  WebResource createResource=client.resource(functionalTestHelper.dataUri() + "node/");
  String entity=JsonHelper.createJsonFrom(Collections.singletonMap("foo","bar"));
  ClientResponse createResponse=createResource.type(MediaType.APPLICATION_JSON).entity(entity).accept(MediaType.APPLICATION_JSON).post(ClientResponse.class);
  doc.doRequest("Get properties for Node","GET",createResponse.getLocation().toString() + "/properties",Response.Status.OK);
}
