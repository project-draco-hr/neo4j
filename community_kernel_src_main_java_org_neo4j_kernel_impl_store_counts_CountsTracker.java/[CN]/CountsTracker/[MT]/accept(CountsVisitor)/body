{
  try {
    visitAll(new Visitor(){
      @Override protected boolean visitKeyValuePair(      CountsKey key,      ReadableBuffer value){
        key.accept(visitor,value.getLong(0),value.getLong(8));
        return true;
      }
    }
);
  }
 catch (  IOException e) {
    throw new UnderlyingStorageException(e);
  }
}
