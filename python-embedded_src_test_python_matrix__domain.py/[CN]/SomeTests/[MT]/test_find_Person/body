def test_find_Person(self):
    with self.graphdb.transaction:
        Person(self.graphdb, name='Thomas Anderson', birthday=datetime.datetime(1978, 5, 21))
        Person(self.graphdb, name='Thomas Olsson', birthday=datetime.datetime(1978, 5, 21))
    neo = Person.filter(self.graphdb, name='Thomas Anderson').single
    self.assertFalse((neo is None))
    self.assertEqual('Thomas Anderson', neo.name)
    self.assertEqual(datetime.datetime(1978, 5, 21), neo.birthday)
