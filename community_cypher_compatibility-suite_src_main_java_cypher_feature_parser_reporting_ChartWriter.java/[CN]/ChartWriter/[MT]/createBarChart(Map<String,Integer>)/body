{
  if (cached != null) {
    return cached;
  }
  JFreeChart chart=ChartFactory.createBarChart("TCK tag distribution","Tags","Occurrences in queries",createCategoryDataset(data));
  CategoryPlot plot=(CategoryPlot)chart.getPlot();
  plot.setBackgroundPaint(Color.lightGray);
  plot.setDomainGridlinePaint(Color.white);
  plot.setDomainGridlinesVisible(true);
  plot.setRangeGridlinePaint(Color.white);
  plot.addRangeMarker(new ValueMarker(20,Color.BLACK,new BasicStroke(2)));
  BarRenderer renderer=(BarRenderer)plot.getRenderer();
  renderer.setSeriesPaint(0,ChartColor.DARK_RED);
  renderer.setBarPainter(new StandardBarPainter());
  CategoryAxis domainAxis=plot.getDomainAxis();
  domainAxis.setCategoryLabelPositions(CategoryLabelPositions.createUpRotationLabelPositions(Math.PI / 6.0));
  cached=chart;
  return chart;
}
