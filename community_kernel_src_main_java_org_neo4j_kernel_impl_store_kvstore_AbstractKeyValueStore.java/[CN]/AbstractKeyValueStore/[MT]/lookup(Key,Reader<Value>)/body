{
  ValueLookup<Value> lookup=new ValueLookup<>(reader);
  while (true) {
    ProgressiveState<Key> originalState=this.state;
    try {
      return lookup.value(!this.state.lookup(key,lookup));
    }
 catch (    IllegalStateException e) {
      if (originalState == this.state) {
        throw e;
      }
    }
  }
}
