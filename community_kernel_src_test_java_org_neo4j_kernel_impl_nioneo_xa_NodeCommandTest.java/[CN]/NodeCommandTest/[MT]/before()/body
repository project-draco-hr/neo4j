{
  File dir=new File("dir");
  fs.get().mkdirs(dir);
  Config config=StoreFactory.configForStoreDir(new Config(),dir);
  @SuppressWarnings("deprecation") StoreFactory storeFactory=new StoreFactory(config,new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),fs.get(),StringLogger.DEV_NULL);
  storeFactory.createNodeStore();
  nodeStore=storeFactory.newNodeStore();
}
