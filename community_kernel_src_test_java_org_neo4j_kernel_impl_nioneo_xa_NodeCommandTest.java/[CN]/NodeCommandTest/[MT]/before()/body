{
  fs=new EphemeralFileSystemAbstraction();
  StoreFactory storeFactory=new StoreFactory(new Config(),new DefaultIdGeneratorFactory(),new DefaultWindowPoolFactory(),fs,StringLogger.DEV_NULL,new DefaultTxHook());
  File storeFile=new File("nodestore");
  storeFactory.createNodeStore(storeFile);
  nodeStore=storeFactory.newNodeStore(storeFile);
}
