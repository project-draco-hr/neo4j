{
  try {
    if (startNodeId == (int)node.getId()) {
      return nodeManager.getNodeById(endNodeId);
    }
    if (endNodeId == (int)node.getId()) {
      return nodeManager.getNodeById(startNodeId);
    }
  }
 catch (  NotFoundException e) {
    throw new RuntimeException(e);
  }
  return null;
}
