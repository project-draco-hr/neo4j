{
  Machine machine=new Machine();
  PackStream.Packer packer=machine.packer();
  packer.pack("name");
  packer.pack(asList(1,2));
  packer.flush();
  PackStream.Unpacker unpacker=newUnpacker(machine.output());
  assertTrue(unpacker.hasNext());
  unpacker.unpack();
  assertTrue(unpacker.hasNext());
  unpacker.unpack();
  assertFalse(unpacker.hasNext());
}
