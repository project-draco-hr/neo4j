{
  try {
    actions.removeFromNodeIndex(indexName,key,value,id);
    return nothing();
  }
 catch (  UnsupportedOperationException e) {
    return output.methodNotAllowed(e);
  }
catch (  NotFoundException nfe) {
    return output.notFound(nfe);
  }
catch (  Exception e) {
    return output.serverError(e);
  }
}
