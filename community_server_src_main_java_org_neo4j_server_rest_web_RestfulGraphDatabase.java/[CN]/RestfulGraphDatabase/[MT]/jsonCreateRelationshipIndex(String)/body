{
  try {
    return output.created(actions.createRelationshipIndex(input.readMap(json)));
  }
 catch (  BadInputException e) {
    return output.badRequest(e);
  }
catch (  IllegalArgumentException e) {
    return output.badRequest(e);
  }
catch (  Exception e) {
    return output.serverError(e);
  }
}
