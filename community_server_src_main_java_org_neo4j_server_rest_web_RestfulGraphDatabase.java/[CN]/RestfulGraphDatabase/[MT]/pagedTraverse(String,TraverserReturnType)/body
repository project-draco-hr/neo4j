{
  try {
    String responseBody=output.format(server.pagedTraverse(traverserId,returnType));
    URI uri=new URI(uriInfo.getBaseUri().toString() + PATH_TO_PAGED_TRAVERSERS + "/"+ traverserId);
    return Response.ok(uri.normalize()).entity(responseBody).build();
  }
 catch (  NotFoundException e) {
    return output.notFound(e);
  }
catch (  URISyntaxException e) {
    return output.serverError(e);
  }
}
