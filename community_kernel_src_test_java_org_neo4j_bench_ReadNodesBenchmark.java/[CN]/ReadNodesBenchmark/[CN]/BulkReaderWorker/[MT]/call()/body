{
  int iterateNode=0, exceptions=0;
  Transaction tx=graphDb.beginTx();
  try {
    for (int j=0; j < 10000000; j++) {
      if (iterator.hasNext()) {
        iterator.next();
        iterateNode++;
      }
    }
  }
  finally {
    tx.finish();
  }
  return new ReaderResultsSample(iterateNode,exceptions);
}
