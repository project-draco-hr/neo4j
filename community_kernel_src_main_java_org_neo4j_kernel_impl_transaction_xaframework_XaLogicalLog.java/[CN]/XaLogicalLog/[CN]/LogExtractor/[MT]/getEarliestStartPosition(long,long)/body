{
  TxPosition earliest=null;
  for (long txId=startTxId; txId <= endTxIdHint; txId++) {
    TxPosition position=txStartPositionCache.get(txId);
    if (position == null)     return null;
    if (earliest == null || position.earlierThan(earliest)) {
      earliest=position;
    }
  }
  return earliest;
}
