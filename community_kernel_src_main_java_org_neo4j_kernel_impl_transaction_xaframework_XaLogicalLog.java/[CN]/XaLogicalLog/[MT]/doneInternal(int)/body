{
  if (writeBuffer != null) {
    LogIoUtils.writeDone(writeBuffer,identifier);
  }
 else {
    sharedBuffer.clear();
    LogIoUtils.writeDone(sharedBuffer,identifier);
    sharedBuffer.flip();
    fileChannel.write(sharedBuffer);
  }
  xidIdentMap.remove(identifier);
  fileChannel.force(false);
}
