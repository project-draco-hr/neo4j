{
  channels=new DefaultChannelGroup();
  clientBootstrap=new ClientBootstrap(new NioClientSocketChannelFactory(Executors.newSingleThreadExecutor(new NamedThreadFactory("Cluster client boss",monitor)),Executors.newFixedThreadPool(2,new NamedThreadFactory("Cluster client worker",monitor)),2));
  clientBootstrap.setOption("tcpNoDelay",true);
  clientBootstrap.setPipelineFactory(new NetworkNodePipelineFactory());
}
