{
  if (fileChannel == null) {
    throw new IOException("Closed id generator");
  }
  if (defragedIdList.size() > 0) {
    int id=defragedIdList.removeFirst();
    if (haveMore && defragedIdList.size() == 0) {
      readIdBatch();
    }
    return id;
  }
  if (nextFreeId < 0) {
    throw new IOException("Capacity exceeded");
  }
  return nextFreeId++;
}
